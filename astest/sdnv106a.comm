                E = 20000.,
                RHO=7900.0,
                NU = 0.49999),
                ECRO_LINE=_F(SY = 0.1,D_SIGM_EPSI = 200.),);
#
#  Materiau "mou" dont le module elastique vaut le module plastique
#  du materiau precedant MATE_DUR
#
MATE_MOU=DEFI_MATERIAU(ELAS=_F( 
                E = 200.,
                RHO=7900.0,
                NU = 0.49999),
                ECRO_LINE=_F(SY = 0.1,D_SIGM_EPSI = 200.),);
