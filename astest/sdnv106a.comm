                       DDL_IMPO=(
                _F( GROUP_NO = 'A',  DX=0., DY=0., DZ=0.),
                _F( GROUP_NO = 'B',  DY=0., DZ=0.),
                _F( GROUP_NO = 'D',  DX=0., DZ=0.),
                _F( GROUP_NO = 'C',  DZ=0.),
                _F( GROUP_NO = 'E',  DX=0., DY=0.,),
                _F( GROUP_NO = 'F',  DY=0.,),
                _F( GROUP_NO = 'H',  DX=0.,),
