  vitesse = vite_min+i*pas
  vite.append(vitesse)

SPRJ  = [None]*nb_poin
RPM0  = [None]*nb_poin
RPP0  = [None]*nb_poin
DYN0  = [None]*nb_poin

for i in range(len(vite)):
