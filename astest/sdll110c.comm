                       SPEC_TURB=SPECTRE1,
                       NB_POIN=1024,
                       FREQ_INIT= 10.2222 ,
                       FREQ_FIN= 30.6666 ,
                       TOUT_CMP='NON',
                       TOUT='OUI',
                       BASE_ELAS_FLUI=MELFLUI0,
                    VITE_FLUI=vite[i],
