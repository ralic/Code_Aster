DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))


C1_I=187.*341.
C2_I=29.*17184.
young=145200.
k=0.43
w=6.09
g1=341.
g2=17184.

# LE C_I DU MODELE VISC_CIN1_CHAB EST DETERMINE PAR C_I = C1*A1
MAT=DEFI_MATERIAU(
                                    ELAS=_F(   E = young,
                                            NU = .3,
                                            ALPHA = 0.,          ),
                           CIN2_CHAB=_F(   R_I = 151.,
                                            R_0 = 87.,
                                            B = 2.3,
                                            K = k,
                                            W = w,
                                            C1_I = C1_I,
                                            C2_I = C2_I,
                                            G1_0 = g1,
                                            G2_0 = g2,
                                             ))

MATF=DEFI_MATERIAU(ELAS=_F(E=young,
                           NU=0.3),
                   Chaboche=_F(YoungModulus = young ,
                               PoissonRatio = 0.3 ,
                               R_inf  = 151.,
                               R_0  = 87.,
                               b  = 2.3,
                               k  = k,
                               w  = w,
                               C_inf_0 = C1_I,
                               C_inf_1 = C2_I,
                               g_0_0 = g1,
                               g_0_1 = g2,
                               a_inf = 1.,),)



LINST=DEFI_LIST_REEL(              DEBUT=0.,INTERVALLE=(
                              _F( JUSQU_A = 0.4, NOMBRE = 1),
                              _F( JUSQU_A = 1.435, NOMBRE = 12)))

