#       ELEMENTS DE POUTRE, METHODES LAGRANGIENNE ET CONTINUE
# CAS TEST  SSNV505H
# MODELE
# MAILLAGE : ELEMENT DE POUTRES SEG2 EN MODELISATION 3D
# UNITES   : NEWTON , MILLIMETRE , SECONDE (N,MM,S)
#
#

DEBUT(CODE=_F( NOM = 'SSNV505H',NIV_PUB_WEB='INTERNET'));

# ---------------------  MAILLAGE ----------------

PRE_GIBI();

#Acquisition du maillage des poutres encastrees

MAILL1=LIRE_MAILLAGE();

MAILL1=MODI_MAILLAGE(reuse=MAILL1,
                     MAILLAGE=MAILL1,
                     ORIE_LIGNE=(_F(GROUP_MA='POU1',
                                    VECT_TANG=( 1,0,0),
                                    GROUP_NO='A',),
                                 _F(GROUP_MA='POU2',
                                    VECT_TANG=(-1,0,0),
                                    GROUP_NO='B',),
                                 ),);

MAILL1=DEFI_GROUP(reuse =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_NO=(_F(GROUP_MA=('POU1',),
                                   NOM='GROUP1',),
                                 _F(GROUP_MA=('POU2',),
                                   NOM='GROUP2',),
                                 ));

#Acquisition du maillage de la section en fibres

PRE_GIBI(UNITE_GIBI=21,UNITE_MAILLAGE=31);

MA_SECT=LIRE_MAILLAGE(UNITE=31);


# ---- MODELE : POUTRE DROITE D'EULER ----

MODE=AFFE_MODELE(MAILLAGE=MAILL1,
                 VERIF='MAILLE',
                 AFFE=(_F(GROUP_MA='POU1',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_TGM',),
                       _F(GROUP_MA='POU2',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='POU_D_TGM',),
                       ),);

# ------------- DEFINITON DU MATERIAU -----------

MPOUTRE=DEFI_MATERIAU(ELAS=_F(E=6.8948E3,NU=0.33333,RHO=7800.,),);

GF=DEFI_GEOM_FIBRE(
   SECTION =_F(GROUP_FIBRE='SECF',
             GROUP_MA_SECT='SECTION',
             MAILLAGE_SECT=MA_SECT,
            COOR_AXE_POUTRE=(0.,0.,),),)

COMPF=DEFI_COMPOR(GEOM_FIBRE=GF,MATER_SECT=MPOUTRE,
                  MULTIFIBRE=_F(GROUP_FIBRE='SECF',MATER=MPOUTRE,
                                 RELATION='ELAS',DEFORMATION='GROT_GDEP',),
                     )


CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                  AFFE=_F(GROUP_MA=('POU1','POU2'),MATER=MPOUTRE),
                  AFFE_COMPOR=_F(GROUP_MA=('POU1','POU2'),COMPOR=COMPF),
                 );

CHAMPCAR=AFFE_CARA_ELEM(MODELE=MODE,INFO=1,
                      POUTRE=_F(GROUP_MA=('POU1','POU2'),
                                SECTION='GENERALE',
                                CARA=('A','IY','IZ','AY','AZ','JX','JG','IYR2','IZR2',),
                                VALE=(645.16,5.54975E3,2.167872E5,1.2,1.2,
                                      5.08,0.0,0.0,0.),),
                      GEOM_FIBRE=GF,
                      MULTIFIBRE=_F(GROUP_MA=('POU1','POU2'),
                                   GROUP_FIBRE='SECF',),);

# ---------- CONDITIONS AUX LIMITES --------------

COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO=('A','B',),
                                     DX=0.0,DY=0.0,DZ=0.0,
                                     DRX=0.0,DRY=0.0,DRZ=0.0),
                                  _F(GROUP_NO=('GROUP1','GROUP2'),
                                     DZ=0.0,DRX=0.0,DRY=0.0,),
                                  ),);

CH1=AFFE_CHAR_MECA(MODELE=MODE,
                   DDL_IMPO=(_F(GROUP_NO=('C'),
                                DY=-790.,),
                             ),);

# ---- METHODE LAGRANGIENNE ----
F_DIST_M=DEFI_CONSTANTE(VALE=31.75);
F_DIST_E=DEFI_CONSTANTE(VALE=31.75);

CHLAG= DEFI_CONTACT(MODELE         = MODE,
                    FORMULATION    = 'DISCRETE',
                    FROTTEMENT     = 'COULOMB',
                    ZONE =(
                           _F(
                              GROUP_MA_MAIT  = 'POU1',
                              GROUP_MA_ESCL  = 'POU2',
                              ALGO_CONT      = 'LAGRANGIEN',
                              ALGO_FROT      = 'LAGRANGIEN',
                              COULOMB        = 0.,
                              COEF_MATR_FROT = 1.,
                              VECT_MAIT      = 'VECT_Y',
                              MAIT_VECT_Y    = (0.,0.,-1.,),
                              DIST_MAIT      = F_DIST_M,
                              DIST_ESCL      = F_DIST_E,
                              ),
                          ),
                    INFO = 1,
                   );

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

#L_INST=DEFI_LIST_REEL(DEBUT=0.0,
#                      INTERVALLE=(_F(JUSQU_A=1.,
#                                     NOMBRE=50,),),);

L_ARCH=DEFI_LIST_REEL(DEBUT=0.0,
                                     INTERVALLE=(_F(JUSQU_A=.1,
                                     NOMBRE=25,),),);
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=50,),),);

ULAG=STAT_NON_LINE(MODELE=MODE,
                CHAM_MATER=CM,
                CARA_ELEM=CHAMPCAR,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                      ),    
                SOLVEUR=_F(SYME='OUI'),
                CONTACT  = CHLAG,
                COMP_INCR=_F(RELATION='MULTIFIBRE'),
                INCREMENT=_F(LIST_INST=L_INST,
                             ),
                NEWTON=_F(REAC_ITER=1,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=30,),
                INFO=1,);

ULAG=CALC_NO(reuse =ULAG,
          RESULTAT=ULAG,

          OPTION='FORC_NODA',


         );

# ---- TABLEAUX ----

T_REA_LA=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('A','B'),
                                 RESULTAT=ULAG,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);

T_DEP_LA=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                                 GROUP_NO=('C','A1','B1'),
                                 RESULTAT=ULAG,
                                 NOM_CHAM='DEPL',
                                 TOUT_CMP='OUI',
                                 OPERATION='EXTRACTION',),);

#IMPR_TABLE(TABLE=T_REA_LA,);
#IMPR_TABLE(TABLE=T_DEP_LA,);

#IMPR_RESU(FORMAT='MED',RESU=_F(RESULTAT=ULAG))

TEST_TABLE(TABLE=T_REA_LA,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.2),
           VALE=3.11885E2,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.7',);

TEST_TABLE(TABLE=T_REA_LA,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4),
           VALE=1.20897E3,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.7',);

TEST_TABLE(TABLE=T_REA_LA,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.6),
           VALE=2414.5320729111,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='10.3.1',);

TEST_TABLE(TABLE=T_REA_LA,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.8),
           VALE=2095.6286117451,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='10.3.1',);

TEST_TABLE(TABLE=T_REA_LA,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.),
           VALE=2.442544E3,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='10.1.4',);

#
# ---- METHODE CONTINUE ----
#      ________________

CHCON= DEFI_CONTACT(MODELE         = MODE,
                    FORMULATION    = 'CONTINUE',
                    ITER_CONT_MAXI = 10,
                    ZONE =(
                           _F(
                              GROUP_MA_MAIT  = 'POU1',
                              GROUP_MA_ESCL  = 'POU2',
                              VECT_MAIT      = 'VECT_Y',
                              MAIT_VECT_Y    = (0.,0.,-1.,),
                              DIST_MAIT      = F_DIST_M,
                              DIST_ESCL      = F_DIST_E,
                             ),
                          ),
                   );


UCON=STAT_NON_LINE(MODELE=MODE,
                CHAM_MATER=CM,
                CARA_ELEM=CHAMPCAR,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                      ),
                CONTACT  = CHCON,
                SOLVEUR=_F(RENUM='MDA'),
                COMP_INCR=_F(RELATION='MULTIFIBRE'),
                INCREMENT=_F(LIST_INST=L_INST,
                             ),
                NEWTON=_F(REAC_ITER=1,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20,),
                INFO=1,);

UCON=CALC_NO(reuse =UCON,
          RESULTAT=UCON,

          OPTION='FORC_NODA',


         );

# ---- TABLEAUX ----

T_REA_CO=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('A','B'),
                                 RESULTAT=UCON,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);

T_DEP_CO=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                                 GROUP_NO=('C','A1','B1'),
                                 RESULTAT=UCON,
                                 NOM_CHAM='DEPL',
                                 TOUT_CMP='OUI',
                                 OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=T_REA_CO,);
IMPR_TABLE(TABLE=T_DEP_CO,);

TEST_TABLE(TABLE=T_REA_CO,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.2),
           VALE=3.11885E2,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.7',);

TEST_TABLE(TABLE=T_REA_CO,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4),
           VALE= 1.21043E3,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.7',);

TEST_TABLE(TABLE=T_REA_CO,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.6),
           VALE=2432.3157105805,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='10.3.1',);

TEST_TABLE(TABLE=T_REA_CO,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.8),
           VALE=2085.1589042336,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='10.3.2',);

TEST_TABLE(TABLE=T_REA_CO,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.),
           VALE=2.34233E3,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='8.3.7',);
#
# ---- METHODE CONTINUE AVEC NORMALE ESCLAVE ----
#      __________________

CHESCL= DEFI_CONTACT(MODELE         = MODE,
                    FORMULATION     = 'CONTINUE',
                    ITER_CONT_TYPE  = 'MULT',
                    ITER_CONT_MULT  = 4,
                    ZONE =(
                           _F(
                              GROUP_MA_MAIT  = 'POU1',
                              GROUP_MA_ESCL  = 'POU2',
                              DIST_MAIT      = F_DIST_M,
                              DIST_ESCL      = F_DIST_E,
                              NORMALE        = 'ESCL',
                              VECT_ESCL      = 'VECT_Y',
                              ESCL_VECT_Y    = (0.,0.,-1.,),
                             ),
                          ),
                   );

UESCL=STAT_NON_LINE(MODELE=MODE,
                CHAM_MATER=CM,
                CARA_ELEM=CHAMPCAR,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                      ),
                CONTACT  = CHESCL,
                SOLVEUR=_F(RENUM='MDA'),
                COMP_INCR=_F(RELATION='MULTIFIBRE'),
                INCREMENT=_F(LIST_INST=L_INST,
                             INST_FIN=0.94,
                             ),
                NEWTON=_F(REAC_ITER=1,),
                CONVERGENCE=_F(ITER_GLOB_MAXI=50,),
                INFO=1,);

UESCL=CALC_NO(reuse =UESCL,
          RESULTAT=UESCL,

          OPTION='FORC_NODA',


         );

# ---- TABLEAUX ----

T_REA_ES=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('A','B'),
                                 RESULTAT=UESCL,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);

T_DEP_ES=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                                 GROUP_NO=('C','A1','B1'),
                                 RESULTAT=UESCL,
                                 NOM_CHAM='DEPL',
                                 TOUT_CMP='OUI',
                                 OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=T_REA_ES,);
IMPR_TABLE(TABLE=T_DEP_ES,);

IMPR_RESU(FORMAT='MED',RESU=_F(RESULTAT=UESCL))

TEST_TABLE(TABLE=T_REA_ES,
           NOM_PARA='DY',
