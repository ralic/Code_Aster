DEBUT(CODE=_F(NOM='SSNV129B',
              NIV_PUB_WEB='INTERNET',),);

PRE_GIBI();

MA1=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI',),);

MA=CREA_MAILLAGE(MAILLAGE=MA1,
                 MODI_MAILLE=(_F(GROUP_MA='Q21',
                                 OPTION='QUAD8_9',),
                              _F(GROUP_MA='Q1',
                                 OPTION='QUAD8_9',),),);


MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=(_F(GROUP_MA=('Q1','Q21',),
                        PHENOMENE='MECANIQUE',
                        MODELISATION='COQUE_3D',),
                     ),);


MA=DEFI_GROUP(reuse =MA,
               MAILLAGE=MA,
               CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);


CAREL=AFFE_CARA_ELEM(MODELE=MO,
                     COQUE=(_F(GROUP_MA='Q21',
                               EPAIS=0.5,),
                            _F(GROUP_MA='Q1',
                               EPAIS=0.5,),),);


MAT=DEFI_MATERIAU(ELAS=_F(E=200000000000.0,
                          NU=0.3,),);


CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MAT,),);


CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    PRES_REP=_F(GROUP_MA='Q21',
                                PRES=-250000000.0,),);

CHA0=AFFE_CHAR_CINE(MODELE=MO,
                    MECA_IMPO=(
                              _F(GROUP_NO='CENTE1',
                                 DX=0.0,
                                 DY=0.0,
                                 DRX=0.0,
                                 DRY=0.0,
                                 DRZ=0.0,),
                              _F(GROUP_NO='CENTE2',
                                 DX=0.0,
                                 DY=0.0,
                                 DRX=0.0,
                                 DRY=0.0,
                                 DRZ=0.0,),
                              _F(GROUP_NO=('DR1','DR21'),
                                 DY=0.0,
                                 DZ=0.0,
                                 DRY=0.0,),
                              _F(GROUP_NO=('DR2','DR22'),
                                 DX=0.0,
                                 DZ=0.0,
                                 DRX=0.0,),
                              _F(GROUP_NO=('DR3','DR23'),
                                 DY=0.0,
                                 DZ=0.0, 
                                 DRY=0.0,),
                              _F(GROUP_NO=('DR4','DR24'),
                                 DX=0.0,
                                 DZ=0.0,
                                 DRX=0.0,),
                                 ),);

CHA2=DEFI_CONTACT(MODELE         = MO,
                  FORMULATION    = 'DISCRETE',
                  REAC_GEOM      = 'SANS',
                  ZONE=_F(
                          GROUP_MA_MAIT='Q1',
                          GROUP_MA_ESCL='Q21',
                          SANS_GROUP_NO=('DR1','DR2','DR3','DR4',
                                              'DR21','DR22','DR23','DR24',),
                        )
                  )

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,
                          1.0,1.0,
                          ),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);


L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);


RESU=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                   MODELE=MO,
                   CHAM_MATER=CHMAT,
                   CARA_ELEM=CAREL,
                   EXCIT=(_F(CHARGE=CHA1,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=CHA0,),
                          ),
                   CONTACT  = CHA2,       
                   COMP_INCR=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   INFO=1,);


RESU=CALC_ELEM(reuse =RESU,
               RESULTAT=RESU,
               OPTION='SIGM_ELNO',);

RESUPOS=POST_CHAMP(RESULTAT=RESU,
                   TOUT='OUI',
                   EXTR_COQUE=_F(NOM_CHAM='SIGM_ELNO',
                                 NUME_COUCHE=1,
                                 NIVE_COUCHE='INF',),);

RESUPOS=CALC_NO(reuse =RESUPOS,
                RESULTAT=RESUPOS,
                OPTION='SIGM_NOEU',);

IMPR_RESU(MODELE=MO,
          FORMAT='GMSH',
          UNITE=37,
          RESU=(_F(RESULTAT=RESU,
                   NOM_CHAM=('DEPL',),),
                _F(RESULTAT=RESUPOS,
                   NOM_CHAM=('SIGM_NOEU',),),),);

TEST_RESU(RESU=(_F(RESULTAT=RESU,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   GROUP_NO='CENTE1',
                   VALE=-0.3426875,
                   CRITERE='RELATIF',
                   PRECISION=0.042,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=RESUPOS,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIYY',
                   GROUP_NO='CENTE1',
                   VALE=7.6275e9,
                   CRITERE='RELATIF',
                   PRECISION=0.005,
                   REFERENCE='ANALYTIQUE',),
                   ),);

TEST_RESU(RESU=(_F(RESULTAT=RESUPOS,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIYY',
                   GROUP_NO='A1',
                   VALE=-4.4943602357159E+05,
                   CRITERE='RELATIF',
                   PRECISION=1.e-7,
                   REFERENCE='NON_REGRESSION',
                   VERSION = '9.1.22',),
                _F(RESULTAT=RESUPOS,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIYY',
                   GROUP_NO='A3',
                   VALE=4.1184276536402E+05,
                   CRITERE='RELATIF',
                   PRECISION=1.e-7,
                   REFERENCE='NON_REGRESSION',
                   VERSION = '9.1.22',),  
                _F(RESULTAT=RESUPOS,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIYY',
                   GROUP_NO='B1',
                   VALE=-4.4943602349259E+05,
                   CRITERE='RELATIF',
                   PRECISION=1.e-7,
                   REFERENCE='NON_REGRESSION',
                   VERSION = '9.1.22',),  
                _F(RESULTAT=RESUPOS,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_NOEU',
                   NOM_CMP='SIYY',
                   GROUP_NO='B3',
                   VALE=4.1184276528115E+05,
                   CRITERE='RELATIF',
                   PRECISION=1.e-7,
                   REFERENCE='NON_REGRESSION',
                   VERSION = '9.1.22',),  
                   ),);

FIN();
