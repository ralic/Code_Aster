                     INTERPOL=('LIN','LOG',),
                       INFO=2,
                    PARA=(0.0, 1.0, ),
                    PROL_GAUCHE='CONSTANT',
                    PROL_DROITE='CONSTANT',
                    FONCTION=(ACCX2, ACCX2,)
                    )
                    
