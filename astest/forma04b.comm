DEBUT(CODE=_F(NOM='FORMA04B',NIV_PUB_WEB='INTERNET'),IGNORE_ALARM='CONTACT3_16',DEBUG=_F(SDVERI='OUI'))
# On ignore l'alarme concernant VALE_CONT qui n'est pas cree

# -- Parametres du calcul

E  =  20000.
nu =  0.3
h  =  2.
R  =  50.

# -- Lecture du maillage et affectation du modele, des chargements

MA=LIRE_MAILLAGE(FORMAT='MED',);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=_F(GROUP_MA=('CONT1','CONT2',),),);

MAT=DEFI_MATERIAU(ELAS=_F(E=E,
                          NU=nu,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(GROUP_MA=('SPH_HAU','SPH_BAS',),
                            MATER=MAT,),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(GROUP_MA=('SPH_HAU','SPH_BAS',),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

CL=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=(_F(GROUP_MA='A1A2',
                               DX=0,),
                            _F(GROUP_MA='A2B2',
                               DY=-h,),
                            _F(GROUP_MA='A1B1',
                               DY=h,),),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',VALE=(0,0,
                          1,1,
                          ),);

# -- Definition du contact

CONT=DEFI_CONTACT(MODELE=MO,
                  FORMULATION='CONTINUE',
                  ZONE=_F(GROUP_MA_MAIT='CONT2',
                          GROUP_MA_ESCL='CONT1',
                          INTEGRATION='GAUSS',
                          CONTACT_INIT='OUI',
                          ),);

TPS=DEFI_LIST_REEL(DEBUT=0,
                   INTERVALLE=_F(JUSQU_A=1,
                                 NOMBRE=20,),);

STNL=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=_F(CHARGE=CL,
                            FONC_MULT=RAMPE,),
                   CONTACT=CONT,
                   COMP_INCR=_F(RELATION='ELAS',),
                   NEWTON=_F(REAC_ITER=1,MATRICE='ELASTIQUE'),
                   INCREMENT=_F(LIST_INST=TPS,),
                   );

# -- Post-traitement

STNL=CALC_ELEM(reuse =STNL,
               RESULTAT=STNL,
               OPTION=('SIEQ_ELGA','SIEQ_ELNO','SIEF_ELNO',),);

STNL=CALC_CHAMP(reuse =STNL,
             RESULTAT=STNL,
             CONTRAINTE='SIEF_NOEU',CRITERES='SIEQ_NOEU');

# -- Groupe de noeuds oriente depuis le centre de la zone de contact esclave

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(OPTION='NOEUD_ORDO',
                               NOM='CONT_ord',
                               GROUP_MA='CONT1',
                               GROUP_NO_ORIG='C1',),);

# -- Pression de contact extraite sur le bord oriente

LAGS_abs=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='LAGS_C en fonction de ABSC_CURV',
                                 RESULTAT=STNL,
                                 NOM_CHAM='DEPL',
                                 INST=1.0,
                                 GROUP_NO='CONT_ord',
                                 NOM_CMP='LAGS_C',),);

IMPR_TABLE(TABLE=LAGS_abs,);

# -- Trace de la pression et de la solution analytique

LAGS_f=RECU_FONCTION(TABLE=LAGS_abs,
                     PARA_X='ABSC_CURV',
                     PARA_Y='LAGS_C',);

V_ABSC=DEFI_LIST_REEL(DEBUT=0,
                      INTERVALLE=_F(JUSQU_A=10,
                                    NOMBRE=100,),);


# -- Pression de hertz et demi-largeur de contact

p0 = -E/(pi*(1.0-nu*nu))*sqrt(2.0*2.0*h/R)
a  = sqrt(R*h)

PRES = FORMULE(VALE='p0*sqrt(1.0-(x/a)*(x/a))',
               NOM_PARA='x',);

PRES_sol=CALC_FONC_INTERP(FONCTION=PRES,
                          LIST_PARA=V_ABSC,
                          NOM_PARA='x',);

# -- Validation des resultats

TEST_RESU(RESU=_F(RESULTAT=STNL,
                  INST=1.0,
                  NOM_CHAM='DEPL',
                  NOM_CMP='LAGS_C',
                  GROUP_NO='C1',
                  VALE=p0,
                  CRITERE='RELATIF',
                  PRECISION=0.03,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=STNL,
                  INST=1.0,
                  NOM_CHAM='DEPL',
                  NOM_CMP='LAGS_C',
                  GROUP_NO='C1',
                  VALE=-2866.7425041924,
                  CRITERE='RELATIF',
                  VERSION='11.0.25',
                  REFERENCE='NON_REGRESSION',),);

# -- Impressions des resultats

IMPR_FONCTION(FORMAT='XMGRACE',
              PILOTE='',
              UNITE=29,
              COURBE=(_F(FONCTION=LAGS_f,),
                      _F(FONCTION=PRES_sol,),),
              TITRE='Pression de Hertz',
              BORNE_X=(0,20,),
              BORNE_Y=(-3000,0,),
              GRILLE_X=1,
              GRILLE_Y=500,
              LEGENDE_X='Abscisse curviligne (mm)',
              LEGENDE_Y='Pression (Mpa)',);

IMPR_RESU(FORMAT='MED',
          RESU=_F(RESULTAT=STNL,),);

FIN();
