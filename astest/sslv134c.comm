# QUART DE FISSURE. TEST DE POST_K1_K2_K3. MAILLAGE BARSOUM

DEBUT(CODE=_F(NOM='SSLV134C', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

PRE_GIBI()

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),)

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
                     CREA_GROUP_NO=_F(
                      GROUP_MA = ('LP8P9', 'LP8P9S', 'LP0P11', 'LP0P8', 'LP0P8S',))
                 )

MO=AFFE_MODELE(  MAILLAGE=MA,
                      AFFE=_F(  TOUT = 'OUI',
                             PHENOMENE = 'MECANIQUE',
                             MODELISATION = 'AXIS') )

MAT=DEFI_MATERIAU(ELAS=_F(  E = 2.E11,
                             NU = 0.3,
                             ALPHA = 0.,
                             RHO = 7850,
                     ) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',
                              MATER = MAT) )

SYMETRIE=AFFE_CHAR_MECA(  MODELE=MO,DDL_IMPO=(
                _F(  GROUP_NO = ('LP8P9', 'LP8P9S',), DX = 0.),
                         _F(  GROUP_NO = 'LP0P11',  DY = 0.))
                         )

PRESSION=AFFE_CHAR_MECA(    MODELE=MO,FORCE_CONTOUR=(
                  _F(  GROUP_MA = 'LP9P10',  FY = 1.E6),
                  _F(  GROUP_MA = 'LP9P10S', FY = -1.E6))
                          )

CHAMDEPL=MECA_STATIQUE(        MODELE=MO,
                              CHAM_MATER=CHMAT,EXCIT=(
                              _F( CHARGE = PRESSION),
                              _F( CHARGE = SYMETRIE))
                           )

CHAMDEPL=CALC_ELEM( reuse=CHAMDEPL,
                           OPTION='SIGM_ELNO',
                         RESULTAT=CHAMDEPL )

FISS=DEFI_FOND_FISS(    MAILLAGE=MA,
                            FOND_FISS=_F( GROUP_NO = ('P0',)),
                            NORMALE=(0., 1., 0.,)
                       )

RSUP=0.25
RINF=0.05

THETA01=CALC_THETA(    MODELE=MO,
                         THETA_2D=_F(  GROUP_NO = ('P0',),
                                    MODULE = 1.,
                                    R_INF = RINF,
                                    R_SUP = RSUP),
                         DIRECTION=(1., 0., 0.,)
                     )

G0=CALC_G        (       RESULTAT=CHAMDEPL,
                         NUME_ORDRE = 1,INFO=2,
                         THETA=_F(THETA=THETA01),
                     )

              
GK0=CALC_G(THETA=_F(FOND_FISS=FISS,THETA=THETA01,),
          RESULTAT=CHAMDEPL,
          NUME_ORDRE=1,
          INFO=2,
          OPTION='CALC_K_G',);
          
IMPR_TABLE(TABLE=GK0,),

# POST_K1_K2_K3


MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,CREA_GROUP_NO=(
                   _F(  NOM = 'L1INF',
                                   OPTION = 'SEGM_DROI_ORDO',
                                   PRECISION = 1.E-3,
                                   CRITERE = 'ABSOLU',
                                   GROUP_NO = 'LP0P8S',
                                   GROUP_NO_ORIG = 'P0',
                                   GROUP_NO_EXTR = 'P8'),
                   _F(  NOM = 'L1SUP',
                                   OPTION = 'SEGM_DROI_ORDO',
                                   PRECISION = 1.E-3,
                                   CRITERE = 'ABSOLU',
                                   GROUP_NO = 'LP0P8',
                                   GROUP_NO_ORIG = 'P0',
                                   GROUP_NO_EXTR = 'P8'))
                 )

DEPH1=POST_RELEVE_T( ACTION=_F(  INTITULE = 'TAB2',
                                GROUP_NO = 'L1SUP',
                                RESULTAT = CHAMDEPL,
                                NOM_CHAM = 'DEPL',
                                NOM_CMP = ('DX', 'DY',),
                                OPERATION = 'EXTRACTION')
                      )


DEPB1=POST_RELEVE_T( ACTION=_F(  INTITULE = 'TAB1',
                                GROUP_NO = 'L1INF',
                                RESULTAT = CHAMDEPL,
                                NOM_CHAM = 'DEPL',
                                NOM_CMP = ('DX', 'DY',),
                                OPERATION = 'EXTRACTION')
                      )

TABL_K1=POST_K1_K2_K3(  MODELISATION='AXIS',
                        TABL_DEPL_SUP=DEPH1,
                        TABL_DEPL_INF=DEPB1,
                        INFO=2,
                        MATER=MAT,
                        ABSC_CURV_MAXI=RSUP,
                        VECT_K1=(0., 1., 0.,)
                     )

IMPR_TABLE( TABLE=TABL_K1 )


# ---------------
# VALEUR DE REFERENCE MILIEU INFINI : G=145.6
GLOC=11.58648
K1=1.595769E6

# POUR UN RADIAN
GREF=23.17296


TEST_TABLE(       TABLE=G0,
                  NOM_PARA='G',
                   REFERENCE='ANALYTIQUE',
                   VALE=GREF,
                 PRECISION=0.018        )

TEST_TABLE(       TABLE=GK0,
                  NOM_PARA='G',
                   REFERENCE='ANALYTIQUE',
                   VALE=GLOC,
                 PRECISION=0.018        )

TEST_TABLE(       TABLE=GK0,
                  NOM_PARA='K1',
                   REFERENCE='ANALYTIQUE',
                   VALE=K1,
                 PRECISION=0.03        )

TEST_TABLE(       TABLE=GK0,
                  NOM_PARA='K1',
                   REFERENCE='NON_REGRESSION',
                   VALE=1.64307E+06,
                 PRECISION=0.0001        )
                 
TEST_TABLE(       TABLE=TABL_K1,
                      REFERENCE='ANALYTIQUE',
                       NOM_PARA='K1',
                           VALE=K1,
                      PRECISION=0.015           )

TEST_TABLE(       TABLE=TABL_K1,
                      REFERENCE='ANALYTIQUE',
                       NOM_PARA='G',
                           VALE=GLOC,
                      PRECISION=0.03           )
                      
                      
# -------------------------------------------------------------
#           ON AJOUTE LA ROTATION POUR CALCULER K
#                    SOUS LA FORCE CONTRIGUGE 
# -------------------------------------------------------------
# LA VITESSE DE TOTATIO EST 150 TRS/MINUTE
VAL_ROT = 15.708

CHAR_ROT=AFFE_CHAR_MECA(  MODELE=MO,
                        DDL_IMPO=(
                          _F(  GROUP_NO = ('LP8P9', 'LP8P9S',), DX = 0.),
                          _F(  GROUP_NO = 'LP0P11',  DY = 0.)  ),
                          
                        FORCE_CONTOUR=(
                          _F(  GROUP_MA = 'LP9P10',  FY = 1.E6),
                          _F(  GROUP_MA = 'LP9P10S', FY = -1.E6)),
                         
                        ROTATION=_F(VITESSE = VAL_ROT,
                                AXE = (0.,1.,0.,),
                                CENTRE =(0.,0.,0.,),
                                  ), 

                                 );  
                                              
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);
                   
DEPL_ROT=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=_F(CHARGE=CHAR_ROT,), 
                   COMP_ELAS=_F(RELATION='ELAS',
                                ),
                   INCREMENT=_F(LIST_INST=L_INST,
                                ),
                   CONVERGENCE = _F(ITER_GLOB_MAXI = 100,),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);  
                                
IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(RESULTAT=DEPL_ROT));

G1=CALC_G        (       OPTION='CALC_G',
                         RESULTAT=DEPL_ROT,
                         NUME_ORDRE = 1,INFO=2,
                         THETA=_F(THETA=THETA01),
                         SYME_CHAR='SANS'
                     )

              
GK1=CALC_G(THETA=_F(FOND_FISS=FISS,THETA=THETA01,),
          RESULTAT=DEPL_ROT,
          NUME_ORDRE=1,
          SYME_CHAR='SANS',INFO=2,
          OPTION='CALC_K_G',);
          
IMPR_TABLE(TABLE=GK1,),

# POST_K1_K2_K3

DEPH2=POST_RELEVE_T( ACTION=_F(  INTITULE = 'TAB2',
                                GROUP_NO = 'L1SUP',
                                RESULTAT = DEPL_ROT,
                                NOM_CHAM = 'DEPL',
                                NOM_CMP = ('DX', 'DY',),
                                OPERATION = 'EXTRACTION')
                      )


DEPB2=POST_RELEVE_T( ACTION=_F(  INTITULE = 'TAB1',
                                GROUP_NO = 'L1INF',
                                RESULTAT = DEPL_ROT,
                                NOM_CHAM = 'DEPL',
                                NOM_CMP = ('DX', 'DY',),
                                OPERATION = 'EXTRACTION')
                      )

TABL_K2=POST_K1_K2_K3(  MODELISATION='AXIS',
                        TABL_DEPL_SUP=DEPH2,
                        TABL_DEPL_INF=DEPB2,
                        INFO=2,
                        MATER=MAT,
                        ABSC_CURV_MAXI=RSUP,
                        VECT_K1=(0., 1., 0.,)
                     )

IMPR_TABLE( TABLE=TABL_K2, ),


FIN()
#
