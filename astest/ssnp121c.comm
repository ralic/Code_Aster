DEBUT(CODE=_F(NOM='SSNP121C', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(GROUP_MA='AB',),
                             _F(GROUP_MA='CD',),
                             _F(GROUP_MA='EF',),
                             _F(GROUP_MA='FG',),
                             _F(GROUP_MA='GH',),
                             _F(GROUP_MA='HE',),),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=(_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='D_PLAN',),),);

MAT2=DEFI_MATERIAU(ELAS=_F(E=2000000.0,
                           NU=0.0,),);

MAT1=DEFI_MATERIAU(ELAS=_F(E=2000000.0,
                           NU=0.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA='PLAQUE1',
                             MATER=MAT2,),
                          _F(GROUP_MA='PLAQUE2',
                             MATER=MAT1,),),);

CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='CD',
                                 DX=0.0,
                                 DY=-0.1,),
                              _F(GROUP_NO='GH',
                                 DX=0.0,
                                 DY=0.0,),),);


CHA2   = DEFI_CONTACT(MODELE      = MO,
                      FORMULATION = 'DISCRETE',
                      ZONE=_F(
                               ALGO_CONT     = 'PENALISATION',
                               E_N           = 1.E7,
                               GROUP_MA_MAIT = 'AB',
                               GROUP_MA_ESCL = 'EF',
                             ),
                      );

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

FONC=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,  0.0,
                         1.0,  1.0,),);

RESU=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                   MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=(_F(CHARGE=CHA1,
                             FONC_MULT=FONC,),),
                   CONTACT  = CHA2,       
                   COMP_ELAS=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=L_INST),
                   NEWTON=_F(REAC_ITER=1),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=20));


RESU=CALC_ELEM(reuse =RESU,
               OPTION='SIEF_ELNO_ELGA',
               RESULTAT=RESU,);

RESU=CALC_NO(reuse =RESU,
             RESULTAT=RESU,
             OPTION='SIEF_NOEU_ELGA',
             );

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  NUME_ORDRE=1,
                  NOM_CHAM='SIEF_NOEU_ELGA',
                  NOM_CMP='SIYY',
                  NOEUD='N14',
                  VALE=-100000.0,
                  CRITERE='RELATIF',
                  PRECISION=0.0213,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  NUME_ORDRE=1,
                  NOM_CHAM='SIEF_NOEU_ELGA',
                  NOM_CMP='SIYY',
                  GROUP_NO='A',
                  VALE=-100000.0,
                  CRITERE='RELATIF',
                  PRECISION=0.075,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  NUME_ORDRE=1,
                  NOM_CHAM='SIEF_NOEU_ELGA',
                  NOM_CMP='SIYY',
                  GROUP_NO='B',
                  VALE=-100000.0,
                  CRITERE='RELATIF',
                  PRECISION=0.075,
                  REFERENCE='ANALYTIQUE',),);

# -------------------------------------------------------

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  NOEUD='N14',
                  VALE=-0.05,
                  CRITERE='RELATIF',
                  PRECISION=0.0213,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A',
                  VALE=-0.05,
                  CRITERE='RELATIF',
                  PRECISION=0.007,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='B',
                  VALE=-0.05,
                  CRITERE='RELATIF',
                  PRECISION=0.007,
                  REFERENCE='ANALYTIQUE',),);

FIN();

