#---------------------------------------------------------------------
#  - CALCUL QUASI-STATIQUE D'UNE POUTRE CONSTITUEE D'UN MATERIAU
#    ELASTIQUE PARFAITEMENT PLASTIQUE
#  - MODELISATION AVEC DES ELEMENTS 'COQUE_C_PLAN'
#---------------------------------------------------------------------
#
DEBUT(CODE=_F(NOM='SSNL501A',NIV_PUB_WEB='INTERNET'),);
PRE_GIBI();
MAIL=LIRE_MAILLAGE();
MOD=AFFE_MODELE(AFFE=_F(TOUT='OUI',
                        PHENOMENE='MECANIQUE',
                        MODELISATION='COQUE_C_PLAN',),
                MAILLAGE=MAIL,);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CAR_ELE=AFFE_CARA_ELEM(MODELE=MOD,
                       COQUE=_F(GROUP_MA='TOUT_ELT',COQUE_NCOU=3,
                                A_CIS=0.8333333,
                                EPAIS=0.025,),);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
MAT=DEFI_MATERIAU(ECRO_LINE=_F(D_SIGM_EPSI=0.,
                               SY=2.35000000E8,),
                  ELAS=_F(NU=0.3,
                          E=2.E11,),);
CHAM_MAT=AFFE_MATERIAU(AFFE=_F(MATER=MAT,
                               TOUT='OUI',),
                       MAILLAGE=MAIL,);
#
#---------------------------------------------------------------------
#                     CHARGEMENTS
#---------------------------------------------------------------------
#
CHAR0=AFFE_CHAR_MECA(DDL_IMPO=(_F(DX=0.0,
                                  DY=0.0,
                                  GROUP_NO='NO1',
                                  DRZ=0.0,),
                               _F(GROUP_NO='NO2',
                                  DRZ=0.0,),),
                     MODELE=MOD,
                     LIAISON_DDL=_F(DDL=('DX','DY'),
                                    COEF_MULT=(1.0,-4.915157031),
                                    COEF_IMPO=0.0,
                                    GROUP_NO=('NO2','NO2'),),);
PRES_LIM=AFFE_CHAR_MECA(MODELE=MOD,
                        FORCE_COQUE=_F(PRES=2.61111111E5,
                                       TOUT='OUI',),);
#
#---------------------------------------------------------------------
#                        RESOLUTION
#---------------------------------------------------------------------
#
LIS_INST=DEFI_LIST_REEL(DEBUT=0.,INTERVALLE=(
                             _F( JUSQU_A = 3.4,   NOMBRE = 17),
                             _F( JUSQU_A = 3.48,  NOMBRE = 5),
                             _F( JUSQU_A = 4.,  NOMBRE = 25),
                             _F( JUSQU_A = 5.2,  NOMBRE = 12),
                             _F( JUSQU_A = 6.0,  NOMBRE = 100),
                             _F( JUSQU_A = 7.0,  NOMBRE = 75),
                             _F( JUSQU_A = 8.0,  NOMBRE = 70),
                             _F( JUSQU_A = 9.0,  NOMBRE = 70),
                             _F( JUSQU_A = 10.0,  NOMBRE = 70),
                             _F( JUSQU_A = 11.0,  NOMBRE = 70),
                             _F( JUSQU_A = 12.0,  NOMBRE = 70),
                             _F( JUSQU_A = 13.,NOMBRE = 70),)
                           );
RESULTAT=STAT_NON_LINE(CHAM_MATER=CHAM_MAT,
                       MODELE=MOD,
                       CONVERGENCE=_F(ITER_GLOB_MAXI=30,
                                      RESI_GLOB_RELA=1.E-06,),
               COMP_INCR=_F(
                                    ITER_INTE_MAXI=30,
                                    RELATION='VMIS_ISOT_LINE',),
                       INCREMENT=_F(LIST_INST=LIS_INST,),
                       CARA_ELEM=CAR_ELE,
                       NEWTON=_F(REAC_ITER=1,
                                 MATRICE='TANGENTE',),
                       PILOTAGE=_F(TYPE='DDL_IMPO',
                                   GROUP_NO='NO2',
                                   NOM_CMP='DX',
                                   COEF_MULT=154.4086,),
                       EXCIT=(_F(CHARGE=CHAR0,),
                              _F(CHARGE=PRES_LIM,
                                 TYPE_CHARGE='FIXE_PILO',),),
                       SOLVEUR=_F(METHODE ='MULT_FRONT',
                                  RENUM='MDA'));
TABLE_DX=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='TAB_DX',
                                 GROUP_NO='NO2',
                                 NOM_CMP='DX',
                                 RESULTAT=RESULTAT,
                                 NOM_CHAM='DEPL',
                                 TOUT_ORDRE='OUI',),);
#IMPR_TABLE(TABLE=TABLE_DX,);
#RUX=RECU_FONCTION(TABLE=TABLE_DX,
#                  PARA_Y='DX',
#                  PARA_X='ABSC_CURV',);
TABLE_DY=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                                 INTITULE='TAB_DY',
                                 GROUP_NO='NO2',
                                 NOM_CMP='DY',
                                 RESULTAT=RESULTAT,
                                 NOM_CHAM='DEPL',
                                 TOUT_ORDRE='OUI',),);
#IMPR_TABLE(TABLE=TABLE_DY,);
#RUY=RECU_FONCTION(TABLE=TABLE_DY,
#                  PARA_Y='DY',
#                  PARA_X='ABSC_CURV',);
DEPX=RECU_FONCTION(RESULTAT=RESULTAT,
                   GROUP_NO='NO2',
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   TOUT_ORDRE='OUI',);
DEPY=RECU_FONCTION(RESULTAT=RESULTAT,
                   GROUP_NO='NO2',
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   TOUT_ORDRE='OUI',);
ETA=RECU_FONCTION(RESULTAT=RESULTAT,
                  NOM_PARA_RESU='ETA_PILOTAGE',
                  TOUT_ORDRE='OUI',);
IMPR_FONCTION(COURBE=_F(FONCTION=DEPX,),
            UNITE=25, UNITE_DIGR=26,
            FORMAT='AGRAF',
            TITRE='Depx en fonction du temps',);
IMPR_FONCTION(COURBE=_F(FONCTION=ETA,),
            UNITE=27, UNITE_DIGR=28,
            FORMAT='AGRAF',
            TITRE='Eta en fonction du temps',);
IMPR_FONCTION(COURBE=_F(FONCTION=DEPY,),
            UNITE=29, UNITE_DIGR=30,
            FORMAT='AGRAF',
            TITRE='Depy en fonction du temps',);
DEPL=CREA_CHAMP(NOM_CHAM='DEPL',
                TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                NUME_ORDRE=1,
                RESULTAT=RESULTAT,);
#
#---------------------------------------------------------------------
#                        VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#
TEST_RESU(RESU=(_F(PARA='ETA_PILOTAGE',
                   RESULTAT=RESULTAT,
                   PRECISION=0.07,
                   VALE=1.0,
                   NUME_ORDRE=17,),
                _F(PARA='ETA_PILOTAGE',
                   RESULTAT=RESULTAT,
                   PRECISION=0.07,
                   VALE=1.0,
                   NUME_ORDRE=500,),),);
FIN();
