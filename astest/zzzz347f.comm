#

MAILLAGE=LIRE_MAILLAGE(FORMAT='MED',    UNITE=20,)
IMPR_RESU(UNITE= 40,RESU=_F(MAILLAGE = MAILLAGE))
#


MODELE=AFFE_MODELE(
                 MAILLAGE=MAILLAGE,AFFE=(
                     _F(  GROUP_MA = 'BETON',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'DKT'),
                     _F(  GROUP_MA = 'CABLE',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'CABLE_GAINE')) )

#

CARAELEM=AFFE_CARA_ELEM(
                      MODELE=MODELE,
                       BARRE=_F(  GROUP_MA = 'CABLE',
                               SECTION = 'GENERALE',
                               CARA = ( 'A', ),
                               VALE = ( 2.5E-3, )),
                        COQUE=_F(  GROUP_MA = 'BETON',
                               EPAIS = 0.10,)

                       )

#

BETON=DEFI_MATERIAU(
                       ELAS=_F(  E = 4.5E+10,
                              NU = 0.),
                 BPEL_BETON=_F() )

#

ACIER=DEFI_MATERIAU(
                       ELAS=_F(  E = 1.85E+11,
                              NU = 0.),
                       CABLE_GAINE_FROT=_F(TYPE='FROTTANT',PENA_LAGR=1.e7,FROT_COURB=3.e-2,FROT_LINE=1.e-2),
