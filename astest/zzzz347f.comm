                METHODE = 'MUMPS',
                                      ),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 50,
                RESI_GLOB_RELA = 1.E-5),
                              INFO=1
              )

EVOLNOLI=CALC_CHAMP(reuse =EVOLNOLI,
                    FORCE=('FORC_NODA','REAC_NODA'),
                    CONTRAINTE  =('SIEF_ELNO','SIEF_NOEU',),
                    GROUP_MA=('CABLE'),
                    RESULTAT=EVOLNOLI,);
PROF_TEN = CREA_TABLE(RESU=_F(RESULTAT = EVOLNOLI, NOM_CHAM = 'SIEF_NOEU', NOM_CMP = 'N',GROUP_MA='CABLE'))
IMPR_TABLE(UNITE = 45, TABLE = PROF_TEN,NOM_PARA=('INST','NOEUD','N'))

