                       SOLVEUR=_F(METHODE='MUMPS',),
                       EXCIT=(
                              _F(CHARGE=CHARMECA,),
                                 ),
                       CABLE_BP=( CABLEPR ,),
                       INFO=1,);


IMPR_RESU(RESU=_F(RESULTAT = EVOLNOLI, NOM_CHAM='SIEF_ELGA', GROUP_MA='BETON',))


