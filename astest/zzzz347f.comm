          RESU=_F(RESULTAT=EVOLNOLI,
                  NOM_CHAM=('DEPL','SIEF_ELGA','FORC_NODA'),GROUP_MA = ('BETON'),NUME_ORDRE=(1,2)))

IMPR_RESU(
