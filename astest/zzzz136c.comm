                                CHEMIN = C1,
                                CHAM_GD = CHEL,
                                NOM_CMP = 'SIXX',
                                OPERATION = 'EXTRACTION'),
                  ) ;

IMPR_TABLE( TABLE=TS1,
            NOM_PARA =('CHEMIN','CHAM_GD','ABSC_CURV','SIXX'),
          ) ;

TEST_TABLE( TABLE=TS1,
            FILTRE=_F(NOM_PARA='ABSC_CURV',VALE=0.00000E+00),
            REFERENCE='ANALYTIQUE',PRECISION=1.E-08,
            NOM_PARA ='SIXX',VALE=4.75,
