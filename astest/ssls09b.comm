DEBUT(CODE=_F(NOM='SSLS09B',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

MAIL=LIRE_MAILLAGE();

#
# DEFINITION DU MODELE

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='AXIS',),);

#
# DEFINITION DU MATERIAU

ACIER=DEFI_MATERIAU(ELAS=_F(E=2.1E11,
                            NU=0.3,
                            RHO=8000.,
                            ALPHA=1.E-5,),);

#
# ATTRIBUTION DU MATERIAU

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);

#
# DEFINITION DES CONDITIONS AUX LIMITES ET CHARGEMENTS

CLIM=AFFE_CHAR_MECA(MODELE=MODELE,
                    DDL_IMPO=_F(GROUP_MA='APPUI',
                                DY=0.,),);

#

F_Y=DEFI_FONCTION(NOM_PARA='Y',VALE=(0.,-78500.,
                        3.,-78500.,
                        6.,-78500.,
                        ),);

CHARGT=AFFE_CHAR_MECA_F(MODELE=MODELE,
                        FORCE_INTERNE=_F(TOUT='OUI',
                                         FY=F_Y,),);

#
# RESOLUTION

DEF=MECA_STATIQUE(MODELE=MODELE,
                  CHAM_MATER=CHMAT,
                  EXCIT=(_F(CHARGE=CLIM,),
                         _F(CHARGE=CHARGT,),),);

#
# RECUPERATION DU CHAMP DE DEPLACEMENTS

U=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
             OPERATION='EXTR',
             RESULTAT=DEF,
             NOM_CHAM='DEPL',
             NUME_ORDRE=1,);

#
# CALCUL DES CONTRAINTES AUX NOEUDS

DEF=CALC_ELEM(reuse =DEF,
              RESULTAT=DEF,
              OPTION='SIGM_ELNO',);

#
# TEST_RESU DEPLACEMENTS

TEST_RESU(CHAM_NO=(_F(CHAM_GD=U,
                      NOEUD='N452',
                      NOM_CMP='DY',
                      VALE=-2.99e-06,
                      PRECISION=0.001,
                      REFERENCE='ANALYTIQUE',),
                   _F(CHAM_GD=U,
                      NOEUD='N504',
                      NOM_CMP='DY',
                      VALE=-2.99e-06,
                      PRECISION=0.001,
                      REFERENCE='ANALYTIQUE',),
                   _F(CHAM_GD=U,
                      NOEUD='N503',
                      NOM_CMP='DY',
                      VALE=-2.99e-06,
                      PRECISION=0.001,
                      REFERENCE='ANALYTIQUE',),),);

#

TEST_RESU(CHAM_NO=_F(CHAM_GD=U,
                     NOEUD='N51',
                     NOM_CMP='DX',
                     VALE=4.49e-07,
                     PRECISION=0.015,
                     REFERENCE='ANALYTIQUE',),);

#
# TEST_RESU CONTRAINTES

TEST_RESU(RESU=(_F(RESULTAT=DEF,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIYY',
                   MAILLE='M4',
                   NOEUD='N51',
                   VALE=-314000.0,
                   PRECISION=0.0001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=DEF,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIXX',
                   MAILLE='M4',
                   NOEUD='N1',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=20.2,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=DEF,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIXX',
                   MAILLE='M4',
                   NOEUD='N101',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=38.0,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=DEF,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIGM_ELNO',
                   NOM_CMP='SIXX',
                   MAILLE='M4',
                   NOEUD='N51',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=29.1,
                   REFERENCE='ANALYTIQUE',),),);

#--------------------------------------------------------------

CH_PESA=AFFE_CHAR_MECA(MODELE=MODELE,
                       PESANTEUR=_F(GRAVITE=9.8125,
                                    DIRECTION=(0.,-1.,0.,),),);

INSTANTS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=1.0,
                                      NOMBRE=1,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.0,0.0,
                          1.0,1.0,
                          ),);

DEF_PESA=STAT_NON_LINE(MODELE=MODELE,
                       CHAM_MATER=CHMAT,
                       EXCIT=(_F(CHARGE=CH_PESA,
                                 FONC_MULT=RAMPE,),
                              _F(CHARGE=CLIM,),),
                       COMP_ELAS=_F(RELATION='ELAS',),
                       INCREMENT=_F(LIST_INST=INSTANTS,),
                       NEWTON=_F(REAC_ITER=1,),);

DEF_PESA=CALC_ELEM(reuse =DEF_PESA,
                   RESULTAT=DEF_PESA,
                   OPTION='SIEF_ELNO',);

DEF_PESA=CALC_ERREUR(reuse =DEF_PESA,
                     RESULTAT=DEF_PESA,
                     OPTION=('ERME_ELEM','ERME_ELNO',),);

IMPR_RESU(RESU=_F(RESULTAT=DEF_PESA,
                  NOM_CHAM='ERME_ELEM',
                  NUME_ORDRE=1,),);

#
# TEST_RESU DEPLACEMENTS

TEST_RESU(RESU=(_F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   NOEUD='N452',
                   VALE=-2.99e-06,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   NOEUD='N504',
                   VALE=-2.99e-06,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   NOEUD='N503',
                   VALE=-2.99e-06,
                   PRECISION=0.001,
                   REFERENCE='ANALYTIQUE',),),);

#

TEST_RESU(RESU=_F(RESULTAT=DEF_PESA,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  NOEUD='N51',
                  VALE=4.49e-07,
                  PRECISION=0.015,
                  REFERENCE='ANALYTIQUE',),);

#---------------------------------------------------------------------
#    TEST EN CONTRAINTES

TEST_RESU(RESU=(_F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIEF_ELNO',
                   NOM_CMP='SIYY',
                   MAILLE='M4',
                   NOEUD='N51',
                   VALE=-314000.0,
                   PRECISION=0.0001,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIEF_ELNO',
                   NOM_CMP='SIXX',
                   MAILLE='M4',
                   NOEUD='N1',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=21.0,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='SIEF_ELNO',
                   NOM_CMP='SIXX',
                   MAILLE='M4',
                   NOEUD='N51',
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   PRECISION=30.0,
                   REFERENCE='ANALYTIQUE',),),);

#---------------------------------------------------------------------
#    ESTIMATEUR D'ERREUR EN RESIDU

TEST_RESU(RESU=(_F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELEM',
                   NOM_CMP='NUEST',
                   MAILLE='M4',
                   POINT=1,
                   VALE=1.64205236398e-08,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.15',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELNO',
                   NOM_CMP='ERREST',
                   MAILLE='M4',
                   NOEUD='N51',
                   VALE=1.45107857188e-06,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.15',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELNO',
                   NOM_CMP='TERMRE',
                   MAILLE='M7',
                   POINT=1,
                   VALE=1.234851e-06,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.15',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELNO',
                   NOM_CMP='TERMSA',
                   MAILLE='M7',
                   POINT=1,
                   VALE=2.28118e-07,
                   REFERENCE='NON_REGRESSION',
                   VERSION='9.03.15',),
                _F(RESULTAT=DEF_PESA,
                   NUME_ORDRE=1,
                   NOM_CHAM='ERME_ELNO',
                   NOM_CMP='TERMNO',
                   MAILLE='M7',
                   POINT=1,
                   VALE=0.0,
                   CRITERE='ABSOLU',
                   REFERENCE='NON_REGRESSION',
                   VERSION='8.01.22',),),);

FIN();

