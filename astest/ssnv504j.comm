DEBUT(CODE=_F(NOM='SSNV504J',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#_____________________________________________________________________
#
#  CALCUL VALIDATION DU PASSAGE DES SINGULARITES GEOMETRIQUES
#_____________________________________________________________________

MAILL1=LIRE_MAILLAGE(FORMAT='MED',);

MAILL1=DEFI_GROUP(reuse   =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_MA=_F(NOM='TOUT',
                                   UNION=('LOPIN','FILIERE')),
                  CREA_GROUP_NO=(_F(GROUP_MA='FIL_SYM1'),
                                 _F(GROUP_MA='FIL_SYM2'),
                                 _F(GROUP_MA='FIL_EXT'),
                                 _F(GROUP_MA='LOP_SYM1'),
                                 _F(GROUP_MA='LOP_SYM2'),
                                 _F(GROUP_MA='LOP_ESC1'),
                                 _F(GROUP_MA='LOP_DDL')));

#_____________________________________________________________________
#
#                    DEFINITION DU MODELE
#_____________________________________________________________________
#

MO=AFFE_MODELE(MAILLAGE=MAILL1,
               AFFE=_F(TOUT        ='OUI',
                       PHENOMENE   ='MECANIQUE',
                       MODELISATION='3D'));
#
# REORIENTATION DU MAILLAGE
#

MAILL1=MODI_MAILLAGE(reuse   =MAILL1,
                     MAILLAGE=MAILL1,
                     ORIE_PEAU_3D=_F(GROUP_MA=('FIL_MAI1','FIL_MAI2')));

MAILL1=MODI_MAILLAGE(reuse   =MAILL1,
                     MAILLAGE=MAILL1,
                     ORIE_PEAU_3D=_F(GROUP_MA=('LOP_SYM1','LOP_SYM2',
                                               'LOP_ESC1','LOP_ESC2')));

#_____________________________________________________________________
#
#                    DEFINITION DES MATERIAUX
#_____________________________________________________________________
#

MLOPIN=DEFI_MATERIAU(ELAS=_F(E =5000.0,
                             NU=0.45000000000000001));

MFILIERE=DEFI_MATERIAU(ELAS=_F(E =2.E5,
                               NU=0.29999999999999999));

CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                 AFFE=(_F(GROUP_MA='LOPIN',
                          MATER=MLOPIN),
                       _F(GROUP_MA='FILIERE',
                          MATER=MFILIERE)));

#_____________________________________________________________________
#
#                    AFFECTATION DES CONDITIONS AUX LIMITES
#                       ET DU DEPLACEMENT IMPOSE
#_____________________________________________________________________
#

COND_LIM=AFFE_CHAR_MECA(MODELE=MO,
                        DDL_IMPO=(_F(GROUP_NO='FIL_EXT',
                                     DX=0.0,DY=0.0,DZ=0.0),
                                  _F(GROUP_NO='FIL_SYM1',
                                     DZ=0.0),
                                  _F(GROUP_NO='FIL_SYM2',
                                     DX=0.0),
                                  _F(GROUP_NO='LOP_SYM1',
                                     DZ=-0.000001),
                                  _F(GROUP_NO='LOP_SYM2',
                                     DX=-0.000001)));

DDLIMP=AFFE_CHAR_MECA(MODELE=MO,
                      DDL_IMPO=_F(GROUP_NO='LOP_DDL',
                                  DY=1.0));

CONTACT  = DEFI_CONTACT(MODELE         = MO,
                        FORMULATION    = 'CONTINUE',
                        LISSAGE        = 'OUI',
                    ZONE =(
                           _F(
                             GROUP_MA_MAIT ='FIL_MAI1',
                             GROUP_MA_ESCL ='LOP_ESC1',
                             CONTACT_INIT  = 'INTERPENETRE',
                             GLISSIERE     = 'OUI',
                             ),
                           _F(
                             GROUP_MA_MAIT ='LOP_ESC2',
                             GROUP_MA_ESCL ='FIL_MAI2',
                             ),
                          ),
                   );

#_____________________________________________________________________
#
#                    DEFINITIONS DES FONCTIONS TEMPORELLES
#_____________________________________________________________________
#

DPLY=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0, 1.0,1.0),
                   PROL_DROITE='LINEAIRE',
                   PROL_GAUCHE='LINEAIRE',);

FFCT=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0, 1.0,1.0),
                   PROL_DROITE='CONSTANT',
                   PROL_GAUCHE='CONSTANT');

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A= 5.0,
                                     PAS    = 5.0),
                                  _F(JUSQU_A=15.0,
                                     PAS    =10.0),
                                  _F(JUSQU_A=20.0,
                                     PAS    = 5.0),
                                  _F(JUSQU_A=50.0,
                                     PAS    =10.0),
                                  _F(JUSQU_A=70.0,
                                     PAS    =10.0),
                                  _F(JUSQU_A=140.0,
                                     PAS    =35.0),
                                  _F(JUSQU_A=155.0,
                                     PAS    =15.0)));

U=STAT_NON_LINE(MODELE=MO,
                INFO=1,
                CHAM_MATER=CM,
                EXCIT=(_F(CHARGE   =COND_LIM),
                       _F(CHARGE   =DDLIMP,
                          FONC_MULT=DPLY)),
                CONTACT  = CONTACT,
                COMP_INCR=_F(RELATION='ELAS',
                             TOUT    ='OUI',),
                INCREMENT=_F(LIST_INST=L_INST,
                             #INST_FIN =70.0
                             ),
                NEWTON=_F(MATRICE  ='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_MAXI=9.9999999999999995E-07));

IMPR_RESU(FORMAT='MED',
          RESU=_F(RESULTAT=U,),);

TEST_RESU(RESU=(_F(RESULTAT  =U,
                   NUME_ORDRE=1,
                   NOM_CHAM  ='DEPL',
                   NOM_CMP   ='DY',
                   GROUP_NO  ='K',
                   VALE      =5.0,
                   PRECISION =1.E-3,
                   REFERENCE ='ANALYTIQUE'),
                _F(RESULTAT  =U,
                   NUME_ORDRE=1,
                   PARA      ='ITER_GLOB',
                   VALE_I    =1,
                   CRITERE = 'ABSOLU',        PRECISION = 0.,
                   REFERENCE ='NON_REGRESSION'),
                _F(RESULTAT  =U,
                   NUME_ORDRE=3,
                   NOM_CHAM  ='DEPL',
                   NOM_CMP   ='DY',
                   GROUP_NO  ='K',
                   VALE      =20.824999999999999,
                   PRECISION =2.0E-2,
                   REFERENCE ='ANALYTIQUE'),
                _F(RESULTAT  =U,
                   NUME_ORDRE=3,
                   PARA      ='ITER_GLOB',
                   VALE_I    =1,
                   CRITERE = 'ABSOLU',        PRECISION = 0.,
                   REFERENCE ='NON_REGRESSION'),
                _F(RESULTAT  =U,
                   NUME_ORDRE=6,
                   NOM_CHAM  ='DEPL',
                   NOM_CMP   ='DY',
                   GROUP_NO  ='K',
                   VALE      =55.880000000000003,
                   PRECISION =1.5E-2,
                   REFERENCE ='ANALYTIQUE'),
                _F(RESULTAT  =U,
                   NUME_ORDRE=6,
                   PARA      ='ITER_GLOB',
                   VALE_I    =1,
                   CRITERE = 'ABSOLU',        PRECISION = 0.,
                   REFERENCE ='NON_REGRESSION'),
                _F(RESULTAT=U,
                   NUME_ORDRE=10,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='L',
                   VALE=140.0,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=U,
                   NUME_ORDRE=10,
                   PARA='ITER_GLOB',
                   VALE_I=1,
                   CRITERE = 'ABSOLU',        PRECISION = 0.,
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=U,
                   NUME_ORDRE=11,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='K',
                   VALE=155.0,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=U,
                   NUME_ORDRE=11,
                   PARA='ITER_GLOB',
                   VALE_I=1,
                   CRITERE = 'ABSOLU',        PRECISION = 0.,
                   REFERENCE='NON_REGRESSION',),
                   ));

FIN();
