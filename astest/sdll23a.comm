                      NUME_MODE=(1, 2,) )

MODEMEC2=CALC_CHAMP(reuse=MODEMEC2,  RESULTAT=MODEMEC2,
                    FORCE='REAC_NODA',
                       NUME_ORDRE=3 )

MODEMEC2=CALC_CHAMP(reuse=MODEMEC2,  RESULTAT=MODEMEC2,
                    FORCE='REAC_NODA',
                       FREQ=12.4675,  CRITERE='ABSOLU', PRECISION=1.E-3 )

