#---------------------------------------------------------------------
#  - CALCUL DE MOYENNE DE LA DEFORMATION
#    JAUGE VIRTUELLE 
#  - RESULTAT OBTENU A PARTIR DE MECA_STATIQUE
#---------------------------------------------------------------------
#
import numpy as NP

DEBUT( CODE=_F( NOM = 'SDLV131A',NIV_PUB_WEB='INTERNET'),
       DEBUG=_F(SDVERI='OUI'),
       IMPR_MACRO='NON',
       PAR_LOT='OUI',
       MEMOIRE=_F(DYNAMIQUE=1),
       ERREUR=_F(ERREUR_F='EXCEPTION',),
     )

force = 1.E3
young = 2.1E11
poisson = 0.3
rho = 7800.
longueur = 1.
section = 0.3*0.2

POUTRE=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(VERIF='OUI'), );

MODEL=AFFE_MODELE(MAILLAGE=POUTRE,
                  AFFE=_F( GROUP_MA = 'TOUT_ELT',
                           PHENOMENE = 'MECANIQUE',
                           MODELISATION = '3D',),);

POUTRE=DEFI_GROUP(reuse =POUTRE,
                MAILLAGE=POUTRE,
                CREA_GROUP_NO=(
                             _F(GROUP_MA='AY',NOM='AY',),
                             _F(GROUP_MA='AZ',NOM='AZ',),
                             _F(GROUP_MA='SURF2',NOM='SURF2',),
                              ),);


#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX 
#---------------------------------------------------------------------
#
ACIER=DEFI_MATERIAU(ELAS=_F( E = young,
                             NU = poisson,
                             RHO = rho),);

MAT=AFFE_MATERIAU( MAILLAGE=POUTRE,
                   AFFE=_F( GROUP_MA = 'TOUT_ELT', MATER = ACIER)
                 );

#
#---------------------------------------------------------------------
#                     CHARGEMENTS 
#---------------------------------------------------------------------
#
CHAR0=AFFE_CHAR_MECA( MODELE=MODEL,
                      DDL_IMPO=(  # traction - compression
                               _F( GROUP_NO = 'AY',DY=0.,),
                               _F( GROUP_NO = 'AZ',DZ=0.,),
                        ),
                      FACE_IMPO=( # encastrement
                               _F( GROUP_MA = 'SURF1',DX=0.,),
                        ),
                      LIAISON_UNIF=( # pour la force de traction
                               _F( GROUP_NO = 'SURF2',DDL=('DX',)),
                        ),
                      );
#
#---------------------------------------------------------------------
#                     RESOLUTION 
#---------------------------------------------------------------------
#
M_EL_RIG=CALC_MATR_ELEM( MODELE=MODEL,
                         CHAM_MATER=MAT,
                         CHARGE=CHAR0,
                         OPTION='RIGI_MECA'
                           );

M_EL_MAS=CALC_MATR_ELEM( MODELE=MODEL,
                         CHAM_MATER=MAT,
                         CHARGE=CHAR0,
                         OPTION='MASS_MECA'
                           );

NU=NUME_DDL( MATR_RIGI=M_EL_RIG)
M_AS_RIG=ASSE_MATRICE( MATR_ELEM=M_EL_RIG,   NUME_DDL=NU)
M_AS_MAS=ASSE_MATRICE( MATR_ELEM=M_EL_MAS,   NUME_DDL=NU)

rota = 90.
rota_rad = rota*NP.pi/180.
cosinus = NP.cos(rota_rad)
sinus = NP.sin(rota_rad)

CHF=AFFE_CHAR_MECA(MODELE=MODEL,
                   FORCE_FACE=(_F(GROUP_MA='SURF2',FX=force),
                                  ),)

statique = 1
if statique:
  REPSTA=MECA_STATIQUE( MODELE=MODEL,
                        CHAM_MATER=MAT,
                        EXCIT=(
                            _F( CHARGE = CHAR0),
                            _F( CHARGE = CHF),
                        ),)

  CALC_ELEM( reuse= REPSTA ,RESULTAT= REPSTA ,
             OPTION=('EPSI_ELGA','EPSI_ELNO',),
                        )

  REPSTA=CALC_NO( reuse=REPSTA,    RESULTAT=REPSTA,
                  OPTION=('EPSI_NOEU', )
                     )

projection = 0
if projection:
# MESURE
    MAILMESU=LIRE_MAILLAGE(UNITE=22)

    MODLMESU=AFFE_MODELE( MAILLAGE=MAILMESU,
                          AFFE=(
                              _F( MAILLE = ('P3','P4','P5','P6','L1','L2','L3',),
                                  PHENOMENE = 'MECANIQUE',
                                  MODELISATION = 'DIS_T'),
                                ),)

    OBSJAU = OBSERVATION(
                   RESULTAT = REPSTA,
                   MODELE_1 = MODEL,
                   MODELE_2 = MODLMESU,
                   PROJECTION = 'OUI',
                   TOUT_ORDRE = 'OUI',
                   NOM_CHAM = 'EPSI_NOEU',
                   EPSI_MOYENNE = (  # pour JAUGE : une occ par jauge
                              _F( GROUP_MA='SURF3',SEUIL_VARI=(0.1,),MASQUE=('EPYY','EPXY','EPXZ','EPYZ'),),
                              _F( GROUP_MA='SURF4',MASQUE=('EPZZ','EPXY','EPXZ','EPYZ'),),
                              _F( GROUP_MA='SURF5',SEUIL_VARI=0.1,MASQUE=('EPYY','EPXY','EPXZ','EPYZ'),),
                              _F( GROUP_MA='SURF6',MASQUE=('EPZZ','EPXY','EPXZ','EPYZ'),),
                              ),
                   MODI_REPERE = _F( 
                                    TYPE_CHAM = 'TENS_3D',
                                    GROUP_MA=('P4','P6',),  # sur le modele mesure (rotation 90 degre)
                                    REPERE = 'DIR_JAUGE',  VECT_X = (cosinus,sinus,0.),  VECT_Y = (-sinus,cosinus,0.),
                              ),
                   FILTRE =(
                              _F( GROUP_NO = 'P3',
                                NOM_CHAM = 'EPSI_NOEU',
                                DDL_ACTIF = ('EPXX','EPZZ',),),
                              _F( GROUP_NO = 'P4',
                                NOM_CHAM = 'EPSI_NOEU',
                                DDL_ACTIF = ('EPXX','EPYY',),),
                              _F( GROUP_NO = 'P5',
                                NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPZZ',),),
                              _F( GROUP_NO = 'P6',
                                NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPYY',),),
                              ),
                  );

    OBSMXT = OBSERVATION(
                   RESULTAT = REPSTA,
                   MODELE_1 = MODEL,
                   MODELE_2 = MODLMESU,
                   PROJECTION = 'OUI',
                   TOUT_ORDRE = 'OUI',
                   NOM_CHAM = ('DEPL','EPSI_NOEU',), 
                   FILTRE =(
                              _F( GROUP_NO = 'P3',
                                NOM_CHAM = 'EPSI_NOEU',
                                DDL_ACTIF = ('EPXX','EPZZ',),),
                              _F( GROUP_NO = 'P5',
                                NOM_CHAM = 'DEPL',
                                DDL_ACTIF = ('DX','DZ',),),
                              ),
                  );

else:
    OBSJAU = OBSERVATION(
                   RESULTAT = REPSTA,
                   MODELE_1 = MODEL,
                   MODELE_2 = MODEL,
                   PROJECTION = 'NON',
                   TOUT_ORDRE = 'OUI',
                   MATR_A=M_AS_RIG,
                   MATR_B=M_AS_MAS,
                   NOM_CHAM = 'EPSI_NOEU',
                   EPSI_MOYENNE = (  # pour JAUGE : une occ par jauge
                              _F(
                                 GROUP_MA=('SURF3',),
                                 SEUIL_VARI=(0.1,), 
                                 MASQUE=('EPYY','EPXY','EPXZ','EPYZ'),
                                 ),
                              _F( GROUP_MA='SURF4',MASQUE=('EPZZ','EPXY','EPXZ','EPYZ'),),
                              _F( GROUP_MA='SURF5',SEUIL_VARI=0.1,MASQUE=('EPYY','EPXY','EPXZ','EPYZ'),),
                              _F( GROUP_MA='SURF6',MASQUE=('EPZZ','EPXY','EPXZ','EPYZ'),),
                              ),
                   MODI_REPERE = (
                               _F( TYPE_CHAM = 'TENS_3D',
                                    GROUP_MA=('SURF4','SURF6',), # (rotation 90 degre)
                                    REPERE = 'DIR_JAUGE',  VECT_X = (cosinus,sinus,0.),  VECT_Y = (-sinus,cosinus,0.),),
                              ),
                   FILTRE = (
                              _F( GROUP_NO = 'P3',
                                NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPZZ',),
                                #  DDL_ACTIF = ('DX','DZ',),
                                 ),
                              _F( GROUP_NO = 'P4',
                                NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPYY',),
                                #  DDL_ACTIF = ('DX','DY',),
                                 ),
                              _F( GROUP_NO = 'P5',
                                NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPZZ',),
                                 ),
                              _F( GROUP_NO = 'P6',
                                NOM_CHAM = 'EPSI_NOEU',
                                  DDL_ACTIF = ('EPXX','EPYY',),
                                 ),
                             ),
                  );

    OBSMXT = OBSERVATION(
                   RESULTAT = REPSTA,
                   MODELE_1 = MODEL,
                   MODELE_2 = MODEL,
                   PROJECTION = 'NON',
                   TOUT_ORDRE = 'OUI',
                   MATR_A=M_AS_RIG,
                   MATR_B=M_AS_MAS,
                   NOM_CHAM = ('DEPL','EPSI_NOEU',),
                   FILTRE =(
                              _F( GROUP_NO = 'P3',
                                NOM_CHAM = 'EPSI_NOEU',
                                DDL_ACTIF = ('EPXX','EPZZ',),),
                              _F( GROUP_NO = 'P5',
                                NOM_CHAM = 'DEPL',
                                DDL_ACTIF = ('DX','DZ',),),
                              ),
                  );

#---------------------------------------------------------------------
#                     VERIFICATION DES RESULTATS 
#---------------------------------------------------------------------
#

if statique:
  exx = force/young
  eyy = -poisson*exx
  ezz = -poisson*exx
  deltal = force*longueur/young

  TEST_RESU(RESU=(
       _F( RESULTAT = OBSJAU,        NOM_CHAM = 'EPSI_NOEU',
             NOM_CMP = 'EPXX',    GROUP_NO = 'P3',
             REFERENCE = 'ANALYTIQUE',
             CRITERE = 'RELATIF',    PRECISION = 1.E-3,
             NUME_ORDRE = 1,                 VALE = exx),
       _F( RESULTAT = OBSJAU,        NOM_CHAM = 'EPSI_NOEU',
             NOM_CMP = 'EPZZ',    GROUP_NO = 'P3',
             REFERENCE = 'ANALYTIQUE',
             CRITERE = 'RELATIF',    PRECISION = 1.E-3,
             NUME_ORDRE = 1,                 VALE = ezz),

# rotation 90 degres : EPXX <-> EPYY
       _F( RESULTAT = OBSJAU,        NOM_CHAM = 'EPSI_NOEU',
             NOM_CMP = 'EPYY',    GROUP_NO = 'P4',
             REFERENCE = 'ANALYTIQUE',
             CRITERE = 'RELATIF',    PRECISION = 1.E-3,
             NUME_ORDRE = 1,                 VALE = exx),
       _F( RESULTAT = OBSJAU,        NOM_CHAM = 'EPSI_NOEU',
             NOM_CMP = 'EPXX',    GROUP_NO = 'P4',
             REFERENCE = 'ANALYTIQUE',
             CRITERE = 'RELATIF',    PRECISION = 1.E-3,
             NUME_ORDRE = 1,                 VALE = eyy),

# observation mixte
       _F( RESULTAT = OBSMXT,        NOM_CHAM = 'EPSI_NOEU',
             NOM_CMP = 'EPXX',    GROUP_NO = 'P3',
             REFERENCE = 'ANALYTIQUE',
             CRITERE = 'RELATIF',    PRECISION = 1.E-3,
             NUME_ORDRE = 1,                 VALE = exx),
       _F( RESULTAT = OBSMXT,        NOM_CHAM = 'DEPL',
             NOM_CMP = 'DX',    GROUP_NO = 'P5',
             REFERENCE = 'ANALYTIQUE',
             CRITERE = 'RELATIF',    PRECISION = 6.E-2,
             NUME_ORDRE = 1,                 VALE = deltal),
  ),);

FIN();
