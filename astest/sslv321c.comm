DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'),PAR_LOT='OUI');

MA=LIRE_MAILLAGE(INFO=1,FORMAT='MED');
MA=MODI_MAILLAGE(reuse=MA,MAILLAGE=MA,ORIE_PEAU_3D=(_F(GROUP_MA=('SURF','SUR','ENV',),),),);
