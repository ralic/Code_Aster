DEBUT(CODE=_F(NOM='SSNV504A',NIV_PUB_WEB='INTERNET'),);
#......................................................................
#  CALCUL VALIDATION DU PASSAGE DES SINGULARITES GEOMETRIQUES
#......................................................................
#
#


PRE_GIBI();
#
#


MAILL1=LIRE_MAILLAGE();

MAILL1=DEFI_GROUP(reuse =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_MA=_F(NOM='TOUT',
                                   UNION=('LOPIN','FILIERE'),),
                  CREA_GROUP_NO=(_F(GROUP_MA='LJA',
                                    NOM='LIM1',),
                                 _F(GROUP_MA='LIJ',
                                    NOM='LIM2',),
                                 _F(GROUP_MA='LHI',
                                    NOM='LIM3',),
                                 _F(GROUP_MA='LMN',
                                    NOM='LMN',),
                                 _F(GROUP_MA='LNK',
                                    NOM='LNK',),
                                 _F(GROUP_MA='SESC1',
                                    NOM='SESC1',),),);
#_____________________________________________________________________
#
#                    DEFINITION DU MODELE
#_____________________________________________________________________
#
#


MO=AFFE_MODELE(MAILLAGE=MAILL1,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);
#
# REORIENTATION DU MAILLAGE
#
#


MAILL1=MODI_MAILLAGE(reuse =MAILL1,
                     MAILLAGE=MAILL1,
                     ORIE_PEAU_2D=_F(GROUP_MA=('SMAI1','SESC1'),),
                     );

MAILL1=MODI_MAILLAGE(reuse =MAILL1,
                     MAILLAGE=MAILL1,
                     ORIE_PEAU_2D=_F(GROUP_MA=('SMAI2','SESC2'),),
                     );
#_____________________________________________________________________
#
#                    DEFINITION DES MATERIAUX
#_____________________________________________________________________
#
#


MLOPIN=DEFI_MATERIAU(ELAS=_F(E=5000.0,
                             NU=0.45000000000000001,),);

MFILIERE=DEFI_MATERIAU(ELAS=_F(E=2.E5,
                               NU=0.29999999999999999,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                 AFFE=(_F(GROUP_MA='LOPIN',
                          MATER=MLOPIN,),
                       _F(GROUP_MA='FILIERE',
                          MATER=MFILIERE,),),);
#_____________________________________________________________________
#
#                    AFFECTATION DES CONDITIONS AUX LIMITES
#                       ET DU DEPLACEMENT IMPOSE 
#_____________________________________________________________________
#
#


CH1=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=(_F(GROUP_NO='LIM1',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_NO='LIM2',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_NO='LIM3',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_NO='LNK',
                                DX=0.0,),),);

CH0=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=_F(GROUP_NO='LMN',
                               DY=1.0,),);

CH2 = DEFI_CONTACT(MODELE       = MO,
                   FORMULATION  = 'DISCRETE',
                   
                   ZONE=(
                        _F(
                            GROUP_MA_MAIT='SMAI1',
                            GROUP_MA_ESCL='SESC1',
                            SANS_GROUP_NO='LIM1',
                         ), 
                        _F(
                            GROUP_MA_MAIT='SMAI2',
                            GROUP_MA_ESCL='SESC2',
                         ),                          
                                                                           
                       ),  
                  )

CH3 = DEFI_CONTACT(MODELE       = MO,
                   FORMULATION  = 'DISCRETE',
                   STOP_INTERP  = 'NON',
                   
                   ZONE=(
                        _F(
                            RESOLUTION   = 'NON',
                            TOLE_INTERP  = 0.5,
                            GROUP_MA_MAIT='SMAI1',
                            GROUP_MA_ESCL='SESC1',
                            SANS_GROUP_NO='LIM1',
                         ), 
                        _F(
                            RESOLUTION   = 'NON',
                            TOLE_INTERP  = 0.5,
                            GROUP_MA_MAIT='SMAI2',
                            GROUP_MA_ESCL='SESC2',
                         ),                          
                                                                           
                       ),  
                  )

#_____________________________________________________________________
#
#                    DEFINITIONS DES FONCTIONS TEMPORELLES 
#_____________________________________________________________________
#
#


DPLY=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,1.0,1.0),
                   PROL_DROITE='LINEAIRE',
                   PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=5.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=20.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=50.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=70.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=140.0,
                                     PAS=5.0,),
                                  _F(JUSQU_A=155.0,
                                     PAS=5.0,),),);

U=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                MODELE=MO,
                CHAM_MATER=CM,
                EXCIT=(_F(CHARGE=CH0,
                          FONC_MULT=DPLY,),
                       _F(CHARGE=CH1,),
                       ),
                CONTACT  = CH2,       
                COMP_INCR=_F(RELATION='ELAS',
                             TOUT='OUI',),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_MAXI=9.9999999999999995E-07,),);

TEST_RESU(RESU=(_F(RESULTAT=U,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='K',
                   VALE=5.0,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',),
               
                _F(RESULTAT=U,
                   NUME_ORDRE=4,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='K',
                   VALE=20.824999999999999,
                   PRECISION=2.E-3,
                   REFERENCE='ANALYTIQUE',),
               
                _F(RESULTAT=U,
                   NUME_ORDRE=10,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='K',
                   VALE=55.880000000000003,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',),
                
                _F(RESULTAT=U,
                   NUME_ORDRE=14,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='K',
                   VALE=78.689999999999998,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',),
                
                _F(RESULTAT=U,
                   NUME_ORDRE=28,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='K',
                   VALE=144.89500000000001,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',),
                
                _F(RESULTAT=U,
                   NUME_ORDRE=31,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='K',
                   VALE=155.096,
                   PRECISION=1.E-3,
                   REFERENCE='ANALYTIQUE',),
                   ),);



U2=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                 MODELE=MO,
                CHAM_MATER=CM,
                EXCIT=(_F(CHARGE=CH0,
                          FONC_MULT=DPLY,),
                       _F(CHARGE=CH1,),
                       ),
                CONTACT  = CH3,       
                COMP_INCR=_F(RELATION='ELAS',
                             TOUT='OUI',),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_MAXI=9.9999999999999995E-07,),);

TAB_VC=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='JEU METHODE VERIF',
                               RESULTAT=U2,
                               NOM_CHAM='VALE_CONT',
                               GROUP_NO='SESC1',
                               NOM_CMP=('CONT','JEU',),),);

IMPR_TABLE(TABLE=TAB_VC);

TEST_RESU(RESU=(
                _F(RESULTAT=U2,
                   NUME_ORDRE=31,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='K',
                   VALE=155.096,
                   PRECISION=1.E-3,
                   VERSION = '8.0.6',
                   REFERENCE='NON_REGRESSION',),
),);

TEST_TABLE(TABLE=TAB_VC,
           FILTRE=(_F(NOM_PARA='ABSC_CURV',
                      VALE=65.0,),
                   _F(NOM_PARA='INST',
                      VALE=100.0,),),
           NOM_PARA='JEU',
           VALE=-5.0,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=TAB_VC,
           FILTRE=(_F(NOM_PARA='ABSC_CURV',
                      VALE=65.0,),
                   _F(NOM_PARA='INST',
                      VALE=155.0,),),
           NOM_PARA='JEU',
           VALE=-2.0,
           REFERENCE='ANALYTIQUE',);

FIN();
