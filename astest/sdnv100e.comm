                     PRECISION = 2.E-2,
                     NOM_CHAM = 'DEPL',
                     NOM_CMP = 'DZ',
                     VALE = -4.E-3))

TEST_RESU( RESU=_F(  RESULTAT = U,
                     NUME_ORDRE = 50,
