                     PRECISION = 15.E-2,
                     NOM_CHAM = 'VITE',
                     NOM_CMP = 'DZ',
                     VALE = 100.E+0))

TEST_RESU( RESU=_F(  RESULTAT = U,
                     NUME_ORDRE = 65,
