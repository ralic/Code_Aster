TEST_RESU( RESU=_F(  RESULTAT = U,
                     NUME_ORDRE = 20,
                     REFERENCE = 'ANALYTIQUE',
                     NOEUD = 'N5',
                     PRECISION = 1.E-3,
                     NOM_CHAM = 'VITE',
                     NOM_CMP = 'DZ',
