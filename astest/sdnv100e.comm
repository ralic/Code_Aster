                     VALE = 100.E+0))

 

TEST_FONCTION(VALEUR=_F(FONCTION=MVC,
                         NOM_PARA='INST',
                         VALE_PARA=1.E-4,
                         VALE_REFE=0.,
                         REFERENCE = 'ANALYTIQUE',
                         CRITERE='ABSOLU',
