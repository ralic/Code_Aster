                     NOM_CHAM = 'VITE',
                     NOM_CMP = 'DZ',
                     CRITERE='ABSOLU',
                     VALE = 0.E+0))

TEST_RESU( RESU=_F(  RESULTAT = U,
                     NUME_ORDRE = 60,
                     REFERENCE = 'ANALYTIQUE',
                     NOEUD = 'N100',
                     PRECISION = 18.E-2,
                     NOM_CHAM = 'VITE',
                     NOM_CMP = 'DZ',
                     VALE = 100.E+0))

TEST_RESU( RESU=_F(  RESULTAT = U,
                     NUME_ORDRE = 60,
