                              NOM_PARA='INST',
                                  VALE=(
                                           0.,   0.,
                                          30.,   300.,
     ))

COEF_CS=DEFI_FONCTION(  
                              NOM_PARA='INST',
                                  VALE=(
                                           0.,   0.,
                                          30.,   50.,
     ))

