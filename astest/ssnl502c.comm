#---------------------------------------------------------------------
#- CALCUL DE LA STABILITE D'UNE POUTRE CONSOLE SOUMISE A UN EFFORT DE
#  COMPRESSION A UNE EXTREMITE.
#- VALIDATION DANS LE DOMAINE QUASI-STATIQUE NON-LINEAIRE GEOMETRIQUE
#  EN GRANDS DEPLACEMENTS ET GRANDES ROTATIONS EN PRESENCE
#  D'INSTABILITE (FLAMBEMENT D'EULER)
#- MODELISATION AVEC DES ELEMENTS 'POU_D_T_GD' (SEG2)
#---------------------------------------------------------------------
# RESPONSABLE MASSIN P.MASSIN
#
DEBUT(CODE=_F(NOM='SSNL502C',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

#

MAIL=LIRE_MAILLAGE()


MAIL=DEFI_GROUP(reuse=MAIL,  MAILLAGE=MAIL,CREA_GROUP_NO=(
                   _F( GROUP_MA = 'TOUT_ELT'),
                 ))

#
MO=AFFE_MODELE(   MAILLAGE=MAIL,
                        AFFE=(_F(GROUP_MA='TOUT_ELT',
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'POU_D_T_GD'),)
                  )

#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CAREL=AFFE_CARA_ELEM(   MODELE=MO,
                  ORIENTATION =_F( GROUP_MA = 'TOUT_ELT',
                     CARA='ANGL_NAUT', VALE=(0., 0., 0.,),),
                     POUTRE=_F( GROUP_MA = 'TOUT_ELT' ,
 SECTION = 'GENERALE',
                CARA = ( 'A',   'IY',    'IZ',    'AY',    'AZ',    'JX', ),
VALE = (  0.0003375, 5.69531E-10,  1.58203E-07, 1.0, 1.0,  2.19201E-09,  ),),
               INFO=2,
                        )

CAREL2=AFFE_CARA_ELEM(   MODELE=MO,
                  ORIENTATION =_F( GROUP_MA = 'TOUT_ELT',
                     CARA='ANGL_NAUT', VALE=(0., 0., 0.,),),
                     POUTRE=_F( GROUP_MA = 'TOUT_ELT' ,
                    SECTION = 'RECTANGLE',
                CARA = ( 'HY',   'HZ',    ),
                VALE = (  0.075, 0.0045  ),),
               INFO=2,
                        )
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
MAT=DEFI_MATERIAU(   ELAS=_F( E = 2.E+11,
                             NU = 0.3 ,  ALPHA = 0.)
                     )

#
CHMAT=AFFE_MATERIAU(   MAILLAGE=MAIL,
                             AFFE=_F( TOUT = 'OUI', MATER = MAT)
                       )

#
#---------------------------------------------------------------------
#                        CHARGEMENTS
#---------------------------------------------------------------------
#
CON_LIM=AFFE_CHAR_MECA(     MODELE=MO,
                            DDL_IMPO=(_F( GROUP_NO = 'A',
                                      DX = 0., DZ = 0.,
                                      DRX = 0., DRY = 0., DRZ = 0.),
                           _F( GROUP_NO = 'TOUT_ELT',
                                      DY = 0.),
                               ),
                          )

#
CHARGE=AFFE_CHAR_MECA(         MODELE=MO,
                            FORCE_NODALE=_F( GROUP_NO = 'B',
                                         FZ = -1124.21)
                          )

PERTUR=AFFE_CHAR_MECA(         MODELE=MO,
                            FORCE_NODALE=_F( GROUP_NO = 'B',
#                                         FX = 1.12421)
                                         FX = 0.112421)
                          )

F_PERT2=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                          PROL_GAUCHE='EXCLU',
                                 VALE=( 0.,     0.,
                                        0.84,   0.,
                                        0.85,   1.,
                                        0.95,  1.,
                                        0.96,  0.,
                                        2.,     0.,
                                       )
                        )
F_PERT=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                          PROL_GAUCHE='EXCLU',
                                 VALE=( 0.,     0.,
                                        0.94,   0.,
                                        0.95,   1.,
                                        1.009,   1.,
                                        1.01,   0.,
                                        2.,     0.,
                                       )
                        )

F_CHARGE=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                          PROL_GAUCHE='EXCLU',
                                 VALE=( 0.,   0.,
                                        2.,   2.,
                                       )
                        )

#
#---------------------------------------------------------------------
#                        RESOLUTION
#---------------------------------------------------------------------
#
LIS_INST=DEFI_LIST_REEL(       DEBUT=0.0,INTERVALLE=(
                           _F( JUSQU_A = 0.94, NOMBRE = 1),
                           _F( JUSQU_A = 0.95, NOMBRE = 1),
                           _F( JUSQU_A = 1.009, NOMBRE = 10),
                           _F( JUSQU_A = 1.01, NOMBRE = 1),
#
# DEPLACEMENT IMPOSE U=0.015 : ON COMPLETE LE DEPLACEMENT OBTENU
# PRECEDEMMENT QUI EST DE W =-1.01816E-02
                          _F( JUSQU_A = 2., NOMBRE = 198),
)
                           )

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = LIS_INST),
                        ECHEC=_F(ACTION='DECOUPE',
                                 SUBD_METHODE='MANUEL',
                                 SUBD_NIVEAU=4,
                                 SUBD_PAS=4,
                                 SUBD_PAS_MINI = 0.00000001),)

#
RESULT=STAT_NON_LINE(       MODELE=MO,
                          CHAM_MATER=CHMAT,
                           CARA_ELEM=CAREL,EXCIT=(
                               _F( CHARGE = CON_LIM),
                                     _F( CHARGE = CHARGE,
                                      FONC_MULT = F_CHARGE),
                                     _F( CHARGE = PERTUR,
                                      FONC_MULT = F_PERT)),
                           COMP_ELAS=_F( RELATION = 'ELAS_POUTRE_GR',
                                      DEFORMATION = 'GROT_GDEP',),
                           INCREMENT=_F( LIST_INST = LIS_INST,
                                      NUME_INST_FIN = 5),
                              NEWTON=_F( REAC_ITER = 1,
                                      MATRICE = 'TANGENTE'),
                         CONVERGENCE=_F(
                                      ITER_GLOB_MAXI = 30,
                                      RESI_GLOB_RELA = 1.E-6)
                        )

# premiere valeur de ETA_PILOTAGE de reference
RESULT=STAT_NON_LINE(reuse=RESULT,      MODELE=MO,
                          CHAM_MATER=CHMAT,
                           CARA_ELEM=CAREL,EXCIT=(
                               _F( CHARGE = CON_LIM),
                                     _F( CHARGE = PERTUR,
                                      FONC_MULT = F_PERT),
                                     _F( CHARGE = CHARGE,
                                      TYPE_CHARGE = 'FIXE_PILO')),
                           COMP_ELAS=_F( RELATION = 'ELAS_POUTRE_GR',
                                      DEFORMATION = 'GROT_GDEP',),
                           INCREMENT=_F( LIST_INST = DEFLIST,
                                      ),
                              NEWTON=_F( REAC_ITER = 1,
                                      MATRICE = 'TANGENTE'),
                         CONVERGENCE=_F(
                                      ITER_GLOB_MAXI = 40,
                                      RESI_GLOB_RELA = 1.E-6),
                           ETAT_INIT=_F( EVOL_NOLI = RESULT),
                            PILOTAGE=_F( TYPE = 'LONG_ARC',
                                      SELECTION='ANGL_INCR_DEPL',
                                      GROUP_NO = 'TOUT_ELT',
                                      NOM_CMP = ('DX','DZ',),
                                      COEF_MULT = -1.,
                                      ETA_PILO_MAX=1.015,

                                      )
                        )
# derniere valeur de ETA_PILOTAGE de reference
RESULT=STAT_NON_LINE(reuse=RESULT,      MODELE=MO,
                          CHAM_MATER=CHMAT,
                           CARA_ELEM=CAREL,EXCIT=(
                               _F( CHARGE = CON_LIM),
                                     _F( CHARGE = PERTUR,
                                      FONC_MULT = F_PERT),
                                     _F( CHARGE = CHARGE,
                                      TYPE_CHARGE = 'FIXE_PILO')),
                           COMP_ELAS=_F( RELATION = 'ELAS_POUTRE_GR',
                                      DEFORMATION = 'GROT_GDEP',),
                           INCREMENT=_F( LIST_INST = DEFLIST,
                                      ),
                              NEWTON=_F( REAC_ITER = 1,
                                      MATRICE = 'TANGENTE'),
                         CONVERGENCE=_F(
                                      ITER_GLOB_MAXI = 40,
                                      RESI_GLOB_RELA = 1.E-6),
                           ETAT_INIT=_F( EVOL_NOLI = RESULT),
                            PILOTAGE=_F( TYPE = 'LONG_ARC',
                                      SELECTION='ANGL_INCR_DEPL',
                                      GROUP_NO = 'TOUT_ELT',
                                      NOM_CMP = ('DX','DZ',),
                                      COEF_MULT = -1.,
                                      ETA_PILO_MAX=9.116,

                                      )
                        )
#

#

RESULT= CALC_ELEM (reuse=RESULT ,
                  OPTION     ='SIEF_ELNO',
                  RESULTAT   = RESULT,
                  TOUT_ORDRE = 'OUI',
                                  );

RESULT= CALC_NO   (reuse=RESULT ,
                  OPTION     ='FORC_NODA',
                  RESULTAT   = RESULT,
                  TOUT_ORDRE = 'OUI',
                             );


ETA=RECU_FONCTION (   RESULTAT = RESULT,
                          NOM_PARA_RESU = 'ETA_PILOTAGE',
                          TOUT_ORDRE = 'OUI')

DEPZ=RECU_FONCTION (   RESULTAT = RESULT,
                          NOM_CMP = 'DZ',
                          NOM_CHAM = 'DEPL',
                          GROUP_NO = 'B',
                          TOUT_ORDRE = 'OUI')

DEPX=RECU_FONCTION (   RESULTAT = RESULT,
                          NOM_CMP = 'DX',
                          NOM_CHAM = 'DEPL',
                          GROUP_NO = 'B',
                          TOUT_ORDRE = 'OUI')

MDEPZ=CALC_FONCTION(COMB= _F(  FONCTION = DEPZ, COEF = -1.)  )

IMPR_FONCTION ( UNITE=29,FORMAT='XMGRACE',
              COURBE=(_F(FONC_Y=ETA, FONC_X = MDEPZ),
                      _F(FONC_Y=ETA, FONC_X = DEPX),),
              TITRE='Dx et Dz en fonction de P/Pcr'
               )


# A PARTIR DES FONCTIONS
#     - ETA  = F(INST,ETA_PILOTAGE),
#     - DEPX = F(INST,DX)
#     - DEPZ = F(INST,DZ)
# ON CREE LES FONCTIONS
#     - DX  = F(ETA),
#     - DZ  = F(ETA),

T_ETA  = CREA_TABLE(FONCTION=_F(FONCTION=ETA),)
T_DEPZ = CREA_TABLE(FONCTION=_F(FONCTION=DEPZ),)
T_DEPX = CREA_TABLE(FONCTION=_F(FONCTION=DEPX),)

TAB_COMB=CALC_TABLE(TABLE=T_ETA,
                    ACTION=(_F(OPERATION='COMB', TABLE=T_DEPZ, NOM_PARA='INST'),
                            _F(OPERATION='COMB', TABLE=T_DEPX, NOM_PARA='INST'),),)

IMPR_TABLE(TABLE=TAB_COMB)

DX_ETA = RECU_FONCTION(TABLE=TAB_COMB, PARA_X='ETA_PILO', PARA_Y='DX',INTERPOL='LIN',
                       PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',)
DZ_ETA = RECU_FONCTION(TABLE=TAB_COMB, PARA_X='ETA_PILO', PARA_Y='DZ',INTERPOL='LIN',
                       PROL_DROITE='CONSTANT',PROL_GAUCHE='CONSTANT',)

IMPR_FONCTION ( COURBE= _F(FONCTION=DX_ETA,),)

IMPR_FONCTION ( COURBE= _F(FONCTION=DZ_ETA,),)

# 3 PREMIERES VALEURS ELOIGNEES DE LA SOLUTION ANALYTIQUE A CAUSE DE LA PERTURBATION
