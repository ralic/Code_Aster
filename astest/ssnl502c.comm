_F(FONCTION=DX_ETA,CRITERE='RELATIF',REFERENCE='ANALYTIQUE',VALE_PARA=1.518, VALE_REFE=0.3960,PRECISION=0.01,),
_F(FONCTION=DX_ETA,CRITERE='RELATIF',REFERENCE='ANALYTIQUE',VALE_PARA=1.884, VALE_REFE=0.4015,PRECISION=0.01,),
_F(FONCTION=DX_ETA,CRITERE='RELATIF',REFERENCE='ANALYTIQUE',VALE_PARA=2.541, VALE_REFE=0.3750,PRECISION=0.01,),
_F(FONCTION=DX_ETA,CRITERE='RELATIF',REFERENCE='ANALYTIQUE',VALE_PARA=4.029, VALE_REFE=0.3125,PRECISION=0.01,),
_F(FONCTION=DX_ETA,CRITERE='RELATIF',REFERENCE='ANALYTIQUE',VALE_PARA=9.116, VALE_REFE=0.2105,PRECISION=0.01,),
                        ),)


