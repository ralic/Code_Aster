#-----------------------------------------------------------------------
# VALIDATION DES GRILLES LINEAIRE QUADRANGLE
#-----------------------------------------------------------------------

MAILTOT=LIRE_MAILLAGE(   )


#
# -----------------------------------------------------
# DEFINITION DES MATERIAUX
# -----------------------------------------------------
#

ACIER=DEFI_MATERIAU(
            ELAS=_F(  E = 2.E11,   NU = 0.0,),
                          )
BETON=DEFI_MATERIAU(
            ELAS=_F(  E = 2.,   NU = 0.0,),
                          )

#
# -------------------------------------------------
# AFFECTATION DES MATERIAUX
# -------------------------------------------------
#
MATAF=AFFE_MATERIAU(    MAILLAGE=MAILTOT,
                          AFFE=(
                          _F(  GROUP_MA = ('GEOX','GEOY','GEOZ'),
                                 MATER = ACIER,
                               ),
                          _F(  GROUP_MA = ('GEOB'),
                                 MATER = BETON,
                               )
                               )
                      )

#
# -------------------------------------------------
# AFFECTATION DES MODELES
# -------------------------------------------------
#
LEMOD=AFFE_MODELE(    MAILLAGE=MAILTOT,     VERIF='MAILLE',
                        AFFE=(
                              _F(  GROUP_MA = ('GEOX','GEOY','GEOZ'),
                               PHENOMENE = 'MECANIQUE',
                               MODELISATION = 'GRILLE_MEMBRANE',
                             ),
                              _F(  GROUP_MA = ('GEOB',),
                               PHENOMENE = 'MECANIQUE',
                               MODELISATION = '3D',
                             )
                          )
                      )

# -------------------------------------------------
# AFFECTATION DES CARACTERISTIQUES
# -------------------------------------------------
#
LACAR=AFFE_CARA_ELEM(    MODELE=LEMOD,
                       GRILLE=(
                               _F( GROUP_MA = 'GEOX',
                                SECTION = .01,
                                ANGL_REP=(30,0),),
                               _F( GROUP_MA = 'GEOY',
                                SECTION = .02,
                                ANGL_REP=(0,40),),
                               _F( GROUP_MA = 'GEOZ',
                                SECTION = .03,
                                ANGL_REP=(15,70),),
                              )
                        )

# ----------------------------------------------
# CHARGEMENT
# ----------------------------------------------
#
ZCHM=AFFE_CHAR_MECA(
             MODELE=LEMOD,
            DDL_IMPO=(
                   _F( GROUP_MA  = 'B0X',
                       DX = 0.,  ),
                   _F( GROUP_MA  = 'B1X',
                       DX = 1.,  ),
                   _F( GROUP_MA  = 'B0Y',
                       DY = 0.,  ),
                   _F( GROUP_MA  = 'B1Y',
                       DY = 2.,  ),
                   _F( GROUP_MA  = 'B0Z',
                       DZ = 0.,  ),
                   _F( GROUP_MA  = 'B1Z',
                       DZ = 3.,  ),

                )
            )

#
# ----------------------------------------------
# CALCUL
# ----------------------------------------------
#
RESU0=MECA_STATIQUE(    MODELE=LEMOD,       CHAM_MATER=MATAF,
                               CARA_ELEM=LACAR,
                               EXCIT=_F(  CHARGE =ZCHM,    )       )
RESU0=CALC_ELEM( reuse = RESU0,
                 OPTION     = ('EPSI_ELGA_DEPL','EPSI_ELNO_DEPL','SIGM_ELNO_DEPL',),
                 RESULTAT   = RESU0,
               )


ZLIST=DEFI_LIST_REEL(   DEBUT=0.,
             INTERVALLE=_F( JUSQU_A = 1.0,  NOMBRE = 1, )
           )

LEDEP=STAT_NON_LINE(
                            MODELE=LEMOD,
                        CHAM_MATER=MATAF,
                         CARA_ELEM=LACAR,
                             EXCIT=_F(
                CHARGE = ZCHM,
                ),
                         COMP_INCR=_F(
                RELATION = 'ELAS',
                ALGO_1D = 'DEBORST'
                ),
                         INCREMENT=_F(
                LIST_INST = ZLIST,
                NUME_INST_FIN = 1,
                ),
              )


LEDEP=CALC_ELEM( reuse = LEDEP,
                 OPTION     = ('EPSI_ELGA_DEPL','EPSI_ELNO_DEPL','SIEF_ELNO_ELGA', 'VARI_ELNO_ELGA',),
                 RESULTAT   = LEDEP,
               )

LEDEP=CALC_NO(reuse = LEDEP,
              RESULTAT = LEDEP,
              OPTION   = ('FORC_NODA',),
              )

EPOT = POST_ELEM(
           RESULTAT = LEDEP,
           ENER_POT = _F(TOUT='OUI'))

