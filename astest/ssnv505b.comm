DEBUT(CODE=_F( NOM = 'SSNV505B',NIV_PUB_WEB='INTERNET'));

# ---------------------  MAILLAGE ----------------
PRE_GIBI();

MAILL1=LIRE_MAILLAGE();

MAILL1=DEFI_GROUP(reuse =MAILL1,
                  MAILLAGE=MAILL1,
                  CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI'),);

# -------------------- MODELE ------------------                   
MODE=AFFE_MODELE(MAILLAGE=MAILL1,
                 VERIF='MAILLE',
                 AFFE=_F(GROUP_MA='STRUCT',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='3D',),);

MAILL1=MODI_MAILLAGE(reuse =MAILL1,
                   MAILLAGE=MAILL1,
                   ORIE_PEAU_3D=_F(GROUP_MA=('SMAI','SESC'),),
                   );

# ------------- DEFINITON DU MATERIAU -----------
MPOUTRE=DEFI_MATERIAU(ELAS=_F(E=6.8948E3,
                              NU=0.3333,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL1,
                 AFFE=_F(GROUP_MA='STRUCT',
                         MATER=MPOUTRE,),);

# ---------- CONDITIONS AUX LIMITES --------------
COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO='SA',
                                     DX=0.0,DY=0.0,DZ=0.0,),
                                  _F(GROUP_NO='SB',
                                     DX=0.0,DY=0.0,DZ=0.0,),
                                  _F(GROUP_NO='STRUCT',
                                     DZ=0.0,),
                                  ),);

CH1=AFFE_CHAR_MECA(MODELE=MODE,
                   DDL_IMPO=(_F(GROUP_NO=('LCC1'),
                                DY=-790.,),
                             ),);

CH2=AFFE_CHAR_MECA(MODELE=MODE,
                   CONTACT=_F(GROUP_MA_MAIT='SMAI',
                              GROUP_MA_ESCL='SESC',
                              RECHERCHE = 'NOEUD_BOUCLE',
                              SANS_GROUP_NO=('LA3A','LBINF'),
                              ),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=50,),),);

L_ARCH=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.,
                                     NOMBRE=25,),),);

U=STAT_NON_LINE(MODELE=MODE,
                CHAM_MATER=CM,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                       _F(CHARGE=CH2,),
                       ),
                COMP_ELAS=_F(RELATION='ELAS',
                             DEFORMATION='GREEN',
                             GROUP_MA='STRUCT',),
                INCREMENT=_F(LIST_INST=L_INST,
                             INST_FIN=1.,
                             SUBD_METHODE='UNIFORME', SUBD_PAS=4,
                             SUBD_PAS_MINI=1E-8,
                             ),
                NEWTON=_F(REAC_ITER=1,
                          ),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20,),
                ARCHIVAGE=_F(ARCH_ETAT_INIT='OUI',
                             LIST_INST=L_ARCH),
                INFO=1,);

U=CALC_NO(reuse =U,
          RESULTAT=U,
          OPTION='FORC_NODA',
          MODELE=MODE,
          CHAM_MATER=CM,
          EXCIT=_F(CHARGE=CH1,),);

TAB_REAC=POST_RELEVE_T(ACTION=_F(INTITULE='REAC',
                                 GROUP_NO=('SA','SB'),
                                 RESULTAT=U,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);
TAB_DEPL=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                                 GROUP_NO='A1',
                                 RESULTAT=U,
                                 NOM_CHAM='DEPL',
                                 NOM_CMP='DY',
                                 OPERATION='EXTRACTION',),);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.2),
           VALE=4.36995E+02,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='6.3.17',);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4),
           VALE=1.66834E+03,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='6.3.17',);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.6),
           VALE=3.26454E+03,
           PRECISION=0.15E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='6.3.17',);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.8),
           VALE=2.73716E+03,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='6.3.17',);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.),
           VALE=3.09524E+03,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='6.3.17',);

FIN();
           
