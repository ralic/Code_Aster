                     VALE_I=5,),
           NOM_PARA='G',
           VALE=5.677E6,
           PRECISION=1.4E-2,
           REFERENCE='ANALYTIQUE',
          );

TEST_TABLE(TABLE=G2,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=10,),
           NOM_PARA='G',
           VALE=1.135E7,
           PRECISION=1.4E-2,
           REFERENCE='ANALYTIQUE',
          );

TEST_TABLE(TABLE=G2,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=15,),
           NOM_PARA='G',
           VALE=1.703E7,
           PRECISION=1.4E-2,
           REFERENCE='ANALYTIQUE',
          );

TEST_TABLE(TABLE=GL11,
           FILTRE=_F(NOM_PARA='ABSC_CURV',
                     VALE=5.0,),
           NOM_PARA='G',
           VALE=4.0345E6,
           PRECISION=1.4E-2,
           REFERENCE='NON_REGRESSION',
          );

