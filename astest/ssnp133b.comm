DEBUT(CODE=_F( NOM = 'SSNP133B',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),PAR_LOT = 'OUI')

# NB DE PAS DE TEMPS DE CALCUL ET NOMBRE DE PAS DE TEMPS ARCHIVE
tfin=159
nbpas=tfin
nbpas_arch = tfin
c_mult_pilo = 10

# DONNEES MATERIAU
young = 30000.E6
poiss = 0.2
Gc = 100.
Sc = 2.E5

#--------------------------------------------------------------
# MAILLAGE ET MODELE
#--------------------------------------------------------------
PRE_GMSH()
MA = LIRE_MAILLAGE()


MA = DEFI_GROUP(reuse = MA,
       MAILLAGE = MA,
       CREA_GROUP_MA = (
          _F(NOM = 'FACE_SUP', GROUP_MA = 'GM4'),
          _F(NOM = 'MATIERE' , GROUP_MA = 'GM5'),
          _F(NOM = 'JOINT_G' , GROUP_MA = 'GM55'),
          _F(NOM = 'JOINT_D' , GROUP_MA = 'GM54'),
          _F(NOM = 'JOINT',    UNION = ('JOINT_G', 'JOINT_D')),          
      ),
       CREA_GROUP_NO = (
          _F(NOM = 'P_RIG',    GROUP_MA = 'GM1'),
          _F(GROUP_MA = 'FACE_SUP'),
          _F(GROUP_MA = 'JOINT'),          
          _F(NOM = 'SYME_BD',GROUP_MA = 'GM56'),
          _F(NOM = 'SYME_BG',GROUP_MA = 'GM57'),
          _F(NOM = 'SYMETRIE',UNION   = ('SYME_BD', 'SYME_BG')),
         )
       )

# RENUMEROTATION LOCALE DES ELEMENTS A DISCONTINUITE     

MA=MODI_MAILLAGE(reuse =MA,
                   MAILLAGE=MA,
                   ORIE_FISSURE=_F(GROUP_MA='JOINT'),
                   INFO=1
                   )

  
MO = AFFE_MODELE(
       MAILLAGE = MA,
       AFFE = (
         _F(
           GROUP_MA     = ('MATIERE','FACE_SUP'),
           PHENOMENE    = 'MECANIQUE',
           MODELISATION = 'D_PLAN' ),
         _F(
           GROUP_MA     = 'JOINT',
           PHENOMENE    = 'MECANIQUE',
           MODELISATION = 'PLAN_ELDI'),
        ), )

#--------------------------------------------------------------
# DEFINITION DES PARAMETRES MATERIAUX
#--------------------------------------------------------------

BETON = DEFI_MATERIAU(
                      ELAS      = _F( E              = young,
                                      NU             = poiss,
                                         ),
                     RUPT_FRAG = _F(
                                    GC             = Gc,
                                    SIGM_C         = Sc,
                                         ),
                         )
        
CHMAT = AFFE_MATERIAU(
          MAILLAGE = MA,
          AFFE = _F(
          GROUP_MA = ('MATIERE','JOINT'),
          MATER    =  BETON
         )
      )

#--------------------------------------------------------------
# DEFINITION DES CHARGEMENTS 
#--------------------------------------------------------------

# ON BLOQUE LES MOUVEMENTS DE CORPS RIGIDE DS LA DIRECTION X AU POINT P_RIG
RIGIDE = AFFE_CHAR_MECA(
  MODELE   = MO,
  DDL_IMPO = _F(GROUP_NO='P_RIG', DX = 0)
  ) 


# ON IMPOSE QUE LE PLAN DE SYMETRIE PASSE PAR LA FACE INFERIEURE DES ELEMENTS DE A DISCONTINUITE
SYMETRIE = AFFE_CHAR_MECA(
  MODELE        = MO,
  DDL_IMPO = (
    _F(
      GROUP_NO = 'SYME_BG',
      DY=0.0,
      ),
    _F(
      GROUP_NO = 'SYME_BD',
      DY=0.0,
      ),
    ),  )
    
# EFFORT IMPOSE SUR LA SURFACE SUPERIEURE DE LA PLAQUE
TRACTION = AFFE_CHAR_MECA(
  MODELE = MO,
  FORCE_CONTOUR = _F(GROUP_MA = 'FACE_SUP', FY =  1),
  )


#--------------------------------------------------------------
# REALISATION DU CALCUL
#--------------------------------------------------------------
  
INSTANTS = DEFI_LIST_REEL(
                          DEBUT = 0.,
                          INTERVALLE = (_F(JUSQU_A =  tfin, NOMBRE = nbpas), ), 
                          )
 
INS_ARCH = DEFI_LIST_REEL(
                          DEBUT = 0.,
                          INTERVALLE = (_F(JUSQU_A =  tfin, NOMBRE = nbpas_arch), ), 
                          )

DEFLIST =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST =INSTANTS ),
                        ECHEC=_F(
                  SUBD_METHODE='UNIFORME',
                  SUBD_PAS  = 10,
                  SUBD_PAS_MINI = 1.E-8,
                  SUBD_NIVEAU=10,
                                 ),)

EVOL = STAT_NON_LINE(
  MODELE = MO,
  CHAM_MATER = CHMAT,
  EXCIT = (
    _F(CHARGE = RIGIDE),
    _F(CHARGE = SYMETRIE),
    _F(CHARGE = TRACTION, 
       TYPE_CHARGE = 'FIXE_PILO',
      )
       ),
  COMP_INCR = (
    _F(RELATION = 'ELAS'      , GROUP_MA = 'MATIERE'),
    _F(RELATION = 'CZM_EXP',    GROUP_MA = 'JOINT'  ),
    ),
  INCREMENT = _F(LIST_INST = DEFLIST,),
  PILOTAGE  = _F (
    TYPE           = 'PRED_ELAS',
    GROUP_MA       = 'JOINT',
    COEF_MULT      = c_mult_pilo,
    ETA_PILO_MIN   = 0.01,
    ETA_PILO_R_MIN = 0,
    PROJ_BORNES    = 'OUI'
    ),
  NEWTON  = _F(
    MATRICE   = 'TANGENTE',
    REAC_ITER = 1,
    ),
  CONVERGENCE = _F(
    RESI_GLOB_RELA = 1.E-6,
    ITER_GLOB_MAXI = 12,
    ),
  SOLVEUR    = _F(METHODE = 'MUMPS', PCENT_PIVOT = 100),
  ARCHIVAGE = _F(
    ARCH_ETAT_INIT = 'OUI',
    LIST_INST      = INS_ARCH
    )
  ) 

#--------------------------------------------------------------
# POST-TRAITEMENT
#--------------------------------------------------------------

CALC_NO( reuse    = EVOL,
         OPTION   = 'FORC_NODA',
         RESULTAT =  EVOL,
       )

FORCE = POST_RELEVE_T(
                      ACTION = _F(
                                  INTITULE   = 'FORCE',
                                  OPERATION  = 'EXTRACTION',
                                  RESULTANTE = 'DY',
                                  RESULTAT   =  EVOL,
                                  NOM_CHAM   = 'FORC_NODA',
                                  GROUP_NO   = 'FACE_SUP',
                                  
                                 ),
                     )

TAB_U = POST_RELEVE_T(
                      ACTION = _F(
                                  INTITULE   = 'DEPL',
                                  RESULTAT   = EVOL,
                                  OPERATION  = 'EXTRACTION',
                                  NOM_CHAM   = 'DEPL',
                                  NOM_CMP    = 'DY',
                                  NOEUD      = 'N2',
                                 ),
                     )

UY = RECU_FONCTION ( TABLE  = TAB_U,
                     PARA_X = 'INST',
                     PARA_Y = 'DY' 
                   )

FY = RECU_FONCTION ( TABLE  = FORCE,
                     PARA_X = 'INST',
                     PARA_Y = 'DY'
                   )

#--------------------------------------------------------------
# TESTS SUR LA REPONSE GLOBALE
#--------------------------------------------------------------

TEST_TABLE(TABLE=TAB_U,
              FILTRE=(_F(NOM_PARA='INST',VALE=50,),),
              NOM_PARA='DY',
              VALE=3.60789E-03,
              REFERENCE='NON_REGRESSION')
TEST_TABLE(TABLE=FORCE,
              FILTRE=(_F(NOM_PARA='INST',VALE=50,),),
              NOM_PARA='DY',
              VALE=5.78596E+07,
              REFERENCE='NON_REGRESSION')

TEST_TABLE(TABLE=TAB_U,
              FILTRE=(_F(NOM_PARA='INST',VALE=75,),),
              NOM_PARA='DY',
              VALE=3.54768E-03,
              REFERENCE='NON_REGRESSION')
TEST_TABLE(TABLE=FORCE,
              FILTRE=(_F(NOM_PARA='INST',VALE=75,),),
              NOM_PARA='DY',
              VALE=5.11012E+07,
              REFERENCE='NON_REGRESSION')

TEST_TABLE(TABLE=TAB_U,
              FILTRE=(_F(NOM_PARA='INST',VALE=100,),),
              NOM_PARA='DY',
              VALE=3.39614E-03,
              REFERENCE='NON_REGRESSION')
TEST_TABLE(TABLE=FORCE,
              FILTRE=(_F(NOM_PARA='INST',VALE=100,),),
              NOM_PARA='DY',
              VALE=4.43668E+07,
              REFERENCE='NON_REGRESSION')

TEST_TABLE(TABLE=TAB_U,
              FILTRE=(_F(NOM_PARA='INST',VALE=159,),),
              NOM_PARA='DY',
              VALE=4.30701E-03,
              REFERENCE='NON_REGRESSION')
TEST_TABLE(TABLE=FORCE,
              FILTRE=(_F(NOM_PARA='INST',VALE=159,),),
              NOM_PARA='DY',
              VALE=1.00675E+07,
              REFERENCE='NON_REGRESSION')



#--------------------------------------------------------------
IMPR_FONCTION(
              FORMAT='TABLEAU',
              COURBE=_F(FONC_X = UY,
                        FONC_Y = FY,
                       )
             )

IMPR_TABLE(TABLE=TAB_U)             
IMPR_TABLE(TABLE=FORCE)             
#IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=EVOL,NOM_CHAM='VARI_ELGA',TYPE_CHAM='SCALAIRE',NOM_CMP=('V1','V2','V3','V4','V7','V8'),GROUP_MA = ('JOINT')))) 
#IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=EVOL,NOM_CHAM='DEPL',TYPE_CHAM='VECT_2D',NOM_CMP=('DX','DY'),GROUP_MA = ('MATIERE','JOINT')))) 

FIN()
