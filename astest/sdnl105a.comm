                  NOEUD = 'O8',             NOM_CMP = 'DY',
                  VALE = 1.04550E-01))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN3,
                  NOM_CHAM = 'DEPL',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'N8',             NOM_CMP = 'DY',
                  VALE = 1.64269E-02))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN3,
                  NOM_CHAM = 'VITE',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'N8',             NOM_CMP = 'DY',
                  VALE = 2.54110E-02))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN3,
                  NOM_CHAM = 'DEPL',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'M8',             NOM_CMP = 'DY',
                  VALE = 1.11633E-02))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN3,
                  NOM_CHAM = 'VITE',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'M8',             NOM_CMP = 'DY',
                  VALE = 4.42591E-02))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN3,
                  NOM_CHAM = 'DEPL',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'O8',             NOM_CMP = 'DY',
                  VALE = 5.88712E-03))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN3,
                  NOM_CHAM = 'VITE',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'O8',             NOM_CMP = 'DY',
                  VALE = 1.04587E-01))

#
## RESTITUTION D'UN DEPLACEMENT
#
LISTINST=DEFI_LIST_REEL(       DEBUT=0.,
                           INTERVALLE=_F( JUSQU_A = 1.,
                                       NOMBRE = 250))

#
FONC1=RECU_FONCTION(  RESU_GENE=TRAN_GE1,
                        NOM_CHAM='DEPL',
                        LIST_INST=LISTINST,
                            NOEUD='M8',
                             NOM_CMP='DY',
                        INTERPOL='LIN'
                       )

#
## RESTITUTION D'UN PARAMETRE DE CHOC
#
FONC2=RECU_FONCTION(   RESU_GENE=TRAN_GE1,
                        NOEUD_CHOC='M8',
                         LIST_PARA=LISTINST,
                           PARA_X='INST',
                           PARA_Y='FN',
                         INTITULE='CHOC1_2')

#
## IMPRESSION D'UN DEPLACEMENT
#
IMPR_FONCTION(FORMAT='XMGRACE',
              PILOTE='POSTSCRIPT',
              TITRE='EULER - DY NOEUD M8',
              LEGENDE_X='TEMPS(S)',
              LEGENDE_Y='DEPLACEMENT(M)',
              COURBE=_F( FONCTION = FONC1))

#
## IMPRESSION D'UN PARAMETRE DE CHOC
#
IMPR_FONCTION(FORMAT='XMGRACE', UNITE=30,
              PILOTE='POSTSCRIPT',
              TITRE='EULER - FN NOEUD M8',
              LEGENDE_X='TEMPS(S)',
              LEGENDE_Y='DEPLACEMENT(M)',
              COURBE=_F( FONCTION = FONC2))

#
## CALCULS STATISTIQUES
#
TABLE=POST_DYNA_MODA_T(  RESU_GENE=TRAN_GE1,
                      CHOC=_F( INST_INIT = 0.,
                            INST_FIN = 1.,
                            NB_BLOC = 10,
                            SEUIL_FORCE = 0.,
                            DUREE_REPOS = 0.))

#
FIN()
