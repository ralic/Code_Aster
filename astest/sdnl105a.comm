                  NOEUD = 'O8',             NOM_CMP = 'DY',
                  VALE = 5.89757E-03))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN1,
                  NOM_CHAM = 'VITE',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'O8',             NOM_CMP = 'DY',
                  VALE = 1.04583E-01))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN2,
                  NOM_CHAM = 'DEPL',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'N8',             NOM_CMP = 'DY',
                  VALE = 1.64264E-02))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN2,
                  NOM_CHAM = 'VITE',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'N8',             NOM_CMP = 'DY',
                  VALE = 2.54155E-02))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN2,
                  NOM_CHAM = 'DEPL',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'M8',             NOM_CMP = 'DY',
                  VALE = 1.11627E-02))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN2,
                  NOM_CHAM = 'VITE',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'M8',             NOM_CMP = 'DY',
                  VALE = 4.41197E-02))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN2,
                  NOM_CHAM = 'DEPL',      NUME_ORDRE = 11,
                  CRITERE = 'RELATIF',    PRECISION = 1.E-4,
                  NOEUD = 'O8',             NOM_CMP = 'DY',
                  VALE = 5.88736E-03))

#
TEST_RESU( RESU=_F( RESULTAT = TRAN2,
                  NOM_CHAM = 'VITE',      NUME_ORDRE = 11,
