DEBUT(CODE=_F(NOM='TTLL01M',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#  TIMPVAR=DEFI_CONSTANTE (
#      NOM_RESU: 'TEMP'   VALE: 100.0D0 );


TIMPVAR=DEFI_FONCTION(
              NOM_RESU='TEMP',    NOM_PARA='INST',
              VALE=(0.0E0,    0.0E0,
                    1.0E-3, 100.0E0,
                    100.,   100.0E0,)
                         )

ACIER1=DEFI_MATERIAU( THER=_F(  LAMBDA = 1.0, RHO_CP = 1.0))

#
# DEFINITION DU MATERIAU HOMOGENEISE ACIER ANALOGUE AU MATERIAU
# ISOTROPE ACIER1.
# LES COEFFICIENTS DE L'ACIER ONT ETE CALCULES EN UTILISANT DES
# POLYNOMES DE LAGRANGE .
#

ACIER=DEFI_MATERIAU( THER_COQUE=_F(
   COND_LMM = 0.533333333,     COND_TMM = 0.533333333,
   COND_LMP = 0.0666666667,    COND_TMP = 0.0666666667,
   COND_LPP = 0.1333333333,    COND_TPP = 0.1333333333,
   COND_LSI = -0.033333333,    COND_TSI = -0.0333333333,
   COND_NMM = 5.333333333,    COND_NMP = -2.6666666667,
   COND_NPP = 2.333333333,    COND_NSI = 0.3333333333,
   CMAS_MM = 0.533333333,    CMAS_MP = 0.06666666667,
   CMAS_PP = 0.1333333333,   CMAS_SI = -0.03333333333)
                         )

MAIL=LIRE_MAILLAGE( )

MAIL=DEFI_GROUP( reuse=MAIL,   MAILLAGE=MAIL,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                AFFE=_F(  TOUT = 'OUI', MATER = ACIER) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                      AFFE=_F( TOUT = 'OUI', MODELISATION = 'COQUE',
                                      PHENOMENE = 'THERMIQUE'))

#

CARA=AFFE_CARA_ELEM(  MODELE=MOTH,
                            COQUE=_F(
       GROUP_MA = 'TOUT',

                                  EPAIS = 1.,
                                  ANGL_REP = (30.,0.,)) )

TEMP0=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R',
MAILLAGE=MAIL,
                   AFFE=_F(   TOUT = 'OUI',
                           NOM_CMP = ('TEMP', 'TEMP_INF','TEMP_SUP',),
                           VALE = (0.0E0,   0.0E0,     0.0E0,    ))
                       )

# ---------------------------------------------------------------------


CHTH=AFFE_CHAR_THER_F(MODELE=MOTH,
          TEMP_IMPO=_F( GROUP_NO = 'GRNM14', TEMP = TIMPVAR,
                                       TEMP_INF = TIMPVAR,
                                       TEMP_SUP = TIMPVAR)
         )

LR8=DEFI_LIST_REEL(        DEBUT=0.,INTERVALLE=(
       _F(    JUSQU_A = 1.E-3, NOMBRE = 10),
       _F(    JUSQU_A = 1.E-2, NOMBRE = 9),
       _F(    JUSQU_A = 1.E-1, NOMBRE = 9),
       _F(    JUSQU_A = 1.E0,  NOMBRE = 9),
       _F(    JUSQU_A = 2.0,   NOMBRE = 10))
                       )

TEMPE=THER_LINEAIRE(        MODELE=MOTH,
           TEMP_INIT=_F( CHAM_NO = TEMP0),
          INCREMENT=_F( LIST_INST = LR8),
             CHAM_MATER=CHMAT, EXCIT=_F( CHARGE = CHTH),
           CARA_ELEM=CARA
             )

TEMP_1=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
RESULTAT=TEMPE, NOM_CHAM='TEMP',
             INST=0.1 )

TEMP_2=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
RESULTAT=TEMPE, NOM_CHAM='TEMP',
             INST=0.2 )

TEMP_3=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
RESULTAT=TEMPE, NOM_CHAM='TEMP',
             INST=0.7 )

TEMP_4=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
RESULTAT=TEMPE, NOM_CHAM='TEMP',
             INST=2.0 )

TEST_RESU(CHAM_NO=(
   # VALEURS DE REFERENCE:
      _F( CHAM_GD = TEMP_1, NOEUD = 'N9', NOM_CMP = 'TEMP',
                                          VALE = 65.48, PRECISION = 5.E-3),
      _F( CHAM_GD = TEMP_2, NOEUD = 'N9', NOM_CMP = 'TEMP',
                                          VALE = 75.58, PRECISION = 5.E-3),
      _F( CHAM_GD = TEMP_3, NOEUD = 'N9', NOM_CMP = 'TEMP',
                                          VALE = 93.01, PRECISION = 5.E-3),
      _F( CHAM_GD = TEMP_4, NOEUD = 'N9', NOM_CMP = 'TEMP',
                                          VALE = 99.72, PRECISION = 5.E-3),

      _F( CHAM_GD = TEMP_1, NOEUD = 'N33', NOM_CMP = 'TEMP',
                                          VALE = 8.09, PRECISION = 1.E-2),
      _F( CHAM_GD = TEMP_2, NOEUD = 'N33', NOM_CMP = 'TEMP',
                                          VALE = 26.37, PRECISION = 2.5E-2),
      _F( CHAM_GD = TEMP_3, NOEUD = 'N33', NOM_CMP = 'TEMP',
                                          VALE = 78.47, PRECISION = 1.E-2),
      _F( CHAM_GD = TEMP_4, NOEUD = 'N33', NOM_CMP = 'TEMP',
                                          VALE = 99.13, PRECISION = 5.E-3),
   # VALEURS TROUVEES JUSQU'A PRESENT:
 _F( CHAM_GD = TEMP_1, NOEUD = 'N9', NOM_CMP = 'TEMP',
                                          VALE = 65.368,  PRECISION = 2.E-3),
 _F( CHAM_GD = TEMP_2, NOEUD = 'N9', NOM_CMP = 'TEMP',
                                          VALE = 75.841,  PRECISION = 4.E-4),
 _F( CHAM_GD = TEMP_3, NOEUD = 'N9', NOM_CMP = 'TEMP',
                                          VALE = 92.874,  PRECISION = 1.E-4),
 _F( CHAM_GD = TEMP_4, NOEUD = 'N9', NOM_CMP = 'TEMP',
                                          VALE = 99.700,  PRECISION = 1.E-4),

 _F( CHAM_GD = TEMP_1, NOEUD = 'N33', NOM_CMP = 'TEMP',
                                          VALE = 8.113, PRECISION = 1.E-2),
 _F( CHAM_GD = TEMP_2, NOEUD = 'N33', NOM_CMP = 'TEMP',
                                          VALE = 25.871, PRECISION = 4.E-3),
 _F( CHAM_GD = TEMP_3, NOEUD = 'N33', NOM_CMP = 'TEMP',
                                          VALE = 78.071, PRECISION = 4.E-4),
 _F( CHAM_GD = TEMP_4, NOEUD = 'N33', NOM_CMP = 'TEMP',
                                          VALE = 99.077, PRECISION = 1.E-4))
    )

