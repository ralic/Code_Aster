#CALCUL ELASTIQUE

U1=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F( 
                CHARGE = PR_LATE,            
                FONC_MULT = EVOL_HYD,         
                ), _F( 
                CHARGE = PR_V,            
                FONC_MULT = EVOL_HYD,         
                ), _F( 
                CHARGE = DEP_SYM,            
                ), _F( 
                CHARGE = P0,            
                )),
                         COMP_INCR=_F( 
                RELATION = 'KIT_HM',          
                RELATION_KIT = (
                  'ELAS',        'LIQU_SATU',
                  'HYDR_UTIL',  ),      
                ),
                         INCREMENT=_F( 
                LIST_INST = TEMPS1,         
                ),
                       CONVERGENCE=_F( 
                RESI_GLOB_RELA = 1.E-6,    
                )
              )
