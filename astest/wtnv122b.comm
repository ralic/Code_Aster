                       CONVERGENCE=_F(
                       ITER_GLOB_MAXI=50, 
                       RESI_GLOB_RELA = 1.E-6,    
                )
              )


IMPR_RESU(RESU=_F(MAILLAGE=MAILLAGE,
                  RESULTAT=U1,
                  NOM_CHAM=('DEPL'),
                  PRECISION = 1.E-10,
                  INST = 6.,),);

