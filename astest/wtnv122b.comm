                         COMP_INCR=_F( 
                RELATION = 'KIT_HM',          
                RELATION_KIT = ('CAM_CLAY','LIQU_SATU','HYDR_UTIL',),     
                ITER_INTE_MAXI=20,     
                ),
                         ETAT_INIT=_F( 
                EVOL_NOLI = U1,        
                ),
                         INCREMENT=_F( 
                LIST_INST = TEMPS3,
                INST_INIT = 6., 
                SUBD_PAS=10,
                SUBD_PAS_MINI=0.001,
                              ),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER=1,           
                ),
                       CONVERGENCE=_F( 
                RESI_GLOB_RELA = 1.E-6, 
                ITER_GLOB_MAXI = 10,    
                )
              )
              
U1=CALC_ELEM(  reuse=U1, 
                  OPTION=('SIEF_ELNO_ELGA',
                          'VARI_ELNO_ELGA',
                          ),
                  RESULTAT=U1,
                        )

U1=CALC_NO(  reuse=U1,     RESULTAT=U1,
                       OPTION=( 'SIEF_NOEU_ELGA',
                       'VARI_NOEU_ELGA',
                           ),
