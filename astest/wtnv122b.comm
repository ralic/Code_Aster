                              THM_DIFFU=_F( 
                                R_GAZ = 8.315,  
                                RHO = 2400.,  
                                CP = 800.,  
                                BIOT_COEF = 1.,  
                                SATU_PRES = UN,  
                                D_SATU_PRES = ZERO,  
                                PESA_X = 0.,  
                                PESA_Y = 0.,  
                                PESA_Z = 0.,  
                                PERM_IN = KINT, 
                                PERM_LIQU = UN,  
                                D_PERM_LIQU_SATU = ZERO,  
                                PERM_GAZ = UN,  
                                D_PERM_SATU_GAZ = ZERO,  
                                D_PERM_PRES_GAZ = ZERO,  
                                 ),
                              THM_INIT=_F( 
                                TEMP = 293.,  
                                PRE1 = 1.0,  
                                PRE2 = 0.1E6,  
                                PORO = 0.14,  
                                PRES_VAPE = 2269.8,  
