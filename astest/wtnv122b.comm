                          MAILLAGE     = MAILLAGE,
                          CREA_GROUP_MA=(_F(NOM='ROCHE',
                                            TOUT='OUI',)),
                          CREA_GROUP_NO=(_F(NOM='ROCHES',
                                            CRIT_NOEUD='SOMMET',
                                            GROUP_MA='ROCHE'),),);
