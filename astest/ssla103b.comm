DEBUT(CODE=_F(NOM='SSLA103B',
              NIV_PUB_WEB='INTERNET',),);

M=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=M,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

E=DEFI_CONSTANTE(VALE=3.E10,);

NU=DEFI_CONSTANTE(VALE=0.2,);

BETON=DEFI_MATERIAU(ELAS_FO=_F(E=E,
                               NU=NU,
                               K_DESSIC=1.66e-05,
                               B_ENDOGE = 1.5E-5,),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=BETON,
                         TEMP_REF=20.0,
                         SECH_REF=100.), );

SECHI=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                 OPERATION='AFFE',
                 MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         NOM_CMP='TEMP',
                         VALE=100.,),);

SECHF=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                 OPERATION='AFFE',
                 MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         NOM_CMP='TEMP',
                         VALE=80.0,),);

SECH1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=(
                _F(CHAM_GD=SECHI,
                   INST=0.0,),
                _F(CHAM_GD=SECHF,
                   INST=3600.0,),),);

TEMP=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=20.0,),);

TEMP1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=(
                _F(CHAM_GD=TEMP,
                   INST=0.0,),),);

HYDRI=CREA_CHAMP(TYPE_CHAM='ELGA_HYDR_R',
                OPERATION='AFFE',
                MODELE = MO,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='HYDR',
                        VALE=0.0,),);

HYDRF=CREA_CHAMP(TYPE_CHAM='ELGA_HYDR_R',
                OPERATION='AFFE',
                MODELE = MO,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='HYDR',
                        VALE=1.0,),);
                        
HYDR1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='HYDR_ELGA',
                AFFE=(
                _F(CHAM_GD=HYDRI,
                   INST=0.0,),
                _F(CHAM_GD=HYDRF,
                   INST=3600.0,  ),),);

CH1=AFFE_CHAR_MECA(MODELE=MO,
                   FACE_IMPO=(_F(GROUP_MA='FACEXY',
                                DZ=0.0,),
                             _F(GROUP_MA='FACEYZ',
                                DX=0.0,),
                             _F(GROUP_MA='FACEXZ',
                                DY=0.0,),),);

CH2=AFFE_CHAR_MECA(MODELE=MO,
                   TEMP_CALCULEE=TEMP1,
                   SECH_CALCULEE=SECH1,
                   HYDR_CALCULEE=HYDR1);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3600.0,
                                     NOMBRE=2,  ),);

U=STAT_NON_LINE(MODELE=MO,
                CHAM_MATER=CM,
                EXCIT=(_F(CHARGE=CH1,),
                       _F(CHARGE=CH2,),),
                COMP_INCR=_F(RELATION='ELAS',),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_MAXI=1e-06,
                               ITER_GLOB_MAXI=25,
                               ITER_INTE_MAXI=100,),);

U=CALC_ELEM(reuse =U,
            MODELE=MO,
            CHAM_MATER=CM,
            RESULTAT=U,
            OPTION=('EPSI_ELNO_DEPL','EPSP_ELNO','SIEF_ELNO_ELGA'),
            EXCIT=( _F(CHARGE=CH1,),
                    _F(CHARGE=CH2,),),);

U=CALC_NO(reuse =U,
          RESULTAT=U,
          OPTION=('EPSI_NOEU_DEPL','EPSP_NOEU','SIEF_NOEU_ELGA'),
          CHAM_MATER=CM,);

TEST_RESU(RESU=(
  _F(  RESULTAT = U,  NOM_CHAM = 'EPSI_NOEU_DEPL', INST = 3600.,
         NOM_CMP = 'EPXX',   NOEUD = 'N5',
         PRECISION = 5.E-3,  REFERENCE = 'ANALYTIQUE',
         VALE = -3.47E-4),
  _F(  RESULTAT = U,  NOM_CHAM = 'EPSI_NOEU_DEPL', INST = 3600.,
         NOM_CMP = 'EPYY',   NOEUD = 'N5',
         PRECISION = 5.E-3,  REFERENCE = 'ANALYTIQUE',
         VALE = -3.47E-4),
  _F(  RESULTAT = U,  NOM_CHAM = 'SIEF_NOEU_ELGA', INST = 3600.,
         NOM_CMP = 'SIXX',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0.,CRITERE = 'ABSOLU'),
  _F(  RESULTAT = U,  NOM_CHAM = 'SIEF_NOEU_ELGA', INST = 3600.,
         NOM_CMP = 'SIYY',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0., CRITERE = 'ABSOLU'),
  _F(  RESULTAT = U,  NOM_CHAM = 'EPSP_NOEU', INST = 3600.,
         NOM_CMP = 'EPXX',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0.,CRITERE = 'ABSOLU'),
  _F(  RESULTAT = U,  NOM_CHAM = 'EPSP_NOEU', INST = 3600.,
         NOM_CMP = 'EPYY',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0., CRITERE = 'ABSOLU'),
                  ));
                  
# CALCUL AVEC MECA_STATIQUE

U2=MECA_STATIQUE(MODELE=MO,
                 CHAM_MATER=CM,
                 EXCIT=(_F(CHARGE=CH2,),
                        _F(CHARGE=CH1,),),
                 INST=3600.0,);

U2=CALC_ELEM(reuse =U2,
             RESULTAT = U2,
             CHAM_MATER=CM,
                EXCIT=(_F(CHARGE=CH1,),
                       _F(CHARGE=CH2,),),
             MODELE=MO,
             OPTION=('EPSI_ELNO_DEPL','SIGM_ELNO_DEPL'),         )
U2=CALC_NO(reuse =U2,
             RESULTAT = U2,
             CHAM_MATER=CM,
             OPTION=('EPSI_NOEU_DEPL','SIGM_NOEU_DEPL'),         )

TEST_RESU(RESU=(
  _F(  RESULTAT = U2,  NOM_CHAM = 'EPSI_NOEU_DEPL', INST = 3600.,
         NOM_CMP = 'EPXX',   NOEUD = 'N5',
         PRECISION = 5.E-3,  REFERENCE = 'ANALYTIQUE',
         VALE = -3.47E-4),
  _F(  RESULTAT = U2,  NOM_CHAM = 'EPSI_NOEU_DEPL', INST = 3600.,
         NOM_CMP = 'EPYY',   NOEUD = 'N5',
         PRECISION = 5.E-3,  REFERENCE = 'ANALYTIQUE',
         VALE = -3.47E-4),
  _F(  RESULTAT = U2,  NOM_CHAM = 'SIGM_NOEU_DEPL', INST = 3600.,
         NOM_CMP = 'SIXX',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0.,CRITERE = 'ABSOLU'),
  _F(  RESULTAT = U2,  NOM_CHAM = 'SIGM_NOEU_DEPL', INST = 3600.,
         NOM_CMP = 'SIYY',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0., CRITERE = 'ABSOLU'),
                  ));          

FIN();

