                 _F(  RESULTAT = U2,
                       GROUP_NO = 'A',
                       NOM_CHAM = 'SIGM_NOEU',
                       NOM_CMP = 'SIXX',
                       NUME_ORDRE = 100,
                       REFERENCE='SOURCE_EXTERNE',
                       PRECISION = 5.E-3,
                       VALE = -400.),

# CONTRAINTE SIYY
                 _F(  RESULTAT = U2,
                       GROUP_NO = 'A',
