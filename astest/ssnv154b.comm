                 _F(  RESULTAT = U2,
                       GROUP_NO = 'A',
                       NOM_CHAM = 'SIGM_NOEU',
                       NOM_CMP = 'SIZZ',
                       NUME_ORDRE = 50,
                       REFERENCE='SOURCE_EXTERNE',
                       PRECISION = 5.E-3,
                       VALE = -1113.00),

                 _F(  RESULTAT = U2,
                       GROUP_NO = 'A',
                       NOM_CHAM = 'SIGM_NOEU',
                       NOM_CMP = 'SIZZ',
                       NUME_ORDRE = 100,
                       REFERENCE='SOURCE_EXTERNE',
                       PRECISION = 5.E-3,
                       VALE = -1111.49),

# CONTRAINTE SIZZ
                 _F(  RESULTAT = U2,
                       GROUP_NO = 'A',
                       NOM_CHAM = 'SIGM_NOEU',
                       NOM_CMP = 'SIYY',
                       NUME_ORDRE = 10,
                       REFERENCE='SOURCE_EXTERNE',
                       PRECISION = 5.E-3,
                       VALE = -532.210),
                 _F(  RESULTAT = U2,
                       GROUP_NO = 'A',
