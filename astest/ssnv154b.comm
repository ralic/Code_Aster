                       NOM_CMP = 'SIYY',
                       NUME_ORDRE = 50,
                       REFERENCE='SOURCE_EXTERNE',
                       PRECISION = 5.E-3,
                       VALE = -637.665),
                 _F(  RESULTAT = U2,
                       GROUP_NO = 'A',
                       NOM_CHAM = 'SIGM_NOEU',
                       NOM_CMP = 'SIYY',
                       NUME_ORDRE = 100,
                       REFERENCE='SOURCE_EXTERNE',
                       PRECISION = 5.E-3,
                       VALE = -637.165),


# CONTRAINTE SIYZ
                 _F(  RESULTAT = U2,
                       GROUP_NO = 'A',
                       NOM_CHAM = 'SIGM_NOEU',
                       NOM_CMP = 'SIYZ',
                       NUME_ORDRE = 10,
                       REFERENCE='SOURCE_EXTERNE',
                       PRECISION = 5.E-3,
                       VALE = -228.994),
                 _F(  RESULTAT = U2,
                       GROUP_NO = 'A',
                       NOM_CHAM = 'SIGM_NOEU',
                       NOM_CMP = 'SIYZ',
                       NUME_ORDRE = 50,
                       REFERENCE='SOURCE_EXTERNE',
                       PRECISION = 5.E-3,
                       VALE = -411.648),
                 _F(  RESULTAT = U2,
                       GROUP_NO = 'A',
                       NOM_CHAM = 'SIGM_NOEU',
                       NOM_CMP = 'SIYZ',
