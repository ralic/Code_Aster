# SSLS105 A                  COMMANDES                         24/09/91
# HEMISPHERE DOUBLEMENT PINCE
# MAILLAGE 1/4 D'HEMISPHERE
#                                              REF: NAFEMS TEST NO LE3
# POINTS: A(10 0 0)   B(0 10 0)   C(0 0 10)
# C FIXE -- BORD AB LIBRE -- FORCE EN A: FX=-2. -- FORCE EN B: FZ=+2.
# MODELISATION DKT: 734 NOEUDS  1373 TRIA3
# NOEUDS: A=NO3       B=NO2       C=NO1
#=======================================================================
#

DEBUT(CODE=_F(  NOM = 'SSLS105A',NIV_PUB_WEB='INTERNET') )

#

MATER=DEFI_MATERIAU( ELAS=_F(  E = 6.825E7,    NU = 0.3) )

MAILL=LIRE_MAILLAGE(   )

MAILL=DEFI_GROUP( reuse=MAILL,   MAILLAGE=MAILL,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAILL,
                          AFFE=_F(  TOUT = 'OUI',   MATER = MATER) )

MODEL=AFFE_MODELE(  MAILLAGE=MAILL,    VERIF='MAILLE',
                        AFFE=_F( TOUT = 'OUI',   MODELISATION = 'DKT',
                              PHENOMENE = 'MECANIQUE') )

CHARGE=AFFE_CHAR_MECA(  MODELE=MODEL,DDL_IMPO=(
            _F( GROUP_NO = 'BCSANSBC',  DX = 0., DRY = 0., DRZ = 0.),
                     _F( GROUP_NO = 'ACSANSAC',  DY = 0., DRX = 0., DRZ = 0.),
                     _F( GROUP_NO = 'A',         DY = 0.,
                                         DRX = 0.,          DRZ = 0.),
                     _F( GROUP_NO = 'B',         DX = 0.,
                                                  DRY = 0., DRZ = 0.),
                     _F( GROUP_NO = 'C',         DX = 0., DY = 0., DZ = 0.,
                                         DRX = 0., DRY = 0., DRZ = 0.)),FORCE_NODALE=(
        _F( GROUP_NO = 'A',         FX = -2.),
                     _F( GROUP_NO = 'B',         FY = +2.)) )

CARELEM=AFFE_CARA_ELEM(   MODELE=MODEL,
                               COQUE=_F(  
       GROUP_MA = 'TOUT',
  EPAIS = 0.04) )

MATELEM=CALC_MATR_ELEM(   MODELE=MODEL,        CHARGE=CHARGE,
                               CARA_ELEM=CARELEM,
                               CHAM_MATER=CHMAT,
                               OPTION='RIGI_MECA'   )

VEL=CALC_VECT_ELEM(   CHARGE=CHARGE,    OPTION='CHAR_MECA' )

NU=NUME_DDL(   MATR_RIGI=MATELEM )

MATASS=ASSE_MATRICE(   MATR_ELEM=MATELEM,   NUME_DDL=NU )

DEPLA=ASSE_VECTEUR(   VECT_ELEM=VEL,       NUME_DDL=NU )

MATASS=FACT_LDLT( reuse=MATASS,  MATR_ASSE=MATASS )

DEPLA=RESO_LDLT( reuse=DEPLA,  MATR_FACT=MATASS,        CHAM_NO=DEPLA )

TEST_RESU(CHAM_NO=( 
       _F(  CHAM_GD = DEPLA, REFERENCE = 'SOURCE_EXTERNE',
       GROUP_NO = 'A',    NOM_CMP = 'DX',   VALE = -0.185,
         CRITERE = 'RELATIF',   PRECISION = 1.E-2),
       _F(  CHAM_GD = DEPLA, REFERENCE = 'SOURCE_EXTERNE',
       GROUP_NO = 'B',    NOM_CMP = 'DY',   VALE = +0.185,
         CRITERE = 'RELATIF',   PRECISION = 1.E-2)) )

FIN()
#
