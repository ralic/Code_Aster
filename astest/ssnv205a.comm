# PRESSION DE CONSOLIDATION P0 = 50KPA


DEBUT(CODE=_F(NOM='SSNV205A',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'));

# modules mecaniques [kPa]
K=516.2E6
G=238.2E6
# =>
YOUNG = 9.*K*G /(3.*K+G)
POISSON = (3.*K-2.*G) /(6.*K+2.*G)

P0 = - 5.E4

