#---------------------------------------------------------------------
#  - CALCUL EN ANALYSE MODALE DYNAMIQUE D'UNE TOLE EN LIBRE-LIBRE
#
#  - MODELISATION AVEC DES ELEMENTS 'COQUE_D_PLAN'
#---------------------------------------------------------------------
#
DEBUT( CODE=_F( NOM = 'SDLS501B',NIV_PUB_WEB='INTERNET'))

#
#PRE_IDEAS ();
#
MA=LIRE_MAILLAGE( )

#
MO=AFFE_MODELE(   MAILLAGE=MA,
                        AFFE=_F( TOUT = 'OUI',
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'COQUE_D_PLAN')
                  )

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CAREL=AFFE_CARA_ELEM(   MODELE=MO,
                           COQUE=_F( GROUP_MA = 'TOUT_ELT',
                                  EPAIS = 0.05,
                                  MODI_METRIQUE = 'OUI')
                        )

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
ACIER=DEFI_MATERIAU(   ELAS=_F( E = 2.E+11,
                               NU = 0.3,
                               RHO = 7800.)
                       )

#
CHAM_MAT=AFFE_MATERIAU(   MAILLAGE=MA,
                                AFFE=_F( TOUT = 'OUI',
                                      MATER = ACIER)
                          )

#
#---------------------------------------------------------------------
#                     RESOLUTION
#---------------------------------------------------------------------
#
K=CALC_MATR_ELEM(       MODELE=MO,
                      CHAM_MATER=CHAM_MAT,
                       CARA_ELEM=CAREL,
                          OPTION='RIGI_MECA'
                    )

#
M=CALC_MATR_ELEM(       MODELE=MO,
                      CHAM_MATER=CHAM_MAT,
                       CARA_ELEM=CAREL,
                          OPTION='MASS_MECA'
                    )

#
NUM=NUME_DDL(   MATR_RIGI=K)

#
MAT_AS_K=ASSE_MATRICE(   MATR_ELEM=K,    NUME_DDL=NUM)

#
MAT_AS_M=ASSE_MATRICE(   MATR_ELEM=M,    NUME_DDL=NUM)

#
MOD=MODE_ITER_SIMULT(      MATR_A=MAT_AS_K,
                             MATR_B=MAT_AS_M,
                            METHODE='TRI_DIAG',
                          CALC_FREQ=_F( OPTION = 'BANDE',
                                     FREQ = (500., 5000.,),)
                        )

#
IMPR_RESU( RESU=_F( FORMAT = 'IDEAS',
                  MAILLAGE = MA,
                  RESULTAT = MOD)
           )

#
#---------------------------------------------------------------------
#                     VERIFICATIONS DES RESULTATS
#---------------------------------------------------------------------
#
TEST_RESU(RESU=(
       _F( RESULTAT = MOD,               PARA = 'FREQ',
             CRITERE = 'RELATIF',    PRECISION = 0.01,
             NUME_ORDRE = 1,                 VALE = 658.2432),

       _F( RESULTAT = MOD,               PARA = 'FREQ',
             CRITERE = 'RELATIF',    PRECISION = 0.01,
             NUME_ORDRE = 2,                 VALE = 1749.3488),

       _F( RESULTAT = MOD,               PARA = 'FREQ',
             CRITERE = 'RELATIF',    PRECISION = 0.01,
             NUME_ORDRE = 3,                 VALE = 3225.4161))
           )

#
FIN()
#
