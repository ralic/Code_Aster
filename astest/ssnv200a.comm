#----------------------------------------------------------------------
# CAS - TEST DE NON REGRESSION DU CODE ASTER
# ATTENTION : LES SOLUTIONS DE REFERENCE ONT ETE OBTENUES
# PAR UN PREMIER PASSAGE DU CODE_ASTER: 6.0.15
#
DEBUT(CODE=_F(NOM='SSNV200A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

CUBE=LIRE_MAILLAGE(  )

TROISD=AFFE_MODELE( MAILLAGE=CUBE,
                    AFFE=_F( PHENOMENE = 'MECANIQUE',
                             MODELISATION = '3D',
                             TOUT = 'OUI'))

R_0_T=DEFI_CONSTANTE(VALE = 72.0 )

ALPHA_T=DEFI_CONSTANTE(VALE = 0.3 )

M_T=DEFI_CONSTANTE(VALE = 0.1 )

A_T=DEFI_CONSTANTE(VALE = 312.0 )

B_T=DEFI_CONSTANTE(VALE = 30.0 )

C1_T=DEFI_CONSTANTE(VALE = -0.012 )

C_INF_T=DEFI_CONSTANTE(VALE = 0.065 )

S_T=DEFI_CONSTANTE(VALE = 450.0 )

ACIER1=DEFI_MATERIAU(
                                    ELAS=_F(   E = 200000.,
                                            NU = .3,
                                            ALPHA = 0.),

                               TAHERI_FO=_F(   R_0 = R_0_T,
                                            ALPHA = ALPHA_T,
                                            M = M_T,
                                            A = A_T,
                                            B = B_T,
                                            C1 = C1_T,
                                            C_INF = C_INF_T,
                                            S = S_T),
                            LEMAITRE=_F(       N = 11.,
                                            UN_SUR_K = 3.28410E-04,
                                            UN_SUR_M = 0.17857)
                           )

MAT1=AFFE_MATERIAU(           MAILLAGE=CUBE,
                                    AFFE=_F(  MAILLE = 'MA1',
                                           MATER = ACIER1,
                                           ))

LINST=DEFI_LIST_REEL(             DEBUT=0.,INTERVALLE=(
                              _F( JUSQU_A = .35, NOMBRE = 1),
                                         _F( JUSQU_A = 1.,  NOMBRE = 5))
                                         )

TR_CS=AFFE_CHAR_MECA(
                    MODELE=TROISD,DDL_IMPO=(
                _F(  NOEUD = 'NO4', DX = 0.,DY = 0.),
                         _F(  NOEUD = 'NO8', DX = 0.,DY = 0.,DZ = 0.),
                         _F(  NOEUD = 'NO2', DX = 0.),
                         _F(  NOEUD = 'NO6', DX = 0.)),FORCE_NODALE=(
            _F(  NOEUD = ('NO1', 'NO5', 'NO3', 'NO7',),       FY = -1.),
                         _F(  NOEUD = ('NO3', 'NO7',),  FX = -2.),
                         _F(  NOEUD = ('NO2', 'NO6',),  FY = 1.))
                                                                     )

COEF=DEFI_FONCTION(
                              NOM_PARA='INST',
                            PROL_DROITE='LINEAIRE',
                           PROL_GAUCHE='LINEAIRE',
                                  VALE=( 0.,  0.,
                                         1.,  1.,
                                         2.,  22.,
                                       ))

SOLNL1=STAT_NON_LINE(
               TITRE='TEST TRACTION_CISAILLEMENT MODELE TAHERI',
                            MODELE=TROISD,
                        CHAM_MATER=MAT1,
                             EXCIT=_F(
                CHARGE = TR_CS,
                FONC_MULT = COEF,
                TYPE_CHARGE = 'FIXE_CSTE'),
                         COMP_INCR=_F(
                RELATION = 'VISC_TAHERI',
                DEFORMATION = 'PETIT',
                ITER_INTE_MAXI = 1000,
                ITER_INTE_PAS = 10,
                TOUT = 'OUI'),
                         INCREMENT=_F(
                LIST_INST = LINST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_INCR = 1,
                REAC_ITER = 10),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 20,
                RESI_GLOB_RELA = 1.E-6),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 4,
                RESI_LINE_RELA = 1.E-3),
                INFO=2
              )

SOLNL1=CALC_ELEM( reuse=SOLNL1,     RESULTAT=SOLNL1,
                               OPTION=( 'SIEF_ELNO',
                                        'VARI_ELNO_ELGA',
                                        'EPSI_ELNO_DEPL',
                                        'EPSP_ELGA',
                                        'EPSP_ELNO',
                                      )
                      )

#
ENGENDRE_TEST (  CO = SOLNL1, )

TEST_RESU(RESU=(

  _F(  REFERENCE = 'NON_REGRESSION',
         RESULTAT = SOLNL1,  NOM_CHAM = 'EPSI_ELNO_DEPL',  NUME_ORDRE = 6,
         MAILLE = 'MA1',  NOEUD = 'NO1',  NOM_CMP = 'EPXX',  VALE = 2.0E-05,
         PRECISION = 1.E-10),

  _F(  REFERENCE = 'NON_REGRESSION',
         RESULTAT = SOLNL1,  NOM_CHAM = 'EPSI_ELNO_DEPL',  NUME_ORDRE = 6,
         MAILLE = 'MA1',  NOEUD = 'NO1',  NOM_CMP = 'EPXY',  VALE = 2.60E-5,
         PRECISION = 1.E-10),

  _F(  RESULTAT = SOLNL1,  NOM_CHAM = 'EPSP_ELGA',       NUME_ORDRE = 6,
         CRITERE = 'ABSOLU', MAILLE = 'MA1',  POINT = 1,    NOM_CMP = 'EPXX',
         VALE = 0.0,            REFERENCE = 'NON_REGRESSION',
         PRECISION = 1.E-10),

  _F(  RESULTAT = SOLNL1,  NOM_CHAM = 'EPSP_ELGA',       NUME_ORDRE = 6,
         MAILLE = 'MA1',  POINT = 1,    NOM_CMP = 'EPXY', CRITERE = 'ABSOLU',
         REFERENCE = 'NON_REGRESSION',         VALE = 0.0,
          PRECISION = 1.E-10),

  _F(  RESULTAT = SOLNL1,  NOM_CHAM = 'EPSP_ELNO',       NUME_ORDRE = 6,
         MAILLE = 'MA1',  NOEUD = 'NO1',  NOM_CMP = 'EPXX',  CRITERE = 'ABSOLU',
         REFERENCE = 'NON_REGRESSION',        VALE = 0.0,
         PRECISION = 1.E-10),

  _F(  RESULTAT = SOLNL1,  NOM_CHAM = 'EPSP_ELNO',       NUME_ORDRE = 6,
         MAILLE = 'MA1',  NOEUD = 'NO1',  NOM_CMP = 'EPXY',  CRITERE = 'ABSOLU',
         REFERENCE = 'NON_REGRESSION',       VALE = 0.0,
         PRECISION = 1.E-10),

  _F(  RESULTAT = SOLNL1,  NOM_CHAM = 'VARI_ELNO_ELGA',  NUME_ORDRE = 6,
         MAILLE = 'MA1',  NOEUD = 'NO1',  NOM_CMP = 'V1', CRITERE = 'ABSOLU',
         REFERENCE = 'NON_REGRESSION',        VALE = 0.0,
         PRECISION = 1.E-10),

  _F(  RESULTAT = SOLNL1,  NOM_CHAM = 'VARI_ELNO_ELGA',  NUME_ORDRE = 6,
         MAILLE = 'MA1',  NOEUD = 'NO1',  NOM_CMP = 'V2',
         REFERENCE = 'NON_REGRESSION',        VALE = 64.800195094755,
         PRECISION = 5.E-6))
         )

FIN()
