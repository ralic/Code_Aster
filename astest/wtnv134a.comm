                                 RESULTAT = U2,
                                 NOM_CHAM = 'SIGM_NOEU',
                                 TOUT_CMP = 'OUI',
                                 OPERATION= 'EXTRACTION'));

SIGMA_N = FORMULE(NOM_PARA=('SIXX','SIZZ'), VALE='0.5*(SIXX+SIZZ)');
SIGMA_D = FORMULE(NOM_PARA=('SIXX','SIZZ'), VALE='0.5*(SIXX-SIZZ)');

TAB_EPS2=POST_RELEVE_T(ACTION=_F(INTITULE = 'EPSI_NOEU',
                                 GROUP_NO = 'C',
                                 RESULTAT = U2,
                                 NOM_CHAM = 'EPSI_NOEU',
                                 TOUT_CMP = 'OUI',
                                 OPERATION= 'EXTRACTION'));

TAB_SIG2=CALC_TABLE(reuse=TAB_SIG2,
                    TABLE=TAB_SIG2,
                    ACTION=(_F(OPERATION='OPER',
                               FORMULE  =SIGMA_N,
                               NOM_PARA ='SIGMA_N',),
                            _F(OPERATION='OPER',
                               FORMULE  =SIGMA_D,
                               NOM_PARA ='SIGMA_D',),),);

TAB_VAR2=POST_RELEVE_T(ACTION=_F(INTITULE = 'VARI_NOEU',
                                 GROUP_NO = 'C',
                                 RESULTAT = U2,
                                 NOM_CHAM = 'VARI_NOEU',
                                 NOM_CMP  = ('V1','V2','V3','V4','V5','V6',
                                            'V7','V8','V23'),
                                 OPERATION= 'EXTRACTION'));

IMPR_TABLE(TABLE=TAB_SIG2);
IMPR_TABLE(TABLE=TAB_EPS2);
IMPR_TABLE(TABLE=TAB_VAR2);

