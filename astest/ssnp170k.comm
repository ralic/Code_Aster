DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),
      INFO=1,);

lisi=DEFI_LIST_REEL(DEBUT=0,
                    INTERVALLE=_F(JUSQU_A=1,
                                  PAS=1,),);
LINST=DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST=lisi,),
                         ECHEC=_F(EVENEMENT='ERREUR',
                         ACTION='DECOUPE',),);                  
RAMPE=DEFI_FONCTION(NOM_PARA='INST',VALE=(0,0,1,1),); 

Mail=LIRE_MAILLAGE(UNITE=20,
                   FORMAT='MED',);
                   
Mail=MODI_MAILLAGE(reuse=Mail,
                   MAILLAGE=Mail,
                   ORIE_PEAU_3D=_F(GROUP_MA=('Group_2','Group_1','Group_3','Group_4',),),);                       

Mail2=CREA_MAILLAGE(MAILLAGE=Mail,
                    DECOUPE_LAC=_F(GROUP_MA_ESCL='Group_1',),)
                    
mat1=DEFI_MATERIAU(ELAS=_F(E=2000,
                           NU=0.3,),);

MODI=AFFE_MODELE(MAILLAGE=Mail2,
                    AFFE=_F(TOUT='OUI',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='3D',),);

AFFE=AFFE_MATERIAU(MAILLAGE=Mail2,
                   MODELE=MODI,
                   AFFE=_F(TOUT='OUI',
                           MATER=mat1,),);

DEFICO=DEFI_CONTACT(MODELE=MODI,
                    INFO=1,
                    FORMULATION='CONTINUE',
                    ALGO_RESO_GEOM='NEWTON',
                    ALGO_RESO_CONT='NEWTON',
                    ZONE=_F(APPARIEMENT='MORTAR',
                            GROUP_MA_MAIT='Group_2',
                            GROUP_MA_ESCL='Group_1',
                            ALGO_CONT='LAC',
                            #CONTACT_INIT='OUI',
                ),);

CHAR1=AFFE_CHAR_CINE(MODELE=MODI,
                     MECA_IMPO=(_F(GROUP_MA='Symx',
                                   DX=0,),
                                _F(GROUP_MA='Symy',
                                   DY=0,),
                                _F(GROUP_MA='Group_3',
                                   DZ=0,),),);
CHAR2= AFFE_CHAR_MECA(MODELE=MODI,
                             PRES_REP=_F(GROUP_MA='Group_4',
                             PRES=25,), );

 

RES=STAT_NON_LINE(INFO=2,
                  AFFICHAGE=_F(INFO_RESIDU='OUI',),
                  MODELE=MODI,
                  CHAM_MATER=AFFE,
                  EXCIT=(_F(CHARGE=CHAR1,
                              FONC_MULT=RAMPE),
                         _F(CHARGE=CHAR2,
                              FONC_MULT=RAMPE,
                            #TYPE_CHARGE='SUIV',
                            ), ),  
                  CONTACT=DEFICO,
                  NEWTON=_F(MATRICE='TANGENTE',
                            REAC_ITER=1,),    
                  COMPORTEMENT=_F(RELATION='ELAS',
                                    DEFORMATION='PETIT',
                                  TOUT='OUI',),
                  INCREMENT=_F(LIST_INST=LINST,),
                  SOLVEUR=_F(ELIM_LAGR='NON',
                             METHODE='MUMPS',
                             RESI_RELA=1.E-6,
                             PCENT_PIVOT= 30,),
                  CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                 ITER_GLOB_MAXI=50,));

CALC_CHAMP(reuse=RES,
           RESULTAT=RES,
           CONTRAINTE=('SIEF_NOEU'),);



TEST_RESU(RESU=(_F(INST=1.0,
                   TYPE_TEST='MIN',
                   RESULTAT=RES,
                   REFERENCE = 'ANALYTIQUE',
                   NOM_CHAM='SIEF_NOEU',
                   LEGENDE='CALC_PRESSION',
                   NOM_CMP='SIZZ',
                   VALE_REFE=-25.0,
                   VALE_CALC=-25.0,
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MAX',
                   RESULTAT=RES,
                   REFERENCE = 'ANALYTIQUE',
                   NOM_CHAM='SIEF_NOEU',
                   LEGENDE='CALC_PRESSION',
                   NOM_CMP='SIZZ',
                   VALE_REFE=-25.0,
                   VALE_CALC=-25.00,
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MIN',
                   RESULTAT=RES,
                   REFERENCE = 'ANALYTIQUE',
                   NOM_CHAM='DEPL',
                   LEGENDE='LAGS_C',
                   NOM_CMP='LAGS_C',
                   VALE_REFE=-25.0,
                   VALE_CALC=-25.0,
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MAX',
                   RESULTAT=RES,
                   REFERENCE = 'ANALYTIQUE',
                   NOM_CHAM='DEPL',
                   LEGENDE='LAGS_C',
                   NOM_CMP='LAGS_C',
                   VALE_REFE=-25.0,
                   VALE_CALC=-25.0,
                   ),
                ),
          ); 

TEST_RESU(RESU=(_F(INST=1.0,
                   TYPE_TEST='MIN',
                   RESULTAT=RES,
                   REFERENCE = 'ANALYTIQUE',
                   NOM_CHAM='CONT_ELEM',
                   LEGENDE='CONT_ELEM',
                   NOM_CMP='X1',
                   VALE_REFE=-25.0,
                   VALE_CALC=-25.0,
                   ),
                _F(INST=1.0,
                   TYPE_TEST='MAX',
                   RESULTAT=RES,
                   REFERENCE = 'ANALYTIQUE',
                   NOM_CHAM='CONT_ELEM',
                   LEGENDE='CONT_ELEM',
                   NOM_CMP='X1',
                   VALE_REFE=-25.0,
                   VALE_CALC=-25.00,
                   ),
                ),
          );                                                    
                                       
FIN()
