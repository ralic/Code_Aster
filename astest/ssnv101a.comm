# TEST VPCS PLAQUE CARREE EN TRACTION CISAILLEMENT CALCUL 3D
# MODELE ELASTOPLASTIQUE DE CHABOCHE AVEC TEMPERATURE IMPOSEE
# MAILLAGE : ELEMENT 3D CUBE A 8 NOEUDS
# UNITES   : NEWTON , MILLIMETRE , SECONDE (N,MM,S)
#
DEBUT(     CODE=_F(  NOM = 'SSNV101A',NIV_PUB_WEB='INTERNET'))

CUBE=LIRE_MAILLAGE(    )

TROISD=AFFE_MODELE(            MAILLAGE=CUBE,
                                    AFFE=_F(   PHENOMENE = 'MECANIQUE',
                                            MODELISATION = '3D',
                                            TOUT = 'OUI'))

C1_I=187.*341.
C2_I=29.*17184.

# LE C_I DU MODELE VISC_CIN1_CHAB EST DETERMINE PAR C_I = C1*A1
ACIER=DEFI_MATERIAU(    
                                    ELAS=_F(   E = 145200.,             
                                            NU = .3,            
                                            ALPHA = 0.,          ),
                           CIN2_CHAB=_F(   R_I = 151.,           
                                            R_0 = 87.,           
                                            B = 2.3,             
                                            K = .43,
                                            W = 6.09,
                                            C1_I = C1_I,           
                                            G1_0 = 341.,            
                                            C2_I = C2_I,           
                                            G2_0 = 17184.,            
                                             ))

MAT=AFFE_MATERIAU(            MAILLAGE=CUBE,
                                    AFFE=_F(  MAILLE = 'MA1',
                                           MATER = ACIER,
                                           TEMP_REF = 20.))

#LINST = DEFI_LIST_REEL   ( DEBUT       :   .4
LINST=DEFI_LIST_REEL(              DEBUT=0.,INTERVALLE=(
                              _F( JUSQU_A = 0.4, NOMBRE = 1),
                              _F( JUSQU_A = 1.435, NOMBRE = 12)))

TR_CS=AFFE_CHAR_MECA( 
            MODELE=TROISD,DDL_IMPO=(
           _F( NOEUD = 'NO4', DX = 0.,DY = 0.),
                    _F( NOEUD = 'NO8', DX = 0.,DY = 0.,DZ = 0.),
                    _F( NOEUD = 'NO2', DX = 0.),
                    _F( NOEUD = 'NO6', DX = 0.)),FORCE_NODALE=(
           _F( NOEUD = ('NO3', 'NO7',),  FX = -50.,  FY = -25.),
                        _F( NOEUD = ('NO4', 'NO8',),  FX = -25.,  FY = 25.),
                        _F( NOEUD = ('NO2', 'NO6',),  FX = 25.,  FY = 25.),
                        _F( NOEUD = ('NO1', 'NO5',),           FY = -25.)) )

COEF=DEFI_FONCTION(      NOM_PARA='INST',
                            PROL_DROITE='LINEAIRE',
                           PROL_GAUCHE='LINEAIRE',
                          VALE=( 0.0,  0.0,
                                 1.0,  1.0,    ))

SOLNL=STAT_NON_LINE( 
               TITRE='TEST TRACTION_CISAILLEMENT LOI CHABOCHE',
                            MODELE=TROISD,
                        CHAM_MATER=MAT,
                EXCIT=_F( 
                CHARGE = TR_CS,
                FONC_MULT = COEF,),
                COMP_INCR=_F( RELATION = 'VMIS_CIN2_CHAB',),
                INCREMENT=_F(  LIST_INST = LINST),
                NEWTON=_F(REAC_ITER = 1),
              )

# 

TEST_RESU(RESU=( 
           _F(  RESULTAT = SOLNL,          NUME_ORDRE = 13,
             PARA = 'INST',             VALE = 1.43500E+00),
           _F(  RESULTAT = SOLNL,          NUME_ORDRE = 13,
             PARA = 'ITER_GLOB',        VALE_I = 0,      
             CRITERE = 'ABSOLU',         PRECISION = 7.),
           _F(  RESULTAT = SOLNL,          NUME_ORDRE = 13,
             CRITERE = 'ABSOLU',        PRECISION = 1.E-06, 
             PARA = 'RESI_GLOB_RELA',   VALE = 0.),
           _F(  RESULTAT = SOLNL,          NUME_ORDRE = 13,
             CRITERE = 'ABSOLU',        PRECISION = 1.E-04,  
             PARA = 'RESI_GLOB',        VALE = 0.),
           _F(  RESULTAT = SOLNL,          NUME_ORDRE = 13,
             CRITERE = 'ABSOLU',         PRECISION = 2.,
             PARA = 'ITER_LINE',        VALE_I = 0))
           )

SOLNL=CALC_ELEM( reuse=SOLNL,       
                            RESULTAT=SOLNL,
                              OPTION=( 'VARI_ELNO_ELGA',
                                       'EPSI_ELNO_DEPL', 
                                       'SIEF_ELNO_ELGA', 
                                       'EPSP_ELGA',)
                      )

TEST_RESU(RESU=( 
           _F(  RESULTAT = SOLNL,          NUME_ORDRE = 13,
             NOM_CHAM = 'SIEF_ELNO_ELGA',  
             MAILLE = 'MA1',  NOEUD = 'NO1',  NOM_CMP = 'SIXX',  VALE = 1.435E+2),

           _F(  RESULTAT = SOLNL,          NUME_ORDRE = 13,
             NOM_CHAM = 'EPSI_ELNO_DEPL',  PRECISION = 1.1E-02,
             MAILLE = 'MA1',  NOEUD = 'NO1',  NOM_CMP = 'EPXX',  VALE = 9.709E-2),

           _F(  RESULTAT = SOLNL,          NUME_ORDRE = 13,
             NOM_CHAM = 'EPSI_ELNO_DEPL',  PRECISION = 1.1E-02,
             MAILLE = 'MA1',  NOEUD = 'NO1',  NOM_CMP = 'EPXY',  VALE = 1.454E-1),

           _F(  RESULTAT = SOLNL,          NUME_ORDRE = 13,
             NOM_CHAM = 'VARI_ELNO_ELGA', PRECISION = 1.1E-02,
             MAILLE = 'MA1',  NOEUD = 'NO1',  NOM_CMP = 'V1',  VALE = 1.922E-1))
           )

FIN( )
#
