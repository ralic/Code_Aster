DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
##########################
# LECTURE MAILLAGE GIBI
##########################

MAIL = LIRE_MAILLAGE(FORMAT='MED',);

#########################################################################
#   MODELISATION D UNE EXCAVATION SANS SOUTENEMENT D UN TUNNEL EN D.P
#########################################################################
# CE CAS TEST CONCERNE L EXCAVATION D UNE GALERIE CIRCULAIRE EN MILIEU
# ELASTIQUE LINEAIRE, EN DEFORMATION PLANE ET EN SUPPOSANT QUE LE CHAMP
# DE CONTRAINTE INITIAL EST ISOTROPE
#
# CE CAS TEST EST DECRIT DANS LA NOTE U2.04.06 (cas numero 1)
#
#########################################################################


#####################################
# DEFINITION DES GROUPES DE NEOUDS
#####################################

MAIL = DEFI_GROUP(reuse=MAIL,
                  MAILLAGE=MAIL,
                  CREA_GROUP_NO=(_F(GROUP_MA='SOL'),
                                 _F(GROUP_MA='SOL_REST'),
                                 _F(NOM='NO_HAUT',
                                    GROUP_MA='MA_HAUT'),
                                 _F(GROUP_MA='NO_DROIT'),
                                 _F(GROUP_MA='NO_LEFT1'),
                                 _F(GROUP_MA='NO_LEFT2'),
                                 _F(GROUP_MA='NO_LEFT3'),
                                 _F(GROUP_MA='NO_BAS1'),
                                 _F(GROUP_MA='NO_BAS2'),
                                 _F(GROUP_MA='NO_BAS3'),
                                 _F(GROUP_MA='LEFT_BET'),
                                 _F(GROUP_MA='BAS_BETO'),
                                 _F(GROUP_MA='BORD'),
                                 _F(NOM='NOEUD1',
                                    NOEUD='NO1'),
                                 _F(NOM='NOEU8359',
                                   NOEUD='NO8359'),
                                 _F(NOM='BORD_SOL',
                                  DIFFE=('BORD','NOEUD1','NOEU8359'),),),),


##################################################
# MODELE DU SOL AVANT L EXCAVATION POUR L'ETAPE
# D'INITIIALISATION DU CHAMP DE CONTRAINTES
##################################################

MO=AFFE_MODELE(MAILLAGE=MAIL,
               AFFE=(_F(GROUP_MA=('SOL','MA_HAUT'),
                        PHENOMENE='MECANIQUE',
                        MODELISATION='D_PLAN',),),);


E_SOL = 4.0E9
NU_FICTIF = 0.49999
NU_SOL = 0.3
E_BETON = 2.0E10
NU_BETON = 0.2
RHO_BIDON = 2000.
ALPHA_BIDON = 0.

################################################
# SOL POUR INITALISER LES CONTRAINTES
################################################

SOL0=DEFI_MATERIAU(ELAS=_F(E=E_SOL,
                          NU=NU_FICTIF,
                          RHO=RHO_BIDON,
                          ALPHA = ALPHA_BIDON,),);

#########################################
# MATERIAU SOL REEL (DONNEES DU CALCUL)
#########################################

SOL=DEFI_MATERIAU(ELAS=_F(E=E_SOL,
                           NU=NU_SOL,
                           RHO=RHO_BIDON,
                           ALPHA=ALPHA_BIDON,),);

####################################################
# MATERIAU VIDE
####################################################
VIDE=DEFI_MATERIAU(ELAS=_F(E=0.0001,
                           NU=0.2,
                           RHO=RHO_BIDON,
                           ALPHA=ALPHA_BIDON,),);



CHMAT0=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(GROUP_MA='SOL',
                             MATER=SOL0,),),);
#
CHMAT1=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(GROUP_MA='SOL_REST',
                             MATER=SOL,),
                         _F(GROUP_MA='EXCAV',
                            MATER=VIDE,),
                         _F(GROUP_MA='BETON',
                            MATER=VIDE,),
                       ),);


########################################################
#             LISTE DES INSTANTS DE CALCUL
########################################################
# DE 0 A 1  ==>  POUR LA PHASE D INITIALISATION
# DE 1 A 10  ==>  POUR LE BLOCAGE DU BORD DE LA GALERIE
# 10 CORRESPOND A UN TEMPS DE DECONFINEMENT = 0
# 500 CORRESPOND A UN TEMPS DE DECONFINEMENT = 50 %
# 1000 CORRESPOND A UN TEMPS DE DECONFINEMENT = 100 %
########################################################

LI=DEFI_LIST_REEL(DEBUT=0,
                  INTERVALLE=(_F(JUSQU_A=1.0,
                                 NOMBRE=1,),
                              _F(JUSQU_A=10.0,
                                 NOMBRE=1,),
                              _F(JUSQU_A=500.0,
                                 NOMBRE=1,),
                              _F(JUSQU_A=1000,
                                 NOMBRE=1,),),);


##################################################
#  LISTE DES INSTANTS POUR LE CALCUL BIDON POUR
#  INITIALISER LES CHAMPS A 0 DANS LE BETON
##################################################

LI1=DEFI_LIST_REEL(DEBUT=0,
                   INTERVALLE=(_F(JUSQU_A=50,
                                  NOMBRE=1),),);


######################################################
# FO FONCTION MULTIPLICATRICE POUR LE DECONFINEMENT
######################################################

F0=DEFI_FONCTION(NOM_PARA='INST',
                 VALE=(10.0,1.0,
                        500.0,0.5,
                        1000.0,0.0,),);


######################################################################
#  CONDITIONS AUX LIMITES EN DEPLACEMENTS SUR LE DOMAINE COMPLET =
#       SYMETRIE SUR LES COTES LATERAUX                 => DX=0
#       CONTINUITE SUR LA PARTIE INFERIEURE             => DY=0
#       POIDS DES TERRES SUR LA FACE SUPERIEURE         => PRES
# CETTE PRESSION EST NECESSAIRE POUR MAINTENIR UN ETAT DE CONTRAINTE
# COMPATIBBLE AVEC LE CHAMP DE CONTRAINTE INITIAL
######################################################################

CH0=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='NO_DROIT',
                                 DX=0.0,),
                              _F(GROUP_NO='NO_LEFT1',
                                 DX=0.0,),
                              _F(GROUP_NO='NO_BAS1',
                                 DY=0.0,),),
                    PRES_REP=_F(GROUP_MA='MA_HAUT',
                    PRES=5.0E6,),),
#################################################
#   1ERE PHASE : INITIALISATION DES CONTRAINTES PAR SNL
#################################################

RESU1=STAT_NON_LINE(MODELE=MO,
                 CHAM_MATER=CHMAT0,
                 EXCIT=(_F(CHARGE=CH0,),
                       ),
                 COMPORTEMENT=(_F(RELATION='ELAS',
                             GROUP_MA='SOL',),),
                 INCREMENT=_F(LIST_INST=LI,
                              INST_INIT=0,
                              INST_FIN=1,),
                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(RESI_GLOB_RELA=5.E-6,
                                ITER_GLOB_MAXI=200,
                                ),
                 );


RES1=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                OPERATION='EXTR',
                RESULTAT=RESU1,
                INST=1,
                NOM_CHAM='SIEF_ELGA',
                )
##############################################################
# BLOCAGE DES NOEUDS AU BORD DE LA PARTIE EXCAVEE => DX+DY=0
##############################################################

CH210=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='BORD_SOL',
                                 DX=0.0,
                                 DY=0.0,),
                              _F(NOEUD=('NO1'),
                                 DX=0.0,),
                              _F(NOEUD=('NO8359'),
                                 DY=0.0,),),);

######################################################################
#  CONDITIONS AUX LIMITES EN DEPLACEMENTS =
#       SYMETRIE SUR LES COTES LATERAUX                 => DX=0
#       CONTINUITE SUR LA PARTIE INFERIEURE             => DY=0
#       POIDS DES TERRES SUR LA FACE SUPERIEURE         => PRES
# CETTE PRESSION EST NECESSAIRE POUR MAINTENIR UN ETAT DE CONTRAINTE
# COMPATIBBLE AVEC LE CHAMP DE CONTRAINTE INITIAL
######################################################################

CH220=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='NO_DROIT',
                                 DX=0.0,),
                              _F(GROUP_NO='NO_LEFT2',
                                 DX=0.0,),
                              _F(GROUP_NO='NO_BAS2',
                                 DY=0.0,),),
                    PRES_REP=_F(GROUP_MA='MA_HAUT',
                    PRES=5.0E6,),),

###########################################################
#    2EME PHASE BLOCAGE DU BORD DE LA GALERIE (OPTION DIDI)
###########################################################

RESU1=STAT_NON_LINE(reuse=RESU1,
                 MODELE=MO,
                 CHAM_MATER=CHMAT0,
                 EXCIT=(_F(CHARGE=CH210,
                           TYPE_CHARGE='DIDI'),
                        _F(CHARGE=CH220,),),
                 COMPORTEMENT=(_F(RELATION='ELAS',
                             GROUP_MA='SOL',),),
                 ETAT_INIT=_F(SIGM=RES1,),
                 INCREMENT=_F(LIST_INST=LI,
                              INST_INIT=1,
                              INST_FIN=10,),
                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(RESI_GLOB_RELA=5.E-6,
                                ITER_GLOB_MAXI=200,
                                ),
                 );


#####################################################################
#  CONDITIONS AUX LIMITES EN DEPLACEMENTS =
#       SYMETRIE SUR LES COTES LATERAUX                 => DX=0
#       CONTINUITE SUR LA PARTIE INFERIEURE             => DY=0
#####################################################################

CH51=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='NO_DROIT',
                                 DX=0.0,),
                              _F(GROUP_NO='NO_LEFT3',
                                 DX=0.0,),
                              _F(NOEUD='NO1',
                                 DY=0.0,),
                              _F(GROUP_NO=('NO_BAS3'),
                                 DY=0.0,),
                              _F(NOEUD='NO8359',
                                 DX=0.0,),),
                    PRES_REP=_F(GROUP_MA='MA_HAUT',
                              PRES=5.0E6,),);


########################
# CALCUL DES REACTIONS
########################

RESU1=CALC_CHAMP(reuse =RESU1,
           RESULTAT=RESU1,
           INST=10.,
           FORCE='REAC_NODA',
           );


#######################################
# RECUPERATION DES REACTIONS NODALES
#######################################

REANODA=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                   OPERATION='EXTR',
                   RESULTAT=RESU1,
                   NOM_CHAM='REAC_NODA',
                   INST=10.,);

CH3=AFFE_CHAR_MECA(MODELE=MO,
                   VECT_ASSE=REANODA,);

##########################################################
#        3EME PHASE = REINJECTION DE LA REACTION
##########################################################

RESU1=STAT_NON_LINE(reuse=RESU1,
                 MODELE=MO,
                 CHAM_MATER=CHMAT1,
                 EXCIT=(_F(CHARGE=CH3,
                           FONC_MULT=F0,),
                        _F(CHARGE=CH51,),),
                 COMPORTEMENT=(_F(RELATION='ELAS',
                               GROUP_MA='SOL_REST',),
                            _F(RELATION='ELAS',
                               GROUP_MA='EXCAV',),
                            _F(RELATION='ELAS',
                               GROUP_MA='BETON',),
                             ),
                 ETAT_INIT=_F(EVOL_NOLI=RESU1),
                 INCREMENT=_F(LIST_INST=LI,
                              INST_INIT=10,
                              INST_FIN=1000,),
                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(RESI_GLOB_RELA=5.E-6,
                                ITER_GLOB_MAXI=500,
                                ),
                                );


###########################################################


#############################
# POST TRAITEMENT
#############################

RESU1=CALC_CHAMP(reuse=RESU1,
                 RESULTAT=RESU1,
                 GROUP_MA='SOL_REST',
                 CONTRAINTE=('SIGM_ELNO','SIGM_NOEU',),);

TEST_RESU(RESU=_F(INST=1000,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU1,
                  NOM_CHAM='DEPL',
                  NOEUD='NO1',
                  NOM_CMP='DX',
                  VALE_CALC=-2.40008349E-03,
                  VALE_REFE=-2.4375E-3,
                  CRITERE='RELATIF',
                  PRECISION=0.02,),
          )

TEST_RESU(RESU=_F(INST=1000,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU1,
                  NOM_CHAM='DEPL',
                  NOEUD='NO8359',
                  NOM_CMP='DY',
                  VALE_CALC=-2.47789788E-03,
                  VALE_REFE=-2.4375E-3,
                  CRITERE='RELATIF',
                  PRECISION=0.02,),
          )

TEST_RESU(RESU=_F(INST=1000,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU1,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='NO1',
                  NOM_CMP='SIYY',
                  VALE_CALC=-1.01134824E+07,
                  VALE_REFE=-1.E7,
                  CRITERE='RELATIF',
                  PRECISION=0.02,
                  MAILLE='MA1',),
          )

TEST_RESU(RESU=_F(INST=1000,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU1,
                  NOM_CHAM='SIGM_ELNO',
                  NOEUD='NO8359',
                  NOM_CMP='SIXX',
                  VALE_CALC=-9.88743182E+06,
                  VALE_REFE=-1.E7,
                  CRITERE='RELATIF',
                  PRECISION=0.02,
                  MAILLE='MA2240',),
          )

FIN();
