import aster_core

def include_maillage(MAILLAGE):
    """Fonction une macro qui indique que le chemin pour chercher le maillage confidentiel"""
    fname  = MAILLAGE
    repdex = aster_core.get_option('repdex')
    fname  = osp.join(repdex, fname)
    return fname

