F1=FORMULE(NOM_PARA=('DX', 'D1',),
                VALE='abs (DX - D1)')

T1=CALC_TABLE(TABLE=DEPLM,
              ACTION=(
                               _F(OPERATION='COMB',
                                  TABLE=DEPL3,
                                  NOM_PARA=('INST',),),
                               _F(OPERATION='OPER',
                                  NOM_PARA='DIFF',
                                  FORMULE=F1),
                      ),
              TITRE='Comparaison calcul / mesure')

#IMPR_TABLE( UNITE = 8, TABLE = T1 )

T2=CALC_TABLE(TABLE=VITEM,
              ACTION=(
                               _F(OPERATION='COMB',
                                  TABLE=VITE5,
                                  NOM_PARA=('INST',),),
                               _F(OPERATION='OPER',
                                  NOM_PARA='DIFF',
                                  FORMULE=F1),
                      ),
              TITRE='Comparaison calcul / mesure')

#IMPR_TABLE( UNITE = 8, TABLE = T2 )

TEST_TABLE(
     TABLE     = T1,
     NOM_PARA  = 'DIFF',
     TYPE_TEST = 'SOMM',
     VALE      = 0.,
     CRITERE   = 'ABSOLU',
     PRECISION = 1.E-7,
     REFERENCE = 'AUTRE_ASTER',)


