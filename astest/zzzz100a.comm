#
#          VALIDATION DE SPEC_OSCI DE L'OPERATEUR CALC_FONCTION
#
#=======================================================================

OMEGA=3.566

AMOR1=0.01

AMOR2=0.03

FREQ1=5.

FREQ2=21.

FREQ3=105.

INSTM11=0.10

INSTM21=0.07

INSTM31=0.09

INSTM12=0.10

INSTM22=0.02

INSTM32=0.08

