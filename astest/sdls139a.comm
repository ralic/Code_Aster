                             PHENOMENE='MECANIQUE',
                             MODELISATION='DIS_TR',),
                          _F(GROUP_MA=('MC','MG',),
                             PHENOMENE='MECANIQUE',
                             MODELISATION='POU_D_T',),),);


MATGUI=DEFI_MATERIAU(ELAS=_F(E=9.84E10,
                             NU=0.3,
                             RHO=6526.0,
                     ),);

MATCRAY=DEFI_MATERIAU(ELAS=_F(E=9.84E10,
                             NU=0.3,
