MODEAIR1=NORM_MODE( 
                    reuse = MODEAIR1,
                    MODE = MODEAIR1,
                    NORME ='MASS_GENE',)


# ON AJOUTE D'UN AMORTISSEMENT MODAL A LA MAIN DE 1%
list_amor = [0.02]*10
LISTAMOR = DEFI_LIST_REEL( VALE = list_amor);

MODEAIR = DEFI_BASE_MODALE( RITZ = _F( MODE_MECA = MODEAIR1 ),
