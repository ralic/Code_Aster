# CAS_TEST__:SSNV128R
# TEST DU GRECO CALCUL 3D
# MODELE
# MAILLAGE : ELEMENT 3D HEXAEDRE A 8 NOEUDS
# UNITES   : NEWTON , METRE , SECONDE (N,M,S)
# LE GROUPE_NO PP CONTIENT LES NOEUDS OU LA 
# REDONDANCE APPARAIT ICI, ON N'UTILISE PAS 
# VECT_Y ET VECT_Z PARCEQUE LES DIRECTIONS 
# DE GLISSEMENT SONT CONSTRUITES AUTOMATIQUEMENT

DEBUT( CODE=_F(  NOM = 'SSNV128R',NIV_PUB_WEB='INTERNET') )

#  CALCUL TEST ELEMENTS FINIS DE CONTACT 3D HEXA8
#  PLAQUE AVEC CONTACT ET FROTTEMENT SUR UN PLAN RIGIDE

PRE_GIBI()

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),)

MA=DEFI_GROUP(reuse=MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(NOM='NCONTA',
                                GROUP_MA='SCONTA',),
                             _F(NOM='PP',
                                UNION=('PPSZ','PPS'),),  
                             _F(NOM='NPLAQ',
                                GROUP_MA='VPLAQ',),
                             _F(NOM='NBLOCZ',
                                DIFFE=('NPLAQ','NCONTA',),),
                             _F(NOM='NENCA',
                                GROUP_MA='SBATI',
                                CRIT_NOEUD='SOMMET',))
              )

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = '3D',)
              )

MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 MODELE=MO,
                 ORIE_PEAU_3D=(_F(GROUP_MA = 'SPRESV',),
                               _F(GROUP_MA = 'SPRESH',))
                )

MATPLA=DEFI_MATERIAU(ELAS=_F(E = 1.3E11,
                             NU = 0.2,)
                    )

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT = 'OUI',
                             MATER = MATPLA,)
                    )

CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    CONTACT=(_F(GROUP_MA_MAIT     = 'SBATI',
                               GROUP_MA_ESCL     = 'SCONTA',
                               APPARIEMENT    = 'MAIT_ESCL',
                               RECHERCHE      = 'NOEUD_VOISIN',
                               NORMALE        = 'MAIT',
                               METHODE        = 'CONTINUE',
                               INTEGRATION = 'NOEUD',
                               MODL_AXIS      = 'NON',
                               CONTACT_INIT   = 'OUI',
                               SEUIL_INIT     = 1.0E-1,
                               SANS_GROUP_NO  = 'PP',
                               COEF_REGU_FROT = 1.0,
                               COEF_REGU_CONT = 1.0,
                               ITER_GEOM_MAXI = 2,
                               ITER_FROT_MAXI = 12,
                               ITER_CONT_MAXI = 4,
                               FROTTEMENT     = 'COULOMB',
                               COULOMB        = 1.0,),
                               ),
                    INFO=2,);

CHA2=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO = 'NENCA',  DX = 0., DY = 0., DZ=0.,),
                              _F(GROUP_MA = 'SBLOCX', DX = 0.,),
                              _F(GROUP_NO = 'NBLOCZ', DZ = 0.,)),
                    PRES_REP=(_F(GROUP_MA = 'SPRESV', PRES = 5.E07,),
                              _F(GROUP_MA = 'SPRESH', PRES = 15.E07,))
                   )

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0,)
                    )

L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 1,)
                      )
#
#-----------------------------------------------------------
#

U2M=STAT_NON_LINE(MODELE    = MO,
                CHAM_MATER = CHMAT,
                EXCIT      =(_F(CHARGE = CHA2,
                                FONC_MULT = RAMPE,),
                             _F(CHARGE = CHA1,)),
                COMP_ELAS  = _F(RELATION = 'ELAS',),
                NEWTON     = _F(REAC_ITER = 1,),
                INCREMENT  = _F(LIST_INST = L_INST,
                                SUBD_PAS = 2,
                                COEF_SUBD_PAS_1 = 1.,
                                SUBD_PAS_MINI = 0.000000001,),
                CONVERGENCE= _F(ARRET = 'OUI',
                               ITER_GLOB_MAXI = 200,
                               RESI_GLOB_RELA = 1.0E-5,),
#                INFO=2,
               )

TEST_RESU(RESU=(_F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPA',
                   NOM_CMP    = 'DX',
                   VALE       = 2.92E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '8.0.22',
                   ),
                _F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPB',
                   NOM_CMP    = 'DX',
                   VALE       = 2.77E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '8.0.22',
                   ),
                _F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPC',
                   NOM_CMP    = 'DX',
                   VALE       = 2.31E-5,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '8.0.22',
                   PRECISION  = 5.E-2,
                   ),
                _F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPD',
                   NOM_CMP    = 'DX',
                   VALE       = 2.01E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '8.0.22',
                   ),
                _F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPE',
                   NOM_CMP    = 'DX',
                   VALE       = 1.56E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '8.0.22',
                ))
       )

FIN()
