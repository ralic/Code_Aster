DEBUT(CODE=_F(NOM='SSLV140A', NIV_PUB_WEB='INTERNET', VISU_EFICAS='NON'), DEBUG=_F(SDVERI='OUI'))

CUBER=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=CUBER,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

U2=DEFI_FONCTION(NOM_PARA='Y',
                 VALE=(0.0,0.0,1.0,1.0,),
                 PROL_DROITE='LINEAIRE',
                 PROL_GAUCHE='LINEAIRE',);

U3=DEFI_CONSTANTE(VALE=0.0,);

CHARGY=AFFE_CHAR_MECA_F(MODELE=MO,
                       DDL_IMPO=(_F(NOEUD='NO3',
                                    DX=U3,
                                    DY=U3,
                                    DZ=U3,),
                                 _F(GROUP_NO='FACEXZ1',
                                    DY=U3,),
                                 _F(NOEUD='NO7',
                                    DX=U3,),
                                 _F(GROUP_NO='FACEXZ2',
                                    DY=U2,),),);

MAT1=DEFI_MATERIAU(ELAS=_F(E=200000.0,
                           NU=0.3,),);

MAT2=DEFI_MATERIAU(ELAS=_F(E=100000.0,
                           NU=0.3,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=CUBER,
                    AFFE=(_F(GROUP_MA='MA1',
                             MATER=MAT1,),
                          _F(GROUP_MA='MA2',
                             MATER=MAT2,),),);

MESTY=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=_F(CHARGE=CHARGY,),);

MESTY=CALC_ELEM(reuse =MESTY,
               RESULTAT=MESTY,);

CONTR=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                 OPERATION='EXTR',
                 RESULTAT=MESTY,
                 NOM_CHAM='SIEF_ELGA_DEPL',);

CHAMELG=CALC_CHAM_ELEM(MODELE=MO,
                       CHAM_MATER=CHMAT,
                       OPTION='COOR_ELGA',);

INT_SIYY=POST_ELEM(MODELE=MO,
                   NUME_ORDRE=1,
                   RESULTAT=MESTY,
                   INTEGRALE=_F(NOM_CHAM='SIEF_ELGA_DEPL',
                                NOM_CMP=('SIYY'),
                                TOUT='OUI'),)

IMPR_TABLE(TABLE=INT_SIYY)


U1=DEFI_FONCTION(NOM_PARA='X',
                 VALE=(0.0,0.0,1.0,1.0,),
                 PROL_DROITE='LINEAIRE',
                 PROL_GAUCHE='LINEAIRE',);



CHARGX=AFFE_CHAR_MECA_F(MODELE=MO,
                       DDL_IMPO=(_F(NOEUD='NO3',
                                    DX=U3,
                                    DY=U3,
                                    DZ=U3,),
                                 _F(GROUP_NO='FACEZY1',
                                    DX=U3,),
#                                _F(GROUP_NO='FACEXY1',
#                                    DZ=U3,),
#                                _F(GROUP_NO='FACEXZ1',
#                                    DY=U3,),
                                 _F(NOEUD='NO1',
                                    DZ=U3,),
                                 _F(NOEUD='NO4',
                                    DZ=U3,),
                                 _F(NOEUD='NO10',
                                    DZ=U3,),
                                 _F(GROUP_NO='FACEZY2',
                                    DX=U1,),),);


MESTX=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=_F(CHARGE=CHARGX,),);

MESTX=CALC_ELEM(reuse =MESTX,
               RESULTAT=MESTX,OPTION='EQUI_ELGA_SIGM');


CONTR1=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                 OPERATION='EXTR',
                 RESULTAT=MESTX,
                 NOM_CHAM='SIEF_ELGA_DEPL',);


INT_SIXX=POST_ELEM(MODELE=MO,
                   NUME_ORDRE=1,
                   RESULTAT=MESTX,
                   INTEGRALE=(_F(NOM_CHAM='SIEF_ELGA_DEPL',
                                NOM_CMP=('SIXX'),
                                TOUT='OUI'),
                              _F(NOM_CHAM='EQUI_ELGA_SIGM',
                                NOM_CMP=('VMIS'),
                                TOUT='OUI'),),)

IMPR_TABLE(TABLE=INT_SIXX)

INT_SIX1=POST_ELEM(MODELE=MO,
                   NUME_ORDRE=1,
                   RESULTAT=MESTX,
                   INTEGRALE=_F(NOM_CHAM='SIEF_ELGA_DEPL',
                                NOM_CMP=('SIXX'),
                                GROUP_MA='MA1'),)

IMPR_TABLE(TABLE=INT_SIX1)


TEST_TABLE(TABLE=INT_SIYY,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1),
           NOM_PARA='MOYE_SIYY',
           REFERENCE='ANALYTIQUE',
           VALE=150000);

TEST_TABLE(TABLE=INT_SIXX,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1),
           NOM_PARA='MOYE_SIXX',
           REFERENCE='ANALYTIQUE',
           PRECISION=0.02,
           VALE=133333);

TEST_TABLE(TABLE=INT_SIXX,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1),
           NOM_PARA='INTE_VMIS',
           REFERENCE='NON_REGRESSION',
           PRECISION=0.02,
           VALE=269681.0);

TEST_TABLE(TABLE=INT_SIXX,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1),
           NOM_PARA='MOYE_VMIS',
           REFERENCE='NON_REGRESSION',
           PRECISION=0.02,
           VALE=134840.0);

TEST_TABLE(TABLE=INT_SIX1,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1),
           NOM_PARA='MOYE_SIXX',
           REFERENCE='ANALYTIQUE',
           PRECISION=0.02,
           VALE=133333);


FIN();
