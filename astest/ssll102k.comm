DEBUT(CODE=_F(NOM='SSLL102K',
              NIV_PUB_WEB='INTERNET',),
      DEBUG=_F(SDVERI='OUI',),);

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_MA=_F(NOM='TOUT',
                               TOUT='OUI',),);

PRE_GMSH(UNITE_GMSH=18,
         UNITE_MAILLAGE=21,);

MASEC=LIRE_MAILLAGE(UNITE=21,);

MASEC=DEFI_GROUP(reuse =MASEC,
                 MAILLAGE=MASEC,
                 CREA_GROUP_MA=_F(NOM='SURF1',
                                  TOUT='OUI',),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='POU_D_EM',),);

GF=DEFI_GEOM_FIBRE(SECTION=_F(GROUP_FIBRE='SEC1',
                              GROUP_MA_SECT='SURF1',
                              MAILLAGE_SECT=MASEC,
                              COOR_AXE_POUTRE=(0.,0.,),),);

MATR=DEFI_MATERIAU(ELAS=_F(E=2.E11,
                           NU=0.3,),);

COMPF=DEFI_COMPOR(GEOM_FIBRE=GF,
                  MATER_SECT=MATR,
                  MULTIFIBRE=_F(GROUP_FIBRE='SEC1',
                                MATER=MATR,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MATR,),
                    AFFE_COMPOR=_F(GROUP_MA='TOUT',
                                   COMPOR=COMPF,),);

CAREL=AFFE_CARA_ELEM(MODELE=MO,
                     POUTRE=_F(GROUP_MA='TOUT',
                               SECTION='GENERALE',
                               CARA=('A','IY','IZ','JX','RY','RZ','RT','JG','AY','AZ',),
                               VALE=(3.14159E-2,7.85398E-5,7.85398E-5,1.57080E-4,0.100,0.100,0.100,10.,10./9.,10./9.,),),
                     GEOM_FIBRE=GF,
                     MULTIFIBRE=_F(GROUP_MA='TOUT',
                                   GROUP_FIBRE='SEC1',),);

F1=DEFI_FONCTION(NOM_PARA='X',VALE=(0.,0.,
                       6.,6000.,
                       ),INTERPOL='LIN',);

CHARG1=AFFE_CHAR_MECA(MODELE=MO,
                      DDL_IMPO=(_F(NOEUD='N1',
                                   DX=0.,
                                   DY=0.,
                                   DZ=0.,
                                   DRX=0.,
                                   DRY=0.,),
                                _F(NOEUD='N11',
                                   DY=0.,),),);

CHARG2=AFFE_CHAR_MECA_F(MODELE=MO,
                        FORCE_POUTRE=_F(GROUP_MA='TOUT',
                                        FY=F1,),);

MEST1=MECA_STATIQUE(MODELE=MO,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CAREL,
                    EXCIT=(_F(CHARGE=CHARG1,),
                           _F(CHARGE=CHARG2,),),
                    SOLVEUR=_F(STOP_SINGULIER='OUI',),);

MEST1=CALC_CHAMP(reuse=MEST1,RESULTAT=MEST1,CONTRAINTE=('EFGE_ELNO'))


TEST_RESU(RESU=_F(RESULTAT=MEST1,
                  NUME_ORDRE=1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  NOEUD='N7',
                  VALE=0.003228713,
                  CRITERE='RELATIF',
                  PRECISION=0.006,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=MEST1,
                  NUME_ORDRE=1,
                  NOM_CHAM='EFGE_ELNO',
                  NOM_CMP='VY',
                  MAILLE='S11',
                  NOEUD='N1',
                  VALE=6000.0,
                  CRITERE='RELATIF',
                  PRECISION=1e-06,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=MEST1,
                  NUME_ORDRE=1,
                  NOM_CHAM='EFGE_ELNO',
                  NOM_CMP='VY',
                  MAILLE='S110',
                  NOEUD='N11',
                  VALE=-12000.0,
                  CRITERE='RELATIF',
                  PRECISION=1e-06,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=MEST1,
                  NUME_ORDRE=1,
                  NOM_CHAM='EFGE_ELNO',
                  NOM_CMP='MFZ',
                  MAILLE='S17',
                  NOEUD='N8',
                  VALE=-13856.41,
                  CRITERE='RELATIF',
                  PRECISION=1e-06,
                  REFERENCE='ANALYTIQUE',),);

FIN();
