DEBUT(CODE=_F(NOM='FORMA02B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='NON'))
#  SDVERI='NON' pour des raisons de cout CPU
Pilote = 'PNG';


ACIER=DEFI_MATERIAU(ELAS=_F(E=195000.,
                            NU=0.3,),
                    ECRO_LINE=_F(D_SIGM_EPSI=1930.,
                                 SY=181.,),);

Sig=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                        1.,151.2,
                        2.,257.2,
                        3.,259.3,
                        4.,0.,
                        ),);

Tau=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                        1.,93.1,
                        2.,33.1,
                        3.,0.,
                        4.,0.,
                        ),);

IMPR_FONCTION(FORMAT='XMGRACE',
              PILOTE=Pilote,
              UNITE=30,
              COURBE=_F(FONC_X=Sig,
                        FONC_Y=Tau,),
              LEGENDE_X='Sig',
              LEGENDE_Y='Tau',);
Ninst = 100;


L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=0.81,
                                     NOMBRE=1,),
                                  _F(JUSQU_A=1.0,
                                     NOMBRE=Ninst,),
                                  _F(JUSQU_A=2.0,
                                     NOMBRE=Ninst,),
                                  _F(JUSQU_A=3.0,
                                     NOMBRE=Ninst,),),);

U=SIMU_POINT_MAT(COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',),
                 MATER=ACIER,
                 INCREMENT=_F(LIST_INST=L_INST,),
                 NEWTON=_F(PREDICTION='ELASTIQUE',
                           MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 SIGM_IMPOSE=_F(SIXX=Sig,
                                SIXY=Tau,),);

IMPR_TABLE(TABLE=U,);

# A

TEST_TABLE(TABLE=U,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,
                     PRECISION=1e-05,),
           NOM_PARA='EPXX',
           VALE=0.01483,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=U,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=1.0,
                     PRECISION=1e-05,),
           NOM_PARA='EPXY',
           VALE=0.0136014,
           REFERENCE='ANALYTIQUE',);

# B

TEST_TABLE(TABLE=U,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=2.0,
                     PRECISION=1e-05,),
           NOM_PARA='EPXX',
           VALE=0.035265,
           PRECISION=0.01,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=U,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=2.0,
                     PRECISION=1e-05,),
           NOM_PARA='EPXY',
           VALE=0.020471,
           PRECISION=0.01,
           REFERENCE='ANALYTIQUE',);

# C

TEST_TABLE(TABLE=U,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.0,
                     PRECISION=1e-05,),
           NOM_PARA='EPXX',
           VALE=0.0352777,
           PRECISION=0.01,
           REFERENCE='AUTRE_ASTER',);

TEST_TABLE(TABLE=U,
           FILTRE=_F(NOM_PARA='INST',
                     VALE=3.0,
                     PRECISION=1e-05,),
           NOM_PARA='EPXY',
           VALE=0.0202462,
           PRECISION=0.01,
           REFERENCE='AUTRE_ASTER',);

# pour trace de courbe et comparaison avec modelisation A

EPXX=RECU_FONCTION(TABLE=U,
                   PARA_X='INST',
                   PARA_Y='EPXX',);

IMPR_FONCTION(FORMAT='TABLEAU',
              COURBE=_F(FONCTION=EPXX,
                        LEGENDE='EPXX',
                        MARQUEUR=1,),
              UNITE=31,);

EPXY=RECU_FONCTION(TABLE=U,
                   PARA_X='INST',
                   PARA_Y='EPXY',);

IMPR_FONCTION(FORMAT='TABLEAU',
              COURBE=_F(FONCTION=EPXY,
                        LEGENDE='EPXY',
                        MARQUEUR=1,),
              UNITE=32,);

FIN();
