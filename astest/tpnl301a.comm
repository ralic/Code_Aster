           TABLE=TT,
           )

TEST_TABLE(
           VALE_CALC=49.334481256188809,
           NOM_PARA='TEMP',
           TYPE_TEST='MAX',
           TABLE=TTDIAG,
           )

TM1=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
