                NOM_CHAM='TEMP',
                INST=0.0,);

T5j=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
               OPERATION='EXTR',
               RESULTAT=RESUTHER,
               NOM_CHAM='TEMP',
               INST=t50h,);

T5JD=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='EXTR',
                RESULTAT=RESUDIAG,
                NOM_CHAM='TEMP',
                INST=t50h,);

T1m=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
               OPERATION='EXTR',
               RESULTAT=RESUTHER,
               NOM_CHAM='TEMP',
               INST=t730h,);

T1MD=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='EXTR',
                RESULTAT=RESUDIAG,
                NOM_CHAM='TEMP',
                INST=t730h,);

TEST_RESU(CHAM_NO=(_F(REFERENCE='ANALYTIQUE',
                      NOM_CMP='TEMP',
                      GROUP_NO='X205',
                      PRECISION=1.E-2,
                      CHAM_GD=TM1,
                      VALE_CALC=27.500000000,
                      VALE_REFE=27.3475,),
                   _F(REFERENCE='ANALYTIQUE',
                      NOM_CMP='TEMP',
                      GROUP_NO='X205',
                      PRECISION=1.E-2,
