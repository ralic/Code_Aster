                      CHAM_GD=T5j,
                      VALE_CALC=49.258577307,
                      VALE_REFE=48.709099999999999,),
                   _F(REFERENCE='ANALYTIQUE',
                      NOM_CMP='TEMP',
                      GROUP_NO='X205',
                      PRECISION=1.E-2,
                      CHAM_GD=T5JD,
                      VALE_CALC=49.070117737,
                      VALE_REFE=48.709099999999999,),
                   _F(REFERENCE='ANALYTIQUE',
                      NOM_CMP='TEMP',
                      GROUP_NO='X205',
                      PRECISION=1.E-2,
                      CHAM_GD=T1m,
                      VALE_CALC=27.870916591,
                      VALE_REFE=27.711600000000001,),
                   _F(REFERENCE='ANALYTIQUE',
                      NOM_CMP='TEMP',
                      GROUP_NO='X205',
                      PRECISION=1.E-2,
                      CHAM_GD=T1MD,
                      VALE_CALC=27.871285206,
                      VALE_REFE=27.711600000000001,),
                   ),
          )

TEST_RESU(CHAM_NO=(_F(
                      NOM_CMP='TEMP',
                      GROUP_NO='X205',
                      CHAM_GD=TM1,
                      VALE_CALC=27.5,),
                   _F(
                      NOM_CMP='TEMP',
                      GROUP_NO='X205',
                      CHAM_GD=TM1D,
                      VALE_CALC=27.5,),
                   _F(
                      NOM_CMP='TEMP',
