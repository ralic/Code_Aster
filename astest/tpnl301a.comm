                   _F(
                      NOM_CMP='TEMP',
                      GROUP_NO='X205',
                      CHAM_GD=T1MD,
                      VALE_CALC=27.871285206037729,),
                   ),
          )

RESUTHER=CALC_CHAMP(reuse =RESUTHER,
                    RESULTAT=RESUTHER,
                    THERMIQUE='HYDR_NOEU',);

RESUDIAG=CALC_CHAMP(reuse =RESUDIAG,
                    RESULTAT=RESUDIAG,
                    THERMIQUE='HYDR_NOEU',);

H49h=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
                OPERATION='EXTR',
                RESULTAT=RESUTHER,
                NOM_CHAM='HYDR_NOEU',
                INST=t50h,);

H49D=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
                OPERATION='EXTR',
                RESULTAT=RESUDIAG,
                NOM_CHAM='HYDR_NOEU',
                INST=t50h,);

H730h=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
                 OPERATION='EXTR',
                 RESULTAT=RESUTHER,
