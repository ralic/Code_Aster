DEBUT(CODE=_F(NOM='SSNV173B', NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAILLAG1=LIRE_MAILLAGE(FORMAT='MED',INFO=1,);

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                   AFFE=_F(GROUP_MA=('VOL',),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),
                     );

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-12.5 ');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN,),
                    INFO=3, );

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         );

E=205000.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('VOL',),
                                MATER=ACIER,
                                ),
                             ),);
ZERO=0.0
DEPX=1.E-6
DEPY=2.E-6
DEPZ=3.E-6

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                 _F(NOEUD=('N1','N6','N11','N16',),
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=ZERO,
                                    ),
                                 _F(NOEUD=('N21','N22','N23','N24',),
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=DEPX,),
                                 ),
                       INFO=1,);

CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                 _F(NOEUD=('N1','N6','N11','N16',),
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=ZERO,
                                    ),
                                 _F(NOEUD=('N21','N22','N23','N24',),
                                    DX=DEPX,
                                    DY=DEPY,
                                    DZ=DEPZ,),
                                 ),
                       INFO=1,);


UTOT1=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CH1),
                          
                          ),
                   ); 
UTOT2=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CH2),
                          
                          ),
                   ); 

# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),) 

RES_X1=POST_CHAM_XFEM( MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        
                        INFO          = 2)

RES_X2=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT2,
                        
                        INFO          = 2)

#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(RESULTAT=RES_X1));


# -------------------------------------------------------------   
#            TEST_TABLE
# -------------------------------------------------------------   

DEP1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU',),
                                RESULTAT=RES_X1,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);

DEP2=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU',),
                                RESULTAT=RES_X2,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=DEP1,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           IMPR_FONCTION='NON',);

IMPR_TABLE(TABLE=DEP2,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           IMPR_FONCTION='NON',);

DEP1P = CALC_TABLE(TABLE = DEP1,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^P]',),),);


DEP1M = CALC_TABLE(TABLE = DEP1,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^M]',),),);


DEP2P = CALC_TABLE(TABLE = DEP1,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^P]',),),);


DEP2M = CALC_TABLE(TABLE = DEP2,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^M]',),),);

