DEBUT(CODE=_F(  NOM = 'SSLP105A',NIV_PUB_WEB='INTERNET') );
##########################
# LECTURE MAILLAGE GIBI
##########################

MAIL = LIRE_MAILLAGE();

#########################################################################
#   MODELISATION D UNE EXCAVATION AVEC SOUTENEMENT D UN TUNNEL EN D.P 
#########################################################################
# CE CAS TEST CONCERNE L EXCAVATION D UNE GALERIE CIRCULAIRE EN MILIEU 
# ELASTIQUE LINEAIRE, EN DEFORMATION PLANE ET EN SUPPOSANT QUE LE CHAMP 
# DE CONTRAINTE INITIAL EST ISOTROPE. UN REVETEMENT EST POSE SUR LE BORD 
# DE LA GALERIE AU COURS DE SON EXCAVATION (DECONFINEMENT DE 50%).
# 
# CE CAS TEST EST DECRIT DANS LA NOTE HT-25/02/045/A (cas numero 3)
#         
#########################################################################


#####################################
# DEFINITION DES GROUPES DE NEOUDS
#####################################

MAIL = DEFI_GROUP(reuse=MAIL,
                  MAILLAGE=MAIL,
                  CREA_GROUP_NO=(_F(GROUP_MA='SOL'),
                                 _F(GROUP_MA='SOL_REST'),
                                 _F(NOM='NO_HAUT',
                                    GROUP_MA='MA_HAUT'),
                                 _F(GROUP_MA='NO_DROIT'),
                                 _F(GROUP_MA='NO_LEFT1'),
                                 _F(GROUP_MA='NO_LEFT2'),
                                 _F(GROUP_MA='NO_LEFT3'),
                                 _F(GROUP_MA='NO_BAS1'),
                                 _F(GROUP_MA='NO_BAS2'),
                                 _F(GROUP_MA='NO_BAS3'),
                                 _F(GROUP_MA='LEFT_BET'),
                                 _F(GROUP_MA='BAS_BETO'),
                                 _F(GROUP_MA='BORD'),
                                 _F(NOM='NOEUD1',
                                    NOEUD='NO1'),
                                 _F(NOM='NOEU8359',
                                   NOEUD='NO8359'),
                                 _F(NOM='BORD_SOL',
                                  DIFFE=('BORD','NOEUD1','NOEU8359'),),),),


##################################################
# MODELE DU SOL AVANT L EXCAVATION POUR L'ETAPE 
# D'INITIIALISATION DU CHAMP DE CONTRAINTES
##################################################

MO=AFFE_MODELE(MAILLAGE=MAIL,
               AFFE=(_F(GROUP_MA=('SOL','MA_HAUT'),
                        PHENOMENE='MECANIQUE',
                        MODELISATION='D_PLAN',),),);

####################
#  MODELE SOL_REST 
####################

MO1=AFFE_MODELE(MAILLAGE=MAIL,
                AFFE=(_F(GROUP_MA=('SOL_REST','MA_HAUT'),
                         PHENOMENE='MECANIQUE',
                         MODELISATION='D_PLAN',),),);                    

#####################################
#  MODELE DU SOL APRES L EXCAVATION
#####################################

MO2=AFFE_MODELE(MAILLAGE=MAIL,
                AFFE=(_F(GROUP_MA='BETON',
                         PHENOMENE='MECANIQUE',
                         MODELISATION='D_PLAN',),
                      _F(GROUP_MA=('SOL_REST','MA_HAUT'),
                         PHENOMENE='MECANIQUE',
                         MODELISATION='D_PLAN',),),);

E_SOL = 4.0E9
NU_FICTIF = 0.49999
NU_SOL = 0.3
E_BETON = 2.0E10
NU_BETON = 0.2
RHO_BIDON = 2000.
ALPHA_BIDON = 0.
################################################
# SOL POUR CALCUL REACTIONS NODALES
################################################

SOL0=DEFI_MATERIAU(ELAS=_F(E=E_SOL,
                          NU=NU_FICTIF,
                          RHO=RHO_BIDON,  
                          ALPHA = ALPHA_BIDON,),);   

#########################################
# MATERIAU SOL REEL (DONNEES DU CALCUL)
#########################################

SOL=DEFI_MATERIAU(ELAS=_F(E=E_SOL,
                           NU=NU_SOL,
                           RHO=RHO_BIDON,
                           ALPHA=ALPHA_BIDON,),); 

####################################################
# MATERIAU BETON POUR LES VOUSSOIRS DU REVETEMENT
####################################################

BETON=DEFI_MATERIAU(ELAS=_F(E=E_BETON,
                             NU=NU_BETON,
                             RHO=RHO_BIDON,
                             ALPHA=ALPHA_BIDON,),);

CHMAT1=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(GROUP_MA='SOL_REST',
                             MATER=SOL0,),),);

CHMAT2=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(_F(GROUP_MA='SOL_REST',
                             MATER=SOL,),),);

CHMAT3=AFFE_MATERIAU(MAILLAGE=MAIL,
                     AFFE=(_F(GROUP_MA='SOL_REST',
                              MATER=SOL,),
                           _F(GROUP_MA='BETON',
                              MATER=BETON,),),);

########################################################
#             LISTE DES INSTANTS DE CALCUL
########################################################
# DE 0 A 1  ==>  POUR LA PHASE D INITIALISATION
# DE 1 A 10  ==>  POUR LE BLOCAGE DU BORD DE LA GALERIE
# 10 CORRESPOND A UN TEMPS DE DECONFINEMENT = 0
# 500 CORRESPOND A UN TEMPS DE DECONFINEMENT = 50 %
# 1000 CORRESPOND A UN TEMPS DE DECONFINEMENT = 100 %
######################################################## 
                            
LI=DEFI_LIST_REEL(DEBUT=0,
                  INTERVALLE=(_F(JUSQU_A=1.0,
                                 NOMBRE=1,),
                              _F(JUSQU_A=10.0,
                                 NOMBRE=1,),
                              _F(JUSQU_A=500.0,
                                 NOMBRE=1,),
                              _F(JUSQU_A=1000,
                                 NOMBRE=1,),),);
                        

##################################################
#  LISTE DES INSTANTS POUR LE CALCUL BIDON POUR 
#  INITIALISER LES CHAMPS A 0 DANS LE BETON
##################################################

LI1=DEFI_LIST_REEL(DEBUT=0,                                     
                   INTERVALLE=(_F(JUSQU_A=50,
                                  NOMBRE=1),),);


######################################################                 
# FO FONCTION MULTIPLICATRICE POUR LE DECONFINEMENT 
######################################################                 

F0=DEFI_FONCTION(NOM_PARA='INST',
                 VALE=(10.0,1.0,
                        500.0,0.5,
                        1000.0,0.0,),);


#################################################
#   1ERE PHASE : INITIALISATION DES CONTRAINTES
#################################################

RES1=CREA_CHAMP(TYPE_CHAM='ELNO_SIEF_R',
                OPERATION='AFFE',
                MODELE=MO,
                AFFE=(_F(TOUT='OUI',
                        NOM_CMP=('SIXX','SIYY','SIZZ','SIXY'),
                        VALE=(-5.0E6,-5.0E6,0.,0.),),),),

                
##############################################################
# BLOCAGE DES NOEUDS AU BORD DE LA PARTIE EXCAVEE => DX+DY=0 
##############################################################

CH210=AFFE_CHAR_MECA(MODELE=MO1,
                    DDL_IMPO=(_F(GROUP_NO='BORD_SOL',
                                 DX=0.0,
                                 DY=0.0,), 
                              _F(NOEUD=('NO1'),                                
                                 DX=0.0,),
                              _F(NOEUD=('NO8359'),                                
                                 DY=0.0,),),);
                                
######################################################################                                
#  CONDITIONS AUX LIMITES EN DEPLACEMENTS =
#       SYMETRIE SUR LES COTES LATERAUX                 => DX=0
#       CONTINUITE SUR LA PARTIE INFERIEURE             => DY=0
#       POIDS DES TERRES SUR LA FACE SUPERIEURE         => PRES
# CETTE PRESSION EST NECESSAIRE POUR MAINTENIR UN ETAT DE CONTRAINTE
# COMPATIBBLE AVEC LE CHAMP DE CONTRAINTE INITIAL
######################################################################

CH220=AFFE_CHAR_MECA(MODELE=MO1,
                    DDL_IMPO=(_F(GROUP_NO='NO_DROIT',
                                 DX=0.0,),
                              _F(GROUP_NO='NO_LEFT2',
                                 DX=0.0,),
                              _F(GROUP_NO='NO_BAS2',
                                 DY=0.0,),),
                    PRES_REP=_F(GROUP_MA='MA_HAUT',
                    PRES=5.0E6,),),
                        
###########################################################
#    2EME PHASE BLOCAGE DU BORD DE LA GALERIE (OPTION DIDI)
###########################################################

RESU1=STAT_NON_LINE(MODELE=MO1,
                 CHAM_MATER=CHMAT1,
                 EXCIT=(_F(CHARGE=CH210,
                           TYPE_CHARGE='DIDI'),
                        _F(CHARGE=CH220,),),
                 COMP_INCR=(_F(RELATION='ELAS',
                             GROUP_MA='SOL_REST',),),
                 ETAT_INIT=_F(SIGM=RES1,),
                 INCREMENT=_F(LIST_INST=LI,
                              INST_INIT=1,
                              INST_FIN=10,),
                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(RESI_GLOB_RELA=5.E-6,
                                ITER_GLOB_MAXI=200,
                                ),
                 );



#####################################################################                                
#  CONDITIONS AUX LIMITES EN DEPLACEMENTS =
#       SYMETRIE SUR LES COTES LATERAUX                 => DX=0
#       CONTINUITE SUR LA PARTIE INFERIEURE             => DY=0
#####################################################################

CH51=AFFE_CHAR_MECA(MODELE=MO2,
                    DDL_IMPO=(_F(GROUP_NO='NO_DROIT',
                                 DX=0.0,),
                              _F(GROUP_NO='NO_LEFT3',
                                 DX=0.0,),
                              _F(NOEUD='NO1',
                                 DY=0.0,),
                              _F(GROUP_NO=('NO_BAS3'),
                                 DY=0.0,),
                              _F(NOEUD='NO8359',
                                 DX=0.0,),), 
                    PRES_REP=_F(GROUP_MA='MA_HAUT',
                              PRES=5.0E6,),);


########################
# CALCUL DES REACTIONS
########################

RESU1=CALC_NO(reuse =RESU1,
           RESULTAT=RESU1,
           INST=10.,
           OPTION='REAC_NODA',
           );
           
                   
#######################################
# RECUPERATION DES REACTIONS NODALES
#######################################

REANODA=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                   OPERATION='EXTR',
                   RESULTAT=RESU1,
                   NOM_CHAM='REAC_NODA',
                   INST=10.,);

CH3=AFFE_CHAR_MECA(MODELE=MO1,
                   VECT_ASSE=REANODA,);
                   
##########################################################
#        3EME PHASE = REINJECTION DE LA REACTION  
##########################################################  

RESU1=STAT_NON_LINE(reuse=RESU1,
                 MODELE=MO1,
                 CHAM_MATER=CHMAT2,
                 EXCIT=(_F(CHARGE=CH3,
                           FONC_MULT=F0,),
                        _F(CHARGE=CH220,),),
                 COMP_INCR=(_F(RELATION='ELAS',
                               GROUP_MA='SOL_REST',),),
                 ETAT_INIT=_F(EVOL_NOLI=RESU1),
                 INCREMENT=_F(LIST_INST=LI,
                              INST_INIT=10,
                              INST_FIN=500,),
                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(RESI_GLOB_RELA=5.E-6,
                                ITER_GLOB_MAXI=500,
                                ),
                                );


###########################################################
# EXTRACTION DES CHAMPS = DEPLACEMENTS, CONTRAINTES, 
# ET VARIABLES INTERNES OBTENUS LORS DU CALCUL PRECEDENT 
###########################################################
                   
SIG1=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                OPERATION='EXTR',
                RESULTAT=RESU1,
                NOM_CHAM='SIEF_ELGA',
                         INST=500.,);

DEP1=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                OPERATION='EXTR',
                RESULTAT=RESU1,
                    NOM_CHAM='DEPL',
                INST=500.,);
   
                                                   
CHBID = AFFE_CHAR_MECA(MODELE=MO1,
                   DDL_IMPO=(_F(GROUP_NO='NO_DROIT',
                                 DX=0.0,),
                              _F(GROUP_NO='NO_LEFT2',
                                 DX=0.0,),
                              _F(GROUP_NO='NO_BAS2',
                                 DY=0.0,),),),
             
             
#########################################################
#           4EME PHASE = CHARGEMENT BIDON, 
#  CALCUL BIDON POUR PERMETTRE :
#    - UNE INITIALISATION DES CHAMPS A 0 DANS LE BETON,
#    - ET ENSUITE L ASSEMBLAGE (POSE BETON)
######################################################### 
                    
BIDON=STAT_NON_LINE(MODELE=MO2,
                 CHAM_MATER=CHMAT3,
                 EXCIT=_F(CHARGE=CHBID),
                 COMP_INCR=(_F(RELATION='ELAS',
                             GROUP_MA='SOL_REST',),
                            _F(RELATION='ELAS',
                             GROUP_MA='BETON',),),
                 INCREMENT=_F(LIST_INST=LI1,),
                 NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(RESI_GLOB_MAXI=1.,
                                ITER_GLOB_MAXI=1,
                                ),
                 );
                        
                        
DEP2=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                  OPERATION='EXTR',
                  RESULTAT=BIDON,
                  NOM_CHAM='DEPL',
                            INST=50);
                   

SIG2=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                   OPERATION='EXTR',
                   RESULTAT=BIDON,
                   NOM_CHAM='SIEF_ELGA',
                             INST=50);

###################
#   ASSEMBLAGE
###################
                                  
DEPINI=CREA_CHAMP(TYPE_CHAM='NOEU_DEPL_R',
                 OPERATION='ASSE',
                 MAILLAGE=MAIL,
                 ASSE=(_F( CHAM_GD=DEP2,
                              GROUP_MA='BETON',
                              CUMUL='OUI',
                              COEF_R=0.),
                       _F(CHAM_GD=DEP1,
                              GROUP_MA='SOL_REST',
                              CUMUL='OUI',),),);

SIGINI=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                   OPERATION='ASSE',
                   MODELE=MO2,
                   ASSE=(_F(CHAM_GD=SIG2,
                                GROUP_MA='BETON',
                                CUMUL='OUI',
                                COEF_R=0.),
                         _F(CHAM_GD=SIG1,
                                GROUP_MA='SOL_REST',
                                CUMUL='OUI',),),);
                  
#######################################################
#        5EME PHASE = DECONFINEMENT DE 50 A 100 AVEC 
#        PRESENCE DES VOUSSOIRS      
####################################################### 

RESU1=STAT_NON_LINE(reuse=RESU1,
                 MODELE=MO2,
                 CHAM_MATER=CHMAT3,
                 EXCIT=(_F(CHARGE=CH3,
                           FONC_MULT=F0,),
                        _F(CHARGE=CH51,),),
                 COMP_INCR=(_F(RELATION='ELAS',
                               GROUP_MA='SOL_REST',),
                            _F(RELATION='ELAS',
                               GROUP_MA='BETON',),),
                 ETAT_INIT=_F(DEPL=DEPINI,
                              SIGM=SIGINI,),
                 INCREMENT=_F(LIST_INST=LI,
                              INST_INIT=500.,
                              INST_FIN=1000.,),
                NEWTON=_F(MATRICE='TANGENTE',
                           REAC_ITER=1,),
                 CONVERGENCE=_F(RESI_GLOB_RELA=5.E-6,
                                ITER_GLOB_MAXI=500,
                                ),
                                );


#############################
# POST TRAITEMENT 
#############################  

RESU1=CALC_ELEM(reuse=RESU1,
                GROUP_MA='SOL_REST',
                OPTION=('SIEF_ELNO_ELGA',),
                RESULTAT=RESU1,);

RESU1=CALC_NO(reuse=RESU1,
                OPTION=('SIEF_NOEU_ELGA',),
                RESULTAT=RESU1);
                
TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  INST = 1000,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  NOEUD='NO1',
                  VALE=-0.0016925,
                  PRECISION = 0.02,
                  CRITERE = 'RELATIF',
                  REFERENCE='ANALYTIQUE',),);
                  
TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  INST = 1000,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  NOEUD='NO8359',
                  VALE=-0.0016925,
                  PRECISION = 0.02,
                  CRITERE = 'RELATIF',
                  REFERENCE='ANALYTIQUE',),);
                  
TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  INST = 1000,
                  NOM_CHAM='SIEF_ELNO_ELGA',
                  NOM_CMP='SIXX',
                  NOEUD='NO1',
                  MAILLE = 'MA1',
                  VALE=-1.52821E6,
                  PRECISION = 0.01,
                  CRITERE = 'RELATIF',
                  REFERENCE='ANALYTIQUE',),);
                  
TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  INST = 1000,
                  NOM_CHAM='SIEF_ELNO_ELGA',
                  NOM_CMP='SIYY',
                  NOEUD='NO8359',
                  MAILLE = 'MA2240',
                  VALE=-1.52821E6,
                  PRECISION = 0.01,
                  CRITERE = 'RELATIF',
                  REFERENCE='ANALYTIQUE',),);
                  
TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  INST = 1000,
                  NOM_CHAM='SIEF_ELNO_ELGA',
                  NOM_CMP='SIYY',
                  NOEUD='NO1',
                  MAILLE = 'MA1',
                  VALE=-8.47179E6,
                  PRECISION = 0.01,
                  CRITERE = 'RELATIF',
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=RESU1,
                  INST = 1000,
                  NOM_CHAM='SIEF_ELNO_ELGA',
                  NOM_CMP='SIXX',
                  NOEUD='NO8359',
                  MAILLE = 'MA2240',
                  VALE=-8.47179E6,
                  PRECISION = 0.01,
                  CRITERE = 'RELATIF',
                  REFERENCE='ANALYTIQUE',),);                 
                                                            
TAB1=POST_ELEM(
                MASS_INER=_F(TOUT='OUI'),
                RESULTAT=RESU1,NUME_ORDRE=1);       
                
TEST_TABLE(TABLE=TAB1,
           REFERENCE='NON_REGRESSION',
           NOM_PARA='MASSE',
           VALE= 7.96466E+05   )
#
# CALCUL DE MASS_INER SUR LE MODELE APRES L'EXCAVATION
#
TAB2=POST_ELEM(
                MASS_INER=_F(TOUT='OUI'),
                MODELE=MO2,CHAM_MATER=CHMAT3);       
                
TEST_TABLE(TABLE=TAB2,
           REFERENCE='NON_REGRESSION',
           NOM_PARA='MASSE',
           VALE= 7.97738E+05   )
FIN();


                  



                        
