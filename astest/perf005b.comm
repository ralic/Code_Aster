DEBUT(CODE=_F(NOM='PERF005B',NIV_PUB_WEB='INTERNET'),MEMOIRE=_F(DYNAMIQUE=400,))

MA=LIRE_MAILLAGE(FORMAT='MED')

#_____________________________________________________________________
#
#                    DEFINITION DU MODELE
#_____________________________________________________________________

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

MA=MODI_MAILLAGE(reuse =MA, MAILLAGE=MA, ORIE_PEAU_3D=_F(GROUP_MA='SESC',), );
MA=MODI_MAILLAGE(reuse =MA, MAILLAGE=MA, ORIE_PEAU_3D=_F(GROUP_MA='SMAI',), );
#_____________________________________________________________________
#
#                    DEFINITION DES MATERIAUX
#_____________________________________________________________________

MAT=DEFI_MATERIAU(ELAS=_F(E=2.E4, NU=0.3),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA, AFFE=_F(TOUT='OUI', MATER=MAT,),);

#_____________________________________________________________________
#
#                    AFFECTATION DES CONDITIONS AUX LIMITES
#                       ET DES DEPLACEMENTS IMPOSES
#_____________________________________________________________________

CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='FACESUP',  DY=-2.0,),
                              _F(GROUP_NO='FACEINF',  DY=2.0,),
                              _F(GROUP_NO='LIM1',     DX=0.0,),
                              _F(GROUP_NO='LIM2',     DZ=0.0,),),);

                               
CHA2 = DEFI_CONTACT(MODELE      = MO,
                    FORMULATION = 'DISCRETE',
                    NB_RESOL    = 30,
                    ZONE = _F(GROUP_MA_MAIT='SMAI',
                              GROUP_MA_ESCL='SESC',
                               # Nombre de noeuds esclaves = 664
                            ),
                    );                            

L_INST=DEFI_LIST_REEL(DEBUT=0.0, INTERVALLE=_F(JUSQU_A=1.0, NOMBRE=1,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,   1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);

#_____________________________________________________________________
#
#                          RESOLUTION
#_____________________________________________________________________
#

RESU=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                   MODELE=MO,
                   CHAM_MATER=CHMAT,
                   CONTACT = CHA2,
                   EXCIT=(_F(CHARGE=CHA1,
                             FONC_MULT=RAMPE,),
                          ),
                   COMP_ELAS=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   NEWTON=_F(MATRICE='ELASTIQUE',
                             REAC_INCR=0,),
                            );

RESU=CALC_ELEM(reuse =RESU, OPTION=('SIEF_ELNO_ELGA'), RESULTAT=RESU,);
RESU=CALC_NO(reuse =RESU, OPTION=('SIEF_NOEU_ELGA'), RESULTAT=RESU,);


SIELNO=CREA_CHAMP(TYPE_CHAM='NOEU_SIEF_R', OPERATION='EXTR', RESULTAT=RESU,
                  NOM_CHAM='SIEF_NOEU_ELGA', INST=1.,);


TEST_RESU(CHAM_NO=_F(CHAM_GD=SIELNO, GROUP_NO='G1', NOM_CMP='SIYY', VALE=-2798.300,
                     CRITERE='RELATIF', PRECISION=0.05, REFERENCE='ANALYTIQUE',),);

# pour compter les noeuds en contact (ceux qui ont CONT != 0.) :
# XXX=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_INFC_R', INFO=2,
#                  NOM_CHAM='VALE_CONT', RESULTAT=RESU, NUME_ORDRE=1)
# # pour compter les noeuds en contact, on peut faire :
# #    cat xxxx.mess |  grep 'N[0-9]* *| *0"backslash".[1-9]' | wc -l

FIN();
