                   GROUP_NO='B',
                   VALE=-1.732,
                   PRECISION=1.E-2,
                   REFERENCE='NON_REGRESSION',
                   VERSION='6.3.13',),),);

TU_NOR=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                               GROUP_NO='LESC',
                               RESULTAT=U_NOR,
                               NOM_CHAM='REAC_NODA',
                               NOM_CMP=('DX','DY'),
                               REPERE='LOCAL',
                               OPERATION='EXTRACTION',
                               INST=1.,),);

TEST_TABLE(TABLE=TU_NOR,
           TYPE_TEST='SOMM',
           NOM_PARA='DY',
           VALE=1.2000E4,
           PRECISION=0.050000000000000003,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=TU_NOR,
