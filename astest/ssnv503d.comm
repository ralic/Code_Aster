# CAS TEST SSNV503D
# - MODELISATION AVEC DES ELEMENTS D_PLAN (MAILLES QUA4)
# - MODELISATION DU CONTACT AVEC LA METHODE CONTACT (AFFE_CHAR_MECA)
# - METHODE PENALISEE POUR LE CONTACT ET LE FROTTEMENT

DEBUT(CODE=_F(NOM='SSNV503D',NIV_PUB_WEB='INTERNET',),);

PRE_GIBI();


MAILL=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI',),);

MAILL=DEFI_GROUP(reuse =MAILL,
                 MAILLAGE=MAILL,
                 CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MODE=AFFE_MODELE(MAILLAGE=MAILL,
                 VERIF='MAILLE',
                 AFFE=(_F(TOUT='OUI',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='D_PLAN',),
                       _F(GROUP_MA='RES_LAT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='2D_DIS_T',),
                       _F(GROUP_MA='RES_HAUT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='2D_DIS_T',),),);

MAILL=MODI_MAILLAGE(reuse =MAILL,
                    MAILLAGE=MAILL,
                    ORIE_PEAU_2D=_F(GROUP_MA=('LMAI','LESC','FACESUP','FACELAT'),),
                    );

#---------------------------------------------------------------------
#                         CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------

CARA=AFFE_CARA_ELEM(MODELE=MODE,
                    DISCRET_2D=(_F(GROUP_MA='RES_LAT',
                                   REPERE='LOCAL',
                                   CARA='K_T_D_L',
                                   VALE=(2.0,0.0),),
                                _F(GROUP_MA='RES_HAUT',
                                   REPERE='LOCAL',
                                   CARA='K_T_D_L',
                                   VALE=(0.005,0.0),),
                              ),);

#---------------------------------------------------------------------
#                         CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------

MPATIN=DEFI_MATERIAU(ELAS=_F(E=2.10000E5,
                             NU=0.0,),);

MBATI=DEFI_MATERIAU(ELAS=_F(E=1.E5,
                            NU=0.0,),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL,
                 MODELE=MODE,
                 AFFE=(_F(GROUP_MA='PATIN',
                          MATER=MPATIN,),
                       _F(GROUP_MA='BATI',
                          MATER=MBATI,),
                       ),);

#---------------------------------------------------------------------
#                         CHARGEMENTS
#---------------------------------------------------------------------

# CONDITIONS AUX LIMITES
COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO='BATI',
                                     DX=0.0,DY=0.0,),
                                  _F(GROUP_NO=('R1','R2','X','Y'),
                                     DX=0.0,DY=0.0,),),);

# CHARGEMENT 1 : APPLICATION DE LA PRESSION NORMALE
PNOR=AFFE_CHAR_MECA(MODELE=MODE,
                    PRES_REP=_F(GROUP_MA='FACESUP',
                                PRES=300.0,),);

# CHARGEMENT 2 : APPLICATION DE LA PRESSION NORMALE ET DE LA PRESSION
#                  TANGENTIELLE (PT=0.3*PN*0.99)
PTAN_99=AFFE_CHAR_MECA(MODELE=MODE,
                       PRES_REP=_F(GROUP_MA='FACELAT',
                                   PRES=178.19999999999999,),);

# CHARGEMENT 3 : APPLICATION DE LA PRESSION NORMALE ET DE LA PRESSION
#                  TANGENTIELLE (PT=0.3*PN*1.01)
PTAN_101=AFFE_CHAR_MECA(MODELE=MODE,
                        PRES_REP=_F(GROUP_MA='FACELAT',
                                    PRES=181.80000000000001,),);

# DEFINITION DU CONTACT
CTAC=AFFE_CHAR_MECA(MODELE=MODE,
                    CONTACT=_F(APPARIEMENT='MAIT_ESCL',
                               RECHERCHE='NOEUD_BOUCLE',
                               REAC_GEOM='CONTROLE',
                               NB_REAC_GEOM=2,
                               METHODE='PENALISATION',
                               FROTTEMENT='COULOMB',
                               COULOMB=0.3,
                               E_T=1.E+5,
                               E_N=1.E+5,
                               COEF_MATR_FROT=0.9,
                               GROUP_MA_MAIT='LMAI',
                               GROUP_MA_ESCL='LESC',
                               ),);

#---------------------------------------------------------------------
#                         RESOLUTION
#---------------------------------------------------------------------

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,
                          1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);
                    
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=2,),);
         
# CHARGEMENT 1          
U_NOR=STAT_NON_LINE(MODELE=MODE,
                    CHAM_MATER=CM,
                    CARA_ELEM=CARA,
                    EXCIT=(_F(CHARGE=PNOR,
                              FONC_MULT=RAMPE,),
                           _F(CHARGE=CTAC,),
                           _F(CHARGE=COND_LIM,),),
                    COMP_INCR=_F(RELATION='ELAS',
                                 TOUT='OUI',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    SOLVEUR=_F(SYME='OUI',
                               METHODE='LDLT',),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=30,),
                    INFO=1,);

U_NOR=CALC_NO(reuse =U_NOR,
              RESULTAT=U_NOR,
              MODELE=MODE,
              CARA_ELEM=CARA,
              OPTION='REAC_NODA',
              EXCIT=(_F(CHARGE=PNOR,
                        FONC_MULT=RAMPE,),
                     _F(CHARGE=CTAC,),
                     _F(CHARGE=COND_LIM,),),
              );
                                               
# CHARGEMENT 2                                 
U_99=STAT_NON_LINE(MODELE=MODE,
                   CHAM_MATER=CM,
                   CARA_ELEM=CARA,
                   EXCIT=(_F(CHARGE=PNOR,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=PTAN_99,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=CTAC,),
                          _F(CHARGE=COND_LIM,),),
                   COMP_INCR=_F(RELATION='ELAS',
                                TOUT='OUI',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   NEWTON=_F(MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   SOLVEUR=_F(SYME='OUI',
                              METHODE='LDLT',),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=1500,),);

U_99=CALC_NO(reuse =U_99,
             RESULTAT=U_99,
             CARA_ELEM = CARA,
             MODELE=MODE,
             OPTION='REAC_NODA',
             EXCIT=(_F(CHARGE=PNOR,
                       FONC_MULT=RAMPE,),
                    _F(CHARGE=PTAN_99,
                       FONC_MULT=RAMPE,),
                    _F(CHARGE=CTAC,),
                    _F(CHARGE=COND_LIM,),),
              );

# CHARGEMENT 3 
U_101=STAT_NON_LINE(MODELE=MODE,
                    CHAM_MATER=CM,
                    CARA_ELEM=CARA,
                    EXCIT=(_F(CHARGE=PNOR,
                              FONC_MULT=RAMPE,),
                           _F(CHARGE=PTAN_101,
                              FONC_MULT=RAMPE,),
                           _F(CHARGE=CTAC,),
                           _F(CHARGE=COND_LIM,),),
                    COMP_INCR=_F(RELATION='ELAS',
                                 TOUT='OUI',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    NEWTON=_F(MATRICE='TANGENTE',
                              REAC_ITER=1,),
                    SOLVEUR=_F(SYME='OUI',
                               METHODE='LDLT',),
                    CONVERGENCE=_F(ITER_GLOB_MAXI=1500,),
                    ARCHIVAGE=_F(ARCH_ETAT_INIT='OUI'),
                    );

U_101=CALC_NO(reuse =U_101,
              RESULTAT=U_101,
              CARA_ELEM = CARA,
              MODELE=MODE,
              OPTION='REAC_NODA',
              EXCIT=(_F(CHARGE=PNOR,
                        FONC_MULT=RAMPE,),
                     _F(CHARGE=PTAN_101,
                        FONC_MULT=RAMPE,),
                     _F(CHARGE=CTAC,),
                     _F(CHARGE=COND_LIM,),),
              );

