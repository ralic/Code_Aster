                   VERSION='6.3.13',),),);

TU_99=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                              GROUP_NO='LESC',
                              RESULTAT=U_99,
                              NOM_CHAM='REAC_NODA',
                              NOM_CMP=('DX','DY'),
                              REPERE='LOCAL',
                              OPERATION='EXTRACTION',
