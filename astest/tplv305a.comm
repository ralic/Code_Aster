#
# LECTURE DU MAILLAGE --------------------------------------------------
#


MAIL=LIRE_MAILLAGE( )

#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ----------------------------
#

# ----- MATERIAU


MATER=DEFI_MATERIAU(   THER=_F(  LAMBDA = 1.,  RHO_CP = 1.) )

#
# AFFECTATIONS: MATERIAU, MODELE, CHARGEMENT ---------------------------
#


CHMAT=AFFE_MATERIAU(   MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI',  MATER = MATER)
                        )

MOTH=AFFE_MODELE(   MAILLAGE=MAIL,
                        AFFE=_F(  TOUT = 'OUI',
                               PHENOMENE = 'THERMIQUE',
                               MODELISATION = 'AXIS_FOURIER')
                      )

CHTH0=AFFE_CHAR_THER(   MODELE=MOTH,
                         TEMP_IMPO=_F(  GROUP_NO = 'NOE_CYL',  TEMP = -17.778)
                         )

CHTH1=AFFE_CHAR_THER(   MODELE=MOTH,
                         TEMP_IMPO=_F(  GROUP_NO = 'NOE_CYL',  TEMP = 44.444)
                         )

#
# DEFINITION DE LA STRATEGIE DE CALCUL ---------------------------------
#

#
# RESOLUTION -----------------------------------------------------------
#

MTRE0=CALC_MATR_ELEM(  OPTION='RIGI_THER',
                             MODELE=MOTH,
                             CHAM_MATER=CHMAT,
                             MODE_FOURIER=0,
                             CHARGE=CHTH0
                          )

VCTER0=CALC_VECT_ELEM(  OPTION='CHAR_THER',
                             CHARGE=CHTH0
                          )

NUDL0=NUME_DDL(   MATR_RIGI=MTRE0,
                        METHODE='LDLT',  RENUM='RCMK'
                    )

MTRA0=ASSE_MATRICE(   MATR_ELEM=MTRE0,
                           NUME_DDL=NUDL0
                        )

VCTA0=ASSE_VECTEUR(   VECT_ELEM=VCTER0,
                           NUME_DDL=NUDL0
                        )

MTRA0=FACTORISER( reuse=MTRA0,  MATR_ASSE=MTRA0 )

TMOD0=RESOUDRE(   MATR=MTRA0,
                        CHAM_NO=VCTA0
                     )

#--------------------------------------------------------


MTRE1=CALC_MATR_ELEM(  OPTION='RIGI_THER',
                             MODELE=MOTH,
                             CHAM_MATER=CHMAT,
                             MODE_FOURIER=1,
                             CHARGE=CHTH1
