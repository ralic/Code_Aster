  VALE      = a,
  PRECISION = 1.E-3,
  TABLE     = RESU,
  NOM_PARA  = 'V1',
  FILTRE    = (
    _F(NOM_PARA='INST',  VALE=tfin_pilo),
    _F(NOM_PARA='COOR_Y',VALE=1),
    ),
  )

TEST_TABLE(REFERENCE = 'ANALYTIQUE',
  VALE      = a,
  PRECISION = 1.E-3,
  TABLE     = RESU,
  NOM_PARA  = 'V1',
  FILTRE    = (
    _F(NOM_PARA='INST',  VALE=tfin_pilo),
    _F(NOM_PARA='COOR_Y',VALE=2),
    ),
  )

