CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(GROUP_MA=('VOL','VOL_RIG',),
                            MATER=ACIER,),);

CH=AFFE_CHAR_MECA(MODELE=MODE,
                  DDL_IMPO=(_F(GROUP_MA='RIGID',
                               DX=0.,
                               DY=0.,
                               DZ=0.,),
