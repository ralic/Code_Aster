                          ECHEC    =(
                            _F(
                               EVENEMENT       = 'ERREUR',
                               ACTION          = 'DECOUPE',
                               SUBD_METHODE    = 'MANUEL',
                              ),  
                            _F(
                               EVENEMENT       = 'COLLISION',
                               ACTION          = 'DECOUPE',
                               SUBD_METHODE    = 'AUTO',
                               PREC_COLLISION  = 5E-2,
                              ),                                 
                          ),
                          INFO = 1,
                        )

RESU=DYNA_NON_LINE(MODELE=MODE,
                   CHAM_MATER=CHMAT,
                   EXCIT=_F(CHARGE=CH,),
                   CONTACT = CHCONT,
                   COMP_INCR=(_F(
                                 RELATION='VMIS_ISOT_LINE',
                                 DEFORMATION='SIMO_MIEHE',
                                 TOUT='OUI',),
                              _F(RELATION='ELAS',
                                 DEFORMATION='PETIT',
                                 GROUP_MA='VOL_RIG',),),
                   ETAT_INIT=_F(VITE=VIT_0,),
                   INCREMENT=_F(LIST_INST=DEFLIST,),
                   SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',   
                                 FORMULATION='DEPLACEMENT',
                                 BETA=0.49,GAMMA=0.9,),
                   NEWTON=_F(REAC_INCR=1,
                             REAC_ITER=1,),
                   SOLVEUR=_F(SYME='OUI',
                              METHODE='LDLT',),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1.E-06,
                                  ITER_GLOB_MAXI=20,),
                                );

TABJEU = POST_RELEVE_T (ACTION = _F(GROUP_NO='B1',
                                 INTITULE = 'MESSAGE',
                                 RESULTAT = RESU,
                                 TOUT_CMP='OUI',
                                 NOM_CHAM   = 'VALE_CONT',
                                 OPERATION = 'EXTRACTION'))

