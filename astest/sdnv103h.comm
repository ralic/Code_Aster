                                GROUP_NO='A1',
                                RESULTAT=RESU,
                                NOM_CHAM='DEPL',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);


VITE_A1=POST_RELEVE_T(ACTION=_F(INTITULE='CITE EN A1',
                                GROUP_NO='A1',
                                RESULTAT=RESU,
                                NOM_CHAM='VITE',
                                NOM_CMP='DY',
                                OPERATION='EXTRACTION',),);

DEPL_B1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL EN B1',
                                GROUP_NO='B1',
                                RESULTAT=RESU,
                                NOM_CHAM='DEPL',
                                NOM_CMP='DZ',
                                OPERATION='EXTRACTION',),);

VITE_B1=POST_RELEVE_T(ACTION=_F(INTITULE='VITE EN B1',
                                GROUP_NO='B1',
                                RESULTAT=RESU,
                                NOM_CHAM='VITE',
                                NOM_CMP='DZ',
                                OPERATION='EXTRACTION',),);

