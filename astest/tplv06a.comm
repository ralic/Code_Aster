DEBUT(  CODE=_F( NOM = 'TPLV06A',NIV_PUB_WEB='INTERNET'))

#
# LECTURE DU MAILLAGE ----------------------------------------------
#

MAIL=LIRE_MAILLAGE()

#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ------------------------
#


MATE=DEFI_MATERIAU( THER=_F( RHO_CP = 1.0, LAMBDA = 1.0))

#
# AFFECTATIONS: MATERIAU  MODELE  CHARGEMENT ----------------------
#

CHMAT=AFFE_MATERIAU(   MAILLAGE=MAIL,
                     AFFE=_F( TOUT = 'OUI',  MATER = MATE))

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
               AFFE=_F( TOUT = 'OUI', MODELISATION = '3D',
                                   PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER(  MODELE=MOTH,
                      TEMP_IMPO=_F( GROUP_NO = 'TEMP_IMP', TEMP = 20.0),
                         SOURCE=_F( TOUT = 'OUI', SOUR = 100.0E0)
         )

#
# RESOLUTION -----------------------------------------------------------
#


TEMPE=THER_LINEAIRE(   MODELE=MOTH,
                            CHAM_MATER=CHMAT,
                            EXCIT=_F(  CHARGE = CHTH)
                         )

#
# ANALYSE DES RESULTATS ------------------------------------------------
#
T2=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMPE,
                   NUME_ORDRE=0)

TEST_RESU(CHAM_NO=(
   
#
#------ R = 1.25
#
   _F( CHAM_GD = T2, NOEUD = 'N16',  NOM_CMP = 'TEMP', VALE = 30.625, PRECISION = 1.E-2),
   _F( CHAM_GD = T2, NOEUD = 'N116', NOM_CMP = 'TEMP', VALE = 30.625, PRECISION = 1.E-2),
   _F( CHAM_GD = T2, NOEUD = 'N20',  NOM_CMP = 'TEMP', VALE = 30.625, PRECISION = 1.E-2),
   _F( CHAM_GD = T2, NOEUD = 'N120', NOM_CMP = 'TEMP', VALE = 30.625, PRECISION = 1.E-2),
#
#------  X = 1.50
#
   _F( CHAM_GD = T2, NOEUD = 'N11',  NOM_CMP = 'TEMP', VALE = 32.500, PRECISION = 1.E-2),
   _F( CHAM_GD = T2, NOEUD = 'N111', NOM_CMP = 'TEMP', VALE = 32.500, PRECISION = 1.E-2),
   _F( CHAM_GD = T2, NOEUD = 'N15',  NOM_CMP = 'TEMP', VALE = 32.500, PRECISION = 1.E-2),
   _F( CHAM_GD = T2, NOEUD = 'N115', NOM_CMP = 'TEMP', VALE = 32.500, PRECISION = 1.E-2),
#
#------  X = 1.75
#
   _F( CHAM_GD = T2, NOEUD = 'N6',   NOM_CMP = 'TEMP', VALE = 28.482, PRECISION = 1.E-2),
   _F( CHAM_GD = T2, NOEUD = 'N106', NOM_CMP = 'TEMP', VALE = 28.482, PRECISION = 1.E-2),
   _F( CHAM_GD = T2, NOEUD = 'N10',  NOM_CMP = 'TEMP', VALE = 28.482, PRECISION = 1.E-2),
   _F( CHAM_GD = T2, NOEUD = 'N110', NOM_CMP = 'TEMP', VALE = 28.482, PRECISION = 1.E-2))

   )

