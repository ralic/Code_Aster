           THER=_F(  LAMBDA = 600.,    RHO_CP = 1.,  ),
           ELAS=_F(  E = 220000., NU = 0.33, ALPHA = 16.E-6, RHO = 8300.,   ) )
#
# 1.2. LES LISTES DES INSTANTS
