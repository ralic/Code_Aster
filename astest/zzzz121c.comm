    TEMP_IMPO=_F(  GROUP_NO = 'ORIGINE',     TEMP = 200.0,    ),
    SOURCE=(_F(  GROUP_MA = 'MOITIE1',    SOUR = 18000.,    ),
            _F(  GROUP_MA = 'MOITIE2',    SOUR = 22000.,    )),
