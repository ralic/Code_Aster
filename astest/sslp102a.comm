#--------------------------------------------------------------------
#                TESTS DES RESULTATS                                #
#--------------------------------------------------------------------


TEST_TABLE(
           VALE_CALC=0.18015297371831,

           NOM_PARA='G',
           TABLE=G1,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

TEST_TABLE(
           VALE_CALC=0.18012031028967,

           NOM_PARA='G',
           TABLE=G2,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

TEST_TABLE(
           VALE_CALC=0.18011043699373,

           NOM_PARA='G',
           TABLE=G3,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

TEST_TABLE(
           VALE_CALC=0.18010917971263,

           NOM_PARA='G',
           TABLE=G4,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

FIN( )
