DEBUT(CODE=_F(NOM='SSLP102A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED',  )

MO=AFFE_MODELE(     MAILLAGE=MA,
                         AFFE=_F(  TOUT = 'OUI',
                                PHENOMENE = 'MECANIQUE',
                                MODELISATION = 'D_PLAN'))

MAT=DEFI_MATERIAU(     ELAS=_F(  E = 20000.,
                                NU = 0.3,
                                RHO = 1.,
                                ALPHA = 0.000005) )

CM=AFFE_MATERIAU(    MAILLAGE=MA,
                           AFFE=_F(  TOUT = 'OUI',
                                  MATER = MAT) )

#--------------------------------------------------------------------

EPSINI=DEFI_FONCTION(   NOM_PARA='X',
                         VALE=(-50., -0.0005,
                                50.,  0.0005,),
                            INTERPOL='LIN',
                          PROL_DROITE='LINEAIRE',
                         PROL_GAUCHE='LINEAIRE'
                         )

ZERO = FORMULE(NOM_PARA=('X','Y'),VALE='0.')

CH=AFFE_CHAR_MECA_F(       MODELE=MO,DDL_IMPO=(
                           _F(  GROUP_NO = 'GRNM2',      DY = ZERO),
                           _F(  GROUP_NO = 'GRNM4',      DY = ZERO),
                           _F(  NOEUD = 'N695',      DX = ZERO)),
                          PRE_EPSI=_F(  TOUT = 'OUI', EPXX = EPSINI,
                                                 EPYY = EPSINI,
                                                 EPZZ = EPSINI)
                        )

#--------------------------------------------------------------------

F=DEFI_FONCTION(      NOM_PARA='INST',
                        PROL_DROITE='LINEAIRE',
                       PROL_GAUCHE='LINEAIRE',
                               VALE=( 0.0,  0.0,
                                      1.0,  1.0,
                                      )
                     )

L_INST=DEFI_LIST_REEL(          DEBUT=0.0,
                              INTERVALLE=_F(  JUSQU_A = 1.0,
                                            NOMBRE = 1)
                           )

RESU=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CM,
                             EXCIT=_F(
                CHARGE = CH,),
                #FONC_MULT = F),
                         COMPORTEMENT=_F(
                RELATION = 'ELAS'),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                MATRICE = 'ELASTIQUE'),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6)
              )

RESU=CALC_CHAMP(reuse=RESU,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=RESU)


DEPLA=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=RESU,
                     NUME_ORDRE=1)

#--------------------------------------------------------------------

THETA1=CALC_THETA(       MODELE=MO,
                         THETA_2D=_F(  NOEUD = 'N3',
                                    MODULE = 1.0,
                                    R_INF = 1.,
                                    R_SUP = 5.),
                        DIRECTION=(1.,0.,0.,)
                    )

THETA2=CALC_THETA(       MODELE=MO,
                         THETA_2D=_F(  NOEUD = 'N3',
                                    MODULE = 1.0,
                                    R_INF = 5.,
                                    R_SUP = 10.),
                       DIRECTION=(1.,0.,0.,)
                    )

THETA3=CALC_THETA(       MODELE=MO,
                         THETA_2D=_F(  NOEUD = 'N3',
                                    MODULE = 1.0,
                                    R_INF = 10.,
                                    R_SUP = 20.),
                        DIRECTION=(1.,0.,0.,) )

THETA4=CALC_THETA(       MODELE=MO,
                         THETA_2D=_F(  NOEUD = 'N3',
                                    MODULE = 1.0,
                                    R_INF = 20.,
                                    R_SUP = 30.),
                       DIRECTION=(1.,0.,0.,) )

#--------------------------------------------------------------------
#  CALCUL DE G AVEC DEFORMATIONS INITIALES
#--------------------------------------------------------------------

G1=CALC_G(        RESULTAT=RESU,
                  NUME_ORDRE=1,
                  THETA=_F(THETA=THETA1 ),
                  COMPORTEMENT=_F(
                           RELATION = 'ELAS'),
                           );

G2=CALC_G(        RESULTAT=RESU,
                  NUME_ORDRE=1,
                  THETA=_F(THETA=THETA2 ),
                  COMPORTEMENT=_F(
                           RELATION = 'ELAS'),);

G3=CALC_G(        RESULTAT=RESU,
                  NUME_ORDRE=1,
                  THETA=_F(THETA=THETA3 ),
                  COMPORTEMENT=_F(
                           RELATION = 'ELAS'),);

G4=CALC_G(        RESULTAT=RESU,
                  NUME_ORDRE=1,
                  THETA=_F(THETA=THETA4 ),
                  COMPORTEMENT=_F(
                           RELATION = 'ELAS'),);

#--------------------------------------------------------------------
#  CALCUL DE G AVEC DEFORMATIONS INITIALES + ETAT INITIAL
#--------------------------------------------------------------------


