# initialisation du calcul de diffusion

MOTH=AFFE_MODELE(MAILLAGE=MA,AFFE=_F(TOUT='OUI',PHENOMENE='THERMIQUE',MODELISATION='PLAN_DIAG',),);

# Dl en m**2/s
