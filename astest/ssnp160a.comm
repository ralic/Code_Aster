   UTH=THER_LINEAIRE(MODELE=MOTH,
                   CHAM_MATER=CMTH,
                   INCREMENT=_F(  LIST_INST = L_INST, INST_FIN=tfin),
                   EXCIT=(
                   _F(CHARGE=CHTH,),
                   _F(CHARGE=CHARGRD0,),
                   ),
                   ETAT_INIT=_F( EVOL_THER = UTH,), reuse=UTH,
                   );
   tinit = tfin

   DETRUIRE(INFO=1,CONCEPT=_F(NOM=(CHTH,NEUTG,CMTH)))



