   CMTH=AFFE_MATERIAU(MAILLAGE=MA,INFO=1,
                      AFFE=_F(GROUP_MA='CUBE_G',MATER=ACIERTH,),
                         AFFE_VARC=(_F(GROUP_MA='CUBE_G',
                                      CHAM_GD=NEUTG,
                                      NOM_VARC='NEUT1'),),
