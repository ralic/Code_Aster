DEBUT(CODE=_F(NOM='SSNV182J', NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                        LINE_QUAD=_F(GROUP_MA='SURF',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=_F(NOM='SURFQUAD',
                                     GROUP_MA='SURF',),);

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('SURF',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN_XFEM_CONT',),
                           _F(GROUP_MA=('LIG1','LIG3',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),),);


THETA=-30./180.*pi

LN=FORMULE(NOM_PARA=('X','Y'),VALE='(Y-10)*cos(THETA)-sin(THETA)*(X-10)');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN,),
                    GROUP_MA_ENRI='SURF',);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='P1P1A',                         
                         INFO=1,);

CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      FROTTEMENT     = 'COULOMB',
                      ITER_CONT_MAXI = 4,
                      ITER_FROT_MAXI = 6,                      
                      REAC_GEOM      = 'SANS',
                      ZONE=(
                            _F(
                               FISS_MAIT    = FISS,
                               INTEGRATION  = 'GAUSS',
                               CONTACT_INIT = 'OUI',
                               COEF_ECHELLE = 1.,
                               COULOMB      = 1.0,
                               SEUIL_INIT   = -5,
                               ALGO_CONT    = 'PENALISATION',
                               COEF_PENA_CONT = 1.0E10,
                               ALGO_FROT    = 'PENALISATION',
                               COEF_PENA_FROT = 1.0E10,                                    
                               ALGO_LAGR    = 'NON',
                             ), 
                         ),
                   );

E=1.0E6
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA=('SURF','LIG1','LIG3',),
                                MATER=ACIER,
                                ),);

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIG1',
                                          DX=0.0,
                                          DY=0.0,),
                                       _F(GROUP_MA='LIG3',
                                          DX=0.0,
                                          DY=-1.E-6,),),
                    INFO=1,);
                    
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

IMPR_RESU(MODELE=MODELEK,FORMAT='ASTER',UNITE=20,
            RESU=(_F( MAILLAGE=MAILLAG2,),),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CHXFEM,),                  
                          _F(CHARGE=CH1,),),
                   CONTACT  = CTXFEM,       
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='SURF',),
                   INCREMENT=_F(LIST_INST=L_INST,
                                INST_FIN=1.0,),
                   CONVERGENCE=(_F(ITER_GLOB_MAXI=5,),),
                   SOLVEUR=_F(METHODE='MUMPS',
                              PCENT_PIVOT=20,),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);


LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                           # maillage 20_20
                             NOEUD=('N261','NS222','NS223','NS224',
                             'NS259','NS260','NS261','NS298','NS299',
                             'NS335','NS336','NS337','NS372','NS373',
                             'NS374','NS411','NS448','NS449','NS485',
                             'NS486','NS487','NS522','NS523',
                             'NS524','NS561','NS562','NS598',
                             'NS599','NS600','NS636','NS638',),
#                             GROUP_NO='SURFQUAD',
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
#                             TOUT_CMP='OUI',
                             NOM_CMP=('LAGS_C','LAGS_F1',),
                             OPERATION='EXTRACTION',),);


IMPR_TABLE(TABLE=LAG,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           FORMAT_C='MODULE_PHASE',
           IMPR_FONCTION='NON',);

LAREF=-.05*3./4.
LA1REF=-1./(3.**.5)

# TESTS
TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MAX',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=1.E-4,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MIN',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=1.E-4,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MAX',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-4,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MIN',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-4,
           REFERENCE='ANALYTIQUE',);

# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAG1,
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),) 

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAG1,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        
                        INFO          = 2)
 
#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(RESULTAT=RES_XFEM));



FIN();

