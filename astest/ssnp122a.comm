DEBUT(CODE=_F(NOM='SSNP122A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

# REM : POUR ASSURER LA PERTINENCE DU TEST AVEC LA MODELISATION
# INCO_GD, ON UTILISE UN MAILLAGE CONTENANT PLUSIEURS MAILLES.
# EN EFFET, CES ELEMENTS NE PASSENT PAS LE PATCH TEST SUR UN ELEMENT
# UNIQUE, CAD QUE  PLUSIEURS SOLUTIONS EXISTENT : ON OBSERVE ALORS UNE
# GRANDE SENSIBILITE DE LA SOLUTION OBTENUE PAR RAPPORT AU SOLVEUR
# UTILISE

MA=LIRE_MAILLAGE(FORMAT='MED',);



MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_MA=_F(NOM= 'ESSAI', TOUT='OUI'),
              CREA_GROUP_NO=(_F(GROUP_MA='AB',),
                             _F(GROUP_MA='DA',),
                             _F(GROUP_MA='CD',),),);

MO_L=AFFE_MODELE(MAILLAGE=MA,
               VERIF='MAILLE',
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN_SI',),);

# MO_NL=AFFE_MODELE(MAILLAGE=MA,
#                VERIF='MAILLE',
#                AFFE=_F(TOUT='OUI',
#                        PHENOMENE='MECANIQUE',
#                        MODELISATION='D_PLAN_GRAD_VARI',),);

MO_NLG=AFFE_MODELE(MAILLAGE=MA,
               VERIF='MAILLE',
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN_INCO_GD',),);

TRACT= DEFI_FONCTION(
  NOM_PARA = 'EPSI',
  VALE = (
    0.002,   400,
    1.002,  2400),
  PROL_DROITE = 'LINEAIRE');

ACIER = DEFI_MATERIAU(
  ELAS       = _F(E = 200000,  NU = 0.3),
  TRACTION   = _F(SIGM = TRACT),
  ROUSSELIER = _F(D = 2,  SIGM_1 = 500,  PORO_INIT = 0.01),
  NON_LOCAL  = _F(
                #   LONG_CARA=0.1,
                   C_GONF=1
                  ));

CHP_MAT=AFFE_MATERIAU(MAILLAGE=MA,
                      AFFE=_F(TOUT='OUI',
                              MATER=ACIER,),);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=(_F(JUSQU_A=1.5,
                                     NOMBRE=50,),),);

U_CHAR=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,0.0,1.5,1.5),
                     PROL_DROITE='CONSTANT',);

CHA_L=AFFE_CHAR_MECA(MODELE=MO_L,
                      DDL_IMPO=(_F(GROUP_NO='AB',DY=0.0,),
                                _F(GROUP_NO='A',DX=0.0,),
                                _F(GROUP_NO='CD',DY=1.0,),),);
# CHA_NL=AFFE_CHAR_MECA(MODELE=MO_NL,
#                       DDL_IMPO=(_F(GROUP_NO='AB',DY=0.0,),
#                                 _F(GROUP_NO='A',DX=0.0,),
#                                 _F(GROUP_NO='CD',DY=1.0,),),);
CHA_NLG=AFFE_CHAR_MECA(MODELE=MO_NLG,
                      DDL_IMPO=(_F(GROUP_NO='AB',DY=0.0,),
                                _F(GROUP_NO='A',DX=0.0,),
                                _F(GROUP_NO='CD',DY=1.0,),),);

U_L=STAT_NON_LINE(MODELE=MO_L,
                CHAM_MATER=CHP_MAT,
                EXCIT=_F(CHARGE=CHA_L,
                         FONC_MULT=U_CHAR,),
                COMP_INCR=_F(RELATION='ROUSSELIER',
                             DEFORMATION='SIMO_MIEHE',
                             RESI_INTE_RELA=1.E-06,
                             ITER_INTE_MAXI=15,),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                RECH_LINEAIRE=_F(ITER_LINE_MAXI=5,),
                CONVERGENCE=_F(RESI_GLOB_RELA=1.E-06,
                               ITER_GLOB_MAXI=15),);

# U_NL=STAT_NON_LINE(MODELE=MO_NL,
#                 CHAM_MATER=CHP_MAT,
#                 EXCIT=_F(CHARGE=CHA_NL,
#                          FONC_MULT=U_CHAR,),
#                 COMP_INCR=_F(RELATION='ROUSSELIER',
#                              DEFORMATION='SIMO_MIEHE',
#                              RESI_INTE_RELA=1.E-06,
#                              ITER_INTE_MAXI=15,),
#                 INCREMENT=_F(LIST_INST=L_INST,),
#                 NEWTON=_F(MATRICE='TANGENTE',
#                           REAC_ITER=1,),
#                 CONVERGENCE=_F(RESI_GLOB_RELA=1.E-06,
#                                ITER_GLOB_MAXI=150),
#                 LAGR_NON_LOCAL=_F(
#                   ITER_DUAL_MAXI=25,
#                   RESI_DUAL_ABSO=1.E-3,
#                   RESI_PRIM_ABSO=1.E-5,
#                   R=50.0,
#                   ITER_PRIM_MAXI=10,),);

# LES SYSTEMES LINEAIRES SONT DIFFICILES A RESOUDRE.
# ON DEVERROUILLE LA DETECTION DE SINGULARITE. LE PROCESSUS DE NEWTON
# CORRIGE LES EVENTUELLES DERIVES.
U_NLG=STAT_NON_LINE(MODELE=MO_NLG,
                CHAM_MATER=CHP_MAT,
                EXCIT=_F(CHARGE=CHA_NLG,
                         FONC_MULT=U_CHAR,),
                COMP_INCR=_F(RELATION='ROUSSELIER',
                             DEFORMATION='SIMO_MIEHE',
                             RESI_INTE_RELA=1.E-06,
                             ITER_INTE_MAXI=15,),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                SOLVEUR=_F(METHODE='MUMPS',PCENT_PIVOT=30,
                           NPREC=-1,),
                CONVERGENCE=_F(RESI_REFE_RELA=1.E-4,
                               SIGM_REFE = 500 ,
                               EPSI_REFE = 1),);


TEST_RESU(RESU=(
 _F(RESULTAT = U_L,
     NOM_CHAM = 'SIEF_ELGA',
     TYPE_TEST='MAX',
     INST = 0.93,
     NOM_CMP = 'SIYY',
     PRECISION = 1.E-3,
     VALE = 942.702,
     REFERENCE = 'NON_REGRESSION'),
  _F(RESULTAT = U_L,
     NOM_CHAM = 'SIEF_ELGA',
     INST = 0.93,
     TYPE_TEST = 'MAX',
     NOM_CMP = 'SIZZ',
     PRECISION = 1.E-3,
     VALE = 135.668,
     REFERENCE = 'NON_REGRESSION'),
   _F(RESULTAT = U_L,
      NOM_CHAM = 'VARI_ELGA',
      INST = 0.93,
     TYPE_TEST = 'MAX',
      NOM_CMP = 'V1',
      PRECISION = 1.E-3,
      VALE = 0.62986,
      REFERENCE = 'NON_REGRESSION'),
   _F(RESULTAT = U_L,
      NOM_CHAM = 'VARI_ELGA',
      INST = 0.93,
      TYPE_TEST = 'MAX',
      NOM_CMP = 'V2',
      PRECISION = 1.E-3,
      VALE = 0.22493,
      REFERENCE = 'NON_REGRESSION'),),)
TEST_RESU(RESU=(
  _F(RESULTAT = U_L,
     NOM_CHAM = 'SIEF_ELGA',
     INST = 1.5,
     TYPE_TEST = 'MAX',
     NOM_CMP = 'SIYY',
     PRECISION = 1.E-3,
     VALE = 721.687,
     REFERENCE = 'NON_REGRESSION'),
  _F(RESULTAT = U_L,
     NOM_CHAM = 'SIEF_ELGA',
     INST = 1.5,
     TYPE_TEST = 'MIN',
     NOM_CMP = 'SIYY',
     PRECISION = 1.E-3,
     VALE = 721.687,
     REFERENCE = 'NON_REGRESSION'),),)


TEST_RESU(RESU=(
 _F(RESULTAT = U_NLG,
     NOM_CHAM = 'SIEF_ELGA',
     INST = 0.93,
     TYPE_TEST = 'MAX',
     NOM_CMP = 'SIYY',
     PRECISION = 1.E-3,
     VALE = 942.702,
     REFERENCE = 'NON_REGRESSION'),
  _F(RESULTAT = U_NLG,
     NOM_CHAM = 'SIEF_ELGA',
     INST = 0.93,
     TYPE_TEST = 'MAX',
     NOM_CMP = 'SIZZ',
     PRECISION = 1.E-3,
     VALE = 135.668,
     REFERENCE = 'NON_REGRESSION'),
   _F(RESULTAT = U_NLG,
      NOM_CHAM = 'VARI_ELGA',
      INST = 0.93,
      TYPE_TEST = 'MAX',
      NOM_CMP = 'V1',
      PRECISION = 1.E-3,
      VALE = 0.62986,
      REFERENCE = 'NON_REGRESSION'),
   _F(RESULTAT = U_NLG,
      NOM_CHAM = 'VARI_ELGA',
      INST = 0.93,
      TYPE_TEST = 'MAX',
      NOM_CMP = 'V2',
      PRECISION = 1.E-3,
      VALE = 0.22493,
      REFERENCE = 'NON_REGRESSION'),),)
TEST_RESU(RESU=(
  _F(RESULTAT = U_NLG,
     NOM_CHAM = 'SIEF_ELGA',
     INST = 1.5,
     TYPE_TEST = 'MAX',
     NOM_CMP = 'SIYY',
     PRECISION = 1.E-3,
     VALE = 721.687,
     REFERENCE = 'NON_REGRESSION'),
  _F(RESULTAT = U_NLG,
     NOM_CHAM = 'SIEF_ELGA',
     INST = 1.5,
     TYPE_TEST = 'MIN',
     NOM_CMP = 'SIYY',
     PRECISION = 1.E-3,
     VALE = 721.687,
     REFERENCE = 'NON_REGRESSION'),),)
FIN();
