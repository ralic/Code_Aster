DEBUT(CODE=_F(NOM='SSNV203B', NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
              
# CETTE MODELISATION TESTE L'APPLICATION D'UNE PRESSION (CONSTANTE ET AFFINE) 
# SUR LES LEVRES D'UNE FISSURE X-FEM EN 3D (EN FAIT, C'EST PLUTOT UNE INTERFACE)

#----------------------------------------------
#                   MAILLAGE
#----------------------------------------------
# precision
n = 1000.0;

# dimensions de la structure
lx = 1.0;

ly = 1.0;


PRE_GIBI();

MAILLAG2=LIRE_MAILLAGE(INFO=1,);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=(_F(GROUP_MA='VOL'),
                                   _F(GROUP_MA='SURFSUP'),
                                   _F(GROUP_MA='SURFINF'),),
                    INFO=1,);

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MO=AFFE_MODELE(MAILLAGE=MAILLAG2,
               AFFE=_F(GROUP_MA=('VOL',),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-0.5');
#LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Y-0.5');
LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='-Y-100.');

FISS=DEFI_FISS_XFEM(MODELE=MO,
                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                    GROUP_MA_ENRI='VOL',
                    ORIE_FOND=_F(PFON_INI= (0. ,0.5 ,0.5 ),
                                 VECT_ORIE=(0. ,0. ,1. ),
                                 POINT_ORIG=(0. ,2. ,0.5 ),),
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MO,
                         FISSURE=FISS,
                         INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      INFO=1,);
#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
E = 10000000000.0;

nu = 0.;

rho = 7800.0;


ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=rho,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MO,
                       AFFE=_F(GROUP_MA='VOL',
                               MATER=ACIER,
                               ),);
#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

CH=AFFE_CHAR_MECA(MODELE=MODELEK,
                  DDL_IMPO=(_F(GROUP_NO='SURFSUP',DX=0.,DY=0.,DZ=0.),
                            _F(GROUP_NO='SURFINF',DX=0.,DY=0.,DZ=0.),),
                  INFO=1,);


# pour tester option CHAR_MECA_PRES_R
CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   PRES_REP=_F(FISSURE=FISS,PRES=10000.)
                   )

# pour tester option CHAR_MECA_PRES_F
FCTPRES = FORMULE(VALE='Y*10000.',
                  NOM_PARA=('Y',),);

CH2=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                     PRES_REP=_F(FISSURE=FISS,PRES=FCTPRES)
                   )

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

L_INS=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=3.0,
                                   NOMBRE=3,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH,),
                           _F(CHARGE=CH1,),),
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='VOL',),
                    INCREMENT=_F(LIST_INST=L_INS,
                                 INST_FIN=1.0,),
                    NEWTON=_F(REAC_ITER=1,),
                    INFO=1,);

UTOT2=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH,),
                           _F(CHARGE=CH2,),),
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='VOL',),
                    INCREMENT=_F(LIST_INST=L_INS,
                                 INST_FIN=1.0,),
                    NEWTON=_F(REAC_ITER=1,),
                    INFO=1,);



MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAG2,
                       MODELE        = MODELEK)

#IMPR_RESU(FORMAT='RESULTAT',RESU=_F(MAILLAGE = MA_XFEM,))
#IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(MAILLAGE = MA_XFEM,))



MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=(_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),
                              );  

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAG2,
                        MAILLAGE_FISS = MA_XFEM,
                        MODELE        = MODELEK,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        NOM_CHAM      = ('DEPL','SIEF_ELGA'))


IMPR_RESU(FORMAT='GMSH',UNITE=82,RESU=_F(RESULTAT=RES_XFEM,));

DEP1=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                             INTITULE='DEPLE',
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             GROUP_NO='VOL',
                             TOUT_CMP='OUI',),);

DEP2=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                             INTITULE='DEPLE',
                             RESULTAT=UTOT2,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             GROUP_NO='VOL',
                             TOUT_CMP='OUI',),);


IMPR_TABLE(TABLE=DEP1,);
IMPR_TABLE(TABLE=DEP2,);

# POUR LE CHARGEMENT 1

TEST_TABLE(TABLE=DEP1,
           NOM_PARA='DCZ',
           FILTRE=_F(NOM_PARA='NOEUD',VALE_K='N11',),
           VALE=-2.e-7,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE',);


TEST_TABLE(TABLE=DEP1,
           NOM_PARA='DCZ',
           FILTRE=_F(NOM_PARA='NOEUD',VALE_K='N21',),
           VALE=1.e-7,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEP1,
           NOM_PARA='H1Z',
           FILTRE=_F(NOM_PARA='NOEUD',VALE_K='N21',),
           VALE=5.e-7,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEP1,
           NOM_PARA='DCZ',
           FILTRE=_F(NOM_PARA='NOEUD',VALE_K='N31',),
           VALE=-1.e-7,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEP1,
           NOM_PARA='H1Z',
           FILTRE=_F(NOM_PARA='NOEUD',VALE_K='N31',),
           VALE=5.e-7,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=DEP1,
           NOM_PARA='DCZ',
           FILTRE=_F(NOM_PARA='NOEUD',VALE_K='N41',),
           VALE=2.e-7,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='ANALYTIQUE',);


