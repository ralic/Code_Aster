           REFERENCE='NON_REGRESSION',);

TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DCY',
           FILTRE=_F(NOM_PARA='NOEUD',VALE_K='N80',),
           VALE= 1.2082287802957E-07,
           CRITERE='RELATIF',
           PRECISION=1.0E-11,
           REFERENCE='NON_REGRESSION',);


TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DCZ',
           FILTRE=_F(NOM_PARA='NOEUD',VALE_K='N80',),
           VALE= -9.9556039601360E-08,
