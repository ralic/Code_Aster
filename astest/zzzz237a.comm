                  REFERENCE='NON_REGRESSION',
                  VERSION='9.2.18',)
                  );

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  INST=20.0,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  GROUP_NO='A',
                  VALE=+2.19780E+10,
                  REFERENCE='NON_REGRESSION',
                  VERSION='9.2.18',)
                  );

TEST_RESU(RESU=_F(RESULTAT=RESU,
                  INST=20.0,
                  NOM_CHAM='FORC_NODA',
                  NOM_CMP='DY',
                  GROUP_NO='B',
                  VALE=+2.19780E+10,
                  REFERENCE='NON_REGRESSION',
                  VERSION='9.2.18',)
                  );

TEST_RESU(CHAM_NO=_F(CHAM_GD=VECT,
                     NOM_CMP='DY',
                     GROUP_NO='A',
                     VALE=+2.19780E+10,
                     REFERENCE='NON_REGRESSION',
                     VERSION='9.2.18',)
                  );

TEST_RESU(CHAM_NO=_F(CHAM_GD=VECT,
                     NOM_CMP='DY',
                     GROUP_NO='B',
                     VALE=+2.19780E+10,
                     REFERENCE='NON_REGRESSION',
                     VERSION='9.2.18',)
                     );

#--------------------------

IMPR_RESU(FORMAT='MED',
          RESU=_F(MAILLAGE=MA,
                  RESULTAT=RESU,
                  TOUT='OUI',),);

# Validation de CREA_CHAMP / COMB en "reentrant" :
# --------------------------------------------------
U=CREA_CHAMP(OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R', NOM_CHAM='DEPL', RESULTAT=RESU,INST=20.0,INFO=1)
U=CREA_CHAMP(reuse = U, OPERATION='COMB', INFO=1, TYPE_CHAM='NOEU_DEPL_R',
                COMB=(_F(CHAM_GD=U, COEF_R=1.),
                      _F(CHAM_GD=U,COEF_R=1.),),)
TEST_RESU(CHAM_NO=_F(CHAM_GD=U, NOM_CMP='DX', GROUP_NO='B', VALE=+8.5714281834519E+00, REFERENCE='NON_REGRESSION' )
                  );

#--------------------------

# Validation de TOLE_APPA en FORMULATION 'DISCRETE' et 'CONTINUE'

CONT_TA = DEFI_CONTACT(MODELE      = MO,
                   FORMULATION  = 'DISCRETE',
                   REAC_GEOM    = 'AUTOMATIQUE',
                   ZONE=(
                        _F(
                             GROUP_MA_MAIT='HE',
                             GROUP_MA_ESCL='AB',
                             TOLE_APPA=+1.0,
                         ),

                       ),
                  )

CONTC_TA = DEFI_CONTACT(MODELE      = MO,
                   FORMULATION  = 'CONTINUE',
                   ALGO_RESO_CONT = 'NEWTON',
                   ALGO_RESO_GEOM = 'NEWTON',
                   ZONE=(
                        _F(
                             GROUP_MA_MAIT='HE',
                             GROUP_MA_ESCL='AB',
                             TOLE_APPA=+1.0,
                         ),

                       ),
                  )

RESU_TA=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   CARA_ELEM=CAR_DIS,
                   EXCIT=(_F(CHARGE=BLOCAGE,),
                          _F(CHARGE=DEPL,
                             FONC_MULT=F_FORC,),
                          _F(CHARGE=BLOC_DIS,),
                          ),
                   CONTACT  = CONT_TA,
                   COMP_INCR=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=L_TPS,),
                #   CONVERGENCE=_F(RESI_GLOB_MAXI=1.0E-6,),
                   SOLVEUR=_F(SYME='OUI',),
                   );

RESUC_TA=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CHMAT,
                   CARA_ELEM=CAR_DIS,
                   EXCIT=(_F(CHARGE=BLOCAGE,),
                          _F(CHARGE=DEPL,
                             FONC_MULT=F_FORC,),
                          _F(CHARGE=BLOC_DIS,),
                          ),
                   CONTACT  = CONTC_TA,
                   COMP_INCR=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=L_TPS,),
                 #  CONVERGENCE=_F(RESI_GLOB_MAXI=1.0E-6,),
                   NEWTON=_F(REAC_ITER=1),
                   SOLVEUR=_F(SYME='OUI',),
                   );


# TESTS ANALTIQUES
# ----------------

TEST_RESU(RESU=_F(RESULTAT=RESU_TA,
                  INST=20.0,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A',
                  VALE=-20.0,
                  CRITERE='ABSOLU',
                  REFERENCE='ANALYTIQUE',)
                  );

TEST_RESU(RESU=_F(RESULTAT=RESU_TA,
                  INST=20.0,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='B',
                  VALE=-20.0,
                  CRITERE='ABSOLU',
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=RESUC_TA,
                  INST=20.0,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='A',
                  VALE=-20.0,
                  CRITERE='ABSOLU',
                  REFERENCE='ANALYTIQUE',)
                  );

TEST_RESU(RESU=_F(RESULTAT=RESUC_TA,
                  INST=20.0,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  GROUP_NO='B',
