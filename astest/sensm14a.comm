def estim(vm, vp, deltap):
   """Retourne l'estimation de la derivee
   """
   if deltap != 0.:
      res = (vp - vm) / deltap
   else:
      print vm, vp, deltap
      res = 0.
   return res

def ecart(diff, deriv):
   """
      Retourne l'ecart relatif entre les derivees si la reference est non nulle,
      la difference finie sinon.
   """
   if abs(deriv) > 1.e-3:
      res = abs(diff - deriv)/deriv
   else:
      res = deriv
   return res

