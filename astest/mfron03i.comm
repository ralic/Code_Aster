epsi_1 = 1.e-5    # s**-1
D      = 10.e-9*Cmm   # m
beta   = 0.2
d_lat  = 1. * Cmm  # m
K_f    = 30.
K_self = 100.
k_b      = 8.62E-5  # "Constante de Boltzmann, en eV/K"

