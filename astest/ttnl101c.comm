DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

from math import *

# LECTURE ET CREATION DES MAILLAGES

MESH_T3 = LIRE_MAILLAGE(FORMAT = 'MED')

MESH_T6 = CREA_MAILLAGE(MAILLAGE  = MESH_T3,
                        LINE_QUAD = _F(TOUT='OUI'));

rho_cp = 2.0
LAMBDA = DEFI_CONSTANTE(VALE = rho_cp)
RHO_CP = DEFI_CONSTANTE(VALE = rho_cp)


# Closed-form solution

w      = 2.0**0.5
T_max  = (1-1/cosh(w))/w**2 - exp(-(w**2+pi**2/4))
T_INF  = FORMULE(NOM_PARA='Y',VALE="(1-cosh(w*Y)/cosh(w))/w**2")
T_INI  = FORMULE(NOM_PARA='Y',VALE="T_INF(Y)-cos(pi*Y/2.0)")

# source(T) = rho_cp - rho_cp*w**2 * T

SOURCE = DEFI_FONCTION( NOM_PARA    = 'TEMP',
                        VALE        = (0,rho_cp, 1,rho_cp-rho_cp*w**2),
                        PROL_DROITE = 'LINEAIRE',
                        PROL_GAUCHE = 'LINEAIRE',
                      )

  
# Usual data setting

FAKE_MAT = DEFI_MATERIAU(THER_NL = _F(LAMBDA=LAMBDA, RHO_CP=RHO_CP),
                        )
TIME = DEFI_LIST_REEL(DEBUT      =  0.0,
                      INTERVALLE = _F(JUSQU_A = 1.0, NOMBRE = 100)
                     )


#===================================================================
# Verification des mailles de type TRIA3 avec une modelisation AXIS
#===================================================================

MAT_T3 = AFFE_MATERIAU(MAILLAGE = MESH_T3,
                        AFFE     = _F(GROUP_MA = 'ROD', MATER = FAKE_MAT),
                      )
  
MODEL_T3 = AFFE_MODELE(MAILLAGE = MESH_T3,
                       AFFE     = _F(GROUP_MA='ROD',PHENOMENE='THERMIQUE',MODELISATION='AXIS'),
                       )
  
BND_T3 = AFFE_CHAR_THER(MODELE    = MODEL_T3,
                        TEMP_IMPO = _F(GROUP_MA = 'END', TEMP=0),
                       )

# Non linear heating source

HEAT_T3 = AFFE_CHAR_THER_F(MODELE  = MODEL_T3,
                           SOUR_NL = _F(GROUP_MA='ROD', SOUR=SOURCE),    
                           )

# Initial temperature field

ND_COOR = CREA_CHAMP(OPERATION = 'EXTR',
                     TYPE_CHAM = 'NOEU_GEOM_R',
                     MAILLAGE  =  MESH_T3,
                     NOM_CHAM  = 'GEOMETRIE',
                    )
  
TEMP_1 = CREA_CHAMP(OPERATION ='AFFE', 
                    TYPE_CHAM ='NOEU_NEUT_F',
                    MAILLAGE  = MESH_T3,
                    AFFE      =_F(TOUT='OUI', NOM_CMP='X1', VALE_F=T_INI),
                   )

TEMP_2 = CREA_CHAMP(OPERATION ='EVAL',
                    TYPE_CHAM ='NOEU_NEUT_R',
                    CHAM_F    = TEMP_1 , 
                    CHAM_PARA = ND_COOR,
                   )

INI_TEMP = CREA_CHAMP(OPERATION ='ASSE', 
                      TYPE_CHAM ='NOEU_TEMP_R',
                      MAILLAGE  = MESH_T3,
                      ASSE      =_F(TOUT='OUI', CHAM_GD=TEMP_2, NOM_CMP='X1', NOM_CMP_RESU='TEMP'),
                     )

# Computation
 
  
EVOL_T3 = THER_NON_LINE( MODELE      = MODEL_T3,
                         INCREMENT   = _F(LIST_INST = TIME),
                         CHAM_MATER  = MAT_T3,
                         EXCIT       = (_F(CHARGE = BND_T3),
                                        _F(CHARGE = HEAT_T3),
                                       ),
                         ETAT_INIT   = _F(CHAM_NO = INI_TEMP),
                         CONVERGENCE = _F(RESI_GLOB_MAXI = 1.E-9),
                         NEWTON      = _F(REAC_ITER = 1),
                        )

# Result checking (middle of the rod)

TEST_RESU(RESU=_F(GROUP_NO='PROBE',
                  INST=1.0,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=EVOL_T3,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=0.25883109573,
                  VALE_REFE=0.25897382948803593,),
          )

#========================================================================
# Verification des mailles de type TRIA3 avec une modelisation AXIS_DIAG
#========================================================================

MAT_T3D = AFFE_MATERIAU(MAILLAGE = MESH_T3,
                        AFFE     = _F(GROUP_MA = 'ROD', MATER = FAKE_MAT),
                      )
  
MODL_T3D = AFFE_MODELE(MAILLAGE = MESH_T3,
                       AFFE     = _F(GROUP_MA='ROD',PHENOMENE='THERMIQUE',MODELISATION='AXIS_DIAG'),
                       )
  
BND_T3D = AFFE_CHAR_THER(MODELE    = MODL_T3D,
                        TEMP_IMPO = _F(GROUP_MA = 'END', TEMP=0),
                       )

# Non linear heating source

HEAT_T3D = AFFE_CHAR_THER_F(MODELE  = MODL_T3D,
                           SOUR_NL = _F(GROUP_MA='ROD', SOUR=SOURCE),    
                           )

# Computation

EVOL_T3D = THER_NON_LINE( MODELE      = MODL_T3D,
                         INCREMENT   = _F(LIST_INST = TIME),
                         CHAM_MATER  = MAT_T3D,
                         EXCIT       = (_F(CHARGE = BND_T3D),
                                        _F(CHARGE = HEAT_T3D),
                                       ),
                         ETAT_INIT   = _F(CHAM_NO = INI_TEMP),
                         CONVERGENCE = _F(RESI_GLOB_MAXI = 1.E-9),
                         NEWTON      = _F(REAC_ITER = 1),
                        )

# Result checking (middle of the rod)

TEST_RESU(RESU=_F(GROUP_NO='PROBE',
                  INST=1.0,
                  PRECISION=0.013,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=EVOL_T3D,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=0.259270309933,
                  VALE_REFE=0.25897382948803593,),
          )

DETRUIRE( CONCEPT = _F(NOM = (ND_COOR,TEMP_1,TEMP_2,INI_TEMP)))

#===================================================================
# Verification des mailles de type TRIA6 avec une modelisation AXIS
#===================================================================

MAT_T6 = AFFE_MATERIAU(MAILLAGE = MESH_T6,
                        AFFE     = _F(GROUP_MA = 'ROD', MATER = FAKE_MAT),
                      )
  
MODEL_T6 = AFFE_MODELE(MAILLAGE = MESH_T6,
                       AFFE     = _F(GROUP_MA='ROD',PHENOMENE='THERMIQUE',MODELISATION='AXIS'),
                       )
  
BND_T6 = AFFE_CHAR_THER(MODELE    = MODEL_T6,
                        TEMP_IMPO = _F(GROUP_MA = 'END', TEMP=0),
                       )

# Non linear heating source

HEAT_T6 = AFFE_CHAR_THER_F(MODELE  = MODEL_T6,
                           SOUR_NL = _F(GROUP_MA='ROD', SOUR=SOURCE),    
                           )

# Initial temperature field

ND_COOR = CREA_CHAMP(OPERATION = 'EXTR',
                     TYPE_CHAM = 'NOEU_GEOM_R',
                     MAILLAGE  =  MESH_T6,
                     NOM_CHAM  = 'GEOMETRIE',
                    )
  
TEMP_1 = CREA_CHAMP(OPERATION ='AFFE', 
                    TYPE_CHAM ='NOEU_NEUT_F',
                    MAILLAGE  = MESH_T6,
                    AFFE      =_F(TOUT='OUI', NOM_CMP='X1', VALE_F=T_INI),
                   )

TEMP_2 = CREA_CHAMP(OPERATION ='EVAL',
                    TYPE_CHAM ='NOEU_NEUT_R',
                    CHAM_F    = TEMP_1 , 
                    CHAM_PARA = ND_COOR,
                   )

INI_TEMP = CREA_CHAMP(OPERATION ='ASSE', 
                      TYPE_CHAM ='NOEU_TEMP_R',
                      MAILLAGE  = MESH_T6,
                      ASSE      =_F(TOUT='OUI', CHAM_GD=TEMP_2, NOM_CMP='X1', NOM_CMP_RESU='TEMP'),
                     )

# Computation
  
EVOL_T6 = THER_NON_LINE( MODELE      = MODEL_T6,
                         INCREMENT   = _F(LIST_INST = TIME),
                         CHAM_MATER  = MAT_T6,
                         EXCIT       = (_F(CHARGE = BND_T6),
                                        _F(CHARGE = HEAT_T6),
                                       ),
                         ETAT_INIT   = _F(CHAM_NO = INI_TEMP),
                         CONVERGENCE = _F(RESI_GLOB_MAXI = 1.E-9),
                         NEWTON      = _F(REAC_ITER = 1),
                        )

# Result checking (middle of the rod)

TEST_RESU(RESU=_F(GROUP_NO='PROBE',
                  INST=1.0,
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=EVOL_T6,
                  NOM_CHAM='TEMP',
                  NOM_CMP='TEMP',
                  VALE_CALC=0.258821437903,
                  VALE_REFE=0.25897382948803593,),
          )

FIN()
