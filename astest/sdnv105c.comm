u_lacher=CALC_ELEM(reuse =u_lacher,
                   MODELE=MO,
                   CHAM_MATER=CHMAT,
                   RESULTAT=u_lacher,
                   TOUT_ORDRE='OUI',
                   TYPE_OPTION='TOUTES',
                   OPTION='SIEF_ELNO_ELGA',);

u_lacher=CALC_NO(reuse =u_lacher,
                 RESULTAT=u_lacher,
                 TOUT_ORDRE='OUI',
                 OPTION='FORC_NODA',
                 MODELE=MO,
                 CHAM_MATER=CHMAT,);

