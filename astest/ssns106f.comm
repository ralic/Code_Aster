MA0=CREA_MAILLAGE(MAILLAGE=MA1,
                  CREA_GROUP_MA=_F(NOM = 'DALLE_G',
                                   PREF_MAILLE = 'G',
                                   GROUP_MA = 'DALLE',),
                 )

DETRUIRE(CONCEPT = _F(NOM = MA1))


MA=CREA_MAILLAGE(MAILLAGE=MA0,
