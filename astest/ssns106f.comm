MA2=CREA_MAILLAGE(MAILLAGE=MA3,
                  CREA_GROUP_MA=_F(NOM = 'ACIER1L',
                                   PREF_MAILLE = 'A1L',
                                   GROUP_MA = 'DALLE',),
                 )

DETRUIRE(CONCEPT = _F(NOM = MA3))

MA1=CREA_MAILLAGE(MAILLAGE=MA2,
                  CREA_GROUP_MA=_F(NOM = 'ACIER1',
                                   PREF_MAILLE = 'A1',
                                   GROUP_MA = 'DALLE',),
