DEBUT(CODE=_F(NOM='SSNS106F',NIV_PUB_WEB='INTERNET',),);

PRE_GIBI();

EP = 0.1
DEP0 = 5.E-4
t1 = 1.0
t2 = 2*t1
dt = 0.03
fx = 5E5

MAIL=LIRE_MAILLAGE();

