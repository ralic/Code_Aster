                                   PREF_MAILLE = 'A1T',
                                   GROUP_MA = 'DALLE',),
                 )

DETRUIRE(CONCEPT = _F(NOM = MA5))

MA3=CREA_MAILLAGE(MAILLAGE=MA4,
                  CREA_GROUP_MA=_F(NOM = 'ACIER2T',
                                   PREF_MAILLE = 'A2T',
                                   GROUP_MA = 'DALLE',),
                 )

