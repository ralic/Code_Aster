#CAS_TEST__: SDLX03A
# SDLX03/A                 COMMANDES                             5/03/91
# ASSEMBLAGE DE PLAQUES RECTANGULAIRES MINCES ENTRETOISEES
# 2 PLAQUES HORIZONTALES: 0.375*0.20*0.005 M
# 2 PLAQUES VERTICALES:   0.375*0.05*0.005 M       REF: SFM.VPCS SDLX03
# MODELISATION DKT
#======================================================================


DEBUT( CODE=_F(  NOM = 'SDLX03A',NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE( )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(  MAILLAGE=MA, 
           AFFE=_F(  TOUT = 'OUI', PHENOMENE = 'MECANIQUE',
                  MODELISATION = 'DKT') )

CARELEM=AFFE_CARA_ELEM(   MODELE=MO, 
                            COQUE=_F(  
       GROUP_MA = 'TOUT',
 EPAIS = 0.005)  )

MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.1E+11,  NU = 0.3,  RHO = 7800.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA, 
                       AFFE=_F(  TOUT = 'OUI',  MATER = MAT) )

# --- STRUCTURE LIBRE-LIBRE ==> PAS DE CHARGE_MECA
#
#--------------------------------------------------------------------
#
MELR=CALC_MATR_ELEM(  MODELE=MO,           CARA_ELEM=CARELEM, 
                        CHAM_MATER=CHMAT,   OPTION='RIGI_MECA' )

MELM=CALC_MATR_ELEM(  MODELE=MO,           CARA_ELEM=CARELEM, 
                        CHAM_MATER=CHMAT,   OPTION='MASS_MECA'     )

#
#--------------------------------------------------------------------
#
NUM=NUME_DDL(  MATR_RIGI=MELR )

MATASSR=ASSE_MATRICE(MATR_ELEM=MELR,NUME_DDL=NUM)

MATASSM=ASSE_MATRICE(MATR_ELEM=MELM,NUME_DDL=NUM)

#====================================================================
#
#               CALCUL MODAL

MODES=MODE_ITER_SIMULT(  MATR_A=MATASSR,    MATR_B=MATASSM,
                           CALC_FREQ=_F(  OPTION = 'BANDE',
                                       FREQ = ( 1.,   1200., ),
                                       )  )

# --------------------------------------------------------------------


TEST_RESU(RESU=(
   _F(   RESULTAT = MODES,       PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.1E-2,
           NUME_ORDRE = 1,       VALE = 584.),
   _F(   RESULTAT = MODES,       PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 0.5E-2,
           NUME_ORDRE = 2,       VALE = 826.),
   _F(   RESULTAT = MODES,       PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.E-2,
           NUME_ORDRE = 3,       VALE = 855.),
   _F(   RESULTAT = MODES,       PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 0.3E-2,
           NUME_ORDRE = 4,       VALE = 911.),
   _F(   RESULTAT = MODES,       PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.5E-2,
           NUME_ORDRE = 5,       VALE = 1113.),
   _F(   RESULTAT = MODES,       PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 2.5E-2,
           NUME_ORDRE = 6,       VALE = 1136.)) )

FIN()
