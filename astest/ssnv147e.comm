DEBUT(CODE=_F(NOM='SSNV147E',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI()

MA=LIRE_MAILLAGE()

MA=DEFI_GROUP( reuse=MA,
               MAILLAGE=MA,
               CREA_GROUP_NO=_F(
                TOUT_GROUP_MA = 'OUI')
              )

MO=AFFE_MODELE(
                MAILLAGE=MA,
                AFFE=_F(
               GROUP_MA = ('BLOC', 'FIXE', 'TRAC',),
               PHENOMENE = 'MECANIQUE',
               MODELISATION = '3D')
             )

CALCAIRE=DEFI_MATERIAU(
                    ELAS=_F(
                E = 20000.,
                NU = 0.),
               BETON_ECRO_LINE=_F(
                SYT = 6.,
                D_SIGM_EPSI = -10000.)
               )

CAL_FAIB=DEFI_MATERIAU(
                    ELAS=_F(
                E = 20000.,
                NU = 0.),
               BETON_ECRO_LINE=_F(
                SYT = 5.,
                D_SIGM_EPSI = -10000.)
               )

CHMAT=AFFE_MATERIAU(
                MAILLAGE=MA,AFFE=(
                    _F(
                GROUP_MA = 'BLOC',
                MATER = CALCAIRE), _F(
                MAILLE = 'M1',
                MATER = CAL_FAIB))
              )

L_INST=DEFI_LIST_REEL(
               DEBUT=0.0,INTERVALLE=(

                _F(  JUSQU_A = 1.,       NOMBRE = 1),
                _F(  JUSQU_A = 100.,     NOMBRE = 2000))
              )

LIAISON=AFFE_CHAR_MECA(
               MODELE=MO,

                FACE_IMPO=_F( GROUP_MA = 'FIXE',  DX = 0., DY = 0., DZ = 0.)
              )

TRACTION=AFFE_CHAR_MECA(
                      MODELE=MO,
               FORCE_FACE=_F( GROUP_MA = 'TRAC', FZ = 1.)
              )

RAMPE=DEFI_FONCTION(
              NOM_PARA='INST',
                  VALE=(0., 0.,
                        1., 1.,)
             )

REPONSE=STAT_NON_LINE(
                     MODELE=MO,
                 CHAM_MATER=CHMAT,EXCIT=(

                _F( CHARGE = LIAISON),
                _F( CHARGE = TRACTION, FONC_MULT = RAMPE)),
                  COMP_INCR=_F(
                RELATION = 'ENDO_ISOT_BETON'),

                INCREMENT=_F(
                LIST_INST = L_INST,
                INST_FIN = 1.),
                     NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 50)
             )

REPONSE=STAT_NON_LINE( reuse=REPONSE,
                  ETAT_INIT=_F(
                EVOL_NOLI = REPONSE),
                     MODELE=MO,
                 CHAM_MATER=CHMAT,EXCIT=(

                _F( CHARGE = LIAISON),
                _F( CHARGE = TRACTION, TYPE_CHARGE = 'FIXE_PILO')),
                  COMP_INCR=_F(
                RELATION = 'ENDO_ISOT_BETON'),

                INCREMENT=_F(
                LIST_INST = L_INST,
                INST_FIN = 1.594,
                SUBD_METHODE='UNIFORME', SUBD_PAS = 4,
                SUBD_PAS_MINI = 1.E-6),
               PILOTAGE=_F(
                TYPE = 'PRED_ELAS',
                ETA_PILO_MIN = 0.50,
                ETA_PILO_R_MIN = 0.,
                ETA_PILO_MAX = 10.),
                     NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 10)
             )

REPONSE=STAT_NON_LINE( reuse=REPONSE,
                  ETAT_INIT=_F(
                EVOL_NOLI = REPONSE),
                     MODELE=MO,
                 CHAM_MATER=CHMAT,EXCIT=(

                _F( CHARGE = LIAISON),
                _F( CHARGE = TRACTION, TYPE_CHARGE = 'FIXE_PILO')),
                  COMP_INCR=_F(
                RELATION = 'ENDO_ISOT_BETON'),

                INCREMENT=_F(
                LIST_INST = L_INST,
                SUBD_METHODE='UNIFORME', SUBD_PAS = 4,
                SUBD_PAS_MINI = 1.E-6),
               PILOTAGE=_F(
                TYPE = 'PRED_ELAS',
                ETA_PILO_R_MIN = 0.,
                ETA_PILO_MIN = 0.50,
                ETA_PILO_MAX = 10.),
                     NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 10)
             )

TABLE = RECU_TABLE(CO=REPONSE, NOM_PARA='ETA_PILOTAGE')

TEST_RESU(RESU=(  _F(
               RESULTAT = REPONSE,
               NUME_ORDRE = 41,
               GROUP_NO = 'E',
               REFERENCE = 'NON_REGRESSION',
               NOM_CHAM = 'DEPL',
               NOM_CMP = 'DZ',
               VALE = 1.91008E-04), _F(
               RESULTAT = REPONSE,
               NUME_ORDRE = 41,
               MAILLE = 'M1',
               POINT = 1,
               REFERENCE = 'NON_REGRESSION',
               NOM_CHAM = 'SIEF_ELGA',
               NOM_CMP = 'SIZZ',
               VALE = 3.31452), _F(
               RESULTAT = REPONSE,
               NUME_ORDRE = 41,
               MAILLE = 'M1',
               POINT = 1,
               REFERENCE = 'NON_REGRESSION',
               NOM_CHAM = 'VARI_ELGA',
               NOM_CMP = 'V1',
               VALE = 3.37097E-01))
             )

FIN()
