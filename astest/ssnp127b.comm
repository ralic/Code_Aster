               NOM_CHAM = 'VARI_ELGA',
               NOM_CMP = 'V1',
               VALE = 1.70285E-03),
               _F(
               RESULTAT = EVOL1,
               REFERENCE = 'NON_REGRESSION',
               NUME_ORDRE = 101,
               MAILLE = 'M31',
               POINT = 2,
               NOM_CHAM = 'VARI_ELGA',
               NOM_CMP = 'V2',
