         (43.75, 46.25) , (43.75, 46.25) ,(43.75, 46.25) , (43.75, 46.25) ,(43.75, 46.25) ]
for i in range(NBALP):
     TABD[i] = POST_FATIGUE( CHARGEMENT = 'MULTIAXIAL',
                              TYPE_CHARGE = 'NON_PERIODIQUE',
                              HISTOIRE = _F(  SIGM_XX = SX[i],
                                             SIGM_YY = SY[i],
                                             SIGM_ZZ = ZERO,
