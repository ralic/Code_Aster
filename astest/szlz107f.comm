                           )    

   TABMC[i] = CALC_TABLE(    reuse = TABMC[i],
                         TABLE = TABMC[i],
                         ACTION = ( _F( OPERATION ='OPER',
                                            FORMULE = PHIZ,
                                            NOM_PARA = 'PHIZ',),)
