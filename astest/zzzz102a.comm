                 SPEC_OSCI=_F(  FONCTION = LBEWA,
                            NATURE = 'ACCE', AMOR_REDUIT = ( 0.01, ),
                            NORME = UNSURG,
                             LIST_FREQ = LFREQ1,)  )

MSLBEWA=INFO_FONCTION(    MAX=_F(  FONCTION = SLBEWA)  )

TEST_TABLE(   TABLE=MSLBEWA,
              FILTRE=( _F(  NOM_PARA = 'FONCTION',
                            VALE_K = 'SLBEWA'),
                       _F(  NOM_PARA = 'AMOR',
                            VALE = 1.00000E-02),
                       _F(  NOM_PARA = 'FREQ',
                            VALE = 3.70000E+00),
                       _F(  NOM_PARA = 'TYPE',
                            VALE_K = 'MAXI')),
              NOM_PARA='ACCE',
              VALE=3.56676E+01               )

VAL=6.38000E-01 * G
TEST_FONCTION(
         VALEUR=_F(  FONCTION = SLBEWA,        NOM_PARA = ('AMOR', 'FREQ',),
                  VALE_PARA = ( 0.01, FMAX, ),
                  VALE_REFE = VAL, PRECISION = 1.E-3),ATTRIBUT=(
       _F(  FONCTION = SLBEWA,         ATTR = 'NOM_PARA',
