                            NATURE = 'DEPL', AMOR_REDUIT = ( 0.01, ),
                            NORME = 1.,
                            LIST_FREQ = LFREQ1,)  )

OMEGA2=4. * pi * pi * FMAX * FMAX
VAL= 6.38000E-01 / OMEGA2

TEST_FONCTION(
         VALEUR=_F(  FONCTION = SLBEWD,        NOM_PARA = ('AMOR', 'FREQ',),
