             NOM_PARA='DX',
             VALE=-6.38000E-01              )

G=9.81
UNSURG=1/G
FMAX = 100.

LFREQ1 = DEFI_LIST_REEL(DEBUT=0.1,INTERVALLE=(_F(JUSQU_A=FMAX,PAS=0.05,),),)


