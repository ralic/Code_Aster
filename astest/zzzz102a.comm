       _F(  FONCTION = SLBEWA,         ATTR = 'NOM_RESU',
                  PARA = 0.01,                ATTR_REFE = 'ACCE'),
       _F(  FONCTION = SLBEWA,         ATTR = 'PROL_DROITE',
                  PARA = 0.01,                ATTR_REFE = 'EXCLU'),
