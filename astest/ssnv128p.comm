#       CAS_TEST SSNV128: 2D, ELEMENTS Q8, METHODE CONTINUE

DEBUT( CODE=_F(  NOM = 'SSNV128P',NIV_PUB_WEB='INTERNET') )

#......................................................
#  CALCUL TEST ELEMENTS FINIS DE CONTACT 2D QUAD8
#  PLAQUE AVEC CONTACT ET FROTTEMENT SUR UN PLAN RIGIDE
#......................................................

PRE_GIBI()

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),)

MA=DEFI_GROUP(reuse =MA,
                    MAILLAGE=MA,
                    CREA_GROUP_NO=(_F(GROUP_MA='LCONTA',
                                      OPTION='NOEUD_ORDO',
                                      NOM='RELEVE',
                                      GROUP_NO_ORIG='PPA',
                                      GROUP_NO_EXTR='PPS',),),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'D_PLAN',)
              )

MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 MODELE=MO,
                 ORIE_PEAU_2D=(_F(GROUP_MA = 'LPRESV',),
                               _F(GROUP_MA = 'LPRESH',))
                )

MATPLA=DEFI_MATERIAU(ELAS=_F(E = 1.3E11,
                             NU = 0.2,)
                    )

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT = 'OUI',
                             MATER = MATPLA,)
                    )

CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA = 'LBATI',  DX = 0.0, DY = 0.0,),
                              _F(GROUP_NO = 'PPS',    DX = 0.0, DY = 0.0,),
                              _F(GROUP_MA = 'LBLOCX', DX = 0.0,)),
                    PRES_REP=(_F(GROUP_MA = 'LPRESV', PRES = 5.E07,),
                              _F(GROUP_MA = 'LPRESH', PRES = 15.E07,))
                   )

CHA2=AFFE_CHAR_MECA(MODELE=MO,
                    CONTACT=_F(GROUP_MA_MAIT     = 'LBATI',
                               GROUP_MA_ESCL     = 'LCONTAR',
                               APPARIEMENT    = 'MAIT_ESCL',
                               RECHERCHE      = 'NOEUD_VOISIN',
                               NORMALE        = 'MAIT',
                               METHODE        = 'CONTINUE',
                               INTEGRATION    = 'NOEUD',
                               MODL_AXIS      = 'NON',
                               CONTACT_INIT   = 'OUI',
                               SEUIL_INIT     = 100.,
                               COEF_REGU_FROT = 100.0,
                               COEF_REGU_CONT = 100.0,
                               ITER_GEOM_MAXI = 3,
                               ITER_FROT_MAXI = 10,
                               ITER_CONT_MAXI = 1,
                               FROTTEMENT     = 'COULOMB',
                               COULOMB        = 1.0,
                               PROJECTION     = 'QUADRATIQUE',)
                    )

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0,)
                    )

L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 5,)
                      )

#-----------------------------------------------------------

U2M=STAT_NON_LINE(MODELE    = MO,
                  CHAM_MATER = CHMAT,
                  EXCIT      =(_F(CHARGE = CHA1,
                                  FONC_MULT = RAMPE,),
                               _F(CHARGE = CHA2,)),
                  COMP_ELAS  = _F(RELATION = 'ELAS',),
                  INCREMENT  = _F(LIST_INST = L_INST,),
                  CONVERGENCE= _F(ARRET = 'OUI',
                                 ITER_GLOB_MAXI = 200,
                                 RESI_GLOB_RELA = 1.0E-5,),
#                  INFO=2,
               )

U2M=CALC_ELEM(reuse=U2M,
              OPTION=('SIEF_ELNO_ELGA', 'VARI_ELNO_ELGA',),
              RESULTAT=U2M,
             )

DEPL2M=CREA_CHAMP(OPERATION='EXTR',
                  TYPE_CHAM='NOEU_DEPL_R',
                  NOM_CHAM='DEPL',
                  RESULTAT=U2M,
                  INST=1.0
                )

DEPLAC=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLAC',
                              GROUP_NO='RELEVE',
                              RESULTAT=U2M,
                              NOM_CHAM='DEPL',
                              INST=1.0,
                              TOUT_CMP='OUI',
                              OPERATION='EXTRACTION',),);
                              
TCONT=POST_RELEVE_T(ACTION=_F(INTITULE='CONT',
                              GROUP_NO='RELEVE',
                              RESULTAT=U2M,
                              NOM_CHAM='VALE_CONT',
                              INST=1.0,
                              TOUT_CMP='OUI',
                              OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=DEPLAC,);
IMPR_TABLE(TABLE=TCONT,);

TEST_RESU(RESU=(_F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPA',
                   NOM_CMP    = 'DX',
                   VALE       = 2.86E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPB',
                   NOM_CMP    = 'DX',
                   VALE       = 2.72E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPC',
                   NOM_CMP    = 'DX',
                   VALE       = 2.28E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPD',
                   NOM_CMP    = 'DX',
                   VALE       = 1.98E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                   ),
                _F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPE',
                   NOM_CMP    = 'DX',
                   VALE       = 1.50E-5,
                   PRECISION  = 5.E-2,
                   REFERENCE  = 'AUTRE_ASTER',
                   VERSION    = '7.1.11',
                ))
       )

FIN()
