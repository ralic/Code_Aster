                        );

    DETRUIRE(INFO=1,CONCEPT=_F(NOM=EVOLT));

    EVOLT = REST_SPEC_TEMP( RESULTAT = EVOLH ,
                          METHODE = 'PROL_ZERO' ,
                          SYMETRIE = 'NON' ,
