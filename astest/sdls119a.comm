                       RESULTAT=EVOLT,
                       NOM_CHAM='DEPL',
                       NOM_CMP='DY',
                       VALE_CALC= 5.67540758E-03,
                       VALE_REFE=5.6729900000000002E-3,
                       CRITERE=('RELATIF', 'RELATIF'),
                       PRECISION=1.E-3, TOLE_MACHINE=(1.E-3, 1.E-3),),
                    ),
              )

  else:

    resunl = CALC_FORC_NONL( RESULTAT = EVOL , TOUT_ORDRE = 'OUI'    ,
