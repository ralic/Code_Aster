DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

# COQUE HELICOIDALE (POUTRE VRILLEE) SOUS CHARGES CONCENTREES
#
# MODELISATION MEC3TR7H: 125 NOEUDS --  48 MAILLES TRIA7
#=======================================================================
#


MATERI_1=DEFI_MATERIAU( ELAS=_F(  E = 29.0E+6,      NU = 0.2200) )

MAILLA_1=LIRE_MAILLAGE(FORMAT='MED',   )

MAILLA_1=DEFI_GROUP( reuse=MAILLA_1,   MAILLAGE=MAILLA_1,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

CH_MAT_1=AFFE_MATERIAU(  MAILLAGE=MAILLA_1,
                            AFFE=_F(  TOUT = 'OUI',   MATER = MATERI_1) )

MODELE_1=AFFE_MODELE(  MAILLAGE=MAILLA_1, 
                     AFFE=_F( TOUT = 'OUI', MODELISATION = 'COQUE_3D',
                                PHENOMENE = 'MECANIQUE') )

CINE=AFFE_CHAR_MECA(MODELE=MODELE_1,
                  DDL_IMPO=_F( GROUP_NO = 'OBC',      DX = 0.,  DY = 0.,  DZ = 0.,
                                              DRX = 0., DRY = 0., DRZ = 0.))

CHFZ=AFFE_CHAR_MECA(MODELE=MODELE_1,
                  FORCE_NODALE=_F( GROUP_NO = 'A',  FZ = 1.000000000000000) )

CHFY=AFFE_CHAR_MECA(MODELE=MODELE_1,
                  FORCE_NODALE=_F( GROUP_NO = 'A',  FY = 1.000000000000000) )



CARAC__1=AFFE_CARA_ELEM(   MODELE=MODELE_1,
                            COQUE=_F(
                                   GROUP_MA = 'TOUT',
                                   EPAIS = 0.320,
                                   A_CIS = 0.83333333333333) )

MATELE_1=CALC_MATR_ELEM(   MODELE=MODELE_1,     CHARGE=CINE,
                               CARA_ELEM=CARAC__1,
                               CHAM_MATER=CH_MAT_1,
                               OPTION='RIGI_MECA' )

VEFZ=CALC_VECT_ELEM(   CHARGE=(CINE, CHFZ,),   CARA_ELEM=CARAC__1,
                          CHAM_MATER=CH_MAT_1,  OPTION='CHAR_MECA'  )

VEFY=CALC_VECT_ELEM(   CHARGE=(CINE, CHFY,),   CARA_ELEM=CARAC__1,
                          CHAM_MATER=CH_MAT_1,  OPTION='CHAR_MECA'  )

NU=NUME_DDL(   MODELE = MODELE_1,
               CHARGE =  CINE,
               INFO = 1 )

NU1=NUME_DDL(   MODELE  = MODELE_1,
                CHARGE  =  CINE,
                METHODE = 'MULT_FRONT',
                RENUM   = 'MDA',
                INFO    = 1 )

NU2=NUME_DDL(   MODELE  = MODELE_1,
                METHODE = 'MULT_FRONT',
                RENUM   = 'MDA',
                INFO    = 1 )

MATASS=ASSE_MATRICE(   MATR_ELEM=MATELE_1,   NUME_DDL=NU )

VEFZAS=ASSE_VECTEUR(   VECT_ELEM=VEFZ,       NUME_DDL=NU )

VEFYAS=ASSE_VECTEUR(   VECT_ELEM=VEFY,       NUME_DDL=NU )

MATASS=FACTORISER( reuse=MATASS,  MATR_ASSE=MATASS )

DEFZ=RESOUDRE( MATR=MATASS,     CHAM_NO=VEFZAS)

DEFY=RESOUDRE( MATR=MATASS,     CHAM_NO=VEFYAS)

TEST_RESU(CHAM_NO=(_F(CRITERE='RELATIF',
                      NOM_CMP='DY',
                      GROUP_NO='A',
                      PRECISION=1.E-2,
                      CHAM_GD=DEFZ,
                      VALE_CALC=-1.7145780162891E-03,
                      VALE_REFE=-1.72E-3,
                      REFERENCE='NON_DEFINI',),
                   _F(CRITERE='RELATIF',
                      NOM_CMP='DZ',
                      GROUP_NO='A',
                      PRECISION=1.E-2,
                      CHAM_GD=DEFZ,
                      VALE_CALC= 5.3996973318971E-03,
                      VALE_REFE=5.4200000000000003E-3,
                      REFERENCE='NON_DEFINI',),
                   _F(CRITERE='RELATIF',
                      NOM_CMP='DY',
                      GROUP_NO='A',
                      PRECISION=1.E-2,
                      CHAM_GD=DEFY,
                      VALE_CALC= 1.7462838301326E-03,
                      VALE_REFE=1.75E-3,
                      REFERENCE='NON_DEFINI',),
                   _F(CRITERE='RELATIF',
                      NOM_CMP='DZ',
                      GROUP_NO='A',
                      PRECISION=1.E-2,
                      CHAM_GD=DEFY,
                      VALE_CALC=-1.7145780162891E-03,
                      VALE_REFE=-1.72E-3,
                      REFERENCE='NON_DEFINI',),
                   ),
          )

FIN()
