DEBUT(CODE=_F(NOM='SSLS119F',NIV_PUB_WEB='INTERNET'),);

PRE_GIBI();

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),);

MAIL=CREA_MAILLAGE(MAILLAGE=MA,
                   MODI_MAILLE=_F(OPTION='TRIA6_7',
                                  TOUT='OUI',),);

MO=AFFE_MODELE(AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='COQUE_3D',),
               MAILLAGE=MAIL,);

CAR_ELE=AFFE_CARA_ELEM(MODELE=MO,
                       COQUE=_F(GROUP_MA='TOUT_ELT',
                                EPAIS=0.0508,),);

MAT=DEFI_MATERIAU(ELAS=_F(NU=0.35,
                          E=22752510.,),);

CHAM_MAT=AFFE_MATERIAU(AFFE=_F(MATER=MAT,
                               TOUT='OUI',),
                       MAILLAGE=MAIL,);

CHA=AFFE_CHAR_MECA(DDL_IMPO=_F(GROUP_MA='ENCASTR',
                               DZ=0.0,
                               DX=0.0,
                               DY=0.0,
                               DRZ=0.0,
                               DRX=0.0,
                               DRY=0.0,),
                   MODELE=MO,
                   FORCE_ARETE=_F(GROUP_MA='LIBRE',
                                  FZ=8.7594,),);

RESU=MECA_STATIQUE(CHAM_MATER=CHAM_MAT,
                   MODELE=MO,
                   CARA_ELEM=CAR_ELE,
                   EXCIT=_F(CHARGE=CHA,),);

RESU=CALC_ELEM(reuse =RESU,
               NIVE_COUCHE='MOY',
               RESULTAT=RESU,
               NUME_COUCHE=1,
               CHAM_MATER=CHAM_MAT,
               MODELE=MO,
               CARA_ELEM=CAR_ELE,
               OPTION='SIGM_ELNO_DEPL',
               TOUT_ORDRE='OUI',);

DEP_Z=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                              INTITULE='TAB_NORM',
                              NOM_CMP='DZ',
                              RESULTAT=RESU,
                              NOM_CHAM='DEPL',
                              GROUP_NO='C',),);

SIGMA=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                              INTITULE='TAB_SIGM ',
                              TOUT_CMP='OUI',
                              GROUP_NO='C',
                              RESULTAT=RESU,
                              NOM_CHAM='SIGM_ELNO_DEPL',),);

IMPR_RESU(RESU=_F(MAILLAGE=MAIL,
                  FORMAT='CASTEM',
                  RESULTAT=RESU,
                  NOM_CHAM=('DEPL','SIGM_ELNO_DEPL'),),
          MODELE=MO,);

IMPR_RESU(RESU=_F(FORMAT='RESULTAT',
                  RESULTAT=RESU,
                  FICHIER='RESULTAT',
                  NOM_CHAM='DEPL',
                  GROUP_NO='C',
                  NOM_CMP='DZ',),);

TEST_RESU(RESU=(
     _F( RESULTAT = RESU,         NOM_CHAM = 'DEPL',
           NUME_ORDRE = 1,             GROUP_NO = 'C',
           NOM_CMP = 'DZ',          VALE = 1.31536E-01,
           REFERENCE='NON_REGRESSION',
           PRECISION = 0.00001,          CRITERE = 'RELATIF'),),)


FIN();
