                  LISSAGE = _F(LISSAGE_THETA='LAGRANGE',
                               LISSAGE_G='LAGRANGE',),
                  THETA=_F(R_INF=RI[i],
                           R_SUP=RS[i],
                  FISSURE=FISS,),);

   IMPR_TABLE(TABLE=SIF3[i],);

# test sans R_INF et R_SUP
SIF1_S=CALC_G(RESULTAT=UTOT1x,
              OPTION='CALC_K_G',
              THETA=_F(FISSURE=FISS,),
              LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
