RS=[4. , 1.666 , 2. , 3. , 4. , 3.9]

nbc=len(RI)
SIF1   = [None]*nbc
SIF1_R = [None]*nbc
SIF2   = [None]*nbc
SIF3   = [None]*nbc

for i in range(0,nbc) :

   SIF1[i]=CALC_G(RESULTAT=UTOT1x,
                 OPTION='CALC_K_G',
                 THETA=_F(FISSURE=FISS,
                          R_INF=RI[i],
                          R_SUP=RS[i],),
                 LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                            LISSAGE_G='LAGRANGE',),
                 INFO=2,);

