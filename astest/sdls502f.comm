#---------------------------------------------------------------------
#  - CALCUL EN ANALYSE MODALE DYNAMIQUE D'UNE PLAQUE CARREE EPAISSE
#    SIMPLEMENT SUPPORTEE
#  - MODELISATION AVEC DES ELEMENTS COQUE_3D (QUAD9)
#---------------------------------------------------------------------
#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PLAQ_M=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),FORMAT='MED', );

PLAQ_MA=CREA_MAILLAGE(MAILLAGE=PLAQ_M,
                      MODI_MAILLE=_F(OPTION='QUAD8_9',
                                     TOUT='OUI',),);
#
MODEL=AFFE_MODELE(   MAILLAGE=PLAQ_MA,
                           AFFE=_F( TOUT = 'OUI',
                                 PHENOMENE = 'MECANIQUE',
                           MODELISATION = 'COQUE_3D',),);
PLAQ_MA=DEFI_GROUP(reuse =PLAQ_MA,
                MAILLAGE=PLAQ_MA,
                CREA_GROUP_NO=_F(GROUP_MA='BORDS',NOM='BORDS',),);
#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES ELEMENTAIRES
#---------------------------------------------------------------------
#
CARA_ELE=AFFE_CARA_ELEM(   MODELE=MODEL,
                              COQUE=_F( GROUP_MA = 'TOUT_ELT',
                                     EPAIS = 1.,
                                     ANGL_REP = (15.5,0.,),),
                           );

#
#---------------------------------------------------------------------
#                     CARACTERISTIQUES MATERIAUX
#---------------------------------------------------------------------
#
ACIER=DEFI_MATERIAU(   ELAS=_F( E = 2.E+11,
                               NU = 0.3,
                               RHO = 8000.),);

#
MAT=AFFE_MATERIAU(   MAILLAGE=PLAQ_MA,
                           AFFE=_F( TOUT = 'OUI', MATER = ACIER)
                                                   );

#
#---------------------------------------------------------------------
#                     CHARGEMENTS
#---------------------------------------------------------------------
#
CHAR0=AFFE_CHAR_MECA(     MODELE=MODEL,
                          DDL_IMPO=_F( GROUP_NO = 'BORDS', DZ = 0.)
                        );

#
#---------------------------------------------------------------------
#                     RESOLUTION
#---------------------------------------------------------------------
#
M_EL_RIG=CALC_MATR_ELEM(       MODELE=MODEL,
                             CHAM_MATER=MAT,
                              CARA_ELEM=CARA_ELE,
                                 CHARGE=CHAR0,
                                 OPTION='RIGI_MECA'
                           );

#
M_EL_MAS=CALC_MATR_ELEM(       MODELE=MODEL,
                             CHAM_MATER=MAT,
                              CARA_ELEM=CARA_ELE,
                                 CHARGE=CHAR0,
                                 OPTION='MASS_MECA'
                           );

#
NU=NUME_DDL(   MATR_RIGI=M_EL_RIG)

#
M_AS_RIG=ASSE_MATRICE(   MATR_ELEM=M_EL_RIG,   NUME_DDL=NU)

#
M_AS_MAS=ASSE_MATRICE(   MATR_ELEM=M_EL_MAS,   NUME_DDL=NU)

#
MODES=MODE_ITER_SIMULT(      MATR_RIGI=M_AS_RIG,
                               MATR_MASS=M_AS_MAS,
                              METHODE='SORENSEN',
                            CALC_FREQ=_F( OPTION = 'BANDE',
                                       FREQ = (10., 350.,))
                          );


#
#---------------------------------------------------------------------
#                     VERIFICATION DES RESULTATS
#---------------------------------------------------------------------
#
TEST_RESU(RESU=(_F(PARA='FREQ',
                   NUME_MODE=4,
                   RESULTAT=MODES,
                   VALE_CALC=43.870046541,
                   VALE_REFE=44.762,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.02,),
                _F(PARA='FREQ',
                   NUME_MODE=5,
                   RESULTAT=MODES,
                   VALE_CALC=106.041297571,
                   VALE_REFE=110.52,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.041000000000000002,),
                _F(PARA='FREQ',
                   NUME_MODE=6,
                   RESULTAT=MODES,
                   VALE_CALC=106.041297571,
                   VALE_REFE=110.52,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.041000000000000002,),
                _F(PARA='FREQ',
                   NUME_MODE=7,
                   RESULTAT=MODES,
                   VALE_CALC=160.055993390,
                   VALE_REFE=169.08000000000001,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.055,),
                _F(PARA='FREQ',
                   NUME_MODE=8,
                   RESULTAT=MODES,
                   VALE_CALC=193.588787715,
                   VALE_REFE=193.93000000000001,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.02,),
                _F(PARA='FREQ',
                   NUME_MODE=11,
                   RESULTAT=MODES,
                   VALE_CALC=206.192667642,
                   VALE_REFE=206.63999999999999,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.029999999999999999,),
                _F(PARA='FREQ',
                   NUME_MODE=12,
                   RESULTAT=MODES,
                   VALE_CALC=206.192667642,
                   VALE_REFE=206.63999999999999,
                   REFERENCE='NON_DEFINI',
                   CRITERE='RELATIF',
                   PRECISION=0.029999999999999999,),
                ),
          )

