DEBUT(CODE=_F( NOM = 'HSNV121D',NIV_PUB_WEB='INTERNET'))

MA=LIRE_MAILLAGE()

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
   CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(
               MAILLAGE=MA,
                  VERIF='MAILLE',
               AFFE=_F(
               TOUT = 'OUI',
               PHENOMENE = 'MECANIQUE',
               MODELISATION = 'COQUE_3D')
             )

CARAC=AFFE_CARA_ELEM(   MODELE=MO,
                           COQUE=_F(   GROUP_MA = 'TOUT',  EPAIS = 1000.,COQUE_NCOU=9,
                                       A_CIS = 0.83333333333333)
                          )

COU_T20=DEFI_FONCTION(
               NOM_PARA='EPSI',
               PROL_DROITE='LINEAIRE',
               VALE=(
               0.010,    1000.,
               1.010,    3500.,
               )
             )

COU_T120=DEFI_FONCTION(
               NOM_PARA='EPSI',
               PROL_DROITE='LINEAIRE',
               VALE=(
               0.005,    1000.,
               1.005,    3000.,
               )
             )

COU_TRAC=DEFI_NAPPE(

              NOM_PARA='TEMP',
                  PARA=(20.,120.,),
              FONCTION=(COU_T20,COU_T120,),
              PROL_GAUCHE='CONSTANT',
               PROL_DROITE='CONSTANT'
             )

ACIER_T=DEFI_MATERIAU(
                   ELAS=_F(
               E = 200000.,
               NU = 0.3,
               ALPHA = 1.E-4),
              TRACTION=_F(  SIGM = COU_TRAC)
             )

CHP_MATT=AFFE_MATERIAU(
               MAILLAGE=MA,
                   AFFE=_F(
               TOUT = 'OUI',
               MATER = ACIER_T,
               TEMP_REF = 20.)
             )

L_INST=DEFI_LIST_REEL(
              DEBUT=0.0,INTERVALLE=(

               _F(  JUSQU_A = 1.00, NOMBRE = 1),
               _F(  JUSQU_A = 2.00, NOMBRE = 20))
             )

F_CHAR=DEFI_FONCTION(
                NOM_PARA='INST',
              PROL_DROITE='CONSTANT',
                    VALE=(
               0.,   0.,
               1.,   0.,
               2.,   293.3,
               )
             )

F_TEMP=DEFI_FONCTION(
                NOM_PARA='INST',
                NOM_RESU='TEMP',
              PROL_DROITE='CONSTANT',
                    VALE=(
               0.,    20.,
               1.,   120.,
               2.,   120.,
               )
             )

CHP_TEMP=CREA_CHAMP(OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_F',
               MAILLAGE=MA,AFFE=(

                   _F(
               TOUT = 'OUI',
               NOM_CMP = 'TEMP_INF',
               VALE_F = F_TEMP),
                   _F(
               TOUT = 'OUI',
               NOM_CMP = 'TEMP',
               VALE_F = F_TEMP),
                   _F(
               TOUT = 'OUI',
               NOM_CMP = 'TEMP_SUP',
               VALE_F = F_TEMP))
             )

TEMP=CREA_RESU(
               OPERATION='AFFE', TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                 AFFE=_F(
               LIST_INST = L_INST,
               CHAM_GD = CHP_TEMP)
             )

CHR_TEMP=AFFE_CHAR_MECA(
                      MODELE=MO,
               TEMP_CALCULEE=TEMP
             )

CHR_LIAI=AFFE_CHAR_MECA(
              MODELE=MO,DDL_IMPO=(

               _F( NOEUD = 'NO2',  DX = 0.,  DY = 0.,  DZ = 0.,
                             DRX = 0., DRY = 0.,  DRZ = 0.),
               _F( NOEUD = 'NO1',  DX = 0.,          DZ = 0.),
               _F( NOEUD = 'NO5',  DX = 0.,          DZ = 0.))
             )

CHR_TRAC=AFFE_CHAR_MECA(
                     MODELE=MO,
                  FACE_IMPO=_F( MAILLE = 'MA2',  DX = 1.)
             )

VT=STAT_NON_LINE(
                            MODELE=MO,
                         CARA_ELEM=CARAC,
                        CHAM_MATER=CHP_MATT,EXCIT=(
                             _F(
                CHARGE = CHR_LIAI), _F(
                CHARGE = CHR_TEMP)),
                         COMP_INCR=_F(
                RELATION = 'VMIS_ISOT_TRAC',
                DEFORMATION = 'PETIT_REAC'),
                         INCREMENT=_F(
                LIST_INST = L_INST,
                NUME_INST_FIN = 1),
                            NEWTON=_F(
                MATRICE = 'ELASTIQUE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_MAXI = 1.E-1,
                ITER_GLOB_MAXI = 50),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 3)
              )

VT=STAT_NON_LINE( reuse=VT,
                         ETAT_INIT=_F(
                EVOL_NOLI = VT,
                NUME_ORDRE = 1),
                            MODELE=MO,
                        CHAM_MATER=CHP_MATT,
                         CARA_ELEM=CARAC,EXCIT=(
                             _F(
                CHARGE = CHR_LIAI), _F(
                CHARGE = CHR_TEMP), _F(
                CHARGE = CHR_TRAC,
                FONC_MULT = F_CHAR,
                TYPE_CHARGE = 'DIDI')),
                         COMP_INCR=_F(
                RELATION = 'VMIS_ISOT_TRAC',
                DEFORMATION = 'PETIT_REAC'),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                MATRICE = 'ELASTIQUE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-06,
                ITER_GLOB_MAXI = 50)
              )

TEST_RESU(RESU=(
              _F(
               RESULTAT = VT,  NOM_CHAM = 'DEPL',  INST = 2.,
               NOEUD = 'NO4',   NOM_CMP = 'DX',
               PRECISION = ( 1.E-2, 1.E-4, ),
               VALE = 303.),
              _F(
               RESULTAT = VT,  NOM_CHAM = 'DEPL',  INST = 2.,
               NOEUD = 'NO4',   NOM_CMP = 'DY',
               PRECISION = ( 1.5E-2, 1.E-4, ),
               VALE = -110.),
              _F(
               RESULTAT = VT,   NOM_CHAM = 'SIEF_ELGA',  INST = 2.,
               MAILLE = 'MA1',  POINT = 1,               NOM_CMP = 'SIXX',
               PRECISION = ( 3.1E-2, 1.E-4, ),
               VALE = 1453.),
              _F(
               RESULTAT = VT,     NOM_CHAM = 'VARI_ELGA',  INST = 2.,
               MAILLE = 'MA1',    POINT = 1,   NOM_CMP = 'V1',
               PRECISION = ( 1.5E-2, 1.E-4, ),
               VALE = 0.2475))
             )

FIN()
