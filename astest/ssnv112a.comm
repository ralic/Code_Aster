# ----------------------------------------------------------------------
#
# CYLINDRE CREUX EN INCOMPRESSIBLE GRANDES DEFORMATIONS(NU=0.4999)
# SOUS DEPLACEMENT RADIAL (U0=6.10E-5)
#
# MODELISATION : ELEMENTS MECANIQUE 3D INCOMP.(HEXA20+QUAD8)
# MAILLAGE     : GIBI
# ----------------------------------------------------------------------
# GROUPES DE NOEUDS ET MAILLES :
# GRNO: A B C D E F  NOEUMI
# GRMA: FACEEF     FACEAE   FACEAB   FACSUP   FACINF VOLUME
# ----------------------------------------------------------------------

DEBUT(CODE=_F(NOM='SSNV112A',
              NIV_PUB_WEB='INTERNET',),
      MEMOIRE=_F(DYNAMIQUE=400,),);

PRE_GIBI();

M=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=M,
               VERIF='MAILLE',
               AFFE=_F(GROUP_MA=('VOLUME','FACEEF','FACEAE','FACEAB','FACSUP','FACINF',),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D_INCO_GD',),);

M=MODI_MAILLAGE(reuse =M,
                MAILLAGE=M,
                ORIE_PEAU_3D=_F(GROUP_MA=('FACEEF','FACEAE',),),);

M=DEFI_GROUP(reuse =M,
             MAILLAGE=M,
             CREA_GROUP_NO=_F(GROUP_MA=('FACEAB','FACSUP','FACINF',),),);

MA=DEFI_MATERIAU(ELAS=_F(E=200000.,
                         NU=0.49999,
                         ALPHA=0.,),
                 ECRO_LINE=_F(D_SIGM_EPSI=1000.,
                              SY=1.E9,),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=MA,),);

CH=AFFE_CHAR_MECA(MODELE=MO,
                  DDL_IMPO=(_F(GROUP_NO='FACEAB',
                               DX=0.,),
                            _F(GROUP_NO='FACSUP',
                               DZ=0.,),
                            _F(GROUP_NO='FACINF',
                               DZ=0.,),),
                  FACE_IMPO=(_F(GROUP_MA='FACEEF',
                                DNOR=0.,),
                             _F(GROUP_MA='FACEAE',
                                DNOR=-6.E-5,),),);

INSTANTS=DEFI_LIST_REEL(DEBUT=0.,
                        INTERVALLE=_F(JUSQU_A=1.,
                                      NOMBRE=1,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',VALE=(0.,0.,
                          1.,1.,
                          ),);

EVOL=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CM,
                   EXCIT=_F(CHARGE=CH,
                            FONC_MULT=RAMPE,),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                                DEFORMATION='SIMO_MIEHE',),
                   INCREMENT=_F(LIST_INST=INSTANTS,),
                   NEWTON=_F(REAC_ITER=1,),
                   CONVERGENCE=_F(SIGM_REFE=60.0,
                                  EPSI_REFE=3.E-4,
                                  RESI_REFE_RELA=1.E-3,),
                   SOLVEUR=_F(METHODE='MUMPS',
                              PCENT_PIVOT=100,),);

EVOL=CALC_ELEM(reuse =EVOL,
               RESULTAT=EVOL,
               OPTION=('SIEF_ELNO_ELGA','EQUI_ELNO_SIGM','ERRE_ELEM_SIGM','ERRE_ELNO_ELEM',),);

EVOL=CALC_NO(reuse =EVOL,
             RESULTAT=EVOL,
             OPTION=('SIEF_NOEU_ELGA','EQUI_NOEU_SIGM',),);

