                 NOM_PARA='G',
                      VALE=3.3598088955088E-05,
                 PRECISION=5.E-7,
                 REFERENCE='ANALYTIQUE'      )

TEST_TABLE(      TABLE=GLOC4,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1099',      ),
                 NOM_PARA='G',
                      VALE=3.582E-5,
                 PRECISION=7.E-2,
                 REFERENCE='ANALYTIQUE'      )

TEST_TABLE(      TABLE=GLOC5,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1099',      ),
                 NOM_PARA='G',
                      VALE=3.582E-5,
                 PRECISION=5.E-2,
                 REFERENCE='ANALYTIQUE'      )

TEST_TABLE(      TABLE=GLOC55,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1099',      ),
                 NOM_PARA='G',
                      VALE=7.617E-6,
                 PRECISION=5.E-2,
                 REFERENCE='NON_REGRESSION'      )

TEST_TABLE(      TABLE=GLOC6,
                   FILTRE=_F(  NOM_PARA = 'NOEUD',  
                            VALE_K = 'NO1099',      ),
                 NOM_PARA='G',
                      VALE=3.582E-5,
                 PRECISION=5.E-2,
                 REFERENCE='ANALYTIQUE'      )

