# TEST SSLV110E (non documente)
# CALCUL DE G ET G LOCAL PAR LES METHODES 'LEGENDRE' ET 'LAGRANGE'
# PRESSION SUR LES LEVRES DE LA FISSURE (UNE SEULE LEVRE REPRESENTEE !)
#
#

DEBUT(  CODE=_F( NOM = 'SSLV110E',NIV_PUB_WEB='INTERNET'))

#
# --------------------------------------------------------------------

MA=LIRE_MAILLAGE(FORMAT='MED',  )

MO=AFFE_MODELE(  MAILLAGE=MA, 
                    AFFE=_F(  TOUT = 'OUI',
                           PHENOMENE = 'MECANIQUE',
                           MODELISATION = '3D', ) )

MA=MODI_MAILLAGE( reuse=MA,   MAILLAGE=MA,
                        ORIE_PEAU_3D=_F(  GROUP_MA = 'GRPRES',  )
                      )

MAT=DEFI_MATERIAU( ELAS=_F(  E = 210000.,  
                            NU = 0.3,    ))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI', 
                              MATER = MAT, ))

# ------------------------------------------------------------------
#    --- STRUCTURE EN APPUI SIMPLE AVEC PRESSION SUR LES LEVRES

CH=AFFE_CHAR_MECA(  MODELE=MO,DDL_IMPO=(
                     _F(  GROUP_NO = 'MACSYM',    DZ = 0.,  ), 
                              _F(  GROUP_NO = 'ELISYM',    DX = 0.,  ), 
                              _F(  GROUP_NO = 'GRNINF',    DY = 0.,  )), 
                     PRES_REP=_F(  GROUP_MA = 'GRPRES',  PRES = 1.,  ) )

PRESF = FORMULE(NOM_PARA=('X','Y','Z'),VALE='1.')
FORCF = FORMULE(NOM_PARA=('X','Y','Z'),VALE='1.')

CHFONC=AFFE_CHAR_MECA_F(  MODELE=MO, 
                            PRES_REP=_F(  GROUP_MA = 'GRPRES', 
                                       PRES = PRESF,  )    )

CHFACER=AFFE_CHAR_MECA(  MODELE=MO,
                          FORCE_FACE=_F(  GROUP_MA = 'GRPRES',  FZ = 1.,))

CHFACEF=AFFE_CHAR_MECA_F(  MODELE=MO, 
                             FORCE_FACE=_F(  GROUP_MA = 'GRPRES', 
                                          FZ = FORCF,   )      )

CHAMDEPL=MECA_STATIQUE(   MODELE=MO,
                             CHAM_MATER=CHMAT,
                              EXCIT=_F( CHARGE = CH,  ))

#
#    --- DEFINITION DU MODULE DE THETA ET DES RAYONS DEFINISSANT LA
#        COURONNE PAR FONCTIONS
#
MODUF=DEFI_FONCTION(  NOM_PARA='ABSC',  VALE=( 0., 1.,     26.69, 1., ))

RINF1=DEFI_FONCTION(  NOM_PARA='ABSC',  VALE=( 0., 0.,     26.69, 0., ))

RSUP1=DEFI_FONCTION(  NOM_PARA='ABSC',  VALE=( 0., 0.1875, 26.69, 0.7812,))

RINF2=DEFI_FONCTION(  NOM_PARA='ABSC',  VALE=( 0., 0.1875, 26.69, 0.7812,))

RSUP2=DEFI_FONCTION(  NOM_PARA='ABSC',  VALE=( 0., 0.375,  26.69, 1.5625,))

RINF3=DEFI_FONCTION(  NOM_PARA='ABSC',  VALE=( 0., 0.375,  26.69, 1.5625,))

RSUP3=DEFI_FONCTION(  NOM_PARA='ABSC',  VALE=( 0., 1.6465, 26.69, 3.125, ))

FISS1=DEFI_FOND_FISS(MAILLAGE=MA,
                     SYME='OUI',
                     FOND_FISS=_F(GROUP_NO='FONFIS',
                                  DTAN_ORIG=( 0.,  1.,  0.,),
                                  DTAN_EXTR=( 1.,  0.,  0.,)),
                     LEVRE_SUP=_F(GROUP_MA='GRPRES',),
                     )

FISS11=DEFI_FOND_FISS(MAILLAGE=MA,
                      SYME='OUI',
                      FOND_FISS=_F(GROUP_NO='FONFIS',
                                  DTAN_ORIG=( 0.,  1.,  0.,),),
                     )

#----------------------------------------------------------------------
#                           COURONNE 1
#----------------------------------------------------------------------

THETA1=CALC_THETA(   MODELE=MO,
                      FOND_FISS=FISS1,
                        THETA_3D=_F(  TOUT = 'OUI', 
                                   MODULE_FO = MODUF, 
                                   R_INF_FO = RINF1,  
                                   R_SUP_FO = RSUP1,  )
                     )

G1=CALC_G(        OPTION='CALC_G_GLOB',
                  RESULTAT=CHAMDEPL,NUME_ORDRE=1,
                  THETA=_F(THETA=THETA1),
                  EXCIT=_F(CHARGE=CHFONC,),)

GLOC1=CALC_G(         RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                      THETA=_F(FOND_FISS=FISS1,
                               R_SUP_FO=RSUP1,
                               R_INF_FO=RINF1),
                      LISSAGE=_F(DEGRE=7,),
                      )

#----------------------------------------------------------------------
#                           COURONNE 2
#----------------------------------------------------------------------

THETA2=CALC_THETA(   MODELE=MO,
                      FOND_FISS=FISS1,
                        THETA_3D=_F(  TOUT = 'OUI', 
                                   MODULE_FO = MODUF, 
                                   R_INF_FO = RINF2,  
                                   R_SUP_FO = RSUP2,  )
                     )

G2=CALC_G(          OPTION='CALC_G_GLOB',
                    RESULTAT=CHAMDEPL,NUME_ORDRE=1,
                    THETA=_F(THETA=THETA2),
                    EXCIT=_F(CHARGE=CHFACER,),)

GLOC2=CALC_G(         RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                      THETA=_F(FOND_FISS=FISS1,
                               R_SUP_FO=RSUP2,
                               R_INF_FO=RINF2),
                      LISSAGE=_F(DEGRE=7,),
                        )

#----------------------------------------------------------------------
#                           COURONNE 3
#----------------------------------------------------------------------

THETA3=CALC_THETA(   MODELE=MO,
                      FOND_FISS=FISS1,
                        THETA_3D=_F(  TOUT = 'OUI', 
                                   MODULE_FO = MODUF, 
                                   R_INF_FO = RINF3,  
                                   R_SUP_FO = RSUP3,  )
                     )

G3=CALC_G(          OPTION='CALC_G_GLOB',
                    RESULTAT=CHAMDEPL,NUME_ORDRE=1,
                    THETA=_F(THETA=THETA3),
                    EXCIT=_F(CHARGE=CHFACEF,),)

GLOC3=CALC_G(         RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                      THETA=_F(FOND_FISS=FISS1,
                               R_SUP_FO=RSUP3,
                               R_INF_FO=RINF3),
                      LISSAGE=_F(DEGRE=7),
                        )

#
#  CALCUL AVEC LA METHODE 'THETA_LAGRANGE'
#

GLOC4=CALC_G(         RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                      THETA=_F(FOND_FISS=FISS1,
                               R_SUP_FO=RSUP2,
                               R_INF_FO=RINF2),
                      LISSAGE=_F(LISSAGE_THETA='LAGRANGE',),
                      )

#
#  CALCUL AVEC LA METHODE 'LAGRANGE_LAGRANGE'
#

GLOC5=CALC_G(         RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                      THETA=_F(FOND_FISS=FISS1,
                               R_SUP_FO=RSUP2,
                               R_INF_FO=RINF2),
                      LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                                 LISSAGE_G='LAGRANGE'),
                        )

GLOC55=CALC_G(         RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                      THETA=_F(FOND_FISS=FISS11,
                               DIRECTION=(1.,0.,0.),
                               R_SUP_FO=RSUP2,
                               R_INF_FO=RINF2),
                      LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                                 LISSAGE_G='LAGRANGE'),
                        )

#
#  CALCUL AVEC LA METHODE 'NOEUD_NOEUD'
#

GLOC6=CALC_G(         RESULTAT=CHAMDEPL,
                      TOUT_ORDRE='OUI',
                      THETA=_F(FOND_FISS=FISS1,
                               R_SUP_FO=RSUP2,
                               R_INF_FO=RINF2),
                      LISSAGE=_F(LISSAGE_THETA='LAGRANGE',
                                 LISSAGE_G='LAGRANGE_NO_NO'),)

#----------------------------------------------------------------------
#              TEST DE G
#----------------------------------------------------------------------

TEST_TABLE(       TABLE=G1,
                       NOM_PARA='G',
                           VALE=7.0503219199279E-04,
                      PRECISION=5.E-6,
                      REFERENCE='ANALYTIQUE'      )

TEST_TABLE(       TABLE=G2,
                       NOM_PARA='G',
                           VALE=7.2452560863439E-04,
                      PRECISION=5.E-6,
                      REFERENCE='ANALYTIQUE'      )

TEST_TABLE(       TABLE=G3,
                       NOM_PARA='G',
                           VALE=6.9745852629086E-04,
                      PRECISION=5.E-6,
                      REFERENCE='ANALYTIQUE'      )

