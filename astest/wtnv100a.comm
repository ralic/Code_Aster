#----------------------------------------
# NIVEAU / CJS1 / SIG_33 = -100 KPA
#----------------------------------
#
#***********************************************************************
#    MAILLAGE + MODELE
#***********************************************************************

MAILLAGE=LIRE_MAILLAGE( )

MODELE=AFFE_MODELE(     MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = '3D')
                           )

#***********************************************************************
#    INCREMENTS DE TEMPS
#***********************************************************************

TEMPS1=DEFI_LIST_REEL(   DEBUT=0.,
                            INTERVALLE=_F(  JUSQU_A = 1.,   NOMBRE = 10)
                         )

#***********************************************************************
#    MATERIAU : SOL
#***********************************************************************

SOL=DEFI_MATERIAU(    ELAS=_F(  E = 22.4E3,  NU = 0.3),
                               CJS=_F(  BETA_CJS = -0.03,
                                     GAMMA_CJS = 0.82,
                                     RM = 0.289,
                                     PA = -100.0)
                           )

CHMAT=AFFE_MATERIAU(    MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',  MATER = SOL)
                          )

#***********************************************************************
#    CHARGEMENT MECANIQUE : FORCE DE COMPRESSION + DDL IMPOSES
#***********************************************************************

CHAR_U1=AFFE_CHAR_MECA(    MODELE=MODELE,
                              PRES_REP=_F(   MAILLE = ( 'M5', 'M6', ),
                                          PRES = 1.)
                       )

CHAR_U2=AFFE_CHAR_MECA(    MODELE=MODELE,
                             PRES_REP=_F(   MAILLE = 'M3',  PRES = 1.)
                        )

DEPL_1=AFFE_CHAR_MECA(    MODELE=MODELE,FACE_IMPO=(
                              _F(  MAILLE = 'M2',  DZ = 0.),
                              _F(  MAILLE = 'M7',  DX = 0.),
                              _F(  MAILLE = 'M4',  DY = 0.))
                           )

COEF1=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                         VALE=( 0.0,  0.0,
                                1.0,  100.,)
                       )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************


U1=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = CHAR_U1,
                FONC_MULT = COEF1), _F(
                CHARGE = CHAR_U2,
                FONC_MULT = COEF1), _F(
                CHARGE = DEPL_1)),
                         COMP_INCR=_F(
                RELATION = 'CJS'),
                         INCREMENT=_F(
                LIST_INST = TEMPS1),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6)
              )

U1=CALC_ELEM( reuse=U1,
                            OPTION=(
                  'SIEF_ELNO',  'VARI_ELNO',),
                          RESULTAT=U1,
              )

TEMPS2=DEFI_LIST_REEL(   DEBUT=1.,INTERVALLE=(
                            _F(  JUSQU_A = 1.05,  NOMBRE = 4),
                            _F(  JUSQU_A = 2.,    NOMBRE = 19))
                         )

#***********************************************************************
#    NOUVEAU CHARGEMENT MECANIQUE : DEPLACEMENT UNIAXIALE UNITAIRE
#***********************************************************************



SIG_INI=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELGA_SIEF_R',
NOM_CHAM='SIEF_ELGA', RESULTAT=U1,
                        INST=1.
                      )

VAR_INI=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELGA_VARI_R',
NOM_CHAM='VARI_ELGA', RESULTAT=U1,
                        INST=1.
                      )

DEPL_2=AFFE_CHAR_MECA(    MODELE=MODELE,
                              FACE_IMPO=_F(  MAILLE = 'M3',  DZ = 1.)
                           )

COEF2=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='CONSTANT',
                         VALE=( 1.0,  0.0,
                                2.0,  -0.2, )
                       )

DEPL_3=AFFE_CHAR_MECA(    MODELE=MODELE,FACE_IMPO=(
                              _F(  MAILLE = 'M6',  DY = 1.),
                              _F(  MAILLE = 'M5',  DX = 1.))
                           )

F = FORMULE(NOM_PARA='INST',VALE='- COEF2(INST) / 2.0 ')

COEF3=CALC_FONC_INTERP(    FONCTION=F,
                             LIST_PARA=TEMPS2,
NOM_PARA='INST', )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************


U2=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = DEPL_1), _F(
                CHARGE = DEPL_2,
                FONC_MULT = COEF2), _F(
                CHARGE = DEPL_3,
                FONC_MULT = COEF3)),
                         COMP_INCR=_F(
                RELATION = 'CJS'),
                         ETAT_INIT=_F(
                SIGM = SIG_INI,
                VARI = VAR_INI),
                         INCREMENT=_F(
                LIST_INST = TEMPS2),
                            NEWTON=_F(
                MATRICE = 'TANGENTE'),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 20,
                RESI_GLOB_RELA = 1.E-6)
              )

U2=CALC_ELEM( reuse=U2,
                            OPTION=(
                  'SIEF_ELNO',  'VARI_ELNO',),
                          RESULTAT=U2,
              )

