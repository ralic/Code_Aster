# FROTTEMENT AVEC DES ELEMENTS X-FEM QUADRATIQUES

DEBUT(CODE=_F(NOM='SSNV508C', NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))



# CETTE MODELISATION TESTE LA DEFORMEE DE L'INTERFACE SUR UN MAILLAGE
# CUB8, LES MAILLES N'ETANT PAS COUPEES PAR L'INTERFACE.



#----------------------------------------------
#                   MAILLAGE
#----------------------------------------------
# dimensions de la structure
#lx = 2.0;ly = 3.0;lz = 0.01.








MAILLAG1=LIRE_MAILLAGE(FORMAT='MED',INFO=1,);

MAILLAG1=MODI_MAILLAGE(reuse =MAILLAG1,
                       MAILLAGE=MAILLAG1,
                       ORIE_PEAU_3D=_F(GROUP_MA=('S1','S2','S3','S4','S6'),),);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO= _F(GROUP_MA=('MTOT'),
                                      NOM='VOL',),
                                      );




MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                   MAILLAGE=MAILLAG1,
                   CREA_GROUP_NO=(_F(NOM='S7',
                   OPTION='PLAN',
                   POINT =(0,0,0),
                   VECT_NORMALE =(1,0,0),
                   PRECISION=0.000001,),),);
#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MO=AFFE_MODELE(MAILLAGE=MAILLAG1,
               AFFE=_F(GROUP_MA=('MTOT','S1','S2','S3','S4','S6'),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);


LN = FORMULE(NOM_PARA=('X','Y','Z'),VALE='Y');

FISS=DEFI_FISS_XFEM(MODELE=MO,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN,),
                    GROUP_MA_ENRI='MTOT',
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MO,
                         FISSURE=FISS,
                         INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      INFO=1,);
#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E = 10000000000.0;

nu = 0.2;

rho = 7800.0;


ACIER=DEFI_MATERIAU(
                    ELAS=_F(E=E,
                            NU=nu,
                            RHO=rho,),);


#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MO,
                       AFFE=_F(GROUP_MA=('MTOT','S1','S2','S3','S4','S6'),
                               MATER=ACIER,),);
#PLAN ABCD, X=0 BLOQUEE, PLAQUE INFERIEURE ENCASTREMENT

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                DDL_IMPO=( _F(GROUP_MA='S6',DZ=0.0,),
                           _F(GROUP_NO='S7',DX=0.0,),
                           _F(GROUP_MA='S4',DX=0.0,DY=0.0,DZ=0.0,),),INFO=1,);
d0 = 1.E-05 ;
d2 = -2.5 * 1.E-06 ;


def deplv(x) :
   if x<0.  :  return d2*x*x + d0
   if x>0.  :  return d2*x*x + d0
   if x==0. :  return d2*x*x + d0

FCTDEPLV = FORMULE(VALE='deplv(X)',
                  NOM_PARA=('X','Y','Z'),);
ZERO = FORMULE(VALE='0.',NOM_PARA=('X','Y','Z'),);


# CONDITION LIMITE SUR LA SURFACE S3

CH2=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                     FACE_IMPO=_F(GROUP_MA='S3',DY=FCTDEPLV,),INFO=1,);
#pression lineaire : une variable independante

p2=100000.;
p1=2.*E*d2;

def pr1(y) :
   if y<0.  :  return 0.
   if y>0.  :  return  - p1*y - p2
   if y==0.  :  return  0.
FCTPR1 = FORMULE(VALE='pr1(Y)',
                  NOM_PARA=('X','Y','Z'),);

def pr2(y) :
   if y<0.  :  return 0.
   if y>0.  :  return   p1*y + p2
   if y==0.  :  return  0.
FCTPR2 = FORMULE(VALE='pr2(Y)',
                  NOM_PARA=('X','Y','Z'),);



# PRESSION IMPOSEE SUR S2 ET S1

CH3=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                  FORCE_FACE=_F(GROUP_MA=('S1'),FX=FCTPR1,),INFO=1,);

CH4=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                  FORCE_FACE=_F(GROUP_MA=('S2'),FX=FCTPR2,),INFO=1,);
#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

L_INS=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.0,
                                   NOMBRE=1,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH1,),
                           _F(CHARGE=CH2,),
                           _F(CHARGE=CH3,),
                           _F(CHARGE=CH4,),),
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='MTOT',),
                    INCREMENT=_F(LIST_INST=L_INS,
                                 INST_FIN=1.0,),
                    NEWTON=_F(REAC_ITER=1,),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1E-6,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',PCENT_PIVOT=200, ),INFO=1,);
DEP=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO='VOL',
                                RESULTAT=UTOT1,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=DEP,);

#----------------------------------------------
#             IMPRESSION
#----------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MODELE        = MODELEK,)

IMPR_RESU(FORMAT='RESULTAT',RESU=_F(MAILLAGE = MA_XFEM,))

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=(_F(GROUP_MA=('MTOT','S1','S2','S3','S4','S6'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);  

RES_X1=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        )

IMPR_RESU(FORMAT='MED',UNITE=80,RESU=_F(RESULTAT=RES_X1,));
IMPR_RESU(FORMAT='GMSH',UNITE=81,RESU=_F(MAILLAGE = MA_XFEM,));
IMPR_RESU(FORMAT='GMSH',UNITE=82,RESU=_F(RESULTAT=RES_X1,));

#----------------------------------------------
#                  TESTS
#----------------------------------------------

DEP1=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                GROUP_NO=('NFISSU'),
                                RESULTAT=RES_X1,
                                NOM_CHAM='DEPL',
                                NUME_ORDRE=1,
                                TOUT_CMP='OUI',
                                OPERATION='EXTRACTION',),);

DEP2 = CALC_TABLE(TABLE = DEP1,
                  ACTION = (_F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'NOEUD',
                               CRIT_COMP = 'REGEXP',
                               VALE_K    = 'N[^M]',),
                            _F(OPERATION = 'FILTRE',
                               NOM_PARA  = 'DY',
                               CRIT_COMP = 'NE',
                               VALE      = 0,),),);

#DX=(-10*x + 0)*1.E-6

DXMIN= -10*1.E-6
DXMAX=10*1.E-6

TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DX',
           TYPE_TEST='MAX',
           VALE=DXMAX,
           CRITERE='ABSOLU',
           PRECISION=1.0E-10,
           REFERENCE='ANALYTIQUE',);


TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DX',
           TYPE_TEST='MIN',
           VALE=DXMIN,
           CRITERE='ABSOLU',
           PRECISION=1.0E-10,
           REFERENCE='ANALYTIQUE',);
#DY=8.02*1.E-6 - 2.5*1.E-6*x*x*1.E-6-1/3 *z*z*1.E-6

DYMIN=8.02*1.E-6 - 2.5*1.E-6-(1/3)*0.01*0.01*1.E-6
DYMAX=8.02*1.E-6

TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DY',
           TYPE_TEST='MIN',
           VALE=DYMIN,
           CRITERE='ABSOLU',
           PRECISION=1.0E-10,
           REFERENCE='ANALYTIQUE',);


TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DY',
           TYPE_TEST='MAX',
           VALE=DYMAX,
           CRITERE='ABSOLU',
           PRECISION=1.0E-10,
           REFERENCE='ANALYTIQUE',);
DZMIN=-2*1.E-8
TEST_TABLE(TABLE=DEP2,
           NOM_PARA='DZ',
           TYPE_TEST='MIN',
           VALE=DZMIN,
           CRITERE='ABSOLU',
           PRECISION=1.0E-10,
           REFERENCE='ANALYTIQUE',);

TEST_RESU(RESU=(_F(  RESULTAT=RES_X1,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DX',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'RELATIF',
                     VALE = 0.01743,PRECISION=1.E-6,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.12'),
                _F(  RESULTAT=RES_X1,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DY',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'RELATIF',
                     VALE = 0.0477546,PRECISION=1.E-6,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.12'),
                _F(  RESULTAT=RES_X1,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DZ',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'RELATIF',
                     VALE =3.403786E-05 ,PRECISION=1.E-6,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '10.2.12'),     
                    ),)


IMPR_TABLE(TABLE=DEP2,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           FORMAT_C='MODULE_PHASE',
           IMPR_FONCTION='NON',);


FIN();
