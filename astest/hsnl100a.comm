                    MODELE=MO,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARA,
                    EXCIT=(_F(  CHARGE = CHA1),
                           _F(  CHARGE = CHA2)  ),
                    INCREMENT=_F( LIST_INST = L_INST2),
###CDURAND          ARCHIVAGE=_F( LIST_ARCH = L_ARCH2),
                    HHT=_F(),
                    ETAT_INIT=_F( INST = 0.,
