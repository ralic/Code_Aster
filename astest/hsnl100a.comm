                    COMP_ELAS=_F( RELATION = 'CABLE',
                                  DEFORMATION = 'GREEN'),
                    CONVERGENCE=_F(  RESI_GLOB_RELA = 1.E-6,
                                     ITER_GLOB_MAXI = 200),
                    NEWTON=_F( REAC_ITER = 1)
                      )

