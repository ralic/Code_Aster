                           PROL_GAUCHE='CONSTANT',
                          VALE=( 0.,     0.5,
                                 1000.,  0.5,      ))

ACIER=DEFI_MATERIAU(            ELAS=_F(   E = 145000.,
                                            NU = 0.3,
                                            ALPHA = 0.),

                             VISCOCHAB_FO=_F(   K = K_T,
                                            A_K = A_K_T,
                                            A_R = A_R_T,
                                            K_0 = K_0_T,
                                            N = N_T,
                                            ALP = ALP_T,
                                            B = B_T,
                                            M_R = M_R_T,
                                            G_R = G_R_T,
                                            MU = MU_T,
                                            Q_M = Q_M_T,
