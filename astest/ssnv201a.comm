DEBUT(CODE=_F(NOM='SSNV201A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

MAILLE=LIRE_MAILLAGE(FORMAT='MED',INFO=1);

MAILLE=DEFI_GROUP(reuse = MAILLE,
                  MAILLAGE = MAILLE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'H',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,10.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                             _F(
                                NOM = 'B',
                                OPTION = 'ENV_SPHERE',
                                POINT  = (0.0,0.0,0.0),
                                RAYON = 1E-3,
                                PRECISION = 2E-3,
                                ),
                            )    
           );

MAILLE=DEFI_GROUP(reuse = MAILLE,
                  MAILLAGE = MAILLE,
            CREA_GROUP_NO =(
                             _F(
                                NOM = 'face_hau',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'H',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
                             _F(
                                NOM = 'face_bas',
                                OPTION = 'PLAN',
                                GROUP_NO_CENTRE  = 'B',
                                VECT_NORMALE = (0.0,0.0,1.0),
                                PRECISION = 1E-3,
                                ),
#                              _F(GROUP_MA='VOL'),                            
                            )    
           );

MAILLAGE=CREA_MAILLAGE(MAILLAGE=MAILLE,COPIE=_F(),
                       );

MAILLAGE=DEFI_GROUP(reuse =MAILLAGE,
                    MAILLAGE=MAILLAGE,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL'),   
                    );
                       
MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAGE,
                   AFFE=_F(GROUP_MA=('VOL'),
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D'),
                     );        


LN1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-5');

FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN1));
 
MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS1,
                         CONTACT='P1P1',
                         INFO=1); 

CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      ZONE=(_F(INTEGRATION  = 'NOEUD',
                               CONTACT_INIT = 'NON',
                               GLISSIERE    = 'OUI',
                               COEF_CONT    = 100.,
                               FISS_MAIT    = FISS1)));
 
E=100.0E6
nu=0.

ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAGE,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA=('VOL'),
                                MATER=ACIER,
                                ),
                             ));
ZERO=0.0
DEPX=1.E-3
DEPY=1.E-3
DEPZ=1.E-3

ENCASTR=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                 _F(GROUP_NO='face_bas',
                                    DX=ZERO,
                                    DY=ZERO,
                                    DZ=ZERO,
                                    ),
                                 ),
                       INFO=1);

CHZ=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                  _F(GROUP_NO='face_hau', 
                                     DZ=DEPZ,
                                     DX=ZERO,
                                     ),
                                 ),
                       INFO=1);

CHY=AFFE_CHAR_MECA(MODELE=MODELEK,
                       DDL_IMPO=(
                                  _F(GROUP_NO='face_hau',
                                    DY=DEPY,   
                                     ),
                                 ),
                       INFO=1);

VARZ=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0,0,1,1,2,-1,3,1),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT');

VARY=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,0.,2.,0.,3.,1.),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT')

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3.0,
                                    NOMBRE=3));

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,              
                   EXCIT=(_F(CHARGE=ENCASTR),
                          _F(CHARGE=CHZ,FONC_MULT=VARZ),
                          _F(CHARGE=CHY,FONC_MULT=VARY),                            
                          _F(CHARGE=CHXFEM)), 
                   CONTACT  = CTXFEM,                        
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL'),
                   INCREMENT=_F(LIST_INST=L_INST),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=8,
                                  RESI_GLOB_MAXI=1e-04,
                                  ),               
                   SOLVEUR=_F(METHODE='MUMPS',
                               ),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                   NEWTON=_F(REAC_ITER=1),
                   INFO=1);

# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D')) 

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        
                        INFO          = 2)


# -------------------------------------------------------------   
#            TEST_TABLE
# -------------------------------------------------------------   
DEP=[None]*4
DEPM=[None]*4
DEPP=[None]*4

valezm = [0,-DEPZ/2,DEPZ/2]
valezp = [DEPZ,-DEPZ/2,DEPZ/2]
valeym = [0,0,0]
valeyp = [0,0,DEPY]

criterezm = ['ABSOLU','RELATIF','RELATIF']
criterezp = ['RELATIF','RELATIF','RELATIF']
critereym = ['ABSOLU','ABSOLU','ABSOLU']
critereyp = ['ABSOLU','ABSOLU','RELATIF']

precisionzm = [1.0E-16,1.0E-11,1.0E-11]
precisionzp = [1.0E-11,1.0E-11,1.0E-11]
precisionym = [1.0E-16,1.0E-16,1.0E-16]
precisionyp = [1.0E-16,1.0E-16,1.0E-11]

for i in [1,2,3]: #Boucle sur les increments 
  DEP[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                 GROUP_NO=('NFISSU'),
                                 RESULTAT=RES_XFEM,
                                 NOM_CHAM='DEPL',
                                 NUME_ORDRE=i,
                                 TOUT_CMP='OUI',
                                 OPERATION='EXTRACTION'));

  DEPM[i] = CALC_TABLE(TABLE = DEP[i],
                      ACTION = (_F(OPERATION = 'FILTRE',
                                   NOM_PARA  = 'NOEUD',
                                   CRIT_COMP = 'REGEXP',
                                   VALE_K    = 'N[^P]')));

  IMPR_TABLE(TABLE=DEPM[i],
             FORMAT='AGRAF',
             FORMAT_R='1PE12.9',
             IMPR_FONCTION='NON');
                                      
  DEPP[i] = CALC_TABLE(TABLE = DEP[i],
                      ACTION = (_F(OPERATION = 'FILTRE',
                                   NOM_PARA  = 'NOEUD',
                                   CRIT_COMP = 'REGEXP',
                                   VALE_K    = 'N[^M]')));

  IMPR_TABLE(TABLE=DEPP[i],
             FORMAT='AGRAF',
             FORMAT_R='1PE12.9',
             IMPR_FONCTION='NON');


  TEST_TABLE(TABLE=DEPM[i],
             NOM_PARA='DZ',
             TYPE_TEST='MIN',
             VALE=valezm[i-1],
             CRITERE=criterezm[i-1],
             PRECISION=precisionzm[i-1],
             REFERENCE='ANALYTIQUE');

  TEST_TABLE(TABLE=DEPM[i],
             NOM_PARA='DZ',
             TYPE_TEST='MAX',
             VALE=valezm[i-1],
             CRITERE=criterezm[i-1],
             PRECISION=precisionzm[i-1],
             REFERENCE='ANALYTIQUE');

  TEST_TABLE(TABLE=DEPP[i],
             NOM_PARA='DZ',
             TYPE_TEST='MIN',
             VALE=valezp[i-1],
             CRITERE=criterezp[i-1],
             PRECISION=precisionzp[i-1],
             REFERENCE='ANALYTIQUE');

  TEST_TABLE(TABLE=DEPP[i],
             NOM_PARA='DZ',
             TYPE_TEST='MAX',
             VALE=valezp[i-1],
             CRITERE=criterezp[i-1],
             PRECISION=precisionzp[i-1],
             REFERENCE='ANALYTIQUE');
             
  TEST_TABLE(TABLE=DEPM[i],
             NOM_PARA='DY',
             TYPE_TEST='MIN',
             VALE=valeym[i-1],
             CRITERE=critereym[i-1],
             PRECISION=precisionym[i-1],
             REFERENCE='ANALYTIQUE');

  TEST_TABLE(TABLE=DEPM[i],
             NOM_PARA='DY',
             TYPE_TEST='MAX',
             VALE=valeym[i-1],
             CRITERE=critereym[i-1],
             PRECISION=precisionym[i-1],
             REFERENCE='ANALYTIQUE');
             
  TEST_TABLE(TABLE=DEPP[i],
             NOM_PARA='DY',
             TYPE_TEST='MIN',
             VALE=valeyp[i-1],
             CRITERE=critereyp[i-1],
             PRECISION=precisionyp[i-1],
             REFERENCE='ANALYTIQUE');

  TEST_TABLE(TABLE=DEPP[i],
             NOM_PARA='DY',
             TYPE_TEST='MAX',
             VALE=valeyp[i-1],
             CRITERE=critereyp[i-1],
             PRECISION=precisionyp[i-1],
             REFERENCE='ANALYTIQUE');


FIN();
