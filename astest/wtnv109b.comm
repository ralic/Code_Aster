DEBUT(CODE=_F(NOM='WTNV109B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE()

MAIL=DEFI_GROUP( reuse=MAIL,  MAILLAGE=MAIL,CREA_GROUP_NO=(
                      _F( GROUP_MA = 'BAS'),
                                _F( GROUP_MA = 'HAUT'))
                   )

MODELE=AFFE_MODELE(   MAILLAGE=MAIL,
                        AFFE=_F(  TOUT = 'OUI',
                      PHENOMENE = 'MECANIQUE',
                      MODELISATION = 'D_PLAN_THM')
                     )

#
####################################
#  LISTE DES INSTANTS DE CALCUL
####################################
LI=DEFI_LIST_REEL(   DEBUT=0.,
                       INTERVALLE=_F(  JUSQU_A = 1.E10, NOMBRE = 1)
                     )

############################################
#
# DEFINITION DES FONCTIONS DE COMPORTEMENT :
# VISCOSITE LIQUIDE ET GAZ : UNITE : PA.S
#
UN=DEFI_CONSTANTE(    VALE=1.)

ZERO=DEFI_CONSTANTE(    VALE=0.)

#
#
#
# PERMEABILITE INTRINSEQUE, RELATIVE DU LIQUIDE, RELATIVE DU GAZ
# UNITE INTRINSEQUE : METRES CARRES , UNITE RELATIVE : SANS
#
KINT=DEFI_CONSTANTE(    VALE=3.E-16)

PERMLIQ=DEFI_CONSTANTE(    VALE=1.)

DPERMLI=DEFI_CONSTANTE(    VALE=0.)

PERMGAZ=DEFI_CONSTANTE(    VALE=1.)

DPERGSA=DEFI_CONSTANTE(    VALE=0.)

DPERGPG=DEFI_CONSTANTE(    VALE=0.)

#
# COEFFICIENT DE FICK
# UNITE METRES CARRES PAR SECONDES
#
FICK    = DEFI_CONSTANTE ( VALE = 1.E-7)
DFICKTE = DEFI_CONSTANTE ( VALE = 0.   )
DFICKPG = DEFI_CONSTANTE ( VALE = 0.   )

DCONDLI = DEFI_CONSTANTE ( VALE = 0.   )
CONDGAZ = DEFI_CONSTANTE ( VALE = 0.02 )
DCONDGA = DEFI_CONSTANTE ( VALE = 0.   )

#
# CONDUCTIVITES DU SOLIDE, DU LIQUIDE, DU GAZ
# UNITES : WATTS PAR METRE CARRE
#
CONDHOMO=DEFI_CONSTANTE(    VALE=1.7)

DCONDHO=DEFI_CONSTANTE(    VALE=0.)

CONDLIQ=DEFI_CONSTANTE(    VALE=3.E-16)

MATERIAU=DEFI_MATERIAU(     THM_LIQU=_F(
   RHO = 1000.,
   UN_SUR_K = 3.77E-9,
   ALPHA = 1.E-4,
   CP = 2.85E6,
   VISC = UN,
   D_VISC_TEMP = ZERO,),
#
                              THM_GAZ=_F(
   MASS_MOL = 28.96E-3,
   CP = 1000.,
   VISC = UN,
   D_VISC_TEMP = ZERO,),
#
                              THM_DIFFU=_F(
   R_GAZ = 8.315,
   RHO = 1.6E3,
   CP = 2.85E6,
   BIOT_COEF = 1.E-12,
   SATU_PRES = UN,
   D_SATU_PRES = ZERO,
   PESA_X = 0.,
   PESA_Y = 0.,
   PESA_Z = 0.,
   PERM_IN = KINT,
   PERM_LIQU = UN,
   D_PERM_LIQU_SATU = ZERO,
   PERM_GAZ = PERMGAZ,
   D_PERM_SATU_GAZ = DPERGSA,
   D_PERM_PRES_GAZ = DPERGPG,
   LAMB_T = CONDHOMO,
   ),
                              THM_INIT=_F(
   TEMP = 273.,
   PRE1 = 0.,
   PRE2 = 0.,
   PORO = 0.4,
   PRES_VAPE = 1.),
                               COMP_THM = 'LIQU_SATU',
                                     ELAS=_F(
   E = 225.E6,
   NU = 0.,
   RHO = 2.E3,
   ALPHA = 8.E-6)
   )

CHMAT0=AFFE_MATERIAU(   MAILLAGE=MAIL,
                          AFFE=_F( TOUT = 'OUI',
                        MATER = MATERIAU))

CHAR0=AFFE_CHAR_MECA(   MODELE=MODELE,DDL_IMPO=(
                          _F( NOEUD=('N1','N2','N3','N4'),
                                     PRE1 = 500.),
                         _F(  GROUP_NO = 'BAS',
                                     DX = 0.,
                                     DY = 0.,),
                         _F(NOEUD=('N1','N2'),
                                     TEMP = 0.),

                         _F(  NOEUD = ('N6','N8',),
                                     DX = 0.,
                                     DY = 0.5E-3),

                         _F(  GROUP_NO = 'HAUT',
                                     DX = 0.,
                                     DY = 1.E-3,
                                     ))
                  )

MESTAT=STAT_NON_LINE(   MODELE=MODELE,
            SOLVEUR=_F( METHODE = 'LDLT'),
                          CHAM_MATER=CHMAT0,
                          EXCIT=_F( CHARGE = CHAR0),
                          COMP_INCR=_F( RELATION = 'KIT_THM',PARM_THETA=1.,
                          RELATION_KIT = ('ELAS', 'LIQU_SATU',  'HYDR_UTIL',),
                          ITER_INTE_MAXI = 5),

                          INCREMENT=_F( LIST_INST = LI),
                          NEWTON=_F( MATRICE = 'TANGENTE',
                                  REAC_ITER = 20),
                         CONVERGENCE=_F(
                                     ITER_GLOB_MAXI = 10,
                                     RESI_GLOB_RELA = 1.E-6),
                        )

TEST_RESU(
           RESU=_F(  RESULTAT = MESTAT,         NUME_ORDRE = 1,
             NOM_CHAM = 'DEPL',
             NOM_CMP = 'TEMP',
             NOEUD = 'N3',
             VALE = -1.2860E-08,
             CRITERE = 'RELATIF',
             PRECISION = 1.E-5,
                  REFERENCE='ANALYTIQUE',)
           )

TEST_RESU(
           RESU=_F(  RESULTAT = MESTAT,         NUME_ORDRE = 1,
             NOM_CHAM = 'DEPL',
             NOM_CMP = 'TEMP',
             NOEUD = 'N4',
             VALE = -1.2860E-08,
             CRITERE = 'RELATIF',
             PRECISION = 1.E-5,
                  REFERENCE='ANALYTIQUE',)
           )

FIN()
