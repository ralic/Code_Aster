DEBUT(CODE=_F(  NOM = 'DEMO003A',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'));

PRE_GMSH();

MA=LIRE_MAILLAGE();


MA=DEFI_GROUP(reuse=MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(NOM='GM12',
                                GROUP_MA='GM12',),
                             ),);   

# On reoriente les group_ma GM11 et GM12 qui 
# interviennent dans le contact car leurs normales
# doivent absolument etre sortantes
MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=(_F(GROUP_MA=('GM11')),
                               _F(GROUP_MA=('GM12'))),
                 );



MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);


MAT=DEFI_MATERIAU(ELAS=_F(E=2.E5,
                           NU=0.29999999999999999,),
                   ECRO_LINE=_F(D_SIGM_EPSI=5000.0,
                                SY=300.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA=('GM15'),
                             MATER=MAT,),),);

CHA=AFFE_CHAR_MECA(MODELE=MO,
                   DDL_IMPO=(_F(GROUP_MA='GM14',
                                DX= 0.0,
                                DY= 0.0,),
                             _F(GROUP_MA=('GM9','GM10','GM11'),
                                DX=0.0,
                                DY=-0.4,),
                             _F(GROUP_MA=('GM13'),
                                DX=0.0,),
                                ),
                   CONTACT=_F(APPARIEMENT='MAIT_ESCL',
                              GROUP_MA_MAIT='GM11',
                              GROUP_MA_ESCL='GM12',
                              NORMALE='MAIT_ESCL',
                              RECHERCHE = 'NOEUD_BOUCLE',
                              ),);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=50,),);

F=DEFI_FONCTION(NOM_PARA='INST',
                VALE=(0.0,  0.0,
                      0.8,  1.0,
                      1.0,  0.98),
                      );


RESU=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                   MODELE=MO,
                   CHAM_MATER=CHMAT,
                   EXCIT=_F(CHARGE=CHA,
                            FONC_MULT=F,),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                                DEFORMATION='PETIT_REAC'),
                   CONVERGENCE=(_F(ITER_GLOB_MAXI=20,)),
                   INCREMENT=_F(LIST_INST=L_INST,
                                INST_FIN=1.,
                                SUBD_METHODE='UNIFORME', SUBD_PAS=4,
                                SUBD_PAS_MINI=1.E-4,
                                SUBD_COEF_PAS_1=1.0,),
                   NEWTON=_F(MATRICE='TANGENTE',
                             PREDICTION='ELASTIQUE',
                             REAC_ITER=1,),
                   ARCHIVAGE=_F(LIST_INST=L_INST,
                                ARCH_ETAT_INIT='OUI',),);


# On definit un fichier pour le post traitement 
# des deplacements avec gmsh

IMPR_RESU (FORMAT = 'GMSH',UNITE=37,
           RESU = _F( RESULTAT = RESU,
                      NOM_CHAM = 'DEPL', )
           );
           
DEFI_FICHIER(ACTION='LIBERER',UNITE=37);

# On calcule la force de reaction du massif
# attention : la force est en n/radian
RESU=CALC_NO(reuse =RESU,
             OPTION='REAC_NODA',
             RESULTAT=RESU,);

TAB_FORC=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                            GROUP_NO='GM12',
                            RESULTAT=RESU,
                            NOM_CHAM='REAC_NODA',
                            TOUT_ORDRE='OUI',
                            RESULTANTE='DY',
                            OPERATION='EXTRACTION',),);
                                 
TEST_RESU(RESU=(_F(RESULTAT=RESU,
                   INST=0.50,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='GM16',
                   VALE=-2.50000E-01,
                   REFERENCE='NON_REGRESSION',
                   VERSION='7.1.07',),
                _F(RESULTAT=RESU,
                   INST=0.50,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   GROUP_NO='GM16',
                   VALE=-1.748511E+01,
                   REFERENCE='NON_REGRESSION',
                   VERSION='7.1.07',),
                _F(RESULTAT=RESU,
                   INST=0.50,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DY',
                   GROUP_NO='GM16',
                   VALE=-1.632748E+01,
                   REFERENCE='NON_REGRESSION',
                   VERSION='8.0.22',),   
                   ),);

TEST_RESU(RESU=(_F(RESULTAT=RESU,
                   INST=1.0,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DY',
                   GROUP_NO='GM16',
                   VALE=-3.92000E-01,
                   REFERENCE='NON_REGRESSION',
                   VERSION='7.1.07',),
                _F(RESULTAT=RESU,
                   INST=1.0,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DX',
                   GROUP_NO='GM16',
                   VALE=-1.89658E+01,
                   REFERENCE='NON_REGRESSION',
                   VERSION='7.1.07',),
                _F(RESULTAT=RESU,
                   INST=1.0,
                   NOM_CHAM='REAC_NODA',
                   NOM_CMP='DY',
                   GROUP_NO='GM16',
                   VALE=-1.37573E+01,
                   REFERENCE='NON_REGRESSION',
                   VERSION='7.1.07',),   
                   ),);
                   
FY=RECU_FONCTION(TABLE=TAB_FORC,
                 PARA_X='INST',
                 PARA_Y='DY')

DY=RECU_FONCTION(RESULTAT=RESU,
                 NOM_CHAM='DEPL',
                 NOM_CMP='DY',
                 GROUP_NO='GM16')

IMPR_FONCTION(FORMAT='AGRAF',
              UNITE=25,
              UNITE_DIGR=26,
              LEGENDE_X='Deplacement (mm)',
              LEGENDE_Y='Force (N/rd)',
              COURBE=(_F(FONC_X = DY,
                         FONC_Y = FY,)));

FIN();
