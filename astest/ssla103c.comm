DEBUT(CODE=_F(NOM='SSLA103C',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

M=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=M,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS',),);

E=DEFI_FONCTION(NOM_PARA='SECH',
                VALE=( 80.,  3.E10,
                      100. , 1.E10),)

NU=DEFI_FONCTION(NOM_PARA='HYDR',
                VALE=(  0.,  0.15,
                        1,   0.25),)

ALPHA=DEFI_CONSTANTE(VALE= 1.E-5)

EPSD0=DEFI_CONSTANTE(VALE=1.E-4,);

AT=DEFI_FONCTION(NOM_PARA='SECH',
                VALE=( 80., 0.8,
                      100., 0.6),)

BT=DEFI_FONCTION(NOM_PARA='HYDR',
                VALE=(  0., 10000.,
                        1., 11000.),)

AC   =DEFI_CONSTANTE(VALE = 1.4)

BC   =DEFI_CONSTANTE(VALE = 2000.)


BETON=DEFI_MATERIAU(ELAS_FO=_F(E=E,
                               NU=NU,
                               K_DESSIC =1.66E-5,
                               B_ENDOGE = 1.5E-5,
                               TEMP_DEF_ALPHA = 20.,
                               ALPHA=ALPHA ),
                    MAZARS_FO  =_F(BETA = 1.06,
                                   EPSD0= EPSD0,
                                   AC   = AC,
                                   AT   = AT,
                                   BC   = BC,
                                   BT   = BT),)

SECHI=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=100.),);

SECHF=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=80.),);

SECH1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=(
               _F(CHAM_GD=SECHI,
                   INST=0.0,),
                _F(CHAM_GD=SECHF,
                   INST=3600.0,),   ),);

TEMPI=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=20.0,),);

TEMPF=CREA_CHAMP(TYPE_CHAM='NOEU_TEMP_R',
                OPERATION='AFFE',
                MAILLAGE=M,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='TEMP',
                        VALE=120.0,),);

TEMP1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='TEMP',
                AFFE=(
                _F(CHAM_GD=TEMPI,
                   INST=0.0,),
                _F(CHAM_GD=TEMPF,
                   INST=3600.0,  ),),);

HYDRI=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
                OPERATION='AFFE',
                MODELE = MO,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='HYDR',
                        VALE=0.0,),);

HYDRF=CREA_CHAMP(TYPE_CHAM='NOEU_HYDR_R',
                OPERATION='AFFE',
                MODELE = MO,
                AFFE=_F(TOUT='OUI',
                        NOM_CMP='HYDR',
                        VALE=1.0,),);

HYDR1=CREA_RESU(OPERATION='AFFE',
                TYPE_RESU='EVOL_THER',
                NOM_CHAM='HYDR_NOEU',
                AFFE=(
                _F(CHAM_GD=HYDRI,
                   INST=0.0,),
                _F(CHAM_GD=HYDRF,
                   INST=3600.0,  ),),);

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=BETON,),
                 AFFE_VARC=(_F(TOUT='OUI',EVOL=HYDR1,NOM_VARC='HYDR',NOM_CHAM='HYDR_NOEU'),
                            _F(TOUT='OUI',EVOL=SECH1,NOM_VARC='SECH',NOM_CHAM='TEMP',VALE_REF=100),
                            _F(TOUT='OUI',EVOL=TEMP1,NOM_VARC='TEMP',NOM_CHAM='TEMP',VALE_REF=20),),);


CH1=AFFE_CHAR_MECA(MODELE=MO,
                   FACE_IMPO=_F(GROUP_MA='D1',
                                DY=0.0,),);

#  ATTENTION :METTRE  SUFFISAMMENT DE PAS DE TEMPS QUAND E VARIE AVEC LE
#  SECHAGE SINON RESULTATS APPROXIMATIFS OU PROBLEME DE CONVERGENCE
L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3600.0,
                                     NOMBRE=20,),)

U=STAT_NON_LINE( MODELE=MO,
                CHAM_MATER=CM,
                EXCIT=_F(CHARGE=CH1,),
                COMP_INCR=_F(RELATION='MAZARS',
                             ITER_INTE_MAXI=100),
                INCREMENT=_F(LIST_INST=L_INST,),
                NEWTON=_F(MATRICE='TANGENTE',
                          REAC_ITER=1,),
                CONVERGENCE=_F(RESI_GLOB_MAXI=1e-06,
                               ITER_GLOB_MAXI=25,),);
U=CALC_ELEM(reuse =U,
             RESULTAT = U,



             OPTION=('EPSI_ELNO','EPSP_ELNO','SIGM_ELNO','VARI_ELNO'),         )

U=CALC_NO(reuse =U,
             RESULTAT = U,

             OPTION=('EPSI_NOEU','SIGM_NOEU','EPSP_NOEU','VARI_NOEU'),         )

TEST_RESU(RESU=(
  _F(  RESULTAT = U,  NOM_CHAM = 'EPSI_NOEU', INST = 3600.,
         NOM_CMP = 'EPXX',   NOEUD = 'N5',
         PRECISION = 5.E-3,  REFERENCE = 'ANALYTIQUE',
         VALE = 6.53E-4),
  _F(  RESULTAT = U,  NOM_CHAM = 'EPSI_NOEU', INST = 3600.,
         NOM_CMP = 'EPYY',   NOEUD = 'N5',
         PRECISION = 5.E-3,  REFERENCE = 'ANALYTIQUE',
         VALE = 6.53E-4),
  _F(  RESULTAT = U,  NOM_CHAM = 'SIGM_NOEU', INST = 3600.,
         NOM_CMP = 'SIXX',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0.,CRITERE = 'ABSOLU'),
  _F(  RESULTAT = U,  NOM_CHAM = 'SIGM_NOEU', INST = 3600.,
         NOM_CMP = 'SIYY',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0., CRITERE = 'ABSOLU'),
  _F(  RESULTAT = U,  NOM_CHAM = 'EPSP_NOEU', INST = 3600.,
         NOM_CMP = 'EPXX',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0.,CRITERE = 'ABSOLU'),
  _F(  RESULTAT = U,  NOM_CHAM = 'EPSP_NOEU', INST = 3600.,
         NOM_CMP = 'EPYY',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0., CRITERE = 'ABSOLU'),
  _F(  RESULTAT = U,  NOM_CHAM = 'VARI_NOEU', INST = 3600.,
         NOM_CMP = 'V1',   NOEUD = 'N5',
         PRECISION = 1.E-6,  REFERENCE = 'ANALYTIQUE',
         VALE = 0., CRITERE = 'ABSOLU'),
                  ));


FIN();
