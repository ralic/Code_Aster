#                           SANS CORRECTION STATIQUE
#                           SPECTRE HORIZONTAL
#                           COMBINAISON QUADRATIQUE COMPLETE
#-----------------------------------------

SISM_MO1=COMB_SISM_MODAL(      MODE_MECA=MODE,
                             AMOR_REDUIT=0.03,
                               MASS_INER=MAS,
                               CORR_FREQ='NON',
                                   EXCIT=_F( MONO_APPUI = 'OUI',
                                             AXE = ( 1., 0., 0., ),
                                             SPEC_OSCI = SPECT_XY,
                                             ECHELLE = 1.),
                               COMB_MODE=_F( TYPE = 'CQC'),
                                  OPTION=( 'DEPL', 'ACCE_ABSOLU', 'REAC_NODA',)
                           )

