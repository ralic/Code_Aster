DEBUT(CODE=_F(NOM='SSNV182C',
              NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='NON'));

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                        LINE_QUAD=_F(GROUP_MA='VOL',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=_F(NOM='VOLQUAD',
                                     GROUP_MA='VOL',),);

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOL',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D_XFEM_CONT',),
                           _F(GROUP_MA=('SURFINF','SURFSUP',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);

PENTE=0.0
B=10.-10.*PENTE

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-PENTE*Y-B');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN,),);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='P1P1A',
                         INFO=1,);

CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      FROTTEMENT     = 'COULOMB',
                      ITER_FROT_MAXI = 6, 
                      ITER_CONT_MAXI = 4,
                      REAC_GEOM      = 'SANS',
                      ZONE=(
                            _F(
                               FISS_MAIT    = FISS,
                               INTEGRATION  = 'FPG4',
                               CONTACT_INIT = 'OUI',
                               COEF_ECHELLE = 1.,
                               COULOMB      = 1.0,
                               ALGO_CONT    = 'STANDARD',
                               COEF_CONT    = 100.,
                               ALGO_FROT    = 'STANDARD',
                               COEF_FROT    = 1000.,                             
                               SEUIL_INIT   = -1.E11,
                             ), 
                         ),
                   );

CHLN=CREA_CHAMP(TYPE_CHAM='NOEU_NEUT_R',
                OPERATION='EXTR',
                NOM_CHAM='LNNO',
                     FISSURE=FISS,);

IMPR_RESU(FORMAT='IDEAS',
          UNITE=30,
          RESU=_F(MAILLAGE=MAILLAG1,
                  CHAM_GD=CHLN,),);




E=100.0E6
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA=('VOL','SURFINF','SURFSUP',),
                                MATER=ACIER,),);

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SURFINF',DX=0.0,DY=0.0,DZ=0.0,),
                             _F(GROUP_MA='SURFSUP',DX=0.0,DY=0.0,DZ=-1.E-6,),),
                    INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CH1,),
                          _F(CHARGE=CHXFEM,),),
                   CONTACT  = CTXFEM, 
                   NEWTON=_F(REAC_ITER=1,),  
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   SOLVEUR=_F(METHODE='MUMPS',PCENT_PIVOT=10),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);


LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                           # maillage 5_20_20
                             NOEUD=('N2493','N2043','N2042','N2041','N2040','N2039','N2038','N2037',
                                    'N2036','N2035','N2034','N2033','N2032','N2031','N2030','N2029',
                                    'N2028','N2027','N2026','N2025','N2492','N2044','N811','N810',
                                    'N809','N808','N807','N806','N805','N804','N803','N802',
                                    'N801','N800','N799','N798','N797','N796','N795','N794',
                                    'N793','N2024','N2045','N812','N847','N846','N845','N844',
                                    'N843','N842','N841','N840','N839','N838','N837','N836',
                                    'N835','N834','N833','N832','N831','N792','N2023','N2046',
                                    'N813','N814','N815','N816','N817','N818','N819','N820',
                                    'N821','N822','N823','N824','N825','N826','N827','N828',
                                    'N829','N830','N791','N2022','N771','N772','N773','N774',
                                    'N775','N776','N777','N778','N779','N780','N781','N782',
                                    'N783','N784','N785','N786','N787','N788','N789','N790',
                                    'N2021','N2001','N2002','N2003','N2004','N2005','N2006','N2007',
                                    'N2008','N2009','N2010','N2011','N2012','N2013','N2014','N2015',
                                    'N2016','N2017','N2018','N2019','N2020','N2491',),
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP=('LAGS_C','LAGS_F1','LAGS_F2',),
                             OPERATION='EXTRACTION',),);



IMPR_TABLE(TABLE=LAG,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           FORMAT_C='MODULE_PHASE',
           IMPR_FONCTION='NON',);

LAREF=-5.
LA1REF=0.
LA2REF=0.

# TESTS
TEST_TABLE(TABLE=LAG,
           FILTRE=(_F(NOM_PARA='NOEUD',
                      VALE_K='N2043',),
                   ),
           NOM_PARA='LAGS_C',
           VALE=LAREF,
           PRECISION=2.E-5,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MAX',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MIN',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F2',
           TYPE_TEST='MAX',
           VALE=LA2REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F2',
           TYPE_TEST='MIN',
           VALE=LA2REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

# -------------------------------------------------------------
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM
# -------------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAG1,
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),)

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAG1,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        
                        INFO          = 2)

