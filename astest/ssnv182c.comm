DEBUT(CODE=_F(NOM='SSNV182C',
              NIV_PUB_WEB='INTERNET',),);

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                        LINE_QUAD=_F(GROUP_MA='VOL',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=_F(NOM='VOLQUAD',
                                     GROUP_MA='VOL',),);
             
MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOL',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D_XFEM',),
                           _F(GROUP_MA=('SURFINF','SURFSUP',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);        

PENTE=0.0
B=10.-10.*PENTE

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-PENTE*Y-B');
LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='X-100. ');                      

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    DEFI_FISS=_F(
                                 FONC_LT=LT,
                                 FONC_LN=LN,               
                                 ),
                    GROUP_MA_ENRI='VOL',
                    ORIE_FOND=_F(PFON_INI=(2.5 ,0. ,2.5 ),
                                 VECT_ORIE=(0. ,0. ,1. ),
                                 PT_ORIGIN=(-0.5,0.,1.5 ),),
                    CONTACT=_F(INTEGRATION='FPG4',
                               COEF_REGU_CONT=100., 
                               ITER_CONT_MAXI=4,   
                               CONTACT_INIT='OUI',    
                               COEF_ECHELLE=1.,
#                               FROTTEMENT='SANS',
                               FROTTEMENT='COULOMB',
                               COULOMB=1.0,
                               ITER_FROT_MAXI=6,
                               COEF_REGU_FROT=1000.,
                               SEUIL_INIT=-1.E11,
                               ),
                    INFO=1,);

CHLN=CREA_CHAMP(TYPE_CHAM='NOEU_NEUT_R',
                OPERATION='EXTR',
                NOM_CHAM='LNNO',
                     FISSURE=FISS,);                 
                    
IMPR_RESU(FORMAT='IDEAS',
          UNITE=30,
          RESU=_F(MAILLAGE=MAILLAG1,
                  CHAM_GD=CHLN,),);


MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         INFO=1,);

E=100.0E6
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA=('VOL','SURFINF','SURFSUP',),
                                MATER=ACIER,
                                TEMP_REF=0.0,),);

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SURFINF',DX=0.0,DY=0.0,DZ=0.0,),
                             _F(GROUP_MA='SURFSUP',DX=0.0,DY=0.0,DZ=-1.E-6,),),     
                    INFO=1,);
          
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                                INFO=1,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

#-----------------------------------------------------------------------
# REMARQUE : LE SOLVEUR CONSCIEMMENT UTILISE ICI EST LDLT BIEN QUE MUMPS
#            SOIT RECOMMANDE AVEC LA METHODE CONTINUE POUR LE CONTACT.
#            CELA PROVOQUE L'EMISSION D'UNE ALARME DANS STAT_NON_LINE.
#-----------------------------------------------------------------------


UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,              
                   EXCIT=(_F(CHARGE=CH1,), 
                          _F(CHARGE=CHXFEM,),),                  
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   SOLVEUR=_F(METHODE='LDLT',
                              RENUM='RCMK',
                              STOP_SINGULIER='OUI',
                                  NPREC=8,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);


LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                           # maillage 5_20_20
                             NOEUD=('N2493','N2043','N2042','N2041','N2040','N2039','N2038','N2037',
                                    'N2036','N2035','N2034','N2033','N2032','N2031','N2030','N2029',
