DEBUT(CODE=_F(NOM='ZZZZ299A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MATER=DEFI_MATERIAU(ELAS=_F(E=2.1e11,
                            NU=0.3,),);

AFFMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                     AFFE=_F(TOUT='OUI',
                             MATER=MATER,),);

AFFMOD=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='POU_D_E',),);

AFFCI=AFFE_CHAR_CINE(MODELE=AFFMOD,
                     MECA_IMPO=(_F(GROUP_NO='A',
                                   DX=0,
                                   DY=0,
                                   DZ=0,
                                   DRX=0,
                                   DRY=0,
                                   DRZ=0,),
                                _F(GROUP_NO='B',
                                   DX=0,
                                   DY=0,
                                   DZ=0,),
                                _F(GROUP_NO='C',
                                   DY=0.03,),),);

CAREL=AFFE_CARA_ELEM(MODELE=AFFMOD,
                     INFO=2,
                     POUTRE=_F(GROUP_MA='Tout',
                               SECTION='GENERALE',
                               CARA=('A','IY','IZ','JX','RY','RZ','RT',),
                               VALE=(0.02,0.166666666666E-4,0.666666666666E-4,0.45776E-4,0.1,0.05,0.0892632,),),
                     ORIENTATION=_F(GROUP_MA='Tout',
                                    CARA='ANGL_NAUT',
                                    VALE=(45,0,0,),),);

MESTA=MECA_STATIQUE(MODELE=AFFMOD,
                    CHAM_MATER=AFFMAT,
                    CARA_ELEM=CAREL,
                    EXCIT=_F(CHARGE=AFFCI,),);

MESTA=CALC_CHAMP(reuse=MESTA,
                 RESULTAT=MESTA,
                 CONTRAINTE=('EFGE_NOEU',),);

TEST_RESU(RESU=(_F(RESULTAT=MESTA,
                   GROUP_NO='POINT',
                   NOM_CHAM = 'DEPL',
                   NOM_CMP='DX',
                   INST=0.,
                   PRECISION=1.E-5,
                   VALE=2.5064E-03,
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=MESTA,
                   GROUP_NO='POINT',
                   NOM_CHAM = 'DEPL',
                   NOM_CMP='DY',
                   INST=0.,
                   PRECISION=1.E-5,
                   VALE=1.8357E-2,
                   REFERENCE='NON_REGRESSION',),),);

FIN();
