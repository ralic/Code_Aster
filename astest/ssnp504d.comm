# CONTACT-FROTTANT EN GRANDS GLISSEMENTS
# AVEC XFEM POUR DES FISSURES OBLIQUES

DEBUT(CODE=_F(NOM='SSNP504D',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

PRE_GIBI();

typma=2

if (typma==1):
  MAILLAG2=LIRE_MAILLAGE(INFO=1,);

if (typma==2):
  MAILLAG1=LIRE_MAILLAGE(INFO=1,);
  MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA='SURF',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                  MAILLAGE=MAILLAG2,
                  CREA_GROUP_NO=(_F(NOM='PHAU',
                                    OPTION ='ENV_SPHERE',
                                    POINT  = (0.0,9.0),
                                    RAYON = 1E-3,
                                    PRECISION = 2E-3,),),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                  MAILLAGE=MAILLAG2,
                  CREA_GROUP_NO =(_F(NOM = 'COTEG',
                                     NOEUD = ('N43','N44','N45','N46','N47','N48',
                                              'N49','N50','N51','N52','N53','N54',
                                              'N55','N56','N57','N58',),),),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,2.125,),
                                       VECT_NORMALE = (-0.24, 1.0,),
                                       PRECISION = 0.125,),),)

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('SURF',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN_XFEM_CONT',),),);       

LN1=FORMULE(NOM_PARA=('X','Y'),VALE='Y-0.24*X-2.125');
                    
LN2=FORMULE(NOM_PARA=('X','Y'),VALE='-Y+0.24*X+6.125');
 
 
FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN1,),);

FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN2,),);
                    
MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='P1P1A',
                         INFO=1,);
                         
CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      REAC_GEOM      = 'AUTOMATIQUE',
                      ITER_FROT_MAXI = 10,
                      ITER_CONT_MAXI = 4,
                      ITER_GEOM_MAXI = 10,
                      FROTTEMENT     = 'COULOMB',
                      ZONE = (_F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,   
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',   
                                 COEF_ECHELLE  = 1.,
                                 TOLE_PROJ_EXT = 0.,
                                 FISS_MAIT     = FISS1,
                                 ALGO_LAGR     = 'VERSION2',
                              ),
                             _F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,  
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'NON',   
                                 COEF_ECHELLE  = 1.,
                                 TOLE_PROJ_EXT = 0.,
                                 FISS_MAIT     = FISS2,
                                 ALGO_LAGR     = 'VERSION2',
                              ),
                            )
                    );                          


E=100.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                                MATER=ACIER,),);

DEPX=2.0
DEPY=1.E-2
ZERO=0.0


CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIGINF',DY=DEPY,),
                             _F(GROUP_MA='LIGSUP',DY=-DEPY,),
                             _F(NOEUD=('N1','N2','N3','N4',),DX=0.0,),),
                   INFO=1,);
                    
CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_NO='COTEG',DX=DEPX,),),
                   
                   # CONDITIONS IMPOSEES A LA MAIN
                   # POUR LES MAILLES XFEM INFERIEURE
                   # ET SUPERIEURE
                    LIAISON_DDL=(_F(NOEUD=('N42','N42',),
                                   DDL=('DX','H1X',),
                                   COEF_MULT=(1,1),
                                   COEF_IMPO=DEPX,),
                                _F(NOEUD=('N59','N59',),
                                   DDL=('DX','H1X',),
                                   COEF_MULT=(1,1),
                                   COEF_IMPO=DEPX,),),
                   INFO=1,);  
                            
VAR1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);
                    
VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    PROL_DROITE='CONSTANT',
                    PROL_GAUCHE='CONSTANT',);               
   
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      CONTACT_XFEM=CTXFEM,
                      LIAISON_XFEM='OUI',
                      INFO=1,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4,),);
                                    
UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT, 
                    CONTACT = CTXFEM,             
                    EXCIT=(
                           _F(CHARGE=CHXFEM,),                                           
                           _F(CHARGE=CH1,FONC_MULT=VAR1),
                           _F(CHARGE=CH2,FONC_MULT=VAR2),),  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=20,
                                    RESI_GLOB_RELA=1E-06)),
                    NEWTON=_F(REAC_ITER=1,),
                    SOLVEUR=_F(METHODE='MUMPS',
                               PCENT_PIVOT=10,
                               NPREC=-1,),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);

LAG = [None]*5
inst = [0.0,0.25,0.5,0.75,1.0]
vale = [[0.0,-5.6047280890127E+04,-5.3536786328800E+04,-4.4909070491740E+04,0.0],
        [0.0,-1.6263864003003E+05,-1.1063071404889E+05,-9.1544228204682E+04,0.0],
        [0.0,0.0,-1.7966653849833E+05,-1.4075454525010E+05,0.0],
        [0.0,0.0,-3.4348433995141E+05,-1.9708554106779E+05,0.0]]
        
if (typma==1):
  noeud = ['N45','N230','N249','N268','N83']

if (typma==2):
  noeud = ['NS283','NS321','NS362','NS403','NS444']

for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                         GROUP_NO=('NFISSU'),
                                         RESULTAT=UTOT1,
                                         NOM_CHAM='DEPL',
                                         INST=inst[i],
                                         NOM_CMP=('LAGS_C','LAGS_F1',),
                                         OPERATION='EXTRACTION',),);
   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              FORMAT_C='MODULE_PHASE',
              NOM_PARA=('NOEUD','INST','COOR_X','LAGS_C','LAGS_F1',),
              IMPR_FONCTION='NON',
              FILTRE=_F(NOM_PARA='LAGS_F1',
                        CRIT_COMP='NE',
                        VALE=0.0,
                        PRECISION=1.E-1,
                        CRITERE='ABSOLU',),
              TRI=_F(NOM_PARA=('COOR_X',),
                     ORDRE='CROISSANT',),);
   
   for j in [0,1,2,3,4]: #Boucle sur les positions testees
      if (vale[i-1] [j] == 0.0):
        precision = 1.0E-12
        critere = 'ABSOLU'
      else:
        precision = 5.E-2 
        critere = 'RELATIF'
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE=vale[i-1] [j],
                 CRITERE=critere,
                 PRECISION=precision,
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=(_F(NOM_PARA='NOEUD',
                           CRIT_COMP='EQ',
                           VALE_K=noeud[j],),),);

FIN();                  

