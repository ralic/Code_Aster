DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

# CUBE EN TRACTION SIMPLE CALCUL 3D
# LOI VISC_TAHERI

MAIL=LIRE_MAILLAGE( )

# parametres du ssnv200a avec C1=0
R_0_T=DEFI_CONSTANTE(VALE = 72.0 )
ALPHA_T=DEFI_CONSTANTE(VALE = 0.3 )
M_T=DEFI_CONSTANTE(VALE = 0.1 )
A_T=DEFI_CONSTANTE(VALE = 312.0 )
B_T=DEFI_CONSTANTE(VALE = 30.0 )
C1_T=DEFI_CONSTANTE(VALE = 0. )
C_INF_T=DEFI_CONSTANTE(VALE = 0.065 )
S_T=DEFI_CONSTANTE(VALE = 450.0 )

MAT=DEFI_MATERIAU(
            ELAS=_F( E = 200.E+3, NU = .3),
            LEMAITRE=_F(N=10.8,UN_SUR_K=6.9E-4,UN_SUR_M=0.102),
            TAHERI_FO=_F(  R_0 = R_0_T,
                        ALPHA = ALPHA_T,
                        M = M_T,
                        A = A_T,
                        B = B_T,
                        C1 = C1_T,
                        C_INF = C_INF_T,
                        S = S_T,)
                     )

CM=AFFE_MATERIAU(MAILLAGE=MAIL,
                 AFFE=_F( TOUT = 'OUI', MATER = MAT)
                     )

MOMECA=AFFE_MODELE(MAILLAGE=MAIL,
   AFFE=_F( TOUT = 'OUI', MODELISATION = '3D', PHENOMENE = 'MECANIQUE')
                     )

CHMECA=AFFE_CHAR_MECA(  MODELE=MOMECA,
                        FACE_IMPO=(
                              _F( MAILLE = 'DEVANT',  DZ = 0.),
                              _F( MAILLE = 'BAS',     DY = 0.),
                              _F( MAILLE = 'GAUCHE',  DX = 0 ),
                                   ),
                        PRES_REP =_F(MAILLE='HAUT', PRES = -520),
                               )

FPRES=DEFI_FONCTION(    NOM_PARA='INST',
                        PROL_DROITE='EXCLU',
                        PROL_GAUCHE='EXCLU',
                        VALE=(0., 0.,120., 1.0, 240., -1.0,))

L_INST=DEFI_LIST_REEL(  DEBUT=0.,INTERVALLE=(
                           _F( JUSQU_A = 240.,NOMBRE = 30),
                                 )
                         )

U=STAT_NON_LINE(        MODELE=MOMECA,
                        CHAM_MATER=CM,
                        EXCIT=(_F(     CHARGE = CHMECA,
                                       FONC_MULT = FPRES),),
                        COMPORTEMENT=_F(RELATION = 'VISC_TAHERI',
                                     ITER_INTE_MAXI = 100),
                        NEWTON=_F(MATRICE = 'TANGENTE', REAC_ITER = 1),
                        INCREMENT=_F(LIST_INST = L_INST),
                        CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-4,
                                       ITER_GLOB_MAXI = 250,),
                         )

U=CALC_CHAMP(reuse=U,CONTRAINTE=('SIGM_ELNO'),DEFORMATION=('EPSI_ELNO','EPSP_ELNO'),RESULTAT=U)


TEST_RESU(RESU=(_F(INST=40.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N6',
                   NOM_CMP='SIYY',
                   VALE_CALC=173.333155641,
                   VALE_REFE=173.333155641,
                   MAILLE='M1',),
                _F(INST=168.0,
                   REFERENCE='AUTRE_ASTER',
                   RESULTAT=U,
                   NOM_CHAM='SIGM_ELNO',
                   NOEUD='N6',
                   NOM_CMP='SIYY',
                   VALE_CALC=104.0,
                   VALE_REFE=104.0,
                   MAILLE='M1',),
                ),
          )

FIN()
