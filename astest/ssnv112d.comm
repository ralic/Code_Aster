# CYLINDRE CREUX EN INCOMPRESSIBLE (NU=0.4999)
# SOUS PRESSION INTERNE (60 MPA) - GRANDES DEFORMATIONS
#
# MODELISATION : ELEMENTS MECANIQUE AXIS_INCO (TRIA6 QUAD8)
# MAILLAGE     : ALIBABA > ASTER
# ----------------------------------------------------------------------
# GROUPES DE NOEUDS ET MAILLES: A B C D E F  NOEUMIA NOEUMIB  FACESUP   FACEINF
#             BORDAB=GRNM11     FACEEF=GRMA12     FACEAE=GRMA13
# ----------------------------------------------------------------------

DEBUT(CODE=_F(NOM='SSNV112D', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE();

PRES = FORMULE(VALE='60.',
               NOM_PARA=('X','Y',),);

MO=AFFE_MODELE(MAILLAGE=M,
               VERIF='MAILLE',
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='AXIS_INCO_GD',),);

MA=DEFI_MATERIAU(ELAS=_F(E=200000.0,
                         NU=0.4999,
                         ALPHA=0.0,),
                 ECRO_LINE=_F(SY=1.E10, D_SIGM_EPSI=0.)        );

CM=AFFE_MATERIAU(MAILLAGE=M,
                 AFFE=_F(TOUT='OUI',
                         MATER=MA,),);

CLIM=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_NO='FACINF',
                                 DY=0.0,),
                              _F(GROUP_NO='FACSUP',
                                 DY=0.0,),),);

CH=AFFE_CHAR_MECA(MODELE=MO,
                  FACE_IMPO=_F(GROUP_MA='GRMA13',
                               DX=6e-05,),);
#                       PRES_REP=_F(  GROUP_MA = 'GRMA13',  PRES = PRES) )

L_INST1=DEFI_LIST_REEL(DEBUT=0.0,
                       INTERVALLE=_F(JUSQU_A=1.0,
                                     NOMBRE=1,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0,),
                    PROL_DROITE='CONSTANT',);

EVOL=STAT_NON_LINE(SOLVEUR=_F(METHODE='MUMPS',PARALLELISME='DISTRIBUE_MD',PCENT_PIVOT=300),
                   MODELE=MO,
                   CHAM_MATER=CM,
                   EXCIT=(_F(CHARGE=CLIM,),
                          _F(CHARGE=CH,
                             FONC_MULT=RAMPE,),),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                                DEFORMATION='SIMO_MIEHE',),
                   INCREMENT=_F(LIST_INST=L_INST1,),
                   NEWTON=_F(REAC_ITER=1,),

                   CONVERGENCE=_F(RESI_REFE_RELA=1.E-3,
                            SIGM_REFE = 60 ,
                            EPSI_REFE = 3.E-4), );

EVOL=CALC_ELEM(reuse =EVOL,
               RESULTAT=EVOL,
               OPTION=(
               #'EPSI_ELNO_DEPL',
               'SIEF_ELNO_ELGA','EQUI_ELNO_SIGM',
               #'EQUI_ELNO_EPSI','ERRE_ELEM_SIGM','ERRE_ELNO_ELEM',
               ),);

EVOL=CALC_NO(reuse =EVOL,
             RESULTAT=EVOL,
             OPTION=(
             #'EPSI_NOEU_DEPL',
             'SIEF_NOEU_ELGA',
             #'EQUI_NOEU_EPSI',
             'EQUI_NOEU_SIGM',),);
