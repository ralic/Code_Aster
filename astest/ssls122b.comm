# AUTEUR: F. LEBOUVIER (DeltaCAD)
# DATE  : 12/12/03
#
# Description du modele :
#
#  La plaque est composee de deux parties (P1,P2):
#     - La partie P1 est composee de deux maillages
#     - La partie P2 est composee d'un seul maillage
#
#  Le plan moyen du maillage passe par le plan moyen de la plaque P2
#
#                               E                  D
#                              --------------------
#                             /                  /
#                            /    PLAQ_P1S      /
#                           /                  /
#      Z                  B ------------------- C
#    /      Y
#    /  F /       H         E
#    /   -------------------                 Normal (<=> Z)
#    /  /                 /                /
#    / /    PLAQ_P2      /                 /
#    //               B /                  /
#    /---------------------->X
#     A        G        B       E                  D
#                              --------------------
#                             /                  /
#                            /    PLAQ_P2I      /
#                           /                  /
#                         B ------------------- C
#
# MAILLAGE
#   - Modelisation DKT
#   - Mailles QUAD4
#
#---------------------------------------------------------------
# PARAMETRES
#---------------------------------------------------------------
#
# - Unite : SI (M,K,s,A)
#
# - Caracteristiques geometriques
#
# -----> Structure
longueur = 10.
largeur  =  1.

# -----> partie P1

epais_P1S =  0.05     # Epaisseur;
excen_P1S =  0.015    # Excentricite;
epais_P1I =  0.05     # Epaisseur;
excen_P1I = -0.035    # Excentricite;

# -----> partie P2

epais_P2 = 0.08       # Epaisseur;
excen_P2 = 0.0        # Excentricite;

# - Caracteristiques mecanique
#
# ----> Materiau  (acier)

young=2.1E11
poisson=0.3

# Chargement au centre suivant -Z

force_totale = 200.E3
force= -force_totale / largeur

# - Modele Element finis

mod_EF='DKT'

# - Repere de calcul des efforts generalises

# ----> [ANGL_REP(alpha,beta)]

alpha=0.
beta=0.
#---------------------------------------------------------------

DEBUT(CODE=_F(NOM='SSLS122B',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),);

MA=LIRE_MAILLAGE(FORMAT='MED',INFO=2);

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=(_F(GROUP_MA='FA',),
                             _F(GROUP_MA='CD',),),
              CREA_GROUP_MA=(_F(GROUP_MA='PLAQ_P1',NOM='PLAQ_P1S',),),
              );

MAIL=CREA_MAILLAGE(MAILLAGE=MA,INFO=2,
                 CREA_GROUP_MA=_F(NOM='PLAQ_P1I',
                                  GROUP_MA='PLAQ_P1S',
                                  PREF_MAILLE='S',),);

MODEL=AFFE_MODELE(MAILLAGE=MAIL,
                  AFFE=_F(TOUT='OUI',
                          PHENOMENE='MECANIQUE',
                          MODELISATION=mod_EF,),);
#
# On impose l'orientation de la normale vers le Z Global
#
MAIL=MODI_MAILLAGE(MAILLAGE=MAIL,reuse=MAIL,
                      ORIE_NORM_COQUE=_F(GROUP_MA=('PLAQ_P2','PLAQ_P1S','PLAQ_P1I',),
                                         VECT_NORM=(0.0,0.0,1.0),
                                         GROUP_NO='B',),
                      );
#
# Definition
#   - des epaisseurs
#   - des excentricites
#   - du repere de calcul des efforts generalises

CARA=AFFE_CARA_ELEM(MODELE=MODEL,
                    COQUE=(
                           _F(GROUP_MA=('PLAQUE',),
                              EPAIS=epais_P2,
                              ANGL_REP=(alpha,beta,),
                              EXCENTREMENT=excen_P2,
                              INER_ROTA='OUI',),

# Plaque P2
                           _F(GROUP_MA='PLAQ_P2',
                              EPAIS=epais_P2,
                              ANGL_REP=(alpha,beta,),
                              EXCENTREMENT=excen_P2,
                              INER_ROTA='OUI',),
# Plaque P1
                           _F(GROUP_MA='PLAQ_P1S',
                              EPAIS=epais_P1S,
                              ANGL_REP=(alpha,beta,),
                              EXCENTREMENT=excen_P1S,
                              INER_ROTA='OUI',),

                           _F(GROUP_MA='PLAQ_P1I',
                              EPAIS=epais_P1I,
                              ANGL_REP=(alpha,beta,),
                              EXCENTREMENT=excen_P1I,
                              INER_ROTA='OUI',),
                           ),
                    );



ACIER=DEFI_MATERIAU(ELAS=_F(E=young,
                            NU=poisson,),);

MATER=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);
#
# Conditions aux limites en deplacement
#
COND_LIM=AFFE_CHAR_MECA(MODELE=MODEL,
                        DDL_IMPO=(
                                  _F(GROUP_NO=('FA','CD',),
                                     DZ=0.0,),
                                  _F(GROUP_NO=('FA',),
                                     DX=0.0,),
                                  _F(GROUP_NO='A',
                                     DX=0.0,
                                     DY=0.0,
                                     DRZ=0.0,),),);
# Chargement

FORCE=AFFE_CHAR_MECA(MODELE=MODEL,
                     FORCE_ARETE=_F(GROUP_MA='BE', FZ=force,),);

RESU=MECA_STATIQUE(MODELE=MODEL,
                   CHAM_MATER=MATER,
                   CARA_ELEM=CARA,
                   EXCIT=(_F(CHARGE=COND_LIM,),
                          _F(CHARGE=FORCE,),),);

RESU=CALC_CHAMP(reuse=RESU,CONTRAINTE=('EFGE_ELNO'),RESULTAT=RESU)


RESU=CALC_CHAMP(reuse =RESU,


             CONTRAINTE='EFGE_NOEU',
             RESULTAT=RESU,);
#------------------------------------------------------
# Test sur les deplacements et les rotations
#          aux points B et E
#------------------------------------------------------
TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-0.350617282,
                   VALE_REFE=-0.3515625,
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='B',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DRZ',
                   VALE_CALC= 0.00000000E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',),
                ),
          )

TEST_RESU(RESU=(_F(NUME_ORDRE=1,
                   GROUP_NO='G',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DZ',
                   VALE_CALC=-0.262474165,
                   VALE_REFE=-0.26297429999999999,
                   PRECISION=1.E-2,),
                _F(NUME_ORDRE=1,
                   GROUP_NO='G',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DRZ',
                   VALE_CALC= 0.00000000E+00,
                   VALE_REFE=0.0,
                   CRITERE='ABSOLU',),
                ),
          )

#------------------------------------------------------
# Test sur les efforts normaux cote plaque P2 (GAUCHE)
#------------------------------------------------------
#
# Point G
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='G',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU,
                  NOM_CHAM='EFGE_ELNO',
                  NOM_CMP='MXX',
                  VALE_CALC=-2.50000000E+05,
                  VALE_REFE=-2.50000E5,
                  MAILLE='M10',),
          )

# Point B
TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='B',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=RESU,
                  NOM_CHAM='EFGE_ELNO',
                  NOM_CMP='MXX',
                  VALE_CALC=-5.00000000E+05,
                  VALE_REFE=-5.E5,
                  MAILLE='M12',),
          )

#---------------------------------------------------------------------------------------
# Validation de CREA_CHAMP / EXTR + CARA_ELEM :
#-----------------------------------------------
#IMPR_CO(CONCEPT=_F(NOM=CARA), NIVEAU=-1)

# Champ .CARCOQUE (carte) :
#---------------------------
CACOQU=CREA_CHAMP(TYPE_CHAM='CART_CACOQU', OPERATION='EXTR', CARA_ELEM=CARA,
               NOM_CHAM='.CARCOQUE', INFO=2,)
TEST_RESU(CARTE=_F(REFERENCE='ANALYTIQUE', NOM_CMP='EP',  VALE_REFE=8.0E-2,
          VALE_CALC=8.0E-2, MAILLE='M14', CHAM_GD=CACOQU,))
TEST_RESU(CARTE=_F(REFERENCE='ANALYTIQUE', NOM_CMP='KAPPA', VALE_REFE=8.333333E-01,
          VALE_CALC=8.333333E-01, MAILLE='M14', CHAM_GD=CACOQU,))

# On verifie egalement que la carte extraite peut etre transformee en table :
TABCOQ=CREA_TABLE(RESU=_F(CHAM_GD=CACOQU, MAILLE=('M13','M14'), TOUT_CMP='OUI',),)
#IMPR_TABLE(TABLE=TABCOQ)
TEST_TABLE(REFERENCE='ANALYTIQUE',NOM_PARA='EP',TABLE=TABCOQ,
           FILTRE=_F(NOM_PARA='MAILLE',VALE_K='M14',),
           VALE_CALC= 8.E-2, VALE_REFE=8.E-2,)

# Champ .CANBSP (cham_elem) :
#----------------------------
CANBSP=CREA_CHAMP(TYPE_CHAM='ELEM_NBSP_I', OPERATION='EXTR', CARA_ELEM=CARA,
               NOM_CHAM='.CANBSP', INFO=2,)
TEST_RESU(CHAM_ELEM=_F( CHAM_GD = CANBSP, MAILLE='SM3', POINT=1, SOUS_POINT=1,
          REFERENCE='ANALYTIQUE', NOM_CMP = 'COQ_NCOU', VALE_REFE_I=1, VALE_CALC_I=1,))

#---------------------------------------------------------------------------------------


#---------------------------------------------------------------------------------------
# Validation de CREA_CHAMP / EXTR + CHAR_MECA :
#-----------------------------------------------
#IMPR_CO(CONCEPT=_F(NOM=FORCE), NIVEAU=-1)

# Champ .CHME.F1D3D (carte) :
#---------------------------
F1D3D=CREA_CHAMP(TYPE_CHAM='CART_FORC_R', OPERATION='EXTR', CHARGE=FORCE,
               NOM_CHAM='.CHME.F1D3D', INFO=2,)
TEST_RESU(CARTE=_F(REFERENCE='ANALYTIQUE', NOM_CMP='FZ', VALE_CALC=-2.E5, VALE_REFE=-2.E5,
          MAILLE='M13', CHAM_GD=F1D3D,))

# On verifie egalement que la carte extraite peut etre transformee en table :
TABF13=CREA_TABLE(RESU=_F(CHAM_GD=F1D3D, MAILLE=('M13','M14'), TOUT_CMP='OUI',),)
#IMPR_TABLE(TABLE=TABF13)
TEST_TABLE(REFERENCE='ANALYTIQUE',NOM_PARA='FZ',TABLE=TABF13,
           FILTRE=_F(NOM_PARA='MAILLE',VALE_K='M13',),
           VALE_CALC=-2.E5 , VALE_REFE=-2.E5,)

#---------------------------------------------------------------------------------------

FIN();
