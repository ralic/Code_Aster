        NOM_CMP = 'SIXX',
        VALE = -1.6512032085562E+04, REFERENCE = 'ANALYTIQUE' )))

TEST_RESU(RESU=(
   _F(  RESULTAT = RESU,   NUME_ORDRE = 2,
        NOM_CHAM = 'SIEF_ELNO',   MAILLE = 'M1',
        SENSIBILITE=(PS_E),
        NOEUD = 'N1',
        CRITERE = 'RELATIF', PRECISION = 1.E-4,
        NOM_CMP = 'SIXX',
        VALE = -1.6665743229718E-01, REFERENCE = 'ANALYTIQUE' )))


# SENSIBILITE PAR RAPPORT A E

TEST_RESU(RESU=(
