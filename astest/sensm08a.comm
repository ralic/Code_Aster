      CRITERE = 'RELATIF', PRECISION = 1.E-4,
      NOM_CMP = 'SIXX',   SENSIBILITE = PS_SIGY,
      VALE = 3.2754E-1, REFERENCE = 'ANALYTIQUE' ),
