TEST_RESU(RESU=(
   _F(RESULTAT = RESU,
      INST = 0.5,
      MAILLE = 'M1',POINT = 1,
      NOM_CHAM = 'SIEF_ELGA',
      CRITERE = 'RELATIF', PRECISION = 1.E-4,
      NOM_CMP = 'SIYY',   SENSIBILITE = PS_SIGY,
      VALE = -6.5508E-1, REFERENCE = 'ANALYTIQUE' ),
   _F(RESULTAT = RESU,
