DEBUT(CODE=_F( NOM = 'SSNP104B',NIV_PUB_WEB='INTERNET')
        )

#......................................................................
# EPROUVETTE DE BETON EN COMPRESSION - CISAILLEMENT
# ELASTOPLASTICITE  NADAI_B (COQUES)
#......................................................................


M=LIRE_MAILLAGE( )

M=DEFI_GROUP( reuse=M,   MAILLAGE=M,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(
            MAILLAGE=M,    VERIF='MAILLE',
           AFFE=_F( TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = 'DKT')
           )

EPRO=DEFI_MATERIAU(
                ELAS=_F( E = 34000., NU = 0.18),

                NADAI_B=_F(
                F_C = 32.8,
                F_T = 3.28,
                CRIT_E_C = 0.,
                EPSP_R_C = 5.E-3,
                EPSP_P_C = 9.64705E-4,
                EPSI_R_T = 5.E-4,
                FAC_T_C = 0.4)
           )

CM=AFFE_MATERIAU(
            MAILLAGE=M,
           AFFE=_F( TOUT = 'OUI',MATER = EPRO)
           )

LACAR=AFFE_CARA_ELEM(    MODELE=MO,
                           COQUE=_F(
       GROUP_MA = 'TOUT',

                                   EPAIS = 1.)
                        )

LIAISON=AFFE_CHAR_MECA(
            MODELE=MO,DDL_IMPO=(
           _F( NOEUD = 'NO4',  DX = 0., DY = 0., DZ = 0.,
                              DRX = 0., DRY = 0., DRZ = 0.),
           _F( NOEUD = 'NO2',  DX = 0., DZ = 0.,
                              DRX = 0., DRY = 0., DRZ = 0.),
           _F( NOEUD = 'NO3',  DZ = 0.,
                              DRX = 0., DRY = 0., DRZ = 0.),
           _F( NOEUD = 'NO1',  DZ = 0.,
                              DRX = 0., DRY = 0., DRZ = 0.))
                         )

COMPRESS=AFFE_CHAR_MECA(
            MODELE=MO,FORCE_NODALE=(
           _F( NOEUD = 'NO1',       FX = 10.),
                        _F( NOEUD = 'NO3',       FX = 10.))
           )

CISAIL=AFFE_CHAR_MECA(
            MODELE=MO,FORCE_NODALE=(
           _F( NOEUD = 'NO1',       FY = -3.),
                        _F( NOEUD = 'NO3',       FY = -3.),
                        _F( NOEUD = 'NO2',       FY = 3.),
                        _F( NOEUD = 'NO4',       FY = 3.),
                        _F( NOEUD = 'NO1',       FX = 3.),
                        _F( NOEUD = 'NO2',       FX = 3.),
                        _F( NOEUD = 'NO3',       FX = -3.),
                        _F( NOEUD = 'NO4',       FX = -3.))
           )

SIGMA_F=DEFI_FONCTION(   NOM_PARA='INST',
           PROL_GAUCHE='LINEAIRE',  PROL_DROITE='LINEAIRE',
           VALE=(0.0,0.0,1.0,1.0,))

TAU_F=DEFI_FONCTION(   NOM_PARA='INST',
           PROL_GAUCHE='LINEAIRE',  PROL_DROITE='LINEAIRE',
           VALE=(0.0,0.0,1.0,1.0,))

L_INST=DEFI_LIST_REEL(  DEBUT=0.0,INTERVALLE=(
             _F( JUSQU_A = 0.01, NOMBRE = 1),
                        _F( JUSQU_A = 0.5,  NOMBRE = 2))
           )

# STAT_NON_LINE 'NADAI_B'
#-------------------------------



U=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CM,
                         CARA_ELEM=LACAR,EXCIT=(
                             _F(
                CHARGE = COMPRESS,
                FONC_MULT = SIGMA_F),
                             _F(
                CHARGE = CISAIL,
                FONC_MULT = TAU_F), _F(
                CHARGE = LIAISON)),
                         COMP_INCR=_F(
                RELATION = 'NADAI_B',
                TOUT = 'OUI'),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 3),
                       CONVERGENCE=_F(
                RESI_GLOB_MAXI = 1.E-6,
                ITER_INTE_MAXI = 10,
                ITER_GLOB_MAXI = 30,
                RESI_INTE_RELA = 1.E-8),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 0)
              )

U=CALC_ELEM( reuse=U,
                            OPTION=(
                  'SIEF_ELNO_ELGA',  'VARI_ELNO_ELGA',),
                          RESULTAT=U,
              )

U=CALC_ELEM(
              reuse=U,
                  RESULTAT=U,
                   OPTION=('EPSI_ELNO_DEPL','SIGM_ELNO_COQU',)
              )

VARI=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELNO_VARI_R',
NOM_CHAM='VARI_ELNO_ELGA',
              RESULTAT=U,
              INST=0.5
              )

# IMPR_RESU ( MODELE:MO
#              RESU  :
#                (
#                RESULTAT: U
#                FORMAT  :'RESULTAT'
#                TOUT    :'OUI'
#                )
#          ) ;

TEST_RESU(RESU=(
                _F(
                RESULTAT = U,
                NOM_CHAM = 'SIEF_ELNO_ELGA',
                MAILLE = 'MA1',
                NOEUD = 'NO2',
                INST = 0.5,
                NOM_CMP = 'NXX',
                VALE = -10.,
                REFERENCE='NON_REGRESSION',),
                _F(
                RESULTAT = U,
                NOM_CHAM = 'SIEF_ELNO_ELGA',
                MAILLE = 'MA1',
                NOEUD = 'NO2',
                INST = 0.5,
               NOM_CMP = 'NXY',
                REFERENCE='NON_REGRESSION',
                VALE = 3.),
                _F(
                RESULTAT = U,
                NOM_CHAM = 'SIEF_ELNO_ELGA',
                MAILLE = 'MA1',
                NOEUD = 'NO2',
                INST = 0.5,
                NOM_CMP = 'NYY',
                VALE = 0.,
                REFERENCE='NON_REGRESSION',
                CRITERE = 'ABSOLU'),
                _F(
                RESULTAT = U,
                NOM_CHAM = 'EPSI_ELNO_DEPL',
                MAILLE = 'MA1',
                NOEUD = 'NO2',
                INST = 0.5,
                NOM_CMP = 'EPXX',
                REFERENCE='NON_REGRESSION',
                VALE = -3.46974963E-4),
                _F(
                RESULTAT = U,
                NOM_CHAM = 'EPSI_ELNO_DEPL',
                MAILLE = 'MA1',
                NOEUD = 'NO2',
                INST = 0.5,
                NOM_CMP = 'EPXY',
                REFERENCE='NON_REGRESSION',
                VALE = 1.31666602E-4),
                _F(
                RESULTAT = U,
                NOM_CHAM = 'EPSI_ELNO_DEPL',
                MAILLE = 'MA1',
                NOEUD = 'NO2',
                INST = 0.5,
                NOM_CMP = 'EPYY',
                REFERENCE='NON_REGRESSION',
                VALE = 9.1913711E-5)),


               CHAM_ELEM=_F(  CHAM_GD = VARI,
                MAILLE = 'MA1',
                NOEUD = 'NO2',
                NOM_CMP = 'V1',
                REFERENCE='NON_REGRESSION',
                VALE = 6.0628739E-5)
             )

FIN()
