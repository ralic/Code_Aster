#       CAS_TEST SSNV209I: AXI, ELEMENTS TRI3, METHODE XFEM

DEBUT( CODE=_F(  NOM = 'SSNV209I',NIV_PUB_WEB='INTERNET'),)

#......................................................
#  CALCUL TEST ELEMENTS FINIS DE CONTACT 2D AXI TRI3
#  CYLINDRE EN CONTACT FROTTANT SUR BATI RIGIDE
#......................................................

PRE_GIBI()

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),)

MA=DEFI_GROUP(reuse =MA,
                    MAILLAGE=MA,
                    CREA_GROUP_NO=(_F(GROUP_MA='LCONTA',
                                      OPTION='NOEUD_ORDO',
                                      NOM='RELEVE',
                                      GROUP_NO_ORIG='PPA',
                                      GROUP_NO_EXTR='PPS',),
                                   _F(NOM='NFISSU',
                                      OPTION='PLAN',
                                      POINT=(0.,0.),
                                      VECT_NORMALE=(0.,1.),
                                      PRECISION=1.E-6,),),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT = 'OUI',
                       PHENOMENE = 'MECANIQUE',
                       MODELISATION = 'AXIS',)
              )

MA=MODI_MAILLAGE(reuse=MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=(_F(GROUP_MA = 'LPRESV',),
                               _F(GROUP_MA = 'LPRESH',),
                               _F(GROUP_MA = 'LCONTA',),
                               _F(GROUP_MA = 'LBATI',),
                               )
                )

MATPLA=DEFI_MATERIAU(ELAS=_F(E = 1.3E11,
                             NU = 0.2,)
                    )
MATBAT=DEFI_MATERIAU(ELAS=_F(E = 1.3E16,
                             NU = 0.0,)
                    )
CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=(_F(GROUP_MA ='SPLAQ',
                             MATER = MATPLA,),
                        _F(GROUP_MA ='SBATI',
                             MATER = MATBAT,),),
                    )
CHA1=AFFE_CHAR_MECA(MODELE=MO,
                    DDL_IMPO=(_F(GROUP_MA = 'LENCAV',  DX = 0.0, DY = 0.0,),
                                    ),
                    PRES_REP=(_F(GROUP_MA = 'LPRESV', PRES = 5.E07,),
                              )
                   )

def pr(y) :
   if y< 1.e-15 : return 0.E07
   if y> 1.e-15 : return  15.E07
   if y==1.e-15 : return  0.
FCTPRES = FORMULE(VALE='pr(Y)',
                  NOM_PARA=('X','Y'),);

CHAF=AFFE_CHAR_MECA_F(MODELE=MO,
                     PRES_REP=_F(GROUP_MA = 'LPRESH',
                                 PRES=FCTPRES,),);

# ==================================================
CHA2   = DEFI_CONTACT(MODELE         = MO,
                      FORMULATION    = 'CONTINUE',
                      ITER_FROT_MAXI = 10,
                      ITER_CONT_MAXI = 30,
                      ITER_GEOM_MAXI = 3,
                      FROTTEMENT     = 'COULOMB',
                      ZONE = (_F(
                                 GROUP_MA_MAIT  = 'LBATI',
                                 GROUP_MA_ESCL  = 'LCONTA',
                                 APPARIEMENT    = 'MAIT_ESCL',
                                 INTEGRATION    = 'NOEUD',
                                 CONTACT_INIT   = 'OUI',
                                 SEUIL_INIT     = 100.,
                                 COEF_FROT = 1.0,
                                 COEF_CONT = 1.0,
                                 EXCLUSION_PIV_NUL='OUI',
                                 COULOMB        = 1.,
                              ),
                             
                           )
                     ); 


RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    PROL_GAUCHE='LINEAIRE',
                    PROL_DROITE='LINEAIRE',
                    VALE=(0.0,0.0,1.0,1.0,)
                    )

L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A = 1.0, NOMBRE = 1,)
                      )

#-----------------------------------------------------------
#                   RESOLUTION
#-----------------------------------------------------------

U2M=STAT_NON_LINE(MODELE    = MO,
                  CHAM_MATER = CHMAT,
                  CONTACT    = CHA2,
                  EXCIT      =(_F(CHARGE = CHA1,
                                  FONC_MULT = RAMPE,),
                               _F(CHARGE = CHAF,
                                  FONC_MULT = RAMPE,),   
                               ),
                  COMP_ELAS  = _F(RELATION = 'ELAS',),
                  NEWTON=_F(REAC_ITER=1),
                  INCREMENT  = _F(LIST_INST = L_INST,),
                  CONVERGENCE= _F(ARRET = 'OUI',
                                 ITER_GLOB_MAXI = 50,
                                 RESI_GLOB_RELA = 1.0E-6,),
                  SOLVEUR=_F(METHODE='MUMPS',
                         PCENT_PIVOT=80,),
               )

IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=U2M,
                                 NOM_CHAM='DEPL',
                                 TYPE_CHAM='VECT_3D',
                                 NOM_CMP=('DX','DY','DZ',),),
                              ),);


#----------------------------------------------
#               TESTS NON- REGRESSION
#----------------------------------------------


TEST_RESU(RESU=(_F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPA',
                   NOM_CMP    = 'DX',
                   VALE       = -2.3712819136401E-05,
                   PRECISION  = 1.E-3,
                   REFERENCE  = 'NON_REGRESSION',
                   VERSION    = '10.0.27',
                   ),
                _F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPC',
                   NOM_CMP    = 'DX',
                   VALE       = -1.8444609394619E-05,
                   PRECISION  = 1.E-3,
                   REFERENCE  = 'NON_REGRESSION',
                   VERSION    = '10.0.27',
                   ),
                _F(RESULTAT   = U2M,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'PPD',
                   NOM_CMP    = 'DX',
                   VALE       = -1.5684504791382E-05,
                   PRECISION  = 1.E-3,
                   REFERENCE  = 'NON_REGRESSION',
                   VERSION    = '10.0.27',
                   ),
                ),);

FIN()
