DEBUT( CODE=_F(  NOM = 'SSNV189A',NIV_PUB_WEB='INTERNET') );

maillage=LIRE_MAILLAGE(FORMAT='MED',VERI_MAIL=_F(APLAT=0.001,),);

maillage=DEFI_GROUP(reuse =maillage,
                    MAILLAGE=maillage,
                    CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

modmeca=AFFE_MODELE(MAILLAGE=maillage,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),);

elast=DEFI_MATERIAU(ELAS_HYPER=_F(C10=2.668,
                                C01=0.271,
                                C20=0.466,
                                K=2939,
                                RHO=1000.0,),);

chmat=AFFE_MATERIAU(MAILLAGE=maillage,
                    AFFE=_F(TOUT='OUI',
                            MATER=elast,),);

encastr=AFFE_CHAR_MECA(MODELE=modmeca,
                       DDL_IMPO=(_F(GROUP_MA='S4',
                                    DX=49.4142,
                                    DY=0,
                                    DZ=0,),
                                 _F(GROUP_MA='S6',
                                    DX=0,
                                    DY=0,
                                    DZ=0,),),);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=2.0,
                                    NOMBRE=50,),);

l=DEFI_FONCTION(NOM_PARA='INST',
                VALE=(0.0,0.0,1.0,1.0,2.0,0.0),);

RESU=STAT_NON_LINE(MODELE=modmeca,
                   CHAM_MATER=chmat,
                   EXCIT=_F(CHARGE=encastr,
                            FONC_MULT=l,),
                   COMP_ELAS=_F(RELATION='ELAS_HYPER',
                                DEFORMATION='GROT_GDEP',
                                TOUT='OUI',),
                   INCREMENT=_F(LIST_INST=L_INST,
                                INST_FIN=2.0,),
                   NEWTON=_F(PREDICTION='TANGENTE',
                             MATRICE='TANGENTE',
                             REAC_ITER=1,),
                   CONVERGENCE=_F(RESI_GLOB_RELA=1E-6,
                                  ITER_GLOB_MAXI=25,),
                   ARCHIVAGE=_F(PAS_ARCH=1,),);

RESU=CALC_ELEM(reuse =RESU,


               RESULTAT=RESU,
               OPTION=('EPSG_ELNO','SIGM_ELNO',),);



RESU=CALC_NO(reuse =RESU,
             RESULTAT=RESU,
             OPTION='FORC_NODA',

             );

TAB_FORC=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                                 GROUP_NO='S4',
                                 RESULTAT=RESU,
                                 NOM_CHAM='FORC_NODA',
                                 LIST_INST=L_INST,
                                 RESULTANTE='DX',
                                 OPERATION='EXTRACTION',),);

IMPR_TABLE(TABLE=TAB_FORC)

FXA=RECU_FONCTION(TABLE=TAB_FORC,
                  PARA_X='INST',
                  PARA_Y='DX',);

DXA=RECU_FONCTION(RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  NOEUD= 'N2',);

IMPR_FONCTION(FORMAT='XMGRACE',
              COURBE=_F(FONC_X=DXA,
                        FONC_Y=FXA,),);

TEST_FONCTION(VALEUR=(
_F(FONCTION=FXA,
   REFERENCE='SOURCE_EXTERNE',
   VALE_PARA = 1.0,
   VALE_REFE=1180.67,
   PRECISION=0.005,
   CRITERE='RELATIF',),
            ),)

TEST_FONCTION(VALEUR=(
_F(FONCTION  = FXA,
   REFERENCE = 'NON_REGRESSION',
   VERSION   = '8.2.9',
   VALE_PARA = 1.0,
   VALE_REFE = 1186.2614145140,
   PRECISION = 1E-10,
   CRITERE   = 'RELATIF',),
            ),)

FIN();
