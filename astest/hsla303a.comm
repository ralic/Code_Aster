                )
TEST_RESU(RESU=_F(  RESULTAT = DEF_TH,
#D
                              NOM_CHAM='EPSI_ELNO',
                        NUME_ORDRE=1,
                        MAILLE = 'M10',
                        NOEUD = 'N4',
                        NOM_CMP = 'EPXX',
