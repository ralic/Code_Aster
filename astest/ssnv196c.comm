DEBUT( CODE=_F( NOM = 'SSNV196C',NIV_PUB_WEB='INTERNET'))

MA=LIRE_MAILLAGE();

MA=DEFI_GROUP(reuse =MA,
              MAILLAGE=MA,
              CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI',),);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D',),);

MATERIAU=DEFI_MATERIAU(ELAS=_F(E=10000.,
                               NU=0.25,),
                       ECRO_LINE=_F(D_SIGM_EPSI=1000.0,
                                    SY=200.0,),
                           );

CH_MATER=AFFE_MATERIAU(MAILLAGE=MA,
                       AFFE=_F(TOUT='OUI',
                               MATER=MATERIAU,),);

zero = DEFI_CONSTANTE(VALE=0.0);

hy=FORMULE(NOM_PARA=('Y'),VALE='15.*(1-(Y*Y)/4.)')


ch=AFFE_CHAR_MECA_F(MODELE=MO,
                        DDL_IMPO=_F(GROUP_MA=('VOL1',),
                                      DZ=zero,),
                        FACE_IMPO=_F(GROUP_MA=('CLIM',),
                                      DX=zero,DY=zero,),
                        FORCE_FACE=_F(GROUP_MA='CHAR',FY=hy,
                               ),)
#
#   TEST EN ELASTICITE
#
RESU=MECA_STATIQUE(MODELE=MO,
                   CHAM_MATER=CH_MATER,
                   EXCIT=_F(CHARGE=ch,
                             ),)

TEST_RESU(RESU=(
   # DEPLACEMENTS
         _F(    CRITERE = 'RELATIF',   PRECISION = 1.E-3,
         RESULTAT = RESU, NOM_CHAM = 'DEPL',   NUME_ORDRE = 1,
         GROUP_NO = 'P3',     NOM_CMP = 'DX',            VALE =-0.439,
            REFERENCE = 'NON_REGRESSION', VERSION = '9.0.6'),
         _F(    CRITERE = 'RELATIF',   PRECISION = 1.E-3,
         RESULTAT = RESU, NOM_CHAM = 'DEPL',   NUME_ORDRE = 1,
         GROUP_NO = 'P3',     NOM_CMP = 'DY',            VALE = 3.720,
            REFERENCE = 'NON_REGRESSION', VERSION = '9.0.6'),
         ))

INSTANTS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=1.0,
                                      NOMBRE=1,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0, 1.0,
                          1.0, 1.0,),);

#
#   TEST EN PLASTICITE
#
RESU2=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CH_MATER,
                   EXCIT=_F(CHARGE=ch,
                             FONC_MULT=RAMPE,),
CONVERGENCE=_F(ITER_GLOB_MAXI=30, RESI_GLOB_RELA=1.E-6),
                   COMP_INCR=_F(RELATION='VMIS_ISOT_LINE',
                        DEFORMATION ='PETIT',),
                   INCREMENT=_F(LIST_INST=INSTANTS,),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(LIST_INST=INSTANTS,),);

TEST_RESU(RESU=(
   # DEPLACEMENTS
         _F(    CRITERE = 'RELATIF',   PRECISION = 1.E-3,
         RESULTAT = RESU2, NOM_CHAM = 'DEPL',   NUME_ORDRE = 1,
         GROUP_NO = 'P3',     NOM_CMP = 'DX',            VALE =-0.5127,
            REFERENCE = 'NON_REGRESSION', VERSION = '9.0.6'),
         _F(    CRITERE = 'RELATIF',   PRECISION = 1.1E-3,
         RESULTAT = RESU2, NOM_CHAM = 'DEPL',   NUME_ORDRE = 1,
         GROUP_NO = 'P3',     NOM_CMP = 'DY',            VALE = 4.547,
            REFERENCE = 'NON_REGRESSION', VERSION = '9.0.6'),
          ))

FIN();
