             MODELE=LEMOD,
            DDL_IMPO=(
                   _F( GROUP_MA  = 'B0X',
                       DX = 0.,  ),
                   _F( GROUP_MA  = 'B1X',
                       DX = 1.,  ),
                   _F( GROUP_MA  = 'B0Y',
                       DY = 0.,  ),
                   _F( GROUP_MA  = 'B1Y',
                       DY = 2.,  ),
                   _F( GROUP_MA  = 'B0Z',
                       DZ = 0.,  ),
                   _F( GROUP_MA  = 'B1Z',
                       DZ = 3.,  ),

                )
            )
