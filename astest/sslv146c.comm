MAIL1=LIRE_MAILLAGE(   )

MAILTOT=CREA_MAILLAGE(
            MAILLAGE = MAIL1,
            LINE_QUAD = _F(TOUT='OUI'))

#
# -----------------------------------------------------
# DEFINITION DES MATERIAUX
# -----------------------------------------------------
#

ACIER=DEFI_MATERIAU(
            ELAS=_F(  E = 2.E11,   NU = 0.0,),
                          )
BETON=DEFI_MATERIAU(
            ELAS=_F(  E = 2.E14,   NU = 0.0,),
                          )

#
# -------------------------------------------------
# AFFECTATION DES MATERIAUX
# -------------------------------------------------
#
MATAF=AFFE_MATERIAU(    MAILLAGE=MAILTOT,
                          AFFE=(
                          _F(  GROUP_MA = ('GEOX','GEOY','GEOZ'),
                                 MATER = ACIER,
                               ),
                          _F(  GROUP_MA = ('GEOB'),
                                 MATER = BETON,
                               )
                               )
                      )

#
# -------------------------------------------------
# AFFECTATION DES MODELES
# -------------------------------------------------
#
LEMOD=AFFE_MODELE(    MAILLAGE=MAILTOT,     
                        AFFE=(
                              _F(  GROUP_MA = ('GEOX','GEOY','GEOZ'),
                               PHENOMENE = 'MECANIQUE',
                               MODELISATION = 'GRILLE_MEMBRANE',
                             ),
                              _F(  GROUP_MA = ('GEOB',),
                               PHENOMENE = 'MECANIQUE',
                               MODELISATION = '3D',
                             )
                          )
                      )

# -------------------------------------------------
# AFFECTATION DES CARACTERISTIQUES
# -------------------------------------------------
#
LACAR=AFFE_CARA_ELEM(    MODELE=LEMOD,
                       GRILLE=(
                               _F( GROUP_MA = 'GEOX',
                                SECTION = .01,
                                ANGL_REP=(30,0),),
                               _F( GROUP_MA = 'GEOY',
                                SECTION = .02,
                                ANGL_REP=(0,40),),
                               _F( GROUP_MA = 'GEOZ',
                                SECTION = .03,
                                ANGL_REP=(15,70),),
                              )
                        )

# ----------------------------------------------
# CHARGEMENT
# ----------------------------------------------
