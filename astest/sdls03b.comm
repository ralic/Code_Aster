DEBUT(CODE=_F(  NOM = 'SDLS03B',NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE( )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

#====================================================================
MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.1E+11,  NU = 0.3,  RHO = 7800.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA, 
                       AFFE=_F(  TOUT = 'OUI',      MATER = MAT) )

#====================================================================
# ---- MODE DE MEMBRANE Q4G SUR APPUIS ELASTIQUES
#
MO2=AFFE_MODELE(  MAILLAGE=MA,AFFE=(
                   _F(  TOUT = 'OUI', PHENOMENE = 'MECANIQUE',
                          MODELISATION = 'Q4G'),
                        _F(  NOEUD = ('N1', 'N11', 'N111', 'N121',),
                          PHENOMENE = 'MECANIQUE',
                          MODELISATION = 'DIS_T'))
                 )

RIG=DEFI_VALEUR(  R8=EVAL("""50. / 2. """) )

CA2=AFFE_CARA_ELEM(   MODELE=MO2,
                          COQUE=_F(  
       GROUP_MA = 'TOUT',
 EPAIS = 0.01),
                        DISCRET=_F(  NOEUD = ('N1', 'N11', 'N111', 'N121',),
                                  CARA = 'K_T_D_N',
                                  REPERE = 'LOCAL',
                                  VALE = ( RIG,   0.,     0., )),
                         ORIENTATION=_F(  CARA = 'ANGL_NAUT',
                                       VALE = (60., 0., 0.,),
                                       NOEUD = ('N1', 'N11', 'N111', 'N121',))
                     )

CH2=AFFE_CHAR_MECA(  MODELE=MO2,DDL_IMPO=(
                       _F(  TOUT = 'OUI',  DZ = 0.),
                       _F(  TOUT = 'OUI',  DRX = 0., DRY = 0.),
                       _F(  TOUT = 'OUI',  DRZ = 0.)),
                        LIAISON_OBLIQUE=_F(  NOEUD = 'N1',
                                          DY = 0.,
                                          ANGL_NAUT = (60., 0., 0.,))
                    )

MACRO_MATR_ASSE(   MODELE=MO2,            CARA_ELEM=CA2,
                       NUME_DDL=CO("NU2"),
                       CHARGE=CH2,           CHAM_MATER=CHMAT,MATR_ASSE=(
               _F(  MATRICE = CO("MATR2"),  OPTION = 'RIGI_MECA'),
                         _F(  MATRICE = CO("MATM2"),  OPTION = 'MASS_MECA'))
                   )

MODE2=MODE_ITER_SIMULT(  MATR_A=MATR2,       MATR_B=MATM2,
              CALC_FREQ=_F(  NMAX_FREQ = 4,   FREQ = (  100., ),
                          OPTION = 'CENTRE'),
               VERI_MODE=_F(  SEUIL = 1.E-5)               )

# --------------------------------------------------------------------

FREQ1=DEFI_VALEUR(  R8=EVAL("""SQRT(100./117.) / (2. * PI)""") )

# --------------------------------------------------------------------

TEST_RESU(
   RESU=_F(   RESULTAT = MODE2,        PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 0.6E-2,
           NUME_ORDRE = 1,         VALE = FREQ1)
          )

# --------------------------------------------------------------------

TEST_RESU(
   RESU=_F(   RESULTAT = MODE2,        PARA = 'ITER_QR',
           CRITERE = 'ABSOLU',       PRECISION = 7.E+00,
           NUME_ORDRE = 1,         VALE_I = 0)
          )

