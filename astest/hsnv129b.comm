     _F(RESULTAT =U, INST=250.,NOM_CHAM ='SIEF_ELNO_ELGA',NOM_CMP ='SIZZ', NOEUD = 'N3',
         MAILLE='M1',REFERENCE = 'ANALYTIQUE',CRITERE = 'RELATIF',PRECISION = 1.E-3, VALE=-18.9153E6,),
#TEMPS= 300
     _F(RESULTAT =U, INST=300.,NOM_CHAM ='VARI_ELNO_ELGA',NOM_CMP ='V1', NOEUD = 'N3',
         MAILLE='M1',REFERENCE = 'ANALYTIQUE',CRITERE = 'RELATIF',PRECISION = 1.E-3, VALE=0.4626,),
     _F(RESULTAT =U, INST=300.,NOM_CHAM ='SIEF_ELNO_ELGA',NOM_CMP ='SIZZ', NOEUD = 'N3',
