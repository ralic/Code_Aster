     _F(RESULTAT =U, INST=150.,NOM_CHAM ='VARI_ELNO_ELGA',NOM_CMP ='V1', NOEUD = 'N3',
         MAILLE='M1',REFERENCE = 'ANALYTIQUE',CRITERE = 'RELATIF',PRECISION = 1.E-3, VALE=0.4247,),
     _F(RESULTAT =U, INST=150.,NOM_CHAM ='SIEF_ELNO_ELGA',NOM_CMP ='SIZZ', NOEUD = 'N3',
         MAILLE='M1',REFERENCE = 'ANALYTIQUE',CRITERE = 'RELATIF',PRECISION = 1.E-3, VALE=-30.3768E6,) ,
#TEMPS= 200
     _F(RESULTAT =U, INST=200.,NOM_CHAM ='VARI_ELNO_ELGA',NOM_CMP ='V1', NOEUD = 'N3',
