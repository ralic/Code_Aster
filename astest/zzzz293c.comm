                 CONVERGENCE=_F(RESI_GLOB_RELA=3.E-5,
                                ITER_GLOB_MAXI=250,),
                 );


TB_0=CREA_TABLE(RESU=_F(RESULTAT=U_0,GROUP_MA='POUTRE',NOM_CHAM='SIEF_ELGA',TOUT_CMP='OUI',NUME_ORDRE = 1))
