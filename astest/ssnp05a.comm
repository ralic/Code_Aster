                _F( JUSQU_A = 30.0,       NOMBRE = 20),
                           _F( JUSQU_A = 3630.0,     NOMBRE = 15),
                           _F( JUSQU_A = 3660.0,     NOMBRE = 3),
                           _F( JUSQU_A = 3720.0,     NOMBRE = 1))   )

U=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CM1,EXCIT=(
                             _F(
                CHARGE = CH1), _F(
                CHARGE = CH2)),
                         COMP_INCR=_F( PARM_THETA = 0.5,
                RELATION = 'LEMAITRE',
                ITER_INTE_MAXI = 1000),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_MAXI = 1.E-6,
                ITER_GLOB_MAXI = 25)
              )

U=CALC_ELEM( reuse=U,
                            OPTION=(
                  'SIEF_ELNO_ELGA',  'VARI_ELNO_ELGA',),
                          RESULTAT=U,
              )

U=CALC_ELEM( reuse=U,             RESULTAT=U,
