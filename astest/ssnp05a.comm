                                       RECORD_9 = ( 1, 4, 4, 2, 2, 6, ),
                             POSI_ORDRE = ( 10, 3,),
                             POSI_INST = ( 12, 1,),
                             NOM_CMP = ('SIXX', 'SIXY',
                                                      'SIYY', 'SIXZ',
                                                     'SIYZ', 'SIZZ',)))
                     )

