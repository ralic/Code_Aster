            MAILLAGE=M,
           AFFE=_F( TOUT = 'OUI',MATER = ACIER, TEMP_REF = 20.),
           AFFE_VARC=_F(TOUT='OUI',EVOL=ANEL,NOM_VARC='EPSA',NOM_CHAM='EPSA_ELNO'),);


