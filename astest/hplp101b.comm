#       OBSTACLE A UN FLUC DE CHALEUR UNIFORME EN MILIEU ISOTROPE
#
# TESTE LES DERIVEES DE G DANS LE CAS D'UN CHARGEMENT EN FORCE DE VOLUME

DEBUT(CODE=_F(NOM='HPLP101B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='NON'))

YOUNG = DEFI_PARA_SENSI(VALE=1.)
FORC = DEFI_PARA_SENSI(VALE=1.)

MA=LIRE_MAILLAGE(FORMAT='MED', )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
                           CREA_GROUP_NO=_F(
                         GROUP_MA = ( 'LP9P10',  'LP9P10B',
                                      'LP8P9',   'LP8P9B',
                                         ))
                       )

MO=AFFE_MODELE( MAILLAGE=MA,
                             AFFE=_F(  TOUT = 'OUI',
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = 'C_PLAN')
                       )
ZERO =    DEFI_CONSTANTE(VALE=0.)
NU =    DEFI_CONSTANTE(VALE=0.3)
ALPHA =    DEFI_CONSTANTE(VALE=0.)

MAT=DEFI_MATERIAU(ELAS_FO=_F(  E = YOUNG, NU = NU,RHO=7800.,))

CHMAT=AFFE_MATERIAU(    MAILLAGE=MA,
                                  AFFE=_F(  TOUT = 'OUI',
                                         MATER = MAT)
                          )

CH=AFFE_CHAR_MECA_F(           MODELE=MO,
                       FORCE_INTERNE=_F(TOUT='OUI',FY=FORC,),
                               DDL_IMPO=(
                                    _F(  GROUP_NO = 'LP8P9',
                                               DX = ZERO),
                                             _F(  GROUP_NO = 'LP8P9B',
                                               DX = ZERO),
                                             _F(  GROUP_NO = 'P11',
                                               DY = ZERO))
                            )

DEP=MECA_STATIQUE(   MODELE=MO,   CHAM_MATER=CHMAT,
                      SENSIBILITE = (FORC),
                              EXCIT=_F( CHARGE = CH),
                     )

DEP=CALC_ELEM(reuse=DEP,RESULTAT=DEP,
              OPTION=('EPSI_ELGA',),)

CALC_SENSI(reuse=DEP,RESULTAT=DEP, SENSIBILITE = (FORC),
              OPTION=('EPSI_ELGA','SIEF_ELGA',),)

FOND=DEFI_FOND_FISS(    MAILLAGE=MA,
                              FOND_FISS=_F( GROUP_NO = ('P0',)),
                      )

THETA0=CALC_THETA(       MODELE=MO,
                         THETA_2D=_F(  GROUP_NO = ('P0',),
                                    MODULE = 1.,
                                    R_INF = 3.75E-5,
                                    R_SUP = 7.50E-5),
                        DIRECTION=(1., 0., 0.,)
                     )

G0=CALC_G(      RESULTAT=DEP,
                        THETA=_F(THETA=THETA0),
                        SYME_CHAR='SANS',
                        COMP_ELAS=_F( RELATION = 'ELAS',
                                      DEFORMATION = 'PETIT')
                      )

DG0=CALC_G(     RESULTAT=DEP,
                        SENSIBILITE = (FORC),
                        THETA=_F(THETA=THETA0),
                        SYME_CHAR='SANS',
                        COMP_ELAS=_F( RELATION = 'ELAS',
                                      DEFORMATION = 'PETIT')
                      )

TEST_TABLE(       TABLE=DG0,
                       NOM_PARA='DG/DF',
                          SENSIBILITE = (FORC),
                           VALE=1.06E-2,
                      REFERENCE='ANALYTIQUE',
                      PRECISION=0.0005    )
FIN()
