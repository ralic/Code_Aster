DEBUT(CODE=_F(NOM='SSNV211A',NIV_PUB_WEB='INTERNET'),PAR_LOT='OUI',DEBUG=_F(SDVERI='OUI'))


#***********************************************************************
#    MAILLAGE + MODELE
#***********************************************************************

MAILLAGE=LIRE_MAILLAGE( )

MODELE=AFFE_MODELE(     MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = 'AXIS')
                           )

#***********************************************************************
#    INCREMENTS DE TEMPS
#***********************************************************************

TEMPS1=DEFI_LIST_REEL(   DEBUT=0.,
                           INTERVALLE=(_F(  JUSQU_A = 3600.0,    NOMBRE = 1),
                                       _F(  JUSQU_A = 7000.0,   NOMBRE =  1),
                         ),)

#***********************************************************************
#    MATERIAU : SOL
#***********************************************************************

SOL=DEFI_MATERIAU(    ELAS=_F(  E  = 5000.E6,
                                NU = 0.12  ,
                                ALPHA = 0.0),
                                VISC_DRUC_PRAG =_F(    PREF        =   0.1E6,
                                             A           =   1.5E-12,
                                             N           =   4.5,
                                             P_PIC       =   0.015,
                                             P_ULT       =   0.028,
                                             ALPHA_0     =   0.065,
                                             ALPHA_PIC   =   0.26,
                                             ALPHA_ULT   =   0.091,
                                             R_0         =   1.3021E6,
                                             R_PIC       =   6.24808E6,
                                             R_ULT       =   1.30808E6,
                                             BETA_0      =   -0.15,
                                             BETA_PIC    =    0.,
                                             BETA_ULT    =    0.13, ),),

CHMAT=AFFE_MATERIAU(    MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',  MATER = SOL)
                          )

#***********************************************************************
#    CHARGEMENT MECANIQUE : FORCE DE COMPRESSION + DDL IMPOSES
#***********************************************************************

CHAR_U1=AFFE_CHAR_MECA(    MODELE=MODELE,
                              PRES_REP=_F(   MAILLE = 'M4',
                                          PRES = 1.)
                       )

CHAR_U2=AFFE_CHAR_MECA(    MODELE=MODELE,
                             PRES_REP=_F(   MAILLE = 'M3',
                                         PRES = 1.)
                        )

DEPL_1=AFFE_CHAR_MECA(    MODELE=MODELE,
                              FACE_IMPO=_F(  MAILLE = 'M2',
                                          DY = 0.)
                           )

COEF1=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                         VALE=( 0.0,       0.0,
                                7000.0,    5.E6,)
                       )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************

############################################


U1=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = CHAR_U1,
                FONC_MULT = COEF1), _F(
                CHARGE = CHAR_U2,
                FONC_MULT = COEF1), _F(
                CHARGE = DEPL_1)),
                         COMP_INCR=_F(
                RELATION = 'VISC_DRUC_PRAG',
                ),
                NEWTON=_F(MATRICE = 'TANGENTE',),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 50),
                         INCREMENT=_F(
                LIST_INST = TEMPS1,),

                )



IMPR_RESU(MODELE=MODELE,
          RESU=(_F(RESULTAT=U1,INST = 7000.),
                   ),);

U1=CALC_ELEM( reuse=U1,
                            OPTION=(
                  'SIGM_ELNO',  'VARI_ELNO',),
                          RESULTAT=U1,
              )

U1=CALC_NO(  reuse      = U1,
             RESULTAT   = U1,
             TOUT_ORDRE = 'OUI',
             OPTION     = ('SIGM_NOEU','VARI_NOEU'),
               )

#***********************************************************************
#    INCREMENTS DE TEMPS
#***********************************************************************

TEMPS2=DEFI_LIST_REEL(   DEBUT=7000.,
              INTERVALLE=(_F(  JUSQU_A = 8000.00,    NOMBRE = 100, ),
                                     _F(  JUSQU_A = 9000.00,    NOMBRE = 100, ),
                                     _F(  JUSQU_A =10000.00,    NOMBRE = 100, ),
                                     _F(  JUSQU_A =11000.00,    NOMBRE = 100, ),
                                     _F(  JUSQU_A =12000.00,    NOMBRE = 100, ),
                                     _F(  JUSQU_A =13000.00,    NOMBRE = 100, ),
                                                        ))

ARCHI=DEFI_LIST_REEL(   DEBUT=7000.,
                         INTERVALLE=(_F(  JUSQU_A = 8000.00,   NOMBRE = 10, ),
                                     _F(  JUSQU_A = 9000.00,   NOMBRE = 10, ),
                                     _F(  JUSQU_A =10000.00,   NOMBRE = 10, ),
                                     _F(  JUSQU_A =11000.00,   NOMBRE = 10, ),
                                     _F(  JUSQU_A =12000.00,   NOMBRE = 10, ),
                                     _F(  JUSQU_A =13000.00,   NOMBRE = 10, ),
                   ))
#***********************************************************************
#    NOUVEAU CHARGEMENT MECANIQUE : DEPLACEMENT UNIAXIALE UNITAIRE
#***********************************************************************


DEPL_2=AFFE_CHAR_MECA(    MODELE=MODELE,
                              FACE_IMPO=_F(  MAILLE = 'M4',
                                          DY = 1.)
                           )

COEF2=DEFI_FONCTION(      NOM_PARA='INST',
                          PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='CONSTANT',
                         VALE=( 7000.0,     -7.6E-4,
                                8000.0,     -0.01,
                                9000.0,     -0.02,
                               10000.0,     -0.03,
                               11000.0,     -0.04,
                               12000.0,     -0.05,
                               13000.0,     -0.06,
                                ) )

#***********************************************************************
#    CALCUL DE LA SOLUTION
#***********************************************************************


U2=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = CHAR_U2,
                FONC_MULT = COEF1), _F(
                CHARGE = DEPL_1), _F(
                CHARGE = DEPL_2,
                FONC_MULT = COEF2)),
                COMP_INCR=_F(
                RELATION = 'VISC_DRUC_PRAG',),
                ETAT_INIT=_F(EVOL_NOLI=U1),
                INCREMENT=_F(LIST_INST=TEMPS2,
                             ),
                NEWTON=_F(MATRICE = 'TANGENTE',
                          REAC_ITER = 1,
                          #PAS_MINI_ELAS=100,
                          ),
                ARCHIVAGE = _F(LIST_INST=ARCHI),
                CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 20)
              )


U2=CALC_ELEM( reuse      = U2,
              OPTION     = ('SIGM_ELNO','VARI_ELNO'),
              RESULTAT   = U2,
              )

U2=CALC_NO(  reuse      = U2,
             RESULTAT   = U2,
             TOUT_ORDRE = 'OUI',
             OPTION     = ('SIGM_NOEU','VARI_NOEU'),
               )



TFIN1 = 7000.
TFIN2 = 13000.

TEST_RESU(RESU=(

# DX
                 _F(  RESULTAT   = U1,
                      REFERENCE  = 'NON_REGRESSION',
                      VERSION    = '9.1.15',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'DEPL',
                      NOM_CMP    = 'DX',
                      INST       = TFIN1,
                      PRECISION  = 1.E-3,
                      VALE       = -7.6E-4),
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'NON_REGRESSION',
                      VERSION    = '9.1.15',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'DEPL',
                      NOM_CMP    = 'DX',
                      INST       = TFIN2,
                      PRECISION  = (1.E-3,1.E-6),
                      VALE       = 3.45984E-2),),)

TEST_RESU(RESU=(

# CONTRAINTE SIYY
                 _F(  RESULTAT   = U1,
                      REFERENCE  = 'NON_REGRESSION',
                      VERSION    = '10.0.6',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'SIGM_NOEU',
                      NOM_CMP    = 'SIYY',
                      INST       = TFIN1,
                      PRECISION  = 1.E-3,
                      VALE       = -5.E6),
                 _F(  RESULTAT   = U2,
                      REFERENCE  = 'NON_REGRESSION',
                      VERSION    = '10.0.6',
                      NOEUD      = 'N4',
                      NOM_CHAM   = 'SIGM_NOEU',
                      NOM_CMP    = 'SIYY',
                      INST       = TFIN2,
                      PRECISION  = (1.E-3,1.E-6),
                      VALE       = -11.667E6),),)


FIN()
