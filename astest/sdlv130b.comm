#       VALIDATION DU CALCUL MODAL AVEC X-FEM

DEBUT(CODE=_F(NOM='SDLV130B',
              NIV_PUB_WEB='INTERNET',
              VISU_EFICAS='NON',),
      DEBUG=_F(SDVERI='OUI',),);

#----------------------------------------------
#                MAILLAGE : hexa_5_30_50.mgib
#----------------------------------------------

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='SURFSUP',),);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=_F(GROUP_MA='SURFINF',),);

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG1,
                     AFFE=(_F(GROUP_MA='VOL',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),
                           _F(GROUP_MA=('SURFINF','SURFSUP',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);

MAILLAG1=MODI_MAILLAGE(reuse =MAILLAG1,
                       MAILLAGE=MAILLAG1,
                       ORIE_PEAU_3D=_F(GROUP_MA=('SURFSUP','SURFINF',),),);

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    DEFI_FISS=_F(FORM_FISS='DEMI_PLAN',
                                 PFON=(0.,5.,15.,),
                                 NORMALE=(0.,0.,1.,),
                                 DTAN=(0.,-1.,0.,),),
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         INFO=1,);

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
E = 205000000000.0;

nu = 0.0;

rho = 7800.0;


ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=rho,),);

CHAMPMA1=AFFE_MATERIAU(MAILLAGE=MAILLAG1,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                               MATER=ACIER,),);

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------
PRES = -10000000.0;


CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(NOEUD='N3751',
                                DX=0.0,
                                DY=0.0,
                                DZ=0.0,),
                             _F(NOEUD='N9276',
                                DZ=0.0,),
                             _F(NOEUD='N3760',
                                DX=0.0,
                                DZ=0.0,),),);

CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   PRES_REP=_F(GROUP_MA=('SURFSUP','SURFINF',),
                               PRES=PRES,),);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      INFO=1,);

#----------------------------------------------
#           CALCUL DES MATRICES ELEMENTAIRES
#----------------------------------------------

MELR=CALC_MATR_ELEM(OPTION='RIGI_MECA',
                    MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA1,
                    CHARGE=(CH1,CHXFEM,CH2,),);

MELM=CALC_MATR_ELEM(OPTION='MASS_MECA',
                    MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA1,
                    CHARGE=(CH1,CHXFEM,CH2,),);

#----------------------------------------------
#           ASSEMBLAGE DES MATRICES
#----------------------------------------------

NUM=NUME_DDL(MATR_RIGI=MELR,
             METHODE='MULT_FRONT',);

MATR=ASSE_MATRICE(MATR_ELEM=MELR,
                  NUME_DDL=NUM,);

MATM=ASSE_MATRICE(MATR_ELEM=MELM,
                  NUME_DDL=NUM,);

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

UTOT1=MECA_STATIQUE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMA1,
                   EXCIT=(_F(CHARGE=CHXFEM),
                          _F(CHARGE=CH1),
                          _F(CHARGE=CH2),
                          ),
                   INFO=1,
                   ); 
# CALCUL DES CONTRAINTES ASSOCIEES

CONTR=CREA_CHAMP(TYPE_CHAM='ELGA_SIEF_R',
                 OPERATION='EXTR',
                 RESULTAT=UTOT1,
                 NOM_CHAM='SIEF_ELGA',
                 NUME_ORDRE=1,);

MELRG=CALC_MATR_ELEM(OPTION='RIGI_GEOM',
                     MODELE=MODELEK,
                     SIEF_ELGA=CONTR,);

MATRG=ASSE_MATRICE(MATR_ELEM=MELRG,
                   NUME_DDL=NUM,);

MATRC=COMB_MATR_ASSE(COMB_R=(_F(MATR_ASSE=MATR,
                                COEF_R=1.0,),
                             _F(MATR_ASSE=MATRG,
                                COEF_R=1.0,),),);

#----------------------------------------------
#                   CALCUL MODAL
#----------------------------------------------

MODES=MODE_ITER_SIMULT(MATR_A=MATRC,
                       MATR_B=MATM,
                       METHODE='SORENSEN',
                       CALC_FREQ=_F(OPTION='PLUS_PETITE',
                                    NMAX_FREQ=8,),);

#----------------------------------------------
#               VISUALISATION DES MODES
#----------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MODELE=MODELEK,
                       INFO=2,);

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='MECANIQUE',
                             MODELISATION='3D',),);

RES_XFEM=POST_CHAM_XFEM(MODELE_VISU=MOD_VISU,
                        RESULTAT=MODES,
                        INFO=1,);

IMPR_RESU(FORMAT='GMSH',
          UNITE=37,
          RESU=_F(RESULTAT=RES_XFEM,
                  TYPE_CHAM='VECT_3D',
                  NOM_CMP=('DX','DY','DZ',),),);

#----------------------------------------------
#               VALIDATION DES RESULTATS
#----------------------------------------------

TEST_RESU(RESU=_F(RESULTAT=MODES,
                  NUME_ORDRE=1,
                  PARA='FREQ',
                  VALE=1.36384279542,
                  CRITERE='RELATIF',
                  PRECISION=0.005,
                  REFERENCE='AUTRE_ASTER',),);

TEST_RESU(RESU=_F(RESULTAT=MODES,
                  NUME_ORDRE=2,
                  PARA='FREQ',
                  VALE=3.22092785153,
                  CRITERE='RELATIF',
                  PRECISION=0.015,
                  REFERENCE='AUTRE_ASTER',),);

TEST_RESU(RESU=_F(RESULTAT=MODES,
                  NUME_ORDRE=3,
                  PARA='FREQ',
                  VALE=4.81490742935,
                  CRITERE='RELATIF',
                  PRECISION=0.005,
                  REFERENCE='AUTRE_ASTER',),);

TEST_RESU(RESU=_F(RESULTAT=MODES,
                  NUME_ORDRE=4,
                  PARA='FREQ',
                  VALE=7.19503041712,
                  CRITERE='RELATIF',
                  PRECISION=0.005,
                  REFERENCE='AUTRE_ASTER',),);

TEST_RESU(RESU=_F(RESULTAT=MODES,
                  NUME_ORDRE=5,
                  PARA='FREQ',
                  VALE=10.0980751404,
                  CRITERE='RELATIF',
                  PRECISION=0.015,
                  REFERENCE='AUTRE_ASTER',),);

TEST_RESU(RESU=_F(RESULTAT=MODES,
                  NUME_ORDRE=6,
                  PARA='FREQ',
                  VALE=11.7898433642,
                  CRITERE='RELATIF',
                  PRECISION=0.015,
                  REFERENCE='AUTRE_ASTER',),);

TEST_RESU(RESU=_F(RESULTAT=MODES,
                  NUME_ORDRE=7,
                  PARA='FREQ',
                  VALE=17.4844516078,
                  CRITERE='RELATIF',
                  PRECISION=0.005,
                  REFERENCE='AUTRE_ASTER',),);

TEST_RESU(RESU=_F(RESULTAT=MODES,
                  NUME_ORDRE=8,
                  PARA='FREQ',
                  VALE=18.2814957496,
                  CRITERE='RELATIF',
                  PRECISION=0.005,
                  REFERENCE='AUTRE_ASTER',),);


FIN();
