unite=DEFI_FICHIER(ACTION='ASSOCIER', FICHIER='./REPE_OUT/MAGF.mail',ACCES='NEW')
IMPR_RESU(FORMAT='ASTER',UNITE=unite,RESU=_F(MAILLAGE=MAGF))
