DEBUT(CODE=_F(NOM='SSNL101A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE( )

MO=AFFE_MODELE(   MAILLAGE=MA,
                     AFFE=_F(  MAILLE = 'D1',
                            PHENOMENE = 'MECANIQUE',
                            MODELISATION = 'DIS_TR')   )

F1=DEFI_FONCTION(   NOM_PARA='INST',
                      VALE=(0., 0., 4., 4., 6., 2., 10., 6., 12., 4., ),
                        PROL_DROITE='LINEAIRE',
                       PROL_GAUCHE='LINEAIRE'   )

LINST=DEFI_LIST_REEL(  DEBUT=0.,
                          INTERVALLE=_F( JUSQU_A = 12., PAS = 1.)  )

MAT02=DEFI_MATERIAU(  ARME=_F(  KYE = 1.67E4,  DLE = 0.048,  KYP = 2.9E3,
                                DLP = 0.7,  KYG = 1.E6)  )

CHMAT=AFFE_MATERIAU(   MAILLAGE=MA,
                          AFFE=_F(  MAILLE = 'D1',  MATER = MAT02)  )

CAREL=AFFE_CARA_ELEM(   MODELE=MO,
                    DISCRET=_F(  MAILLE = 'D1',
                              CARA = 'K_TR_D_L',
                              VALE = (1.E6, 1.E6, 1.E6, 1.E9, 1.E9, 1.E9,)),
                    ORIENTATION=_F(  MAILLE = 'D1',
                                  CARA = 'ANGL_NAUT',
                                  VALE = ( 0.,  0., 0., ))
                         )

CH1=AFFE_CHAR_MECA(   MODELE=MO,
                  DDL_IMPO=_F(  NOEUD = 'N2',
                             DX = 0.,  DY = 0.,  DZ = 0.,
                             DRX = 0.,  DRY = 0.,  DRZ = 0.),
                  FORCE_NODALE=_F(  NOEUD = 'N3',
                                 FX = 0.,  FY = 500.,  FZ = 0.,
                                 MX = 0.,  MY = 0.,    MZ = 0.)
                      )

STATNL=STAT_NON_LINE(
                            MODELE=MO,
                        CHAM_MATER=CHMAT,
                         CARA_ELEM=CAREL,
                             EXCIT=_F(
                CHARGE = CH1,
                FONC_MULT = F1),
                         COMP_INCR=_F(
                RELATION = 'ARME',
                MAILLE = 'D1'),
                         INCREMENT=_F(
                LIST_INST = LINST,
                NUME_INST_FIN = 12),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 40),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1)
              )

STATNL=CALC_ELEM( reuse=STATNL,
                            OPTION=(
                  'SIEF_ELNO_ELGA',  'VARI_ELNO_ELGA',),
                          RESULTAT=STATNL,
              )

#
TEST_RESU(RESU=(
              _F(
                    RESULTAT = STATNL, NOM_CHAM = 'DEPL', NOM_CMP = 'DY',
              NUME_ORDRE = 2, NOEUD = 'N3', VALE = 0.116414),
              _F(
                    RESULTAT = STATNL, NOM_CHAM = 'DEPL', NOM_CMP = 'DY',
              NUME_ORDRE = 8, NOEUD = 'N3', VALE = 0.461241),
              _F(
                    RESULTAT = STATNL, NOM_CHAM = 'DEPL', NOM_CMP = 'DY',
              NUME_ORDRE = 10, NOEUD = 'N3', VALE = 0.700308),
              _F(
        RESULTAT = STATNL, NOM_CHAM = 'VARI_ELNO_ELGA', NOM_CMP = 'V1',
        NUME_ORDRE = 2, MAILLE = 'D1', NOEUD = 'N3', VALE = 6.84138E-2),
              _F(
        RESULTAT = STATNL, NOM_CHAM = 'VARI_ELNO_ELGA', NOM_CMP = 'V1',
        NUME_ORDRE = 8, MAILLE = 'D1', NOEUD = 'N3', VALE = 4.13241E-1),
              _F(
        RESULTAT = STATNL, NOM_CHAM = 'VARI_ELNO_ELGA', NOM_CMP = 'V1',
        NUME_ORDRE = 10, MAILLE = 'D1', NOEUD = 'N3', VALE = 6.52E-1))

          )

FIN()
