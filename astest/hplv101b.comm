                      NOM_CMP='DZ',
                      CHAM_GD=DEM_KIO,
                      VALE_REFE=-7.0328571430000002,
                      VALE_CALC=-7.0328571430000002,
                      PRECISION=1.0E-07,),
                   _F(NOEUD='N8',
                      CRITERE='RELATIF',
                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='DZ',
                      CHAM_GD=DEM_KIO,
                      VALE_REFE=-7.0328571430000002,
                      VALE_CALC=-7.0328571430000002,
                      PRECISION=1.0E-07,),
                   _F(NOEUD='N4',
                      CRITERE='RELATIF',
                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='DZ',
                      CHAM_GD=DEF_KSO,
                      VALE_REFE=57.704592849999997,
                      VALE_CALC=57.704592849999997,
                      PRECISION=1.0E-07,),
                   _F(NOEUD='N8',
                      CRITERE='RELATIF',
                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='DZ',
                      CHAM_GD=DEF_KSO,
                      VALE_REFE=57.704592849999997,
                      VALE_CALC=57.704592849999997,
                      PRECISION=1.0E-07,),
                   ),
          )

TEST_TABLE(REFERENCE='ANALYTIQUE',
           PRECISION=9.9999999999999995E-07,
           VALE_CALC=8.205000000,
           VALE_REFE=8.2050000000000001,
           NOM_PARA='TOTALE',
           TYPE_TEST='SOMM_ABS',
           TABLE=TTO,)

TEST_TABLE(REFERENCE='ANALYTIQUE',
