PMO=POST_ELEM(   MODELE=MODME,   CHAM_MATER=CHMATO,
                     CHAM_GD=DEM_KIO,
                      CARA_ELEM=ESSAI0, ENER_POT=_F(  TOUT = 'OUI') )

PFO=POST_ELEM(   MODELE=MODME,   CHAM_MATER=CHMATO,
                     CHAM_GD=DEF_KSO,
                      CARA_ELEM=ESSAI0, ENER_POT=_F(  TOUT = 'OUI') )

#

TEST_RESU(CHAM_NO=(_F(NOEUD='N8',
                      CRITERE='RELATIF',
                      REFERENCE='ANALYTIQUE',
                      NOM_CMP='TEMP',
