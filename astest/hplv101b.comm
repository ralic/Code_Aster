PMOO=POST_ELEM(   MODELE=MODME,   CHAM_MATER=CHMATOO,
                     CHAM_GD=DEM_KIOO,
                      CARA_ELEM=ESSAI00, ENER_POT=_F(  TOUT = 'OUI') )

PFOO=POST_ELEM(   MODELE=MODME,   CHAM_MATER=CHMATOO,
                     CHAM_GD=DEF_KSOO,
                      CARA_ELEM=ESSAI00, ENER_POT=_F(  TOUT = 'OUI') )

