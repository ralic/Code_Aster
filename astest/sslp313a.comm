DEBUT(CODE=_F(NOM='SSLP313A', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

PRE_GIBI()

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='OUI'),)

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
               CREA_GROUP_NO=_F( GROUP_MA = ( 'LL2',  'LL4', ))
                 )

MO=AFFE_MODELE(  MAILLAGE=MA,
                 AFFE=_F(  TOUT = 'OUI',
                           PHENOMENE = 'MECANIQUE',
                           MODELISATION = 'C_PLAN')  )

MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.E11,
                            NU = 0.3,
                            ALPHA = 0.)  )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                      AFFE=_F(  TOUT = 'OUI',  MATER = MAT)
                      )

CH=AFFE_CHAR_MECA(  MODELE=MO,
                    DDL_IMPO=( _F(  GROUP_NO = 'PS6',  DX = 0.),
                               _F(  GROUP_NO = 'LL4',  DY = 0.)),
                    FORCE_CONTOUR=_F( GROUP_MA = 'LL2',  FY = 100.E6)
                    )

CHAMDEPL=MECA_STATIQUE(  MODELE=MO,
                         CHAM_MATER=CHMAT,
                         EXCIT=_F( CHARGE = CH)
                          )

FOND1=DEFI_FOND_FISS(  MAILLAGE=MA,
                       FOND_FISS=_F( GROUP_NO = ('P0',)),
#                       NORMALE=(-0.6018, 0.7986, 0.,)
                       LEVRE_SUP=_F(GROUP_MA='LPOP8',),
                       LEVRE_INF=_F(GROUP_MA='LPOP8B',),
                       INFO = 2,
                     )

THETA01=CALC_THETA(  MODELE=MO,
                     THETA_2D=_F(  GROUP_NO = ('P0',),
                                   MODULE = 1.,
                                   R_INF = 0.00001875,
                                   R_SUP = 0.000075),
                     DIRECTION=(0.7986, 0.6018, 0.,)
                     )

G01=CALC_G(          RESULTAT=CHAMDEPL,
                     NUME_ORDRE=1,
                     THETA=_F(THETA=THETA01),
                   )

GK01=CALC_G(              RESULTAT=CHAMDEPL,
                          NUME_ORDRE=1,
                          THETA=_F(
                                   R_INF = 0.00001875,
                                   R_SUP = 0.000075,
                                   DIRECTION=(0.7986, 0.6018, 0.,),
                                   FOND_FISS=FOND1),
                          OPTION='CALC_K_G'
                      )

TEST_TABLE(       TABLE=GK01,
                       NOM_PARA='K1',
                           VALE=3.574968087E6,
                      PRECISION=0.01           )

TEST_TABLE(       TABLE=GK01,
                       NOM_PARA='K2',
                           VALE=2.693931681E6,
                      PRECISION=0.01           )

TEST_TABLE(       TABLE=GK01,
                       NOM_PARA='G',
                           VALE=1.001883E2,
                      PRECISION=0.02           )

# ------------------------------------------------------------------

TABL_K=POST_K1_K2_K3(  MODELISATION='C_PLAN',
                        INST = 0.,
                        INFO=2,
                        MATER=MAT,
                        RESULTAT = CHAMDEPL,
                        FOND_FISS = FOND1,
                        ABSC_CURV_MAXI = 7.51000E-05,
                        VECT_K1        = ( 0., 1., 0., )
                       )



# ---------------

TEST_TABLE(       TABLE=TABL_K,
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='K1',
                           VALE=3.574968087E6,
                      PRECISION=0.025           )

TEST_TABLE(       TABLE=TABL_K,
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='K2',
                           VALE=2.693931681E6,
                      PRECISION=0.04            )

TEST_TABLE(       TABLE=TABL_K,
                      REFERENCE='AUTRE_ASTER',
                       NOM_PARA='G',
                           VALE=1.001883E2,
                      PRECISION=0.056           )

# ---------------


TEST_TABLE(       TABLE=TABL_K,
                      REFERENCE='NON_REGRESSION',
                       NOM_PARA='K1',
                           VALE= 3.49969E+06   )


TEST_TABLE(       TABLE=TABL_K,
                      REFERENCE='NON_REGRESSION',
                       NOM_PARA='K2',
                           VALE=2.58730E+06  )


TEST_TABLE(       TABLE=TABL_K,
                      REFERENCE='NON_REGRESSION',
                       NOM_PARA='G',
                           VALE= 9.47097E+01  )



# ---------------
# ON EXTRAIT PREALABLEMENT LES DEPLACEMENTS DES LEVRES
# LES RESULTATS DOIVENT ETRE IDENTIQUES
# ---------------
LEVH=INTE_MAIL_2D(  MAILLAGE=MA,
                    DEFI_CHEMIN=_F(  GROUP_MA = 'LPOP8'),
                    GROUP_NO_ORIG='P0'
                       )

LEVB=INTE_MAIL_2D(  MAILLAGE=MA,
                    DEFI_CHEMIN=_F(  GROUP_MA = 'LPOP8B'),
                    GROUP_NO_ORIG='P0'
                       )

DEPH=POST_RELEVE_T(  ACTION=_F(  INTITULE = 'DEPL_DH',
                                  CHEMIN = LEVH,
                                  RESULTAT = CHAMDEPL,
                                  NOM_CHAM = 'DEPL',
                                  NOM_CMP = ( 'DX',  'DY', ),
                                  OPERATION = 'EXTRACTION')
                       )

DEPB=POST_RELEVE_T(  ACTION=_F(  INTITULE = 'DEPL_DB',
                                  CHEMIN = LEVB,
                                  RESULTAT = CHAMDEPL,
                                  NOM_CHAM = 'DEPL',
                                  NOM_CMP = ( 'DX',  'DY', ),
                                  OPERATION = 'EXTRACTION')
                       )

# ---------------
TABL_K1=POST_K1_K2_K3(  MODELISATION='C_PLAN',
                       INST = 0.,
                       INFO=2,
                       MATER=MAT,
                       TABL_DEPL_SUP  = DEPH,
                       TABL_DEPL_INF  = DEPB,
                       ABSC_CURV_MAXI = 7.51000E-05,
                       VECT_K1        = ( 0., 1., 0., )
                       )
# ---------------

TEST_TABLE(       TABLE=TABL_K1,
                      REFERENCE='NON_REGRESSION',
                       NOM_PARA='K1',
                           VALE= 3.49969E+06   )


TEST_TABLE(       TABLE=TABL_K1,
                      REFERENCE='NON_REGRESSION',
                       NOM_PARA='K2',
                           VALE=2.58730E+06  )


TEST_TABLE(       TABLE=TABL_K1,
                      REFERENCE='NON_REGRESSION',
                       NOM_PARA='G',
                           VALE= 9.47097E+01  )
FIN()
#
