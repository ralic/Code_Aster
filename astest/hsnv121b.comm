DEBUT( CODE=_F(  NOM = 'HSNV121B',NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE()

MO=AFFE_MODELE(    MAILLAGE=MA,
                              VERIF='MAILLE',
                           AFFE=_F(  TOUT = 'OUI',
                                  PHENOMENE = 'MECANIQUE',
                                  MODELISATION = 'AXIS')
                       )

YOUNG=DEFI_FONCTION(     NOM_PARA='TEMP',
                            PROL_DROITE='CONSTANT',
                                  VALE=(  20.,  250000., 
                                         120.,  200000.,  )
                         )

POISSON=DEFI_CONSTANTE( VALE=0.3)

DILAT=DEFI_CONSTANTE( VALE=0.0001)

SIGY=DEFI_CONSTANTE( VALE=1000.)

PENTE=DEFI_FONCTION(     NOM_PARA='TEMP',
                            PROL_DROITE='CONSTANT',
                                  VALE=(  20.,  2500.,
                                         120.,  2000.,  )
                         )

ACIER_L=DEFI_MATERIAU(        ELAS_FO=_F(  E = YOUNG,
                                           NU = POISSON,
                                           ALPHA = DILAT, TEMP_DEF_ALPHA=20.0),
                            ECRO_LINE_FO=_F(  D_SIGM_EPSI = PENTE,
                                           SY = SIGY)
                          )

CHP_MATL=AFFE_MATERIAU(    MAILLAGE=MA,
                                 AFFE=_F(  TOUT = 'OUI',
                                        MATER = ACIER_L,
                                        TEMP_REF = 20.)
                          )

L_INST=DEFI_LIST_REEL(         DEBUT=0.0,INTERVALLE=(
                              _F(  JUSQU_A = 1.0,  NOMBRE = 1),
                                         _F(  JUSQU_A = 2.0,  NOMBRE = 20))
                          )

F_CHAR=DEFI_FONCTION(      NOM_PARA='INST',
                             PROL_DROITE='CONSTANT',
                                   VALE=(  0.,      0.,
                                           1.,      0.,
                                           2.,    293.3,   )
                         )

F_TEMP=DEFI_FONCTION(      NOM_PARA='INST',
                               NOM_RESU='TEMP',
                             PROL_DROITE='CONSTANT',
                                   VALE=(  0.,     20.,
                                           1.,    120.,
                                           2.,    120.,    )
                         )

CHP_TEMP=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_F', 
MAILLAGE=MA,
                                AFFE=_F(  TOUT = 'OUI',
                                       NOM_CMP = 'TEMP',
                                       VALE_F = F_TEMP)
                        )

TEMP=CREA_RESU(    OPERATION='AFFE', TYPE_RESU='EVOL_THER',
                          NOM_CHAM='TEMP',
                           AFFE=_F(  LIST_INST = L_INST,
                                     CHAM_GD = CHP_TEMP)
                     )

CHR_TEMP=AFFE_CHAR_MECA(           MODELE=MO,
                              TEMP_CALCULEE=TEMP
                          )

CHR_LIAI=AFFE_CHAR_MECA(      MODELE=MO,DDL_IMPO=(
                              _F(  NOEUD = 'NO1',  DY = 0.),
                                       _F(  NOEUD = 'NO2',  DY = 0.))
                          )

CHR_TRAC=AFFE_CHAR_MECA(       MODELE=MO,
                              FACE_IMPO=_F(  MAILLE = 'MA2',  DY = 1.)
                          )

VL=STAT_NON_LINE( 
                            MODELE=MO,
                        CHAM_MATER=CHP_MATL,EXCIT=(
                             _F( 
                CHARGE = CHR_LIAI), _F( 
                CHARGE = CHR_TEMP)),
                         COMP_INCR=_F( 
                RELATION = 'VMIS_ISOT_LINE',
                DEFORMATION = 'SIMO_MIEHE'),
                         INCREMENT=_F( 
                LIST_INST = L_INST,
                NUME_INST_FIN = 1),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F( 
                RESI_GLOB_MAXI = 1.E-1,
                ITER_GLOB_MAXI = 50),
                     RECH_LINEAIRE=_F( 
                ITER_LINE_MAXI = 3)
              )

VL=STAT_NON_LINE( reuse=VL,
                            MODELE=MO,
                        CHAM_MATER=CHP_MATL,
                         ETAT_INIT=_F( 
                EVOL_NOLI = VL,
                NUME_ORDRE = 1),EXCIT=(
                             _F( 
                CHARGE = CHR_LIAI), _F( 
                CHARGE = CHR_TEMP), _F( 
                CHARGE = CHR_TRAC,
                FONC_MULT = F_CHAR,
                TYPE_CHARGE = 'DIDI')),
                         COMP_INCR=_F( 
                RELATION = 'VMIS_ISOT_LINE',
                DEFORMATION = 'SIMO_MIEHE'),
                         INCREMENT=_F( 
                LIST_INST = L_INST),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F( 
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 50),
                     RECH_LINEAIRE=_F( 
                ITER_LINE_MAXI = 0)
              )

TEST_RESU(RESU=(  _F( 
               RESULTAT = VL,  NOM_CHAM = 'DEPL',  INST = 2.,
               NOEUD = 'NO3',   NOM_CMP = 'DY',
               PRECISION = 1.E-2,
               VALE = 303.),
              _F( 
               RESULTAT = VL,  NOM_CHAM = 'DEPL',  INST = 2.,
               NOEUD = 'NO3',   NOM_CMP = 'DX',
               PRECISION = 1.E-2,
               VALE = -110.),
              _F( 
               RESULTAT = VL,   NOM_CHAM = 'SIEF_ELGA',  INST = 2.,
               MAILLE = 'MA1',  POINT = 1,   NOM_CMP = 'SIYY',
               PRECISION = 1.E-2,
               VALE = 1453.),
              _F( 
               RESULTAT = VL,     NOM_CHAM = 'VARI_ELGA',  INST = 2.,
               MAILLE = 'MA1',  POINT = 1,   NOM_CMP = 'V1',
               PRECISION = 1.5E-2,
               VALE = 0.2475))
             )

FIN(  )
