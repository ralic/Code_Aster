DEBUT(CODE=_F(NOM='SSNP302A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MAYA=LIRE_MAILLAGE( )

MODEL=AFFE_MODELE(    MAILLAGE=MAYA,
                                VERIF='MAILLE',
                                 AFFE=_F(  TOUT = 'OUI',
                                        PHENOMENE = 'MECANIQUE',
                                        MODELISATION = 'C_PLAN')
                         )

ACIER=DEFI_MATERIAU(        ELAS=_F(  E = 200000.,
                                     NU = 0.,
                                     ALPHA = 1.E-6)
                      )


TRAC_FO=DEFI_FONCTION(       NOM_PARA='INST',
                             PROL_DROITE='EXCLU',
                            PROL_GAUCHE='EXCLU',
                                   VALE=( 0.,      0.,
                                          1.,      100., )
                        )
T0=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R',
MAILLAGE=MAYA,AFFE=(
                               _F(  TOUT='OUI',
                                      NOM_CMP = 'TEMP',
                                      VALE = 0.))
                      )

T_CAL=CREA_CHAMP( OPERATION='AFFE', TYPE_CHAM='NOEU_TEMP_R',
MAILLAGE=MAYA,AFFE=(
                               _F(  NOEUD = 'NO1',
                                      NOM_CMP = 'TEMP',
                                      VALE = 0.),
                                    _F(  NOEUD = 'NO4',
                                      NOM_CMP = 'TEMP',
                                      VALE = 0.),
                                    _F(  NOEUD = 'NO2',
                                      NOM_CMP = 'TEMP',
                                      VALE = 1000.),
                                    _F(  NOEUD = 'NO3',
                                      NOM_CMP = 'TEMP',
                                      VALE = 1000.))
                      )

TCALC=CREA_RESU( OPERATION='AFFE',
                 TYPE_RESU='EVOL_THER',
                 NOM_CHAM='TEMP',
                 AFFE=_F(CHAM_GD=T0,
                         INST=.0,),);
CREA_RESU(reuse =TCALC,
                 OPERATION='AFFE',
                 TYPE_RESU='EVOL_THER',
                 NOM_CHAM='TEMP',
                 AFFE=_F(CHAM_GD=T_CAL,
                         INST= 1.0,),);


TRAC=AFFE_CHAR_MECA(      MODELE=MODEL,FORCE_NODALE=(
                             _F(  NOEUD = 'NO2',
                                             FX = 0.5),
                                          _F(  NOEUD = 'NO3',
                                             FX = 0.5))
                        )

CONDLIM=AFFE_CHAR_MECA(     MODELE=MODEL,DDL_IMPO=(
                              _F(  NOEUD = 'NO1',  DX = 0.,
                                                      DY = 0.),
                                       _F(  NOEUD = 'NO4',  DX = 0.))
                          )

CAT=DEFI_LIST_REEL(    DEBUT=0.0,
                          INTERVALLE=_F(  JUSQU_A = 1.,     NOMBRE = 1)
                      )

MATER=AFFE_MATERIAU(   MAILLAGE=MAYA,
                  AFFE=_F(  TOUT = 'OUI', MATER = ACIER),
                  AFFE_VARC =_F(TOUT='OUI', EVOL=TCALC, VALE_REF=0.,NOM_VARC='TEMP',),

                         )

CALCNL=STAT_NON_LINE(   MODELE=MODEL,
                        CHAM_MATER=MATER,
                         COMP_ELAS=_F(
                RELATION = 'ELAS',
                DEFORMATION = 'PETIT'),
                         INCREMENT=_F(
                LIST_INST = CAT),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_INCR = 1,
                REAC_ITER = 1),EXCIT=(
                             _F(
                CHARGE = CONDLIM), _F(
                CHARGE = TRAC,
                FONC_MULT = TRAC_FO),  ),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 3)
              )

CALCNL=CALC_ELEM( reuse=CALCNL,
                            OPTION=(
                  'SIEF_ELNO_ELGA',  'VARI_ELNO_ELGA',),
                          RESULTAT=CALCNL,
              )

CALCNL=CALC_ELEM( reuse=CALCNL,
                                    OPTION='EPSI_ELNO_DEPL',
                                  RESULTAT=CALCNL
                     )

TEST_RESU(RESU=(
          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'SIEF_ELNO_ELGA',
            NUME_ORDRE = 1,
            MAILLE = 'MA1',
            NOEUD = 'NO1',
            NOM_CMP = 'SIXX',
            CRITERE = 'RELATIF',
            VALE = 200.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'SIEF_ELNO_ELGA',
            NUME_ORDRE = 1,
            MAILLE = 'MA1',
            NOEUD = 'NO4',
            NOM_CMP = 'SIXX',
            CRITERE = 'RELATIF',
            VALE = 200.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'SIEF_ELNO_ELGA',
            NUME_ORDRE = 1,
            MAILLE = 'MA1',
            NOEUD = 'NO2',
            NOM_CMP = 'SIXX',
            CRITERE = 'ABSOLU',
            VALE = 0.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'SIEF_ELNO_ELGA',
            NUME_ORDRE = 1,
            MAILLE = 'MA1',
            NOEUD = 'NO3',
            NOM_CMP = 'SIXX',
            CRITERE = 'ABSOLU',
            VALE = 0.))

          )

FIN()
