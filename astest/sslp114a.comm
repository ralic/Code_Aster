FYX=FORMULE(NOM_PARA=('X','Y',),VALE='SIGMA(X,Y,2)');
FYY=FORMULE(NOM_PARA=('X','Y',),VALE='SIGMA(X,Y,1)');

FXXF=FORMULE(NOM_PARA=('X','Y',),VALE='-SIGMA(X,Y,0)');
