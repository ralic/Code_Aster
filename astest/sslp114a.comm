    Px=1.
    Py=1.
elif (alpha0<0)&(alpha0>=-90) :
    Px=1.
    Py=0.
elif alpha0<-90 :
    Px=0.
    Py=0.
elif alpha0>90 :
    Px=0.
    Py=1.

