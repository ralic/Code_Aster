## CREATION GBE/IRCN, LE 8/8/96


DEBUT( CODE=_F(  NOM = 'SDLD313B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

#

MASRES=LIRE_MAILLAGE( )

MASRES=DEFI_GROUP( reuse=MASRES,   MAILLAGE=MASRES,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

#

MODELE=AFFE_MODELE(  MAILLAGE=MASRES,AFFE=(
                       _F(  TOUT = 'OUI', PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'POU_D_T'),
              _F(  NOEUD = ('B','C',),  PHENOMENE = 'MECANIQUE',
        MODELISATION = 'DIS_T'))       )

#
MATER=DEFI_MATERIAU(  ELAS=_F( E = 28000.00, NU = 0.0,  RHO = 0.0,
                         AMOR_HYST = 0.))

MATHYST=DEFI_MATERIAU(  ELAS=_F( E = 28000.00, NU = 0.0,  RHO = 0.0,
                         AMOR_HYST = 0.1))

#
CHAMPMAT=AFFE_MATERIAU(  MAILLAGE=MASRES,AFFE=(
                                _F( MAILLE = 'K2', MATER = MATER),
                                _F( GROUP_MA = 'AMORT', MATER = MATHYST)))

#
RIGMAS=AFFE_CARA_ELEM(  MODELE=MODELE,
                 POUTRE=_F(
       GROUP_MA = 'TOUT',
  SECTION = 'RECTANGLE',
                          CARA = ('H',), VALE = (1.,)),DISCRET=(

                _F(  NOEUD = 'B',
                          CARA = 'M_T_D_N',
                          VALE = ( 10.,)),
                _F(  NOEUD = 'B',
                          CARA = 'K_T_D_N',
                          VALE = ( 0.,0.,0.,)),
                _F(  NOEUD = 'C',
                          CARA = 'M_T_D_N',
                          VALE = ( 5.,)),
                _F(  NOEUD = 'C',
                          CARA = 'K_T_D_N',
                          VALE = ( 0.,0.,0.,))
                 )
                        )

#
CONDLIM=AFFE_CHAR_MECA(  MODELE=MODELE,DDL_IMPO=(
                         _F(  TOUT = 'OUI', DY = 0., DZ = 0.,
                                    DRX = 0., DRY = 0., DRZ = 0.),
                                  _F(  NOEUD = 'A',  DX = 0.))
                        )

FORCENO=AFFE_CHAR_MECA(  MODELE=MODELE,
                         FORCE_NODALE=_F(  NOEUD = 'C', FX = 100.)  )

RIGIELEM=CALC_MATR_ELEM(  MODELE=MODELE,       OPTION='RIGI_MECA',
                            CARA_ELEM=RIGMAS,  CHARGE=CONDLIM,
                            CHAM_MATER=CHAMPMAT                )

MASSELEM=CALC_MATR_ELEM(  MODELE=MODELE,       OPTION='MASS_MECA',
                            CARA_ELEM=RIGMAS,  CHARGE=CONDLIM,
                            CHAM_MATER=CHAMPMAT                )

RIGCMPLX=CALC_MATR_ELEM(  MODELE=MODELE,       OPTION='RIGI_MECA_HYST',
                            RIGI_MECA=RIGIELEM,    CARA_ELEM=RIGMAS,
                            CHAM_MATER=CHAMPMAT,
                            CHARGE=CONDLIM              )

SCNDMEMB=CALC_VECT_ELEM(  OPTION='CHAR_MECA',
                            CARA_ELEM=RIGMAS,  CHARGE=FORCENO     )

INCONNUS=NUME_DDL(  MATR_RIGI=RIGCMPLX,    METHODE='LDLT')

MATRRIGI=ASSE_MATRICE(  MATR_ELEM=RIGCMPLX,  NUME_DDL=INCONNUS )

MATRMASS=ASSE_MATRICE(  MATR_ELEM=MASSELEM,  NUME_DDL=INCONNUS )

VECTASS=ASSE_VECTEUR(  VECT_ELEM=SCNDMEMB,  NUME_DDL=INCONNUS )

# DEFINITION DES PULSATIONS/FREQUENCES DE LA REPONSE HARMONIQUE
#-------------------------------------------------
#

L_REEL=( 0., 3.3687, 6.4848, 8.0006, 11.8746, 13.4747,
              15.5802, 21.0543, )

LISFREQ=DEFI_LIST_REEL(  VALE=L_REEL )

REPHARM=DYNA_VIBRA(TYPE_CALCUL='HARM',BASE_CALCUL='PHYS',
                              MATR_MASS=MATRMASS,
                              MATR_RIGI=MATRRIGI,
                              LIST_FREQ=LISFREQ,
                                  EXCIT=_F(  VECT_ASSE = VECTASS,
                                          COEF_MULT = 1.,
  PHAS_DEG = 0.)
                         )

F1=RECU_FONCTION(  RESULTAT=REPHARM,    NOM_CHAM='DEPL',
                       LIST_FREQ=LISFREQ,   NOEUD='C',  NOM_CMP='DX'  )

TEST_FONCTION(VALEUR=( _F(  FONCTION = F1,   VALE_PARA = 0.,
                          PRECISION = 5.E-03,
                          VALE_REFE_C = 0.0071075-0.0003536j),
                        _F(  FONCTION = F1,   VALE_PARA = 3.3687,
                          PRECISION = 5.E-03,
                          VALE_REFE_C = 0.009388216-0.000731196j),
                        _F(  FONCTION = F1,   VALE_PARA = 6.4848,
                          PRECISION = 5.E-03,
                          VALE_REFE_C = -0.0050269-0.0707103j),
                        _F(  FONCTION = F1,   VALE_PARA = 8.0006,
                          PRECISION = 5.E-03,
                          VALE_REFE_C = -0.00954931-0.0022154j),
                        _F(  FONCTION = F1,   VALE_PARA = 11.8746,
                          PRECISION = 5.E-03,
                          VALE_REFE_C = -4.23259E-5-0.000357193j),
                        _F(  FONCTION = F1,   VALE_PARA = 13.4747,
                          PRECISION = 5.E-03,
                          VALE_REFE_C = 0.00235524-0.000501765j),
                        _F(  FONCTION = F1,   VALE_PARA = 15.5802,
                          PRECISION = 5.E-03,
                          VALE_REFE_C = -0.016395374-0.06871471j),
                        _F(  FONCTION = F1,   VALE_PARA = 21.0543,
                          PRECISION = 5.E-03,
                          VALE_REFE_C = -0.00188977-5.53314E-6j))
               )

FIN( )
#
