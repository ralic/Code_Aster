              EXCIT=_F( CHARGE = CHANT), 
               RESULTAT=TEMPA2,
               CARA_ELEM=EPANT,
              NIVE_COUCHE='SUP',
               CHAM_MATER=CMANT, 
              OPTION='FLUX_ELNO_TEMP' )

