                                 2.0,           25.,
                                 7.70,          25.,
                                 20.0,          10.,
                                 100.0,         10.,  ) )

SPECT=DEFI_NAPPE(  NOM_PARA='AMOR',
                         INTERPOL=( 'LIN', 'LOG', ),
                       PARA=(     0.005,   0.01,    0.015,  ),
                       FONCTION=( ACCE_X,  ACCE_X,  ACCE_X, )        )

