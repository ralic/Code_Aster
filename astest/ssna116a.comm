                          PROL_DROITE='LINEAIRE',
                          PROL_GAUCHE='CONSTANT',
                         VALE=( 2.0,  0.0,
                                6.0,  -0.04, )
                       )


#***********************************************************************
#    EXTRACTION DES CONTRAINTES ET VARIABLES INTERNES DEFINISSANT
#    L ETAT INITIAL
#***********************************************************************

SIG_INI=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELGA_SIEF_R',
                     NOM_CHAM='SIEF_ELGA', RESULTAT=U1,
                     INST=2.
                      )

VAR_INI=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELGA_VARI_R',
                     NOM_CHAM='VARI_ELGA', RESULTAT=U1,
                     INST=2.
                      )


#***********************************************************************
#    CALCUL DE LA SOLUTION : SIMULATION DE L ESSAI TRIAXIAL
#***********************************************************************
U2=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = CHAR_U2,
                FONC_MULT = COEF1), _F(
                CHARGE = DEPL_1), _F(
                CHARGE = DEPL_2,
                FONC_MULT = COEF2)
                ),
                         COMPORTEMENT=_F(
                RESI_INTE_RELA = 1.E-8,
                ITER_INTE_MAXI = 100,
                ITER_INTE_PAS  = -10,
                RELATION = 'HOEK_BROWN'),
                         ETAT_INIT=_F(
                SIGM = SIG_INI,
                VARI = VAR_INI),
                         INCREMENT=_F(
                LIST_INST = TEMPS2,
                INST_FIN=4.5),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1,
                ),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-8,
                ITER_GLOB_MAXI = 100),
              )



U2=CALC_CHAMP(reuse=U2,CONTRAINTE=('SIGM_ELNO'),RESULTAT=U2)


