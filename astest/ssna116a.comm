CONFINEMENT=5.0;
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET', VISU_EFICAS='OUI'), DEBUG=_F(SDVERI='OUI'))

#***********************************************************************
#    MAILLAGE + MODELE
#***********************************************************************

MAILLAGE=LIRE_MAILLAGE( )

MODELE=AFFE_MODELE(     MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = 'AXIS')
                           )

#***********************************************************************
#    LISTE DES INSTANTS DE CALCUL
#***********************************************************************

TEMPS1=DEFI_LIST_REEL(   DEBUT=0.,
                           INTERVALLE=_F(  JUSQU_A = 2.0,   NOMBRE = 1)
                         )

#***********************************************************************
#    CARACTERISTIQUES DU MATERIAU
#***********************************************************************

SOL=DEFI_MATERIAU(    ELAS=_F(  E  = 4500.0,
                                NU = 0.3  ,
                                ALPHA = 0.0
                                ),
                                HOEK_BROWN =
                                _F(GAMMA_RUP            =0.005,
                                   GAMMA_RES            =0.017,
                                     S_END              =225.0,
                                     S_RUP              =482.5675,
                                     M_END              =13.5,
                                     M_RUP              =83.75,
                                     BETA              =3.0,
                                     ALPHAHB           =3.3,
                                     PHI_RUP           =15.0,
                                     PHI_RES           =15.0,
                                     PHI_END           =15.0)

                                     )

CHMAT=AFFE_MATERIAU(    MAILLAGE=MAILLAGE,
                             AFFE=_F(  TOUT = 'OUI',  MATER = SOL)
                          )

#***********************************************************************
#    CHARGEMENT MECANIQUE : FORCE DE COMPRESSION + DDL IMPOSES
#***********************************************************************

CHAR_U1=AFFE_CHAR_MECA(    MODELE=MODELE,
                              PRES_REP=_F(   MAILLE = 'M4',
                                          PRES = 1.)
                       )

CHAR_U2=AFFE_CHAR_MECA(    MODELE=MODELE,
                             PRES_REP=_F(   MAILLE = 'M3',
                                         PRES = 1.)
                        )

DEPL_1=AFFE_CHAR_MECA(    MODELE=MODELE,
                              FACE_IMPO=_F(  MAILLE = 'M2',
                                          DY = 0.)
                            )

COEF1=DEFI_FONCTION(      NOM_PARA='INST',
                           PROL_DROITE='CONSTANT',
                         VALE=( 0.0,  0.0,
                                2.0,  CONFINEMENT,)
                       )

#***********************************************************************
#    CALCUL DE LA SOLUTION DEFINISSANT L ETAT INITIAL
#***********************************************************************

U1=STAT_NON_LINE(
                            MODELE=MODELE,
                        CHAM_MATER=CHMAT,EXCIT=(
                             _F(
                CHARGE = CHAR_U1,
                FONC_MULT = COEF1), _F(
                CHARGE = CHAR_U2,
                FONC_MULT = COEF1), _F(
                CHARGE = DEPL_1)),
                         COMPORTEMENT=_F(
                RELATION = 'HOEK_BROWN',
                RESI_INTE_RELA = 1.E-6,
                ITER_INTE_MAXI = 100),
                            NEWTON=_F(
                MATRICE = 'TANGENTE'),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 50,),
                         INCREMENT=_F(
                LIST_INST = TEMPS1)
              )

U1=CALC_CHAMP(reuse=U1,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),RESULTAT=U1)


#***********************************************************************
#    LISTE DES INSTANTS DE CALCUL
#***********************************************************************

TEMPS2=DEFI_LIST_REEL(   DEBUT=2.,
                            INTERVALLE=(_F(  JUSQU_A = 15.0,   NOMBRE = 520 )
                    ))

#***********************************************************************
#    NOUVEAU CHARGEMENT MECANIQUE : DEPLACEMENT UNIAXIALE UNITAIRE
#***********************************************************************

DEPL_2=AFFE_CHAR_MECA(    MODELE=MODELE,
                              FACE_IMPO=_F(  MAILLE = 'M4',
                                          DY = 1.)
