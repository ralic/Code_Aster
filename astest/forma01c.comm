DEBUT(CODE=_F(NOM='FORMA01C',
              NIV_PUB_WEB='INTERNET',
              VISU_EFICAS='NON',),
      DEBUG=_F(SDVERI='OUI'))

MA=DEFI_MATERIAU(ELAS=_F(E=200000.0,
                         NU=0.3,),);
nbadap = 3
MAIL=[None]*(nbadap+1)
MODE=[None]*(nbadap+1)
MATE=[None]*(nbadap+1)
CHAR=[None]*(nbadap+1)
RESU=[None]*(nbadap+1)

MAIL[1]=LIRE_MAILLAGE(FORMAT='MED',);

MAILTR=LIRE_MAILLAGE(FORMAT='MED',UNITE=21,);

for k in range(1,nbadap+1):

  MODE[k]=AFFE_MODELE(MAILLAGE=MAIL[k],
                      AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),);

  if k==1:

    MAIL[k]=MODI_MAILLAGE(reuse =MAIL[k],
                          MAILLAGE=MAIL[k],
                          ORIE_PEAU_2D=_F(GROUP_MA='haut',),);

  MATE[k]=AFFE_MATERIAU(MAILLAGE=MAIL[k],
                        AFFE=_F(TOUT='OUI',
                                MATER=MA,),);

  CHAR[k]=AFFE_CHAR_MECA(MODELE=MODE[k],
                         DDL_IMPO=(_F(GROUP_MA='bas',
                                      DY=0.0,),
                                   _F(GROUP_MA='gauche',
                                      DX=0.0,),),
                          PRES_REP=_F(GROUP_MA='haut',
                                      PRES=-100.0,),);

  RESU[k]=MECA_STATIQUE(MODELE=MODE[k],
                        CHAM_MATER=MATE[k],
                        EXCIT=_F(CHARGE=CHAR[k],),);

  RESU[k]=CALC_CHAMP(reuse =RESU[k],
                     RESULTAT=RESU[k],
                     CONTRAINTE=('SIGM_ELNO','SIGM_NOEU',),
                     CRITERES=('SIEQ_ELNO','SIEQ_ELGA','SIEQ_NOEU'),);

  nomfic="MED_%d" %k

  DEFI_FICHIER (UNITE=80+k,FICHIER=nomfic);

  IMPR_RESU(FORMAT='MED',
            UNITE=80+k,
            RESU=_F(MAILLAGE=MAIL[k],
                    RESULTAT=RESU[k],
                    NOM_CHAM=('DEPL','SIGM_NOEU','SIEQ_NOEU','SIEQ_ELGA',),),);

  DEFI_FICHIER(ACTION='LIBERER',UNITE=80+k,)
  
  RESU[k]=CALC_ERREUR(reuse =RESU[k],
                      RESULTAT=RESU[k],
                      OPTION='ERME_ELEM',);

  if k == nbadap :
    break

  MAIL[k+1]=CO('MAIL_%d' % (k+1))

  MACR_ADAP_MAIL(MAILLAGE_N=MAIL[k],
                 MAILLAGE_NP1=MAIL[k+1],
                 ADAPTATION='RAFFINEMENT',
                 RESULTAT_N=RESU[k],
                 MAILLAGE_FRONTIERE=MAILTR,
                 GROUP_MA_FRONT='trou',
                 NOM_CHAM='ERME_ELEM',
                 NOM_CMP='ERREST',
                 CRIT_RAFF_PE=0.1,);

