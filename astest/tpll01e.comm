#CAS_TEST__: TPLL01E
# CAS TEST TPLL01E

DEBUT(CODE=_F(NOM='TPLL01E',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PHI0=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=0.0           )

PHI1=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=-1200.0       )

COEH=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=30.0          )

T_EX=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=140.0         )

T_IM=DEFI_CONSTANTE(    NOM_RESU='FLUN',  VALE=100.0         )

MATERI=DEFI_MATERIAU( THER=_F(  LAMBDA = 0.75, RHO_CP = 2.0))

MAIL=LIRE_MAILLAGE(FORMAT='MED', )

MAIL=DEFI_GROUP(reuse=MAIL,MAILLAGE=MAIL, CREA_GROUP_NO=_F( TOUT_GROUP_MA = 'OUI'))

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                AFFE=_F(  TOUT = 'OUI', MATER = MATERI) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                      AFFE=_F( TOUT = 'OUI', MODELISATION = '3D',
                                      PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER_F(  MODELE=MOTH,
          FLUX_REP=_F( GROUP_MA = 'G2',    FLUN = PHI1),
           ECHANGE=_F( GROUP_MA = 'F1SUP',  COEF_H = COEH, TEMP_EXT = T_EX),
          TEMP_IMPO=_F( GROUP_NO = 'F1INF',   TEMP = T_IM)
         )

LR8=DEFI_LIST_REEL(        DEBUT=0.,
       INTERVALLE=_F(    JUSQU_A = 10.0,   NOMBRE = 2)
                       )

TEMPE=THER_LINEAIRE(
                                MODELE=MOTH,
                             ETAT_INIT=_F( STATIONNAIRE = 'OUI'),
                             INCREMENT=_F( LIST_INST = LR8),
                            CHAM_MATER=CHMAT,
                                 EXCIT=_F( CHARGE = CHTH),
                               SOLVEUR=_F( METHODE = 'LDLT', RENUM = 'RCMK')
                          )

TEMPE=CALC_ELEM( reuse=TEMPE,
                 RESULTAT=TEMPE,
                 OPTION=('FLUX_ELNO',
                 'FLUX_ELGA',),);


TEST_RESU(RESU=(
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='TEMP',
   GROUP_NO = 'A',     NOM_CMP = 'TEMP', VALE = 100.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='TEMP',
   GROUP_NO = 'B',     NOM_CMP = 'TEMP', VALE = 20.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='TEMP',
   GROUP_NO = 'G',     NOM_CMP = 'TEMP', VALE = 60.0),

   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELNO',
   MAILLE = 'M1', POINT = 3, NOM_CMP = 'FLUX', VALE = 960.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELNO',
   MAILLE = 'M2', POINT = 2, NOM_CMP = 'FLUY', VALE = 720.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELGA',
   MAILLE = 'M1', POINT = 3, NOM_CMP = 'FLUX', VALE = 960.0),
   _F( RESULTAT = TEMPE, NUME_ORDRE=2, NOM_CHAM='FLUX_ELGA',
   MAILLE = 'M2', POINT = 2, NOM_CMP = 'FLUY', VALE = 720.0))
          )

FIN()
