#            CONFIGURATION MANAGEMENT OF EDF VERSION
#
# CONTACT-FROTTANT EN GRANDS GLISSEMENTS
# AVEC XFEM POUR DES FISSURES HORIZONTALES
#

DEBUT(CODE=_F(NOM='SSNP503F',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

PRE_GIBI();

typma=1

if (typma==1):
  MAILLAG2=LIRE_MAILLAGE(INFO=1,);
  Y=2.625

if (typma==2):
  MAILLAG1=LIRE_MAILLAGE(INFO=1,);
  MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA='SURF',),);
  Y=2.5

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'NFISSU',
                                       OPTION = 'PLAN',
                                       POINT  = (0.0,Y,),
                                       VECT_NORMALE = (0.0,1.0),
                                       PRECISION = 1E-1,),),)

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('SURF',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='D_PLAN_XFEM_CONT',),),); 

LN1=FORMULE(NOM_PARA=('X','Y'),VALE='Y-2.5');

LN2=FORMULE(NOM_PARA=('X','Y'),VALE='-Y+6.5');
  
FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN1,),
                     GROUP_MA_ENRI='SURF',);

FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN2,),
                     GROUP_MA_ENRI='SURF');
                    
MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='OUI',
                         INFO=1,);
                         
CTXFEM = AFFE_CHAR_MECA( CONTACT=(_F(METHODE='XFEM',
                                     INTEGRATION='NOEUD',
                                     FROTTEMENT='COULOMB',
                                     COULOMB=0.5,
                                     COEF_REGU_FROT=100,
                                     ITER_FROT_MAXI=10,
                                     COEF_REGU_CONT=100., 
                                     ITER_CONT_MAXI=4,
                                     ITER_GEOM_MAXI=10,
                                     TOLE_PROJ_EXT =0.0,   
                                     CONTACT_INIT='OUI',
                                     GLISSIERE='NON',   
                                     COEF_ECHELLE=1.,
                                     FISS_MAIT = FISS1,
                                     ALGO_LAGR='VERSION2',
                                     ),
                                  _F(METHODE='XFEM',
                                     INTEGRATION='NOEUD',
                                     FROTTEMENT='COULOMB',
                                     COULOMB=0.5,
                                     COEF_REGU_FROT=100,
                                     ITER_FROT_MAXI=10,
                                     COEF_REGU_CONT=100., 
                                     ITER_CONT_MAXI=4,
                                     ITER_GEOM_MAXI=10,
                                     TOLE_PROJ_EXT =0.0, 
                                     CONTACT_INIT='OUI', 
                                     GLISSIERE='NON',   
                                     COEF_ECHELLE=1.,
                                     FISS_MAIT = FISS2,
                                     ALGO_LAGR='VERSION2',
                                    ),  
                                  ),
                                MODELE=MODELEK,
                          ),
                          
E=100.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                                MATER=ACIER,),);

DEPX=2.0
DEPY=1.E-3
ZERO=0.0

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIGINF',
                                          DY=DEPY,),
                             _F(GROUP_MA='LIGSUP',
                                          DY=-DEPY,),                        
                             _F(GROUP_NO=('P1','P2','P3','P4'),
                                DX=0.0,),),
                   INFO=1,);
                    
CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LICOTE',
                                DX=DEPX,),),
                   # CONDITIONS IMPOSEES A LA MAIN
                   # POUR LES MAILLES XFEM INFERIEURE
                   # ET SUPERIEURE
                   LIAISON_DDL=(_F(
                                   NOEUD=('N51','N51','N390','N390',),
                                   DDL=('DCX','H1X','DCX','H1X',),
                                   COEF_MULT=(.5,0.5,0.5,.5),
                                   COEF_IMPO=DEPX,),
                                _F(
                                   NOEUD=('N33','N33','N70','N70',),
                                   DDL=('DCX','H1X','DCX','H1X',),
                                   COEF_MULT=(.5,0.5,0.5,.5),
                                   COEF_IMPO=DEPX,),
                                ),
                    INFO=1,);               

VAR1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );
                    
VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );   
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      CONTACT_XFEM=CTXFEM,
                      LIAISON_XFEM='OUI',
                      INFO=1,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,              
                    EXCIT=(_F(CHARGE=CTXFEM,),
                           _F(CHARGE=CHXFEM,),                                    
                           _F(CHARGE=CH1,FONC_MULT=VAR1),
                           _F(CHARGE=CH2,FONC_MULT=VAR2),),  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=20,
                                    RESI_GLOB_MAXI=1E-06
                                    )),
                    NEWTON=_F(REAC_ITER=1,),
                    SOLVEUR=_F(METHODE='MUMPS',
                               PCENT_PIVOT=80,),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);

LAG=[None]*5
inst=[0.0,0.25,0.5,0.75,1.0]

vale = [[0.0,-7.1942608427242E+03,-6.5408343967577E+03,-6.1360066750219E+03,-2.4914329082053E+03],
        [0.0,-2.4822528478454E+04,-1.2993429314030E+04,-1.2178296348092E+04,-3.4642908121105E+03],
        [0.0,0.0,-2.0219781259344E+04,-1.8168928613392E+04,-4.8271685695499E+03],
        [0.0,0.0,-4.3387251217166E+04,-2.4197888759692E+04,-7.2647039764449E+03]]

for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                         GROUP_NO=('NFISSU'),
                                         RESULTAT=UTOT1,
                                         NOM_CHAM='DEPL',
                                         INST=inst[i],
                                         NOM_CMP=('LAGS_C','LAGS_F1',),
                                         OPERATION='EXTRACTION',),);
   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              FORMAT_C='MODULE_PHASE',
              NOM_PARA=('NOEUD','INST','COOR_X','LAGS_C','LAGS_F1',),
              IMPR_FONCTION='NON',
              TRI=_F(NOM_PARA=('COOR_X',),
                     ORDRE='CROISSANT',),);
   
   for j in [0,1,2,3,4]: #Boucle sur les positions testees
      if (vale[i-1] [j] == 0):
        precision = 1.0E-10
        critere = 'ABSOLU'
      else:
        precision = 5.E-2 
        critere = 'RELATIF'
      if ((i >= 3) and (j == 4)):precision = 6.5E-2
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE=vale[i-1] [j],
                 CRITERE=critere,
                 PRECISION=precision,
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=_F(NOM_PARA='COOR_X',
                           CRIT_COMP='EQ',
                           VALE=j,
                           PRECISION=0.03,
                           CRITERE='ABSOLU',),);
FIN();                  

