DEBUT( CODE=_F( NOM = 'SSNV145B',NIV_PUB_WEB='INTERNET'))

#
#
# MODELISATION : COQUE_3D
#
# MAILLAGE     : MEC3TR7H
#

MA1=LIRE_MAILLAGE(FORMAT='MED',  )

MA1=DEFI_GROUP( reuse=MA1,   MAILLAGE=MA1,
                     CREA_GROUP_MA=_F(  NOM = 'TOUT',  TOUT = 'OUI'))

#
#

MO1=AFFE_MODELE(  MAILLAGE=MA1,
          AFFE=_F( TOUT = 'OUI', MODELISATION = 'COQUE_3D',
          PHENOMENE = 'MECANIQUE') )

#

MAT=DEFI_MATERIAU(   ELAS=_F(  E = 12.E6,  NU = 0.3,  ALPHA = 0.)
                        )

#

CHMAT1=AFFE_MATERIAU(   MAILLAGE=MA1,
                          AFFE=_F(  TOUT = 'OUI',  MATER = MAT)
                        )

#
#

CHA1=AFFE_CHAR_MECA(   MODELE=MO1,

                          DDL_IMPO=_F( GROUP_NO = 'G_41',  DX = 0., DY = 0., DZ = 0.,
                                            DRX = 0., DRY = 0., DRZ = 0.)
                        )

#

CHA2=AFFE_CHAR_MECA(         MODELE=MO1,
                FORCE_COQUE=_F(  TOUT = 'OUI',   PRES = 1.)
                        )

#

CARA1=AFFE_CARA_ELEM(
                               MODELE=MO1,

                                     COQUE=_F(
                                       GROUP_MA = 'TOUT',
                                       EPAIS = 0.1,
                                       COEF_RIGI_DRZ = 1.E-4)
                          )

#
#
########################################################################
#
#

L_INST=DEFI_LIST_REEL(   DEBUT=0.0,
                             INTERVALLE=_F(  JUSQU_A = 22.,  NOMBRE = 10)
                         )

#

D_FONC=DEFI_FONCTION(
                            NOM_PARA='INST',
                                VALE=(
                                        0.,    0.,
                                       1000., 1000.,
                                     )
                         )

#
#

NONLI=STAT_NON_LINE(
                            MODELE=MO1,
                        CHAM_MATER=CHMAT1,
                         CARA_ELEM=CARA1,EXCIT=(
                             _F(
                CHARGE = CHA1), _F(
                CHARGE = CHA2,
                FONC_MULT = D_FONC,
                TYPE_CHARGE = 'SUIV')),
                         COMP_ELAS=_F(
                RELATION = 'ELAS',
                DEFORMATION = 'GROT_GDEP'),
                       CONVERGENCE=_F(
                ITER_GLOB_MAXI = 50),
                         INCREMENT=_F(
                LIST_INST = L_INST),
                            NEWTON=_F(
                REAC_ITER = 1,
                MATRICE = 'TANGENTE')
              )

