# POURSUITE

RESO=STAT_NON_LINE(MODELE=MO,
                   reuse=RESO,
                   CHAM_MATER=MAT,
                   CARA_ELEM=CARA,
                   EXCIT=(_F(CHARGE=CL,),
                          _F(CHARGE=FORCE,
