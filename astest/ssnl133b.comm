                             REAC_ITER=1,),
                   PILOTAGE=_F(TYPE='LONG_ARC',
                               COEF_MULT=0.1,
                               ETA_PILO_MAX=5.0,
                               SELECTION='ANGL_INCR_DEPL',
                               GROUP_NO='LAME',
                               NOM_CMP=('DX','DZ',),),
