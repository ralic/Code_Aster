                    DDL_IMPO=(_F(GROUP_NO='BAS',
                                 DY=0.0,),
                              _F(GROUP_NO='GAUCHE',
                                 DX=0.0,),
                              _F(GROUP_NO='DROIT',
                                 DX=0.001,),
                                 ),);


# MATERIAU DE TYPE CHABOCHE, SOIT MACRO, SOIT MONOCRISTAL
