TEST_RESU(RESU=(_F(RESULTAT=EXPL1,
                   INST=TFIN,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=95.999316,
                   REFERENCE='AUTRE_ASTER',),
                 ),);



FIN();
                   
