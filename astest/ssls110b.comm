                   VALE_CALC=2.2727272727273E-07,
                   VALE_REFE=2.2727272727273E-07,
                   PRECISION= 1.E-6),
                _F(NUME_ORDRE=1,
                   GROUP_NO='G_3',
                   MAILLE ='MA000240',
                   REFERENCE='ANALYTIQUE',
                   RESULTAT=RESU,
                   NOM_CHAM='DEGE_ELNO',
