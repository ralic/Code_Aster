def ecart_relatif(diff, ref):
    if abs(ref) == 0 :
       err=diff
    else :
       err=diff/ref
    return err

TEST1=CALC_TABLE(TABLE=TDIF1,
ACTION= _F(OPERATION='COMB',TABLE= TRHO1, NOM_PARA ='INST'))

ecarela1=FORMULE(NOM_PARA=('NEUT1','RHO_5'),VALE=("""ecart_relatif(NEUT1,RHO_5)"""))

