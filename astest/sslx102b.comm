# MODELISATION TUYAU

DEBUT(CODE=_F(NOM='SSLX102B',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED',INFO=2 )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MA=DEFI_GROUP( reuse=MA,   INFO=2,
                MAILLAGE=MA,
           CREA_GROUP_NO=_F(  NOM = 'AD',
OPTION = 'NOEUD_ORDO',
GROUP_MA = 'AD',
GROUP_NO_ORIG = 'A',
GROUP_NO_EXTR = 'D')
                   )

MO=AFFE_MODELE(        MAILLAGE=MA, INFO=2,
     AFFE=_F(  TOUT = 'OUI',   PHENOMENE = 'MECANIQUE',
     MODELISATION = 'TUYAU_3M')
               )

CAREL=AFFE_CARA_ELEM(   MODELE=MO, INFO=2,
          POUTRE=_F(
       GROUP_MA = 'TOUT',
  SECTION = 'CERCLE',
           CARA = (   'R',  'EP', ), VALE = (  0.10959,  0.00818, )),
ORIENTATION=_F( GROUP_NO = 'D', CARA = 'GENE_TUYAU', VALE = (0., 0., 1.,))
                              )

MAT=DEFI_MATERIAU(   ELAS=_F( E = 2.E11, NU = 0.3,  RHO = 7900.),
                   ECRO_LINE=_F( D_SIGM_EPSI = 12.5E9, SY = 200.E6)
                   )

CHMAT=AFFE_MATERIAU(   MAILLAGE=MA,
                       AFFE=_F( TOUT = 'OUI', MATER = MAT))

#FLEXION PLANE

CHAR1=AFFE_CHAR_MECA(  MODELE=MO,
     DDL_IMPO=_F( GROUP_NO = 'A', DX = 0., DY = 0., DZ = 0., DRX = 0., DRY = 0., DRZ = 0.),
     FORCE_NODALE=_F( GROUP_NO = 'D', MZ = 17.E3)
    )

RESU1=MECA_STATIQUE(  MODELE=MO,
                          CHAM_MATER=CHMAT,   CARA_ELEM=CAREL,
                         EXCIT=_F(  CHARGE = CHAR1)
                    )

TEST_RESU(
           RESU=_F( RESULTAT = RESU1,
                 NUME_ORDRE = 1,
                 NOM_CHAM = 'DEPL',
                 GROUP_NO = 'D',
                 NOM_CMP = 'DY',
                 VALE = 2.E-2,
                 PRECISION = 7.E-2)
                    )

# FLEXION HORS PLAN

CHAR2=AFFE_CHAR_MECA(  MODELE=MO,
     DDL_IMPO=_F( GROUP_NO = 'A', DX = 0., DY = 0., DZ = 0., DRX = 0., DRY = 0., DRZ = 0.),
     FORCE_NODALE=_F( GROUP_NO = 'D', MY = 17.E3)
    )

RESU2=MECA_STATIQUE(  MODELE=MO,
                          CHAM_MATER=CHMAT,   CARA_ELEM=CAREL,
                         EXCIT=_F(  CHARGE = CHAR2)
                    )

TEST_RESU(
           RESU=_F( RESULTAT = RESU2,
                 NUME_ORDRE = 1,
                 NOM_CHAM = 'DEPL',
                 GROUP_NO = 'D',
                 NOM_CMP = 'DZ',
                 VALE = -1.5657E-2,
                 PRECISION = 2.E-2)
                    )

MASSEAB=POST_ELEM(   MODELE=MO,
       CHAM_MATER=CHMAT,
       CARA_ELEM=CAREL,
       MASS_INER=_F(
     GROUP_MA = 'AB')
     )

MASSEBC=POST_ELEM(   MODELE=MO,
       CHAM_MATER=CHMAT,
       CARA_ELEM=CAREL,
       MASS_INER=_F(
     GROUP_MA = 'BC')
     )

TEST_TABLE( TABLE=MASSEAB,
                NOM_PARA='MASSE',
               REFERENCE='ANALYTIQUE',
                VALE=51.40365,   PRECISION=1.E-5)

TEST_TABLE( TABLE=MASSEBC,
                NOM_PARA='MASSE',
                VALE=20.5226,   PRECISION=1.E-5)

TEST_TABLE( TABLE=MASSEAB,
                NOM_PARA='CDG_Y',
                VALE=0.905,   PRECISION=1.E-7)

TEST_TABLE( TABLE=MASSEBC,
                NOM_PARA='CDG_X',
                VALE=0.099197,   PRECISION=1.E-5)

TEST_TABLE( TABLE=MASSEBC,
                NOM_PARA='CDG_Y',
                VALE=0.099197,   PRECISION=1.E-5)


MASS2AB=POST_ELEM(   MODELE=MO,RESULTAT = RESU2,GEOMETRIE='DEFORMEE',
                 NUME_ORDRE = 1,
       CHAM_MATER=CHMAT,
       CARA_ELEM=CAREL,
       MASS_INER=_F(
     GROUP_MA = 'AB')
     )

MASS2BC=POST_ELEM(   MODELE=MO,RESULTAT = RESU2,GEOMETRIE='DEFORMEE',
                 NUME_ORDRE = 1,
       CHAM_MATER=CHMAT,
       CARA_ELEM=CAREL,
       MASS_INER=_F(
     GROUP_MA = 'BC')
     )

TEST_TABLE( TABLE=MASS2AB,
                NOM_PARA='MASSE', REFERENCE='NON_REGRESSION',
                VALE=51.4037,   PRECISION=1.E-5)

TEST_TABLE( TABLE=MASS2BC,
                NOM_PARA='MASSE', REFERENCE='NON_REGRESSION',
                VALE=20.5226,   PRECISION=1.E-5)

TEST_TABLE( TABLE=MASS2AB,
                NOM_PARA='CDG_Y', REFERENCE='NON_REGRESSION',
                VALE=0.905,   PRECISION=1.E-7)

TEST_TABLE( TABLE=MASS2BC,
                NOM_PARA='CDG_X', REFERENCE='NON_REGRESSION',
                VALE=0.099197,   PRECISION=1.E-5)

TEST_TABLE( TABLE=MASS2BC,
                NOM_PARA='CDG_Y', REFERENCE='NON_REGRESSION',
                VALE=0.099197,   PRECISION=1.E-5)


# TEST DES NOEUDS ORDONNES PAR DEFI_GROUP

EVOL=POST_RELEVE_T(    ACTION=_F( INTITULE = 'DEPL',
  GROUP_NO = 'AD',  RESULTAT = RESU1, NOM_CHAM = 'DEPL',
  TOUT_CMP = 'OUI',  OPERATION = 'EXTRACTION'))

TEST_TABLE( TABLE=EVOL,
                NOM_PARA='ABSC_CURV',
       FILTRE=_F( NOM_PARA = 'COOR_X',
               VALE = 1.505),
               REFERENCE='ANALYTIQUE',
                VALE=2.87909288E+00  )

FIN()
