# CAS TEST TPLL01A
DEBUT(  CODE=_F( NOM = 'TPLL01A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

ACIER=DEFI_MATERIAU(  THER=_F(  LAMBDA = 0.75, RHO_CP = 2.0))

MAIL=LIRE_MAILLAGE( )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MAIL,
                        AFFE=_F(  TOUT = 'OUI', MATER = ACIER) )

MOTH=AFFE_MODELE(  MAILLAGE=MAIL,
                     AFFE=_F( TOUT = 'OUI', MODELISATION = 'PLAN_DIAG',
                                      PHENOMENE = 'THERMIQUE'))

CHTH=AFFE_CHAR_THER(   MODELE=MOTH,FLUX_REP=(
                          _F( GROUP_MA = 'GRMA13', FLUN = 0.0),
                          _F( GROUP_MA = 'GRMA12', FLUN = -1200.0)),
                           ECHANGE=_F( GROUP_MA = 'GRMA14', COEF_H = 30.0,
                                    TEMP_EXT = 140.0),
                          TEMP_IMPO=_F( GROUP_NO = 'GRNM15', TEMP = 100.0),
                         LIAISON_DDL=_F( NOEUD = ('N3', 'N1',),
#  LA LIGNE CI-DESSOUS EST FACULTATIVE , PAR DEFAUT LA LISTE DE
#  DDL EST 'TEMP'
#                                     DDL: ('TEMP' 'TEMP')
                                      COEF_MULT = (1., -1.,),
                                      COEF_IMPO = 40.)
                       )

TEMPE=THER_LINEAIRE(   MODELE=MOTH,
                       CHAM_MATER=CHMAT,
                       EXCIT=_F(  CHARGE = CHTH),
                       )


TEMPE=CALC_ELEM(reuse=TEMPE, RESULTAT=TEMPE,

                NUME_ORDRE=0,
                OPTION=('FLUX_ELNO','FLUX_ELGA'),
               )

TEST_RESU(RESU=(
   _F( RESULTAT = TEMPE, NOEUD = 'N5', NUME_ORDRE=0, NOM_CHAM='TEMP',
   NOM_CMP = 'TEMP', VALE = 100.0),
   _F( RESULTAT = TEMPE, NOEUD = 'N1', NUME_ORDRE=0, NOM_CHAM='TEMP',
   NOM_CMP = 'TEMP', VALE = 20.0),
   _F( RESULTAT = TEMPE, NOEUD = 'N3', NUME_ORDRE=0, NOM_CHAM='TEMP',
   NOM_CMP = 'TEMP', VALE = 60.0),

   _F( RESULTAT = TEMPE, MAILLE = 'M1', NUME_ORDRE=0, NOM_CHAM='FLUX_ELNO',
   POINT = 3, NOM_CMP = 'FLUX', VALE = 960.0),
   _F( RESULTAT = TEMPE, MAILLE = 'M2', NUME_ORDRE=0, NOM_CHAM='FLUX_ELNO',
   POINT = 2, NOM_CMP = 'FLUY', VALE = 720.0),
   _F( RESULTAT = TEMPE, MAILLE = 'M1', NUME_ORDRE=0, NOM_CHAM='FLUX_ELGA',
   POINT = 3, NOM_CMP = 'FLUX', VALE = 960.0),
   _F( RESULTAT = TEMPE, MAILLE = 'M2', NUME_ORDRE=0, NOM_CHAM='FLUX_ELGA',
   POINT = 2, NOM_CMP = 'FLUY', VALE = 720.0))
          )

#----------------------------------------------------------------------
# VALIDATION DE TRAC_NOR DE LA COMMANDE POST_RELEVE_T:
#
# CHEMIN ORIENTE DE E VERS D, LA NORMALE VERS L'EXTERIEUR DU DOMAINE
# --------------------------

CHEM_1=INTE_MAIL_2D(   MAILLAGE=MAIL,
                         DEFI_SEGMENT=_F( ORIGINE = ( 0.04,  0.07, ),
                                       EXTREMITE = ( 0.07,  0.03, )),
                           INFO=2
                       )

FLUX_G=POST_RELEVE_T(    ACTION=_F(  INTITULE = 'FLUX_GLO',
                           CHEMIN = CHEM_1,
                           RESULTAT = TEMPE,
                           NUME_ORDRE=0,
                           NOM_CHAM='FLUX_ELNO',
                           NOM_CMP = ( 'FLUX', 'FLUY', ),
                           REPERE = 'GLOBAL',
                           OPERATION = 'EXTRACTION')  )

IMPR_TABLE(   TABLE=FLUX_G,
              NOM_PARA=( 'ABSC_CURV', 'FLUX', 'FLUY', )  )

TEST_TABLE(    TABLE=FLUX_G,
                FILTRE=_F(  NOM_PARA = 'ABSC_CURV',
                         VALE = 2.50000E-02),
                NOM_PARA='FLUX',
                    VALE=960.0
            )

TEST_TABLE(    TABLE=FLUX_G,
                FILTRE=_F(  NOM_PARA = 'ABSC_CURV',
                         VALE = 2.50000E-02),
                NOM_PARA='FLUY',
                    VALE=720.0
            )

#
# LES RESULTATS DOIVENT ETRE IDENTIQUE A TRAC_NOR
# CALCUL DANS LE REPERE LOCAL :
# ---------------------------

FLUX_L=POST_RELEVE_T(    ACTION=_F(  INTITULE = 'FLUX_LOC',
                           CHEMIN = CHEM_1,
                           RESULTAT = TEMPE,
                           NUME_ORDRE=0,
                           NOM_CHAM='FLUX_ELNO',
                           NOM_CMP = ( 'FLUX', 'FLUY', ),
                           REPERE = 'LOCAL',
                           OPERATION = 'EXTRACTION')  )

IMPR_TABLE(   TABLE=FLUX_L,
              NOM_PARA=( 'ABSC_CURV', 'FLUX', 'FLUY', )  )

TEST_TABLE(    TABLE=FLUX_L,
                FILTRE=_F(  NOM_PARA = 'ABSC_CURV',
                         VALE = 2.50000E-02),
                NOM_PARA='FLUX',
                 CRITERE='ABSOLU',
                    VALE=0.0
            )

TEST_TABLE(    TABLE=FLUX_L,
                FILTRE=_F(  NOM_PARA = 'ABSC_CURV',
                         VALE = 2.50000E-02),
                NOM_PARA='FLUY',
                    VALE=1200.0
            )

#
# LES RESULTATS DOIVENT ETRE IDENTIQUE AU REPERE LOCAL
# CALCUL PAR LA TRAC_NORMALE :
# --------------------------

FLUX_N=POST_RELEVE_T(    ACTION=_F(  INTITULE = 'FLUX_NOR',
                           CHEMIN = CHEM_1,
                           RESULTAT = TEMPE,
                           NUME_ORDRE=0,
                           NOM_CHAM='FLUX_ELNO',
                           TRAC_NOR = 'OUI',
                           NOM_CMP = ( 'FLUX', 'FLUY', ),
                           OPERATION = 'EXTRACTION')  )

TEST_TABLE(    TABLE=FLUX_N,
                FILTRE=_F(  NOM_PARA = 'ABSC_CURV',
                         VALE = 0.00000E+00),
                NOM_PARA='TRAC_NOR',
                    VALE=1200.0
            )

TEST_TABLE(    TABLE=FLUX_N,
