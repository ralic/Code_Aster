                             NOM_PARA=('INST','RX','RXG',),),
                          _F(OPERATION='OPER',
                             FORMULE=D_REL_RX,
                             NOM_PARA='RXG_RX',),),);

DETRUIRE(CONCEPT=_F(NOM=(TRX_R1,TRX_G1,),),);

