                                     MATER = MAT1),
                              _F(  GROUP_MA = 'LPB1PA1',
                                     MATER = MAT1),
                              _F(  GROUP_MA = 'LPA1PA',
                                     MATER = MAT1),
                              _F(  GROUP_MA = 'LPBPB1',
                                     MATER = MAT1),
                              _F(  GROUP_MA = 'VTOT2',
                                     MATER = MAT2),
                              _F(  GROUP_MA = 'LPA2PB2',
                                     MATER = MAT2),
                              _F(  GROUP_MA = 'LPAPA2',
                                     MATER = MAT2),
                              _F(  GROUP_MA = 'LPB2PB',
                                     MATER = MAT2))
                     )

MA=DEFI_GROUP( reuse=MA,        MAILLAGE=MA,
                     CREA_GROUP_NO=_F(  GROUP_MA = ( 'LPB1PA1',  'LPA2PB2',
                                                  'LPA1PA',   'LPBPB1',
                                                  'LPAPA2',   'LPB2PB',
