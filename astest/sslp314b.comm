                                               DX = 0.),
                                             _F(  GROUP_NO = 'LPAPA2',
                                               DX = 0.),
                                             _F(  GROUP_NO = 'PA',
                                               DY = 0.)),LIAISON_UNIF=(
                                _F(  GROUP_NO = 'LPBPB1',
                                                DDL = 'DX'),
                                             _F(  GROUP_NO = 'LPB2PB',
                                                DDL = 'DX')),FORCE_CONTOUR=(
                               _F(  GROUP_MA = 'LPB1PA1',
                                               FY = 75.E6),
                                             _F(  GROUP_MA = 'LPA2PB2',
                                               FY = -75.E6))
                           )

CHAMDEPL=MECA_STATIQUE(        MODELE=MO,
                              CHAM_MATER=CHMAT,
                                   EXCIT=_F( CHARGE = CH)
                          )

FOND1=DEFI_FOND_FISS(    MAILLAGE=MA,
                            FOND_FISS=_F( GROUP_NO = ('P0',)),
                      )

