                       SPEC_TURB=SPECTRE,
                   CHAM_NO=CHAMNO,
               MODELE_INTERFACE=INTERF,
               VECT_X=(0., 0., 1.,),
               ORIG_AXE=(0., 0., 0.,),
                FREQ_INIT=1.59155E-3,
                 FREQ_FIN=1.59155E-2,
                 NB_POIN=2
