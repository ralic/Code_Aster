                  SPEC_CORR_CONV_1=_F( 
                     LONG_COR_1 = 1.,
                     LONG_COR_2 = 1.,
                      VITE_FLUI = 4.,
                      FREQ_COUP = 15.,
                      K = 3.4021E-5,
                      RHO_FLUI = 1000.,
                       D_FLUI = 1.5,
                      COEF_VITE_FLUI_A = 0.65,
                      COEF_VITE_FLUI_O = 1.,
                       METHODE = 'AU_YANG')
                            )

# PROJECTION DE CE SPECTRE SUR LA BASE MODALE EN
# EAU DU CYLINDRE ET CALCUL DE LA FONCTION ACCEPTANCE


