DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET',),);

# Read the mesh
mesh=LIRE_MAILLAGE(FORMAT='MED',);

# Thermal conductivity depending by the temperature (W.mm-1.K-1)
Lambda=DEFI_FONCTION(
                     NOM_PARA='TEMP',
                     VALE=(20.0 ,0.014 ,
                           100.0 ,0.0152 ,
                           200.0 ,0.0166 ,
                           300.0 ,0.0179 ,
                           400.0 ,0.019 ,
                           500.0 ,0.0206 ,
                           600.0 ,0.0218 ,
                           700.0 ,0.0231 ,
                           800.0 ,0.0243 ,
                           900.0 ,0.026 ,
                           1000.0 ,0.0273 ,
                           1200.0 ,0.0299 ,),
                     PROL_DROITE='LINEAIRE',
                     PROL_GAUCHE='LINEAIRE',);

# Volumetric heat capacity depending by the temperature (J.mm-3.K-1)
rho_cp=DEFI_FONCTION(
                     NOM_PARA='TEMP',
                     VALE=(20.0 ,0.0036 ,
                           100.0 ,0.0039053 ,
                           200.0 ,0.0041685 ,
                           300.0 ,0.0043 ,
                           400.0 ,0.004396 ,
                           500.0 ,0.004446 ,
                           600.0 ,0.004495 ,
                           700.0 ,0.0045815 ,
                           800.0 ,0.0047875 ,
                           900.0 ,0.0049465 ,
                           1000.0 ,0.0049962 ,
                           1200.0 ,0.00504365 ,),
                     PROL_DROITE='LINEAIRE',
                     PROL_GAUCHE='LINEAIRE',);
# Heat transfer coefficient with air (W.mm-2.K-1)
C_echang=DEFI_CONSTANTE(VALE=1.5e-05,);
# Heat transfer coefficient with one face of the cube (W.mm-2.K-1)
C_echanf=DEFI_CONSTANTE(VALE=1.0,);
# Ambient temperature
T_ex=DEFI_CONSTANTE(VALE=20.0,);
# Temperature of one face of the cube
T_im=DEFI_FONCTION(
                   NOM_PARA='INST',
                   VALE=(0.0 ,20.0 ,
                         1.0 ,30.0 ,
                         2.0 ,50.0 ,
                         3.0 ,90.0 ,
                         4.0 ,140.0 ,
                         5.0 ,240.0 ,
                         6.0 ,350.0 ,
                         7.0 ,570.0 ,
                         8.0 ,800.0 ,
                         9.0 ,1000.0 ,),
                   PROL_DROITE='CONSTANT',
                   PROL_GAUCHE='LINEAIRE',);
# Emissivity
C_epsi=DEFI_CONSTANTE(VALE=0.75,);
# Stefan–Boltzmann constant (J.mm-2.s-1.K-4)
C_sigma=DEFI_CONSTANTE(VALE=5.67e-14,);
# Define the material
steel=DEFI_MATERIAU(THER_NL=_F(LAMBDA=Lambda,
                             RHO_CP=rho_cp,),);
# Define the material field
chmat=AFFE_MATERIAU(MAILLAGE=mesh,
                    AFFE=_F(TOUT='OUI',
                            MATER=steel,),);
# Thermic model
model=AFFE_MODELE(MAILLAGE=mesh,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='THERMIQUE',
                       MODELISATION='3D',),);
# Limit condition
chg_th=AFFE_CHAR_THER_F(MODELE=model,
                        RAYONNEMENT=_F(GROUP_MA='S_e',
                                       SIGMA=C_sigma,
                                       EPSILON=C_epsi,
                                       TEMP_EXT=T_ex,),
                        ECHANGE=(_F(GROUP_MA='S_e',
                                    COEF_H=C_echang,
                                    TEMP_EXT=T_ex,),
                                 _F(GROUP_MA='S_f',
                                    COEF_H=C_echanf,
                                    TEMP_EXT=T_im,),),);
list_t=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=10.0,
                                    PAS=1.0,),);

list_i=DEFI_LIST_INST(DEFI_LIST=_F(METHODE='MANUEL',
                                   LIST_INST=list_t,),);

thnl=THER_NON_LINE(MODELE=model,
                   CHAM_MATER=chmat,
                   EXCIT=_F(CHARGE=chg_th,),
                   INCREMENT=_F(LIST_INST=list_i,),
                   ETAT_INIT=_F(VALE=20.0,),
                   NEWTON=_F(REAC_ITER=1,
                             ITER_LINE_MAXI=3,),
                   CONVERGENCE=_F(RESI_GLOB_MAXI=1e-5,),);

# Valeurs de référence (calcul complet)

TEST_RESU(RESU=(
                _F(RESULTAT=thnl,
                   INST= 1.0,
                   NOM_CHAM='TEMP',
                   NOEUD='N27',
                   NOM_CMP='TEMP',
                   VALE_CALC=29.9053048593,
                   ),
                _F(RESULTAT=thnl,
                   INST= 4.0,
                   NOM_CHAM='TEMP',
                   NOEUD='N27',
                   NOM_CMP='TEMP',
                   VALE_CALC=139.462715634,
                   ),
                _F(RESULTAT=thnl,
                   INST= 7.0,
                   NOM_CHAM='TEMP',
                   NOEUD='N27',
                   NOM_CMP='TEMP',
                   VALE_CALC=567.08693147,
                   ),
                _F(RESULTAT=thnl,
                   INST= 10.0,
                   NOM_CHAM='TEMP',
                   NOEUD='N27',
                   NOM_CMP='TEMP',
                   VALE_CALC=999.349943608,
                   ),
                ),
          );

base_p=LIRE_RESU(TYPE_RESU  = 'MODE_EMPI',
                 FORMAT     = 'MED',
                 MODELE     = model,
                 UNITE      = 70,
                 FORMAT_MED =_F(NOM_CHAM_MED = 'base_p__TEMP',
                                NOM_CHAM     = 'TEMP',),
                 TOUT_ORDRE = 'OUI',);


redu=THER_NON_LINE(MODELE=model,
                    CHAM_MATER=chmat,
                    EXCIT=_F(CHARGE=chg_th,),
                    INCREMENT=_F(LIST_INST=list_i,),
                    ETAT_INIT=_F(VALE=20.0,),
                    METHODE='MODELE_REDUIT',
                    MODELE_REDUIT=_F(
                    REAC_ITER=1,
                    BASE_PRIMAL     = base_p,
                    DOMAINE_REDUIT  = 'NON',
                    ),
                   CONVERGENCE=_F(RESI_GLOB_MAXI=1e-5,),);


# Test du modèle réduit par rapport au modèle complet

TEST_RESU(RESU=(
                _F(RESULTAT=redu,
                   INST= 1.0,
                   NOM_CHAM='TEMP',
                   NOEUD='N27',
                   NOM_CMP='TEMP',
                   VALE_CALC=29.9036894091,
                   VALE_REFE=29.9053048593,
                   REFERENCE='AUTRE_ASTER',
                   PRECISION=6.E-5,
                   ),
                _F(RESULTAT=redu,
                   INST= 4.0,
                   NOM_CHAM='TEMP',
                   NOEUD='N27',
                   NOM_CMP='TEMP',
                   VALE_CALC=139.460161085,
                   VALE_REFE=139.462715634,
                   REFERENCE='AUTRE_ASTER',
                   PRECISION=2.E-5,
                   ),
                _F(RESULTAT=redu,
                   INST= 7.0,
                   NOM_CHAM='TEMP',
                   NOEUD='N27',
                   NOM_CMP='TEMP',
                   VALE_CALC=567.082979485,
                   VALE_REFE=567.08693147,
                   REFERENCE='AUTRE_ASTER',
                   PRECISION=7.E-6,
                   ),
                _F(RESULTAT=redu,
                   INST= 10.0,
                   NOM_CHAM='TEMP',
                   NOEUD='N27',
                   NOM_CMP='TEMP',
                   VALE_CALC=999.344384033,
                   VALE_REFE=999.349943608,
                   REFERENCE='AUTRE_ASTER',
                   PRECISION=6.E-6,
                   ),
                ),
          );
FIN();
