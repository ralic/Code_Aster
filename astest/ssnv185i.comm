DEBUT(CODE=_F(NOM='SSNV185I',NIV_PUB_WEB='INTERNET',));

# MEME MODELISATION QUE LA E, AVEC CHARGEMENT DE NEUMANN A LA PLACE DE DIRICHLET

#----------------------------------------------
#             MAILLAGE : hexa_3_11_31.mgib
#----------------------------------------------

# precision
n=1000.

# dimensions de la structure
lx=1.
ly=10.
lz=30.


PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_NO=(
                                   _F(NOM='ASUP',OPTION='ENV_SPHERE',POINT=(lx  ,0.        ,lz/2. + 15./31. ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='AINF',OPTION='ENV_SPHERE',POINT=(lx  ,0.        ,lz/2. - 15./31. ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='BSUP',OPTION='ENV_SPHERE',POINT=(0.  ,0.        ,lz/2. + 15./31. ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='BINF',OPTION='ENV_SPHERE',POINT=(0.  ,0.        ,lz/2. - 15./31. ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='CSUP',OPTION='ENV_SPHERE',POINT=(0   ,2.*ly/11. ,lz/2. + 15./31. ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                   _F(NOM='CINF',OPTION='ENV_SPHERE',POINT=(0   ,2.*ly/11. ,lz/2. - 15./31. ),PRECISION=1/(4.*n),RAYON=1/(4.*n)),
                                  ),
                    INFO=2,);

#MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
#                       LINE_QUAD=_F(GROUP_MA=('VOL','SURFGAU','SURFDRO',),),);
MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA=('VOL','SURFINF','SURFSUP','SURFGAU','SURFDRO',),),);

#IMPR_RESU(FORMAT='RESULTAT',RESU=_F(MAILLAGE=MAILLAG2,));

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

#MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
#                     AFFE=(_F(GROUP_MA=('VOL','SURFGAU','SURFDRO',),
#                              PHENOMENE='MECANIQUE',
#                              MODELISATION='3D_XFEM',),
#                           _F(GROUP_MA=('SURFINF','SURFSUP',),
#                              PHENOMENE='MECANIQUE',
#                              MODELISATION='3D',),),);  
MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOL','SURFINF','SURFSUP','SURFGAU','SURFDRO',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D_XFEM',),
                           ),);  

MAILLAG2=MODI_MAILLAGE(reuse =MAILLAG2,
                       MAILLAGE=MAILLAG2,
                       ORIE_PEAU_3D=_F(GROUP_MA=('SURFSUP','SURFINF',),),
                       MODELE=MODELEIN,);

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-15.');
LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='-Y+5.');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                    GROUP_MA_ENRI='VOL',
                    ORIE_FOND=_F(PFON_INI= (0. ,5. ,15. ),
                                 VECT_ORIE=(0. ,0. ,1. ),
                                 PT_ORIGIN=(0. ,20. ,15. ),),
                    CONTACT=_F(ALGO_LAGR='NON',),);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,FISSURE=FISS,INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,LIAISON_XFEM='OUI',INFO=1,);

#----------------------------------------------
#                   MATERIAU
#----------------------------------------------

E=100.0E6
nu=0.
rho=7800.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=rho,),);

CHAMPMA1=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT = 'OUI',
                                MATER=ACIER,
                                TEMP_REF=0.0,),
                             );

#--------------------------------------------------------
#                   CHARGEMENT : CHAMP AUXILIAIRE MODE I
#--------------------------------------------------------

# blocage des modes rigides

CH=AFFE_CHAR_MECA(MODELE=MODELEK,
                  LIAISON_DDL=(_F(NOEUD=('N529','N496',),
                                   DDL=('DX','DX',),
                                   COEF_MULT=(1.0,1.0),
                                   COEF_IMPO=0.0,),
                                _F(NOEUD=('N529','N496',),
                                   DDL=('DY','DY',),
                                   COEF_MULT=(1.0,1.0),
                                   COEF_IMPO=0.0,),
                                _F(NOEUD=('N529','N496',),
                                   DDL=('DZ','DZ',),
                                   COEF_MULT=(1.0,1.0),
                                   COEF_IMPO=0.0,),

                                _F(NOEUD=('N1294','N1283',),
                                   DDL=('DX','DX',),
                                   COEF_MULT=(1.0,1.0),
                                   COEF_IMPO=0.0,),
                                _F(NOEUD=('N1294','N1283',),
                                   DDL=('DZ','DZ',),
                                   COEF_MULT=(1.0,1.0),
                                   COEF_IMPO=0.0,),     

                                _F(NOEUD=('N1474','N1473',),
                                   DDL=('DZ','DZ',),
                                   COEF_MULT=(1.0,1.0),
                                   COEF_IMPO=0.0,),
                             ),);

R = FORMULE(NOM_PARA=('X','Y','Z'),VALE='sqrt((Y-5.)**2+(Z-15.)**2)');
T = FORMULE(NOM_PARA=('X','Y','Z'),VALE='atan2(Z-15.,-Y+5.)');
   
coef=1./sqrt(2.*pi)

# formules des contraintes et de leurs opposees

SIG11 = FORMULE(NOM_PARA=('X','Y','Z'),VALE='coef*1./sqrt(R(X,Y,Z))*cos(T(X,Y,Z)/2.)*(1-sin(T(X,Y,Z)/2.)*sin(3.*T(X,Y,Z)/2.))');
SIG22 = FORMULE(NOM_PARA=('X','Y','Z'),VALE='coef*1./sqrt(R(X,Y,Z))*cos(T(X,Y,Z)/2.)*(1+sin(T(X,Y,Z)/2.)*sin(3.*T(X,Y,Z)/2.))');
SIG12 = FORMULE(NOM_PARA=('X','Y','Z'),VALE='coef*1./sqrt(R(X,Y,Z))*cos(T(X,Y,Z)/2.)*sin(T(X,Y,Z)/2.)*cos(3.*T(X,Y,Z)/2.)');
SIG33 = FORMULE(NOM_PARA=('X','Y','Z'),VALE='nu*(SIG11(X,Y,Z)+SIG22(X,Y,Z))');

OSIG11 = FORMULE(NOM_PARA=('X','Y','Z'),VALE='-coef*1./sqrt(R(X,Y,Z))*cos(T(X,Y,Z)/2.)*(1-sin(T(X,Y,Z)/2.)*sin(3.*T(X,Y,Z)/2.))');
OSIG22 = FORMULE(NOM_PARA=('X','Y','Z'),VALE='-coef*1./sqrt(R(X,Y,Z))*cos(T(X,Y,Z)/2.)*(1+sin(T(X,Y,Z)/2.)*sin(3.*T(X,Y,Z)/2.))');
OSIG12 = FORMULE(NOM_PARA=('X','Y','Z'),VALE='-coef*1./sqrt(R(X,Y,Z))*cos(T(X,Y,Z)/2.)*sin(T(X,Y,Z)/2.)*cos(3.*T(X,Y,Z)/2.)');
OSIG33 = FORMULE(NOM_PARA=('X','Y','Z'),VALE='-nu*(SIG11(X,Y,Z)+SIG22(X,Y,Z))');

ZERO   = FORMULE(NOM_PARA=('X','Y','Z'),VALE='0.');

CHF=AFFE_CHAR_MECA_F(MODELE=MODELEK,
                     FORCE_FACE=(_F(GROUP_MA='SURFGAU', FX=ZERO,   FY=OSIG11, FZ= SIG12),
                                 _F(GROUP_MA='SURFDRO', FX=ZERO,   FY= SIG11, FZ=OSIG12),
                                 _F(GROUP_MA='SURFSUP', FX=ZERO,   FY=OSIG12, FZ= SIG22),
                                 _F(GROUP_MA='SURFINF', FX=ZERO,   FY= SIG12, FZ=OSIG22),
                       ),);

 
#CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
#                PRES_REP=_F(GROUP_MA=('SURFSUP','SURFINF',),PRES=-1.e6,),); 

#----------------------------------------------
#                   RESOLUTION
#----------------------------------------------

L_INS1=DEFI_LIST_REEL(DEBUT=0.0,INTERVALLE=_F(JUSQU_A=3.0,NOMBRE=3,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMA1,              
                   EXCIT=(_F(CHARGE=CHXFEM,),                                           
                          _F(CHARGE=CHF,),
                          _F(CHARGE=CH,),
                          ),  
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INS1,
                                INST_FIN=1.0,),
                   SOLVEUR=_F(METHODE='MUMPS',
                              PCENT_PIVOT=250,),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);

#------------------------------------------------------------------------
# POST-TRAITEMENT : CALCUL DES SIFS PAR LA METHODE ENERGETIQUE AVEC X-FEM
#------------------------------------------------------------------------

# TEST AVEC PLUSIEURS COURONNES
RI=[0.666 , 1. , 1.]
RS=[1.666 , 2. , 3.]

nbc=len(RI)
SIF = [None]*nbc

for i in range(0,nbc) :

   SIF[i]=CALC_G(MODELE=MODELEK,
                     CHAM_MATER=CHAMPMA1,
                     RESULTAT=UTOT1,
                     SYME_CHAR='SANS',
                     COMP_ELAS=_F(RELATION='ELAS',),
                     OPTION='CALC_K_G',
                     THETA=_F(FISSURE=FISS,
                              R_INF=RI[i],
                              R_SUP=RS[i],),
                     LISSAGE=_F( LISSAGE_THETA='LAGRANGE',
                                 LISSAGE_G='LAGRANGE',),
                     INFO=2,);

   IMPR_TABLE(TABLE=SIF[i],);

# SOLUTION ANALYTIQUE
K1REF=1.

for i in range(0,nbc) :
   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='K1_LOCAL',
              TYPE_TEST='MAX',               
              VALE=K1REF,
              CRITERE='RELATIF',
              PRECISION=0.06,
              REFERENCE='ANALYTIQUE',);

   TEST_TABLE(TABLE=SIF[i],
              NOM_PARA='K1_LOCAL',
              TYPE_TEST='MIN',               
              VALE=K1REF,
              CRITERE='RELATIF',
              PRECISION=0.06,
              REFERENCE='ANALYTIQUE',);


FIN();
