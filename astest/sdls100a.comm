# ETUDES DE MAILLAGE SUR PLAQUE CARREE MINCE
# DIMENSION 10*10*0.05 M -- POSEE SUR UN COTE+ROTY BLOQUEE
#TEST0:MAILLAGEFIN==>SOLDEREFERENCEREF:NAFEMSTEST16
# MODELISATION DKT : 121 NOEUDS -- 200 MAILLES TRIA3
#=======================================================================


DEBUT(CODE=_F(NOM='SDLS100A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

MA=LIRE_MAILLAGE(FORMAT='MED', )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(  MAILLAGE=MA,
                  AFFE=_F(  TOUT = 'OUI',      PHENOMENE = 'MECANIQUE',
                         MODELISATION = 'DKT') )

CARELEM=AFFE_CARA_ELEM(   MODELE=MO,
                            COQUE=_F(
       GROUP_MA = 'TOUT',
  EPAIS = 0.05) )

# --------------------------------------------------------------------

MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.0E+11,  NU = 0.3,  RHO = 8000.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA,
                       AFFE=_F(  TOUT = 'OUI',  MATER = MAT) )

# --------------------------------------------------------------------
#
# ---- PLAQUE POSEE SUR UN COTE + ROT/Y BLOQUEE

CHMEC=AFFE_CHAR_MECA(  MODELE=MO,
              DDL_IMPO=_F(  GROUP_NO = 'COTE',
                         DX = 0., DY = 0., DZ = 0., DRY = 0.) )

#--------------------------------------------------------------------

MELR=CALC_MATR_ELEM(  MODELE=MO,           CHARGE=CHMEC,
                        CARA_ELEM=CARELEM,
                        CHAM_MATER=CHMAT,   OPTION='RIGI_MECA' )

MELM=CALC_MATR_ELEM(  MODELE=MO,           CHARGE=CHMEC,
                        CARA_ELEM=CARELEM,
                        CHAM_MATER=CHMAT,   OPTION='MASS_MECA' )

#--------------------------------------------------------------------

NUM=NUME_DDL(  MATR_RIGI=MELR )

MATASSR=ASSE_MATRICE(MATR_ELEM=MELR,NUME_DDL=NUM)

MATASSM=ASSE_MATRICE(MATR_ELEM=MELM,NUME_DDL=NUM)

#====================================================================

#               CALCUL MODAL

MODE=MODE_ITER_SIMULT(  MATR_A=MATASSR,       MATR_B=MATASSM,
             METHODE='TRI_DIAG',
             CALC_FREQ=_F(  OPTION = 'PLUS_PETITE',
                         NMAX_FREQ = 12)  )

# --------------------------------------------------------------------


TEST_RESU(RESU=(
   _F(   RESULTAT = MODE, REFERENCE = 'SOURCE_EXTERNE', PARA = 'FREQ',
           CRITERE = 'RELATIF',     PRECISION = 1.E-2,
