DEBUT( CODE=_F( NOM = 'HPLP311E',NIV_PUB_WEB='INTERNET'))

PRE_GIBI()

#
MA=LIRE_MAILLAGE( )

#
MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
                           CREA_GROUP_NO=_F( 
                         GROUP_MA = ( 'LP9P10',  'LP9P10B', 
                                      'LP8P9',   'LP8P9B',
                                         ))
                       )

#
#
# -------------------------------------------------------------------
#                    DEBUT DU THERMIQUE
# -------------------------------------------------------------------
MOTH=AFFE_MODELE(    MAILLAGE=MA,
                                AFFE=_F(  TOUT = 'OUI',
                                       PHENOMENE = 'THERMIQUE',
                                       MODELISATION = 'PLAN')
                        )

#
MATH=DEFI_MATERIAU(    THER=_F(  RHO_CP = 0.,
                                     LAMBDA = 54.)
                          )

#
CMTH=AFFE_MATERIAU(        MAILLAGE=MA,
                                      AFFE=_F(  TOUT = 'OUI',TEMP_REF=0.,
                                             MATER = MATH)
                          )

#
CHTH=AFFE_CHAR_THER(       MODELE=MOTH,TEMP_IMPO=(
                               _F(  GROUP_NO = 'LP9P10',
                                           TEMP = 100.),
                               _F(  GROUP_NO = 'LP9P10B',
                                           TEMP = -100.))
                           )

#
METH=CALC_MATR_ELEM(        MODELE=MOTH,
                                   CHARGE=CHTH,
                               CHAM_MATER=CMTH,
                                   OPTION='RIGI_THER'
                           )

#
VETH=CALC_VECT_ELEM(        CHARGE=CHTH,
                                   OPTION='CHAR_THER'
                           )

#
NUTH=NUME_DDL(          MATR_RIGI=METH )

#
KTH=ASSE_MATRICE(      MATR_ELEM=METH,
                                NUME_DDL=NUTH
                           )

#
FTH=ASSE_VECTEUR(      VECT_ELEM=VETH,
                                NUME_DDL=NUTH
                           )

#
KTH=FACTORISER(      reuse=KTH,   MATR_ASSE=KTH )

#
TEMP=RESOUDRE(         MATR=KTH,
                                 CHAM_NO=FTH
                           )

#
# -------------------------------------------------------------------
#                    FIN DU THERMIQUE
# -------------------------------------------------------------------
#
#
MO=AFFE_MODELE( MAILLAGE=MA, 
                             AFFE=_F(  TOUT = 'OUI',
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = 'C_PLAN')
                       )

#
MAT=DEFI_MATERIAU(    ELAS=_F(  E = 2.E11,
                                     NU = 0.3,
                                     RHO = 7800.,
                                     ALPHA = 1.2E-5)
                          )

#
CHMAT=AFFE_MATERIAU(    MAILLAGE=MA,
                                  AFFE=_F(  TOUT = 'OUI',TEMP_REF=0.,
                                         MATER = MAT)
                          )

#
CH=AFFE_CHAR_MECA(           MODELE=MO,
                               TEMP_CALCULEE=TEMP,DDL_IMPO=(
                                    _F(  GROUP_NO = 'LP8P9',
                                               DX = 0.),
                                             _F(  GROUP_NO = 'LP8P9B',
                                               DX = 0.),
                                             _F(  GROUP_NO = 'P11',
                                               DY = 0.))
                            )

#
RIGI_ELE=CALC_MATR_ELEM(        MODELE=MO,
                               CHAM_MATER=CHMAT,
                                   CHARGE=CH,
                                   OPTION='RIGI_MECA'
                           )

#
VECT_ELE=CALC_VECT_ELEM(        CHARGE=CH,
                               CHAM_MATER=CHMAT,
                                   OPTION='CHAR_MECA'
                           )

#
NUME_DDL=NUME_DDL(  MATR_RIGI=RIGI_ELE)

#
RIGIDITE=ASSE_MATRICE(  MATR_ELEM=RIGI_ELE,
                            NUME_DDL=NUME_DDL
                        )

#
FORCE=ASSE_VECTEUR(    VECT_ELEM=VECT_ELE,
                              NUME_DDL=NUME_DDL
                         )

#
RIGIDITE=FACTORISER(    reuse=RIGIDITE,  MATR_ASSE=RIGIDITE)

#
DEP=RESOUDRE(   MATR=RIGIDITE,
                            CHAM_NO=FORCE
                       )

RESU=CREA_RESU(  OPERATION='AFFE',
                 TYPE_RESU='EVOL_ELAS',
                 NOM_CHAM='DEPL',
                      AFFE=_F(  CHAM_GD = DEP, 
                                INST=0.,
                                MODELE=MO,
                                CHAM_MATER = CHMAT)     )
#
FOND=DEFI_FOND_FISS(    MAILLAGE=MA,
                         FOND_FISS=_F( GROUP_NO = ('P0',)),
                           NORMALE=(0., 1., 0.,)
                      )

#
THETA0=CALC_THETA(       MODELE=MO,
                         THETA_2D=_F(  GROUP_NO = ('P0',),
                                    MODULE = 1.,
                                    R_INF = 3.75E-3,
                                    R_SUP = 7.50E-3),
                        DIRECTION=(1., 0., 0.,)
                     )

#
G00=CALC_G(        RESULTAT=RESU,
                   NUME_ORDRE=1,
                   THETA=_F(THETA=THETA0),
                   EXCIT=_F(CHARGE=CH,),
                   SYME_CHAR='SANS'
                     )

#
GK00=CALC_G(        RESULTAT=RESU,
                    NUME_ORDRE=1,
                    THETA=_F( MODULE = 1.,
                              R_INF = 3.75E-3,
                              R_SUP = 7.50E-3,
                              DIRECTION=(1., 0., 0.,),
                              FOND_FISS=FOND),
                    EXCIT=_F(CHARGE=CH,),
                    SYME_CHAR='SANS',
                    OPTION='CALC_K_G'
                      )

#
#
#

TEST_TABLE(       TABLE=G00,
                       NOM_PARA='G',
                           VALE=2.4969E+3,
                      PRECISION=0.05   )

#

TEST_TABLE(       TABLE=GK00,
                       NOM_PARA='K2',
                           VALE=2.234704E7,
                      PRECISION=0.05   )

#
THETA01=CALC_THETA(       MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0',),
                                     MODULE = 1.,
                                     R_INF = 7.50E-3,
                                     R_SUP = 1.125E-2),
                         DIRECTION=(1., 0., 0.,)
                     )

#
G01=CALC_G(        RESULTAT=RESU,
                   NUME_ORDRE=1,
                   THETA=_F(THETA=THETA01),
                   EXCIT=_F(CHARGE=CH,),
                   SYME_CHAR='SANS'
                     )

#
GK01=CALC_G(       RESULTAT=RESU,
                   NUME_ORDRE=1,
                   THETA=_F(  MODULE = 1.,
                              R_INF = 7.50E-3,
                              R_SUP = 1.125E-2,
                              DIRECTION=(1., 0., 0.,),
                              FOND_FISS=FOND),
                   EXCIT=_F(CHARGE=CH,),
                   SYME_CHAR='SANS',
                   OPTION='CALC_K_G'
                      )

#
#
#

TEST_TABLE(       TABLE=G01,
                       NOM_PARA='G',
                           VALE=2.4969E+3,
                      PRECISION=0.05   )

#

TEST_TABLE(       TABLE=GK01,
                       NOM_PARA='K2',
                           VALE=2.234704E7,
                      PRECISION=0.05   )

#
THETA02=CALC_THETA(       MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0',),
                                     MODULE = 1.,
                                     R_INF = 1.125E-2,
                                     R_SUP = 1.500E-2),
                         DIRECTION=(1., 0., 0.,)
                     )

#
G02=CALC_G(        RESULTAT=RESU,
                   NUME_ORDRE=1,
                   THETA=_F(THETA=THETA02),
                   EXCIT=_F(CHARGE=CH,),
                   SYME_CHAR='SANS'
                     )

#
GK02=CALC_G(        RESULTAT=RESU,
                    NUME_ORDRE=1,
                    THETA=_F( MODULE = 1.,
                              R_INF = 1.125E-2,
                              R_SUP = 1.500E-2,
                              DIRECTION=(1., 0., 0.,),
                              FOND_FISS=FOND),
                    EXCIT=_F(CHARGE=CH,),
                    SYME_CHAR='SANS',
                    OPTION='CALC_K_G'
                      )

#
#
#

TEST_TABLE(       TABLE=G02,
                       NOM_PARA='G',
                           VALE=2.4969E+3,
                      PRECISION=0.05   )

#

TEST_TABLE(       TABLE=GK02,
                       NOM_PARA='K2',
                           VALE=2.234704E7,
                      PRECISION=0.05   )

#
THETA03=CALC_THETA(       MODELE=MO,
                          THETA_2D=_F(  GROUP_NO = ('P0',),
                                     MODULE = 1.,
                                     R_INF = 1.500E-2,
                                     R_SUP = 1.875E-2),
                         DIRECTION=(1., 0., 0.,)
                     )

#
G03=CALC_G(        RESULTAT=RESU,
                   NUME_ORDRE=1,
                   THETA=_F(THETA=THETA03),
                   EXCIT=_F(CHARGE=CH,),
                   SYME_CHAR='SANS'
                    )

#
GK03=CALC_G(        RESULTAT=RESU,
                    NUME_ORDRE=1,
                    THETA=_F( MODULE = 1.,
                              R_INF = 1.500E-2,
                              R_SUP = 1.875E-2,
                              DIRECTION=(1., 0., 0.,),
                              FOND_FISS=FOND),
                    EXCIT=_F(CHARGE=CH,),
                    SYME_CHAR='SANS',
                    OPTION='CALC_K_G'
                      )

#
#
#

TEST_TABLE(       TABLE=G03,
                       NOM_PARA='G',
                           VALE=2.4969E+3,
                      PRECISION=0.05   )

#

TEST_TABLE(       TABLE=GK03,
                       NOM_PARA='K2',
                           VALE=2.234704E7,
                      PRECISION=0.05   )

FIN()
#
