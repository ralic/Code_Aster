DEBUT(CODE=_F(  NOM = 'SSLP106A',NIV_PUB_WEB='INTERNET') );

PRE_GIBI();

MA=LIRE_MAILLAGE();

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='D_PLAN_SI',),);

MA=MODI_MAILLAGE(reuse =MA,
                 MAILLAGE=MA,
                 ORIE_PEAU_2D=_F(GROUP_MA=('LBC','LDA',),),
                 );

L = 100.;

B = 100.;


FSZ = FORMULE(NOM_PARA='Y',VALE='(1. - 4.*Y**2/B**2)');

FSX  = FORMULE(NOM_PARA='Y',VALE='8.*L*Y/B**2');

MATERIAU=DEFI_MATERIAU(ELAS=_F(E=100.,
                               NU=0.4999,),);

CH_MATER=AFFE_MATERIAU(MAILLAGE=MA,
                       AFFE=_F(TOUT='OUI',
                               MATER=MATERIAU,),);

SYMETRIE=AFFE_CHAR_MECA(MODELE=MO,
                        DDL_IMPO=(_F(GROUP_NO='A',
                                     DY=0.0,),
                                  _F(GROUP_NO='D',
                                     DX=0,),),
                        FACE_IMPO=_F(GROUP_MA='LAB',
                                     DX=0,),);
                                     
CHARGE=AFFE_CHAR_MECA_F(MODELE=MO,
                        PRES_REP=(_F(GROUP_MA='LBC',
                                    CISA_2D=FSZ,),
                                  _F(GROUP_MA='LDA',
                                     CISA_2D=FSZ,
                                     PRES=FSX),
                                 ),
                           );

INSTANTS=DEFI_LIST_REEL(DEBUT=0.0,
                        INTERVALLE=_F(JUSQU_A=1.0,
                                      NOMBRE=1,),);

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,  0.0,
                          1.0,  1.0,),);

RESU=STAT_NON_LINE(MODELE=MO,
                   CHAM_MATER=CH_MATER,
                   EXCIT=(_F(CHARGE=CHARGE,
                             FONC_MULT=RAMPE,),
                          _F(CHARGE=SYMETRIE,),),
                   COMP_INCR=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=INSTANTS,
                                SUBD_METHODE='UNIFORME', SUBD_PAS=4,
                                SUBD_PAS_MINI=0.001,),
                   NEWTON=_F(REAC_ITER=1,),
                   ARCHIVAGE=_F(LIST_INST=INSTANTS,),);

TEST_RESU(RESU=(_F(RESULTAT   = RESU,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'C',
                   NOM_CMP    = 'DX',
                   VALE       = -1.5,
                   PRECISION  = 0.02,
                   REFERENCE  = 'ANALYTIQUE',),
                _F(RESULTAT   = RESU,
                   INST       = 1.0,
                   NOM_CHAM   = 'DEPL',
                   GROUP_NO   = 'C',
                   NOM_CMP    = 'DY',
                   VALE       = 4.25,
                   PRECISION  = 0.003,
                   REFERENCE  = 'ANALYTIQUE',)
                   ));



FIN();
