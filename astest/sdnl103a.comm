_F( CHAM_GD = DEPL6, NOEUD = 'N9', NOM_CMP = 'DX', VALE = 2.9706E-02, PRECISION = 1.E-4),
_F( CHAM_GD = DEPL7, NOEUD = 'N9', NOM_CMP = 'DX', VALE = -2.6290E-02, PRECISION = 1.E-4),
_F( CHAM_GD = DEPL8, NOEUD = 'N9', NOM_CMP = 'DX', VALE = 2.5126E-02, PRECISION = 1.E-4),
_F( CHAM_GD = DEPL9, NOEUD = 'N9', NOM_CMP = 'DX', VALE = -2.5488E-02, PRECISION = 1.E-4))
           )

