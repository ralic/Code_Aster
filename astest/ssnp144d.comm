DEBUT(CODE=_F(NOM='SSNP144D',NIV_PUB_WEB='INTERNET',),);
#----------------------------------------------
#                MAILLAGE : ssnp144d.mgib
#----------------------------------------------

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE();

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       );

#----------------------------------------------
#                   MODELE ET FISSURE
#----------------------------------------------

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA='SURF',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',                              
                              ),
                           _F(GROUP_MA=('LIG1','LIG3',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),),);
 
MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=(_F(GROUP_MA='SURF',
                                      NOM='SURF',),
                                   _F(NOM='SURF_SUP',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N3',
                                      PRECISION=2.46,
                                      VECT_NORMALE=(0.0,1.0),),   
                                   _F(NOM='SURF_INF',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N1',
                                      PRECISION=2.46,
                                      VECT_NORMALE=(0.0,1.0),),
                                   _F(NOM='FISS_INF',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N41',
                                      PRECISION=1.0E-10,
                                      VECT_NORMALE=(0.0,1.0),),
                                   _F(NOM='FISS_SUP',
                                      OPTION='PLAN',
                                      NOEUD_CENTRE='N42',
                                      PRECISION=1.0E-10,
                                      VECT_NORMALE=(0.0,1.0),),                                      
                                  _F(NOM='NINF',
                                      NOEUD=('N106','N393','N394','N395',
                                      'N396','N397','N398','N399','N400',
                                      'N401','N402','N41'),),
                                   _F(NOM='NSUP',
                                      NOEUD=('N105','N403','N404','N405',
                                      'N406','N407','N408','N409','N410',
                                      'N411','N412','N42'),),
                                   ),);

# FORME ANALYTIQUE POUR LA FISSURE

LN = FORMULE(VALE='Y-2.5',
             NOM_PARA=('X','Y',),);


FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    TYPE_DISCONTINUITE='INTERFACE',
                    DEFI_FISS=_F(FONC_LN=LN,),
                    GROUP_MA_ENRI='SURF',
                    INFO=1,);
                                        
#-------------------------------------------------
#                   CONTACT
#-------------------------------------------------

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='P1P1',                         
                         );

CTXFEM = DEFI_CONTACT(
            MODELE         = MODELEK,
            FORMULATION    = 'XFEM',
            FROTTEMENT     = 'SANS',
            ZONE=(
                  _F(INTEGRATION='FPG2',
                     ALGO_CONT='CZM',                                   
                     COEF_ECHELLE=1.,
                     FISS_MAIT = FISS,
                     ALGO_LAGR='VERSION2',
                     RELATION='CZM_EXP_REG',),),);
                       
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM,
                      INFO=2,);
                      

           
#----------------------------------------------
#                   MATERIAU
#----------------------------------------------
nu = 0.0;
E = 500000.0;

MATE=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,),
                    RUPT_FRAG=_F(GC=0.9E3,
                                 SIGM_C=1.1E6,
                                 PENA_ADHERENCE=1.0E-3,
                                 ),
                   );
CHAMPMA=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=(_F(GROUP_MA='SURF',MATER=MATE,),
                            ),
                      );

#----------------------------------------------
#                   CHARGEMENTS
#----------------------------------------------
L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=(
                                  _F(JUSQU_A=0.5,
                                    NOMBRE=2,),
                                  _F(JUSQU_A=0.75,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=2,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=3.5,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=4.5,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=5.5,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=7,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=9.5,
                                     NOMBRE=2,),
                                  _F(JUSQU_A=12,
                                     NOMBRE=10,),
                                  _F(JUSQU_A=15,
                                     NOMBRE=10,),
                                    ),);
LINST=[0.5 , 0.75 , 2. , 3.5 , 4.5 , 5.5 , 7. , 9.5 , 12 , 15 ]

LCOEF=[1.83335E6 ,-1.83335E6 , 1.83335E6 , 1000.54574726 ,
-1000. , 1000. , 1000. , -1000., 1000. , 1000. ]   
nbc=len(LINST)                
#----------------MODE-I------------------------

DEPY1 = 1.0;

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIG1',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_MA='LIG3',
                                DX=0.0,
                                DY=DEPY1,),),);

#----------------MODE-II-----------------------

DEPX2 = 1.0;
DEPY2 = 1.0E-13;

CH21=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_NO='SURF_SUP',
                                DX=DEPX2,),),);

CH22=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_NO='SURF_INF',
                                DX=0.0,
                                DY=0.0,),
                             _F(GROUP_NO='SURF_SUP',
                                DY=DEPY2,),
                             _F(GROUP_NO=('FISS_INF','FISS_SUP',),
                                H1Y=DEPY2/2,),),
                      LIAISON_GROUP=(_F(GROUP_NO_1 = ('NSUP'),
                                        GROUP_NO_2 = ('NSUP'),
                                        DDL_1      = 'DX',
                                        DDL_2      = 'H1X',
                                        COEF_MULT_1=  1.,
                                        COEF_MULT_2=  -1.,
                                        COEF_IMPO= 0.,),
                                     _F(GROUP_NO_1 = ('NINF'),
                                        GROUP_NO_2 = ('NSUP'),
                                        DDL_1      = 'H1X',
                                        DDL_2      = 'H1X',
                                        COEF_MULT_1=  1.,
                                        COEF_MULT_2=  -1.,
                                        COEF_IMPO= 0.,),),);


#--------------------------------------------------------
#                       CALCUL - MODE I
#--------------------------------------------------------

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH1,
                              TYPE_CHARGE='FIXE_PILO',),),
                    CONTACT =  CTXFEM,  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=0.5,),
                    NEWTON=_F(REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='SAUT_LONG_ARC',
                                SELECTION='ANGL_INCR_DEPL',
                                COEF_MULT=1.83335E6,
                                FISSURE=FISS,
                                ETA_PILO_R_MIN=0.0,
                                DIRE_PILO='DY',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=30,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',
                               NPREC=-1,
                               ),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);

for i in range(1,nbc) :
  UTOT1=STAT_NON_LINE(reuse =UTOT1,
                    ETAT_INIT=_F(EVOL_NOLI=UTOT1,),
                    MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH1,
                              TYPE_CHARGE='FIXE_PILO',),),
                    CONTACT = CTXFEM,  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=LINST[i],),
                    NEWTON=_F(REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='SAUT_LONG_ARC',
                                SELECTION='ANGL_INCR_DEPL',
                                COEF_MULT=LCOEF[i],
                                FISSURE=FISS,
                                ETA_PILO_R_MIN=0.0,
                                DIRE_PILO='DY',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=30,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',
                               NPREC=-1,
                               ),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);

#--------------------------------------------------------
#                      CALCUL - MODE II
#--------------------------------------------------------

UTOT2=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH22,),
                           _F(CHARGE=CH21,
                              TYPE_CHARGE='FIXE_PILO',),),
                    CONTACT =  CTXFEM,  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=0.5,),
                    NEWTON=_F(REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='SAUT_LONG_ARC',
                                COEF_MULT=1.83335E6,
                                SELECTION='ANGL_INCR_DEPL',
                                FISSURE=FISS,
                                DIRE_PILO='DX',),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=30,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',
                               PCENT_PIVOT=50,),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,); 

for i in range(1,nbc) :
   UTOT2=STAT_NON_LINE(
                    reuse =UTOT2,
                    ETAT_INIT=_F(EVOL_NOLI=UTOT2,),
                    MODELE=MODELEK,
                    CHAM_MATER=CHAMPMA,
                    EXCIT=(_F(CHARGE=CHXFEM,),
                           _F(CHARGE=CH22,),
                           _F(CHARGE=CH21,
                              TYPE_CHARGE='FIXE_PILO',),),
                    CONTACT =  CTXFEM,  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='SURF',),
                    INCREMENT=_F(LIST_INST=L_INST,
                                 INST_FIN=LINST[i],),
                    NEWTON=_F(REAC_ITER=1,),
                    PILOTAGE=_F(TYPE='SAUT_LONG_ARC',
                                SELECTION='ANGL_INCR_DEPL',
                                COEF_MULT=LCOEF[i],
                                DIRE_PILO='DX',
                                FISSURE=FISS,
                               ),
                    CONVERGENCE=_F(RESI_GLOB_RELA=1.E-6,
                                   ITER_GLOB_MAXI=30,
                                   ARRET='OUI',),
                    SOLVEUR=_F(METHODE='MUMPS',
                               PCENT_PIVOT=50,),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                    INFO=1,);
# -------------------------------------------------------------
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM
# -------------------------------------------------------------

MA_XFEM=POST_MAIL_XFEM(MODELE=MODELEK,
                       PREF_GROUP_CO='NFISSU',);

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='MECANIQUE',
                             MODELISATION='C_PLAN',),);

RESXFEM1=POST_CHAM_XFEM(RESULTAT=UTOT1,
                        MODELE_VISU=MOD_VISU,);

RESXFEM2=POST_CHAM_XFEM(RESULTAT=UTOT2,
                        MODELE_VISU=MOD_VISU,);
#--------------------------------------------------------
#                  VALIDATION DES RESULTATS
#--------------------------------------------------------
TOLE=1.0E-8 

LLAG=[3.66296853301E+05,
 1.8314842665E+05,
 1.098890559903E+06,
 1.75867720687844E+05,
 5.8622573562549E+04,
 1.75867720687844E+05,
 2.8117686527187E+04,
 4.68628108785798E+03,
 2.8117686527187E+04,
 7.18731177854856E+02]
 
LDEP=[2.72724793411E-07 ,
 1.36362396705E-07 ,
 8.18174380233E-07 ,
 1.50000000001E-03 ,
 5.0000000001E-04 ,
 1.50000000001E-03 ,
 3.00000000001E-03 ,
 5.00000000001E-04 ,
 3.00000000001E-03 ,
 6.00000000001E-03 ]

TAB1 = [None]*nbc
TAB2 = [None]*nbc
TAB3 = [None]*nbc
TAB4 = [None]*nbc

TAB10 = [None]*nbc
TAB20 = [None]*nbc
TAB30 = [None]*nbc
TAB40 = [None]*nbc
#--------------------------------------------------------
#                           MODE I    
#--------------------------------------------------------
SAUT=FORMULE(NOM_PARA=('DYP','DYM'),
             VALE='DYP-DYM',);
             
for i in range(0,nbc) :
                                   
   TAB1[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                   GROUP_NO=('NFISSU',),
                                   RESULTAT=RESXFEM1,
                                   NOM_CHAM='DEPL',
                                   INST=LINST[i],
                                   TOUT_CMP='OUI',
                                   OPERATION='EXTRACTION',),);
   TAB2[i]=CALC_TABLE(TABLE=TAB1[i],
                      ACTION=(_F(OPERATION='FILTRE',
                                NOM_PARA='NOEUD',
                                CRIT_COMP='REGEXP',
                                VALE_K='NM',),
                               _F(OPERATION='RENOMME',
                                NOM_PARA=('DY','DYM'),), 
                               ),);
                                                               
   TAB3[i]=CALC_TABLE(TABLE=TAB1[i],
                      ACTION=(_F(OPERATION='FILTRE',
                                NOM_PARA='NOEUD',
                                CRIT_COMP='REGEXP',
                                VALE_K='NP',),
                             _F(OPERATION='RENOMME',
                                NOM_PARA=('DY','DYP'),),
                              ),);
                                
   TAB4[i]=CALC_TABLE(TABLE=TAB2[i],
                      ACTION=(_F(OPERATION='COMB',
                                TABLE=TAB3[i],
                                NOM_PARA=('INST','COOR_X'),),
                              _F(OPERATION='OPER',
                                FORMULE=SAUT,
                                NOM_PARA='SAUY',),
                              ),);                            
                                   
                                                              
   TEST_TABLE(TABLE=TAB1[i],
              NOM_PARA='LAGS_C',
              TYPE_TEST='MAX',
              VALE=LLAG[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
           
   TEST_TABLE(TABLE=TAB1[i],
              NOM_PARA='LAGS_C',
              TYPE_TEST='MIN',
              VALE=LLAG[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',); 
                        
   TEST_TABLE(TABLE=TAB4[i],
              NOM_PARA='SAUY',
              TYPE_TEST='MAX',
              VALE=LDEP[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
           
   TEST_TABLE(TABLE=TAB4[i],
              NOM_PARA='SAUY',
              TYPE_TEST='MIN',
              VALE=LDEP[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);  

#--------------------------------------------------------
#                           MODE II    
#--------------------------------------------------------
SAUTX=FORMULE(NOM_PARA=('DXP','DXM'),
             VALE='DXP-DXM',);
             
for i in range(0,nbc) :
                                   
   TAB10[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                   GROUP_NO=('NFISSU',),
                                   RESULTAT=RESXFEM2,
                                   NOM_CHAM='DEPL',
                                   INST=LINST[i],
                                   TOUT_CMP='OUI',
                                   OPERATION='EXTRACTION',),);
   TAB20[i]=CALC_TABLE(TABLE=TAB10[i],
                       ACTION=(_F(OPERATION='FILTRE',
                                NOM_PARA='NOEUD',
                                CRIT_COMP='REGEXP',
                                VALE_K='NM',),
                               _F(OPERATION='RENOMME',
                                NOM_PARA=('DX','DXM'),), 
                               ),);
                                                               
   TAB30[i]=CALC_TABLE(TABLE=TAB10[i],
                      ACTION=(_F(OPERATION='FILTRE',
                                NOM_PARA='NOEUD',
                                CRIT_COMP='REGEXP',
                                VALE_K='NP',),
                             _F(OPERATION='RENOMME',
                                NOM_PARA=('DX','DXP'),),
                              ),);
                                
   TAB40[i]=CALC_TABLE(TABLE=TAB20[i],
                      ACTION=(_F(OPERATION='COMB',
                                TABLE=TAB30[i],
                                NOM_PARA=('INST','COOR_X'),),
                              _F(OPERATION='OPER',
                                FORMULE=SAUTX,
                                NOM_PARA='SAUX',),
                              ),);                            
   TEST_TABLE(TABLE=TAB10[i],
              NOM_PARA='LAGS_F1',
              TYPE_TEST='MAX',
              VALE_ABS='OUI',
              VALE=LLAG[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
           
   TEST_TABLE(TABLE=TAB10[i],
              NOM_PARA='LAGS_F1',
              TYPE_TEST='MIN',
              VALE_ABS='OUI',
              VALE=LLAG[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',); 
                        
   TEST_TABLE(TABLE=TAB40[i],
              NOM_PARA='SAUX',
              TYPE_TEST='MAX',
              VALE=LDEP[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
           
   TEST_TABLE(TABLE=TAB40[i],
              NOM_PARA='SAUX',
              TYPE_TEST='MIN',
              VALE=LDEP[i],
              CRITERE='RELATIF',
              PRECISION=TOLE,
              REFERENCE='ANALYTIQUE',);
FIN();
