# MODELISATION A : POU_D_E



DEBUT(CODE=_F(NOM='SSLL105A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

POUTRE=LIRE_MAILLAGE( )

MODELE=AFFE_MODELE(   MAILLAGE=POUTRE,
                         AFFE=_F( TOUT = 'OUI',   PHENOMENE = 'MECANIQUE',
                    MODELISATION = 'POU_D_E', ))

MATER1=DEFI_MATERIAU(  ELAS=_F( E = 71240.,   NU = 0.3,  ) )

CHAMPMAT=AFFE_MATERIAU(  MAILLAGE=POUTRE,
              AFFE=_F( GROUP_MA = ('POUTRE',),MATER = MATER1, ))

CHAMPCAR=AFFE_CARA_ELEM(  MODELE=MODELE,
         POUTRE=_F(  GROUP_MA = ('POUTRE',),  SECTION = 'GENERALE',
            CARA = ('A','IY','IZ','JX','AY','AZ',),
            VALE = (18.,0.54,1350.,2.16,1.2,1.2, ), ))

CHADDL=AFFE_CHAR_MECA(  MODELE=MODELE,
         DDL_IMPO=_F( GROUP_NO = ('PA',),DX = 0.0,   DY = 0.0, DZ = 0.0,
                                 DRX = 0.0,  DRY = 0., DRZ = 0.0, ))

RIGIELE=CALC_MATR_ELEM(  MODELE=MODELE,  OPTION='RIGI_MECA',
              CARA_ELEM=CHAMPCAR,  CHARGE=CHADDL,
              CHAM_MATER=CHAMPMAT )

NUMEDDL=NUME_DDL(  MATR_RIGI=RIGIELE,  METHODE='LDLT',  RENUM='RCMK' )

RIGI=ASSE_MATRICE(  MATR_ELEM=RIGIELE,  NUME_DDL=NUMEDDL )

# CAS DE CHARGE 1 : FX = -1

CHARGE1=AFFE_CHAR_MECA(  MODELE=MODELE,
            FORCE_NODALE=_F( GROUP_NO = ( 'PC', ),  FX = -1.E0,   ))

STAT1=MECA_STATIQUE(MODELE=MODELE,
                   CHAM_MATER=CHAMPMAT,
                   CARA_ELEM=CHAMPCAR,
                   EXCIT=(_F(CHARGE=CHADDL,),
                          _F(CHARGE=CHARGE1,),),);

SIGM1=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='ELGA_SIEF_R',
                    RESULTAT=STAT1, NUME_ORDRE=1,
                    NOM_CHAM='SIEF_ELGA' )

RGEOEL1=CALC_MATR_ELEM(  MODELE=MODELE,  OPTION='RIGI_GEOM',
              CARA_ELEM=CHAMPCAR,  SIEF_ELGA=SIGM1 )

RGEO1=ASSE_MATRICE(  MATR_ELEM=RGEOEL1,  NUME_DDL=NUMEDDL )

MODE1=MODE_ITER_SIMULT(  MATR_A=RIGI,  MATR_B=RGEO1,
      TYPE_RESU='MODE_FLAMB',
    CALC_FREQ=_F(  OPTION = 'BANDE',  CHAR_CRIT = (0.063, 2.47,),
                  )  )

TEST_RESU(
   RESU=_F( RESULTAT = MODE1, NUME_MODE = 1,  PARA = 'CHAR_CRIT', VALE = 1.088,
    PRECISION = 1.E-2,))

# CAS DE CHARGE 2 : FX = +1

CHARGE2=AFFE_CHAR_MECA(  MODELE=MODELE,
            FORCE_NODALE=_F( GROUP_NO = ( 'PC', ),  FX = +1.E0,   ))

STAT2=MECA_STATIQUE(MODELE=MODELE,
                   CHAM_MATER=CHAMPMAT,
                   CARA_ELEM=CHAMPCAR,
                   EXCIT=(_F(CHARGE=CHADDL,),
                          _F(CHARGE=CHARGE2,),),);

SIGM2=CREA_CHAMP( OPERATION='EXTR', TYPE_CHAM='ELGA_SIEF_R',
                    RESULTAT=STAT2, NUME_ORDRE=1,
                    NOM_CHAM='SIEF_ELGA' )

RGEOEL2=CALC_MATR_ELEM(  MODELE=MODELE,  OPTION='RIGI_GEOM',
              CARA_ELEM=CHAMPCAR,  SIEF_ELGA=SIGM2 )

RGEO2=ASSE_MATRICE(  MATR_ELEM=RGEOEL2,  NUME_DDL=NUMEDDL )

MODE2=MODE_ITER_SIMULT(  MATR_A=RIGI,  MATR_B=RGEO2,
      TYPE_RESU='MODE_FLAMB',
    CALC_FREQ=_F(  OPTION = 'BANDE',  CHAR_CRIT = (0.063, 2.47,),
                 )  )

TEST_RESU(
   RESU=_F( RESULTAT = MODE2, NUME_MODE = 1,  PARA = 'CHAR_CRIT', VALE = 0.68,
    PRECISION = 1.E-2,))

FIN( )
