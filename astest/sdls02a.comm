#CAS_TEST__: SDLS02A
# SDLS02/A                 COMMANDES                             5/03/91
# PLAQUE LOSANGE MINCE   ENCASTREE SUR UN BORD
# DIMENSION 1M*1M*0.01M
# ALPHA= 30 DEGRES                                 REF: SFM.VPCS SDLS02
# MODELISATION DKT: 121 NOEUDS -- 200 MAILLES TRIA3
#======================================================================


DEBUT(CODE=_F(  NOM = 'SDLS02A',NIV_PUB_WEB='INTERNET') )

MA=LIRE_MAILLAGE( )

MA=DEFI_GROUP( reuse=MA,   MAILLAGE=MA,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

MO=AFFE_MODELE(  MAILLAGE=MA, 
                  AFFE=_F(  TOUT = 'OUI',       PHENOMENE = 'MECANIQUE',
                         MODELISATION = 'DKT') )

CARELEM=AFFE_CARA_ELEM(   MODELE=MO, 
                            COQUE=_F(  
       GROUP_MA = 'TOUT',
    EPAIS = 0.01) )

MAT=DEFI_MATERIAU( ELAS=_F(  E = 2.1E+11,  NU = 0.3,  RHO = 7800.) )

CHMAT=AFFE_MATERIAU(  MAILLAGE=MA, 
                       AFFE=_F(  TOUT = 'OUI',    MATER = MAT) )

#--------------------------------------------------------------------
#
# ---- PLAQUE ENCASTREE SUR UN COTE

CH=AFFE_CHAR_MECA(  MODELE=MO,
        DDL_IMPO=_F(  GROUP_NO = 'BORDAB',  DX = 0.,   DY = 0.,  DZ = 0.,
                                      DRX = 0., DRY = 0., DRZ = 0.) )

#-------------------------------------------------------------------
#

MELR=CALC_MATR_ELEM(  MODELE=MO,           CHARGE=CH, 
                        CARA_ELEM=CARELEM, 
                        CHAM_MATER=CHMAT,   OPTION='RIGI_MECA' )

MELM=CALC_MATR_ELEM(  MODELE=MO,           CHARGE=CH, 
                        CARA_ELEM=CARELEM, 
                        CHAM_MATER=CHMAT,   OPTION='MASS_MECA' )

NUM=NUME_DDL(    MATR_RIGI=MELR  )

MATASSR=ASSE_MATRICE(MATR_ELEM=MELR,NUME_DDL=NUM)

MATASSM=ASSE_MATRICE(MATR_ELEM=MELM,NUME_DDL=NUM)

#====================================================================
#
# -------- CALCUL MODAL

MODE=MODE_ITER_SIMULT(  MATR_A=MATASSR,         MATR_B=MATASSM,
              METHODE='TRI_DIAG',
             CALC_FREQ=_F(  OPTION = 'PLUS_PETITE',   NMAX_FREQ = 2) )

# --------------------------------------------------------------------


TEST_RESU(RESU=(
   _F(   RESULTAT = MODE,        PARA = 'FREQ', REFERENCE = 'ANALYTIQUE',
           CRITERE = 'RELATIF',     PRECISION = 1.5E-2,
           NUME_ORDRE = 1,       VALE = 9.7355),
   _F(   RESULTAT = MODE,        PARA = 'FREQ', REFERENCE = 'ANALYTIQUE',
           CRITERE = 'RELATIF',     PRECISION = 1.5E-2,
           NUME_ORDRE = 2,       VALE = 23.2745)) )

FIN()
