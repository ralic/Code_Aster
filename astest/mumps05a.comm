#########################################################################
#
#   MODELISATION A : CUBE HEXA8 / 210131 DDLs / 8090562 NNZ
#   ON TESTE LES DIFFERENTES VARIANTES DE MUMPS (CENTRALISEE/DISTRIBUEE
#   PAR MAILLE ET PAR SOUS-DOMAINES), EN INFLUANT SUR L'EQUILIBRAGE
#   DE CHARGE VIA LES MOT-CLES CHARGE_PROC0_MA/SD ET SUR LE CARACTERE
#   IN_CORE/OUT_OF_CORE.
#   ON PANACHE LE SOLVEUR MUMPS ET LE SOLVEUR FETI EN PARALLELE POUR
#   VALIDER LEUR INTER-OPERABILITE.
#   ON TESTE AUSSI, SUR LA PREMIERE CONFIGURATION, LES RENUMEROTEURS
#   METIS ET SCOTCH.
#   PARALLELE: VALEUR CONSEILLEE 8 PROCS
#########################################################################
#
# Choix du solveur
solveur = 'MUMPS';
#
# Pourcentage de memoire supplementaire que peut s'allouer MUMPS
# Rappel :
# --------------------------------------------------------------#
#                                                               #
#         JEVEUX          'DISPO'==>UTILISABLE PAR MUMPS        #
#  I-------------------I----------------------------------I     #
#    mem_aster*MEM/100        (1-mem_aster)*MEM/100             #
#                                                               #
#    ou MEM est la memoire totale indiquee dans ASTK            #
#                                                               #
# --------------------------------------------------------------#
#
# Choix de la methode de renumeration
meth_renum = 'METIS';
meth_renu2 = 'SCOTCH';
pcent_pivot=30
#
# Mode verbeux 1/2
niv_info = 2
#
DEBUT(CODE=_F( NOM = 'MUMPS05A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'),
            MESURE_TEMPS=_F(NIVE_DETAIL=2))

# pour obtenir un maillage quadratique :
#----------------------------------------
# MA1=LIRE_MAILLAGE( )
# MA=CREA_MAILLAGE(MAILLAGE=MA1,LINE_QUAD=_F(TOUT='OUI'))

# pour obtenir un maillage lineaire :
#----------------------------------------
MA=LIRE_MAILLAGE(FORMAT='MED');
MAT=DEFI_MATERIAU(ELAS=_F(E=1.E+05,
                          NU=0.3));

CHAM_MAT=AFFE_MATERIAU(MAILLAGE=MA,
                       AFFE=_F(TOUT='OUI',
                               MATER=MAT));

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(GROUP_MA=('BLOC','COTE_B','COTE_H'),
                       PHENOMENE='MECANIQUE',
                       MODELISATION='3D'));

CHARF=AFFE_CHAR_MECA(MODELE=MO,
                     FORCE_FACE=_F(GROUP_MA='COTE_H',
                                   FZ=1.E+4));

CHARD=AFFE_CHAR_MECA(MODELE=MO,
                     DDL_IMPO=(_F(GROUP_NO='A',
                                  DX=0.,
                                  DY=0.),
                               _F(GROUP_NO='B',
                                  DY=0.),
                               _F(GROUP_MA='COTE_B',
                                  DZ=0.)));

TPS=DEFI_LIST_REEL(DEBUT=0.0,
                   INTERVALLE=_F(JUSQU_A=1,
                                 NOMBRE=1));

# 1.1 : AVEC CHARGE DUALISEE ET MUMPS CENTRALISE + METIS
#----------------------------------------------------------

MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='CENTRALISE'))

MECAC=MECA_STATIQUE(INFO=1,
                     MODELE=MO,
                     CHAM_MATER=CHAM_MAT,
                     EXCIT=(_F(CHARGE=CHARF),
                            _F(CHARGE=CHARD)),
                     OPTION='SANS',
                     SOLVEUR=_F(METHODE=solveur,
                                RENUM=meth_renum,
                                RESI_RELA=-1.0,
                                GESTION_MEMOIRE='OUT_OF_CORE',
                                PCENT_PIVOT=pcent_pivot,
                                NPREC=-1,
                                ));

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAC,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=0.1,
                  VALE_REFE=0.1,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAC,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=0.0,
                  VALE_REFE=0.0,
                  CRITERE='ABSOLU',
                  PRECISION=1.E-08,),
          )

# 1.2 : AVEC CHARGE DUALISEE ET MUMPS CENTRALISE + SCOTCH
#--------------------------------------------------------

MECAC2=MECA_STATIQUE(INFO=niv_info,
                     MODELE=MO,
                     CHAM_MATER=CHAM_MAT,
                     EXCIT=(_F(CHARGE=CHARF),
                            _F(CHARGE=CHARD)),
                     OPTION='SANS',
                     SOLVEUR=_F(METHODE=solveur,
                                RENUM=meth_renu2,
                                RESI_RELA=-1.0,
                                GESTION_MEMOIRE='OUT_OF_CORE',
                                PCENT_PIVOT=pcent_pivot,
                                NPREC=-1,
                                ));

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAC2,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=0.1,
                  VALE_REFE=0.1,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAC2,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=0.,
                  VALE_REFE=0.,
                  CRITERE='ABSOLU',
                  PRECISION=1.E-08,),
          )

# 2.1 : AVEC CHARGE DUALISEE ET MUMPS DISTRIBUE PAR MAILLES
#       EQUILIBRAGE DE CHARGE AUTOMATIQUE
#----------------------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='MAIL_DISPERSE',CHARGE_PROC0_MA=0))

MECAD1=MECA_STATIQUE(INFO=niv_info,
                     MODELE=MO,
                     CHAM_MATER=CHAM_MAT,
                     EXCIT=(_F(CHARGE=CHARF),
                            _F(CHARGE=CHARD)),
                     OPTION='SANS',
                     SOLVEUR=_F(METHODE=solveur,
                                RENUM=meth_renum,
                                RESI_RELA=-1.0,
                                GESTION_MEMOIRE='IN_CORE',
                                PCENT_PIVOT=pcent_pivot,
                                NPREC=-1,                       
                                ));

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAD1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=0.1,
                  VALE_REFE=0.1,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAD1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=0.,
                  VALE_REFE=0.,
                  CRITERE='ABSOLU',
                  PRECISION=1.E-08,),
          )

# 2.2 : AVEC CHARGE DUALISEE ET MUMPS DISTRIBUE PAR MAILLES
#       EQUILIBRAGE DE CHARGE FORCE POUR SOULAGER LE PROC 0
#----------------------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='MAIL_DISPERSE',CHARGE_PROC0_MA=70))

MECAD2=MECA_STATIQUE(INFO=niv_info,
                     MODELE=MO,
                     CHAM_MATER=CHAM_MAT,
                     EXCIT=(_F(CHARGE=CHARF),
                            _F(CHARGE=CHARD)),
                     OPTION='SANS',
                     SOLVEUR=_F(METHODE=solveur,
                                RENUM=meth_renum,
                                RESI_RELA=-1.0,
                                GESTION_MEMOIRE='OUT_OF_CORE',
                                PCENT_PIVOT=pcent_pivot,
                                NPREC=-1,
                                ));

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAD2,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=0.1,
                  VALE_REFE=0.1,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAD2,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=0.,
                  VALE_REFE=0.,
                  CRITERE='ABSOLU',
                  PRECISION=1.E-08,),
          )

# 3.1 : AVEC CHARGE DUALISEE ET MUMPS DISTRIBUE PAR SOUS-DOMAINES
#       EQUILIBRAGE DE CHARGE AUTOMATIQUE
#----------------------------------------------------------
SDFETI = DEFI_PART_FETI(
                MODELE=MO,
                NB_PART=24,
                EXCIT=(_F(CHARGE=CHARF),_F(CHARGE=CHARD)),
                METHODE='KMETIS',
                NOM_GROUP_MA='A_');

MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='SOUS_DOMAINE',PARTITION=SDFETI, CHARGE_PROC0_SD=0))

MECAD3=MECA_STATIQUE(INFO=niv_info,
                     MODELE=MO,
                     CHAM_MATER=CHAM_MAT,
                     EXCIT=(_F(CHARGE=CHARF),
                            _F(CHARGE=CHARD)),
                     OPTION='SANS',
                     SOLVEUR=_F(METHODE=solveur,
                                RENUM=meth_renum,
                                RESI_RELA=-1.0,
                                PCENT_PIVOT=pcent_pivot,
                                GESTION_MEMOIRE='IN_CORE',
                                NPREC=-1,
                                ));

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAD3,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=0.1,
                  VALE_REFE=0.1,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAD3,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=0.,
                  VALE_REFE=0.,
                  CRITERE='ABSOLU',
                  PRECISION=1.E-08,),
          )

# 3.2 : AVEC CHARGE DUALISEE ET MUMPS DISTRIBUE PAR SOUS-DOMAINES
#       EQUILIBRAGE DE CHARGE FORCE POUR SOULAGER LE PROC 0
#----------------------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='SOUS_DOMAINE',PARTITION=SDFETI, CHARGE_PROC0_SD=2))

MECAD4=MECA_STATIQUE(INFO=niv_info,
                     MODELE=MO,
                     CHAM_MATER=CHAM_MAT,
                     EXCIT=(_F(CHARGE=CHARF),
                            _F(CHARGE=CHARD)),
                     OPTION='SANS',
                     SOLVEUR=_F(METHODE=solveur,
                                RENUM=meth_renum,
                                RESI_RELA=-1.0,
                                GESTION_MEMOIRE='OUT_OF_CORE',
                                PCENT_PIVOT=pcent_pivot,
                                NPREC=-1,
                                ));

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAD4,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=0.1,
                  VALE_REFE=0.1,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAD4,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=0.,
                  VALE_REFE=0.,
                  CRITERE='ABSOLU',
                  PRECISION=1.E-08,),
          )

# 3.3 : AVEC CHARGE DUALISEE ET MUMPS DISTRIBUE PAR SOUS-DOMAINES
#       AVEC MATRICE_DISTRIBUEE
#----------------------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='SOUS_DOMAINE',PARTITION=SDFETI, CHARGE_PROC0_SD=0))
MECAD5=MECA_STATIQUE(INFO=niv_info,
                     MODELE=MO,
                     CHAM_MATER=CHAM_MAT,
                     EXCIT=(_F(CHARGE=CHARF),
                            _F(CHARGE=CHARD)),
                     OPTION='SANS',
                     SOLVEUR=_F(METHODE=solveur,
                                RENUM=meth_renum,
                                RESI_RELA=-1.0,
                                GESTION_MEMOIRE='OUT_OF_CORE',
                                PCENT_PIVOT=pcent_pivot,
                                NPREC=-1,
                                ));

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAD5,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=0.1,
                  VALE_REFE=0.1,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAD5,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=0.,
                  VALE_REFE=0.,
                  CRITERE='ABSOLU',
                  PRECISION=1.E-08,),
          )

# 4.1 : FETI SUR LA PARTITION EN SD UTILISEE POUR MUMPS
#       DISTRIBUE PAR SOUS-DOMAINES
#----------------------------------------------------------

MECAF1=MECA_STATIQUE(INFO=niv_info,
                     MODELE=MO,
                     CHAM_MATER=CHAM_MAT,
                     EXCIT=(_F(CHARGE=CHARF),
                            _F(CHARGE=CHARD)),
                     OPTION='SANS',
                     SOLVEUR=_F(METHODE='FETI',
                                PARTITION=SDFETI,
                                NMAX_ITER=100,
                                NB_REORTHO_DD=20,
                                INFO_FETI='FFFFFFFFFFTFFF',
                                RESI_RELA=1.E-8,
                                ));

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAF1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=0.1,
                  VALE_REFE=0.1,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAF1,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=0.,
                  VALE_REFE=0.,
                  CRITERE='ABSOLU',
                  PRECISION=1.E-08,),
          )

# 4.2 : IDEM 4.1 MAIS SUR UNE PARTITION EN SD MOINS FINE
#----------------------------------------------------------

SDFETI2 = DEFI_PART_FETI(
                MODELE=MO,
                NB_PART=8,
                EXCIT=(_F(CHARGE=CHARF),_F(CHARGE=CHARD)),
                METHODE='KMETIS',
                NOM_GROUP_MA='B_');

MECAF2=MECA_STATIQUE(INFO=niv_info,
                     MODELE=MO,
                     CHAM_MATER=CHAM_MAT,
                     EXCIT=(_F(CHARGE=CHARF),
                            _F(CHARGE=CHARD)),
                     OPTION='SANS',
                     SOLVEUR=_F(METHODE='FETI',
                                PARTITION=SDFETI2,
                                NMAX_ITER=100,
                                NB_REORTHO_DD=20,
                                INFO_FETI='FFFFFFFFFFTFFF',
                                RESI_RELA=1.E-8,
                                ));

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAF2,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DZ',
                  VALE_CALC=0.1,
                  VALE_REFE=0.1,),
          )

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='D',
                  REFERENCE='ANALYTIQUE',
                  RESULTAT=MECAF2,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  VALE_CALC=0.,
                  VALE_REFE=0.,
                  CRITERE='ABSOLU',
                  PRECISION=1.E-08,),
          )

FIN();
