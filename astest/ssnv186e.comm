DEBUT(CODE=_F(NOM='SSNV186E',NIV_PUB_WEB='INTERNET'));

PRE_GMSH()

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAGE=CREA_MAILLAGE(MAILLAGE=MAILLAG1,);

MAILLAG1=DEFI_GROUP(reuse =MAILLAG1,
                    MAILLAGE=MAILLAG1,
                    CREA_GROUP_MA =(_F(NOM='SURFINF',GROUP_MA='GM1'),
                                    _F(NOM='SURFSUP',GROUP_MA='GM2'),
                                    _F(NOM='VOL',    GROUP_MA='GM3'),),
                    CREA_GROUP_NO=_F(GROUP_MA='VOL',),);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                       LINE_QUAD=_F(GROUP_MA='VOL',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=_F(NOM='VOLQUAD',GROUP_MA='VOL',),);                        

#IMPR_RESU(FORMAT='RESULTAT',
#          RESU=_F(MAILLAGE=MAILLAG2,));

             
MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOL',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D_XFEM',),
                           _F(GROUP_MA=('SURFINF','SURFSUP',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D',),),);        

LN=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Z-17.5');
LT=FORMULE(NOM_PARA=('X','Y','Z'),VALE='X-100. ');                      

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                    DEFI_FISS=_F(
                                 FONC_LT=LT,
                                 FONC_LN=LN,               
                                 ),
                    GROUP_MA_ENRI='VOL',
                    ORIE_FOND=_F(PFON_INI=(2.5 ,0. ,2.5 ),
                                 VECT_ORIE=(0. ,0. ,1. ),
                                 PT_ORIGIN=(-0.5,0.,1.5 ),),
                    CONTACT=_F(INTEGRATION='FPG4',
                               COEF_REGU_CONT=100., 
                               ITER_CONT_MAXI=4,   
                               CONTACT_INIT='OUI',    
                               ALGO_LAGR='VERSION1',
                               COEF_ECHELLE=1.E11,
                               FROTTEMENT='SANS',
#                               FROTTEMENT='COULOMB',
#                               COULOMB=1.0,
#                               ITER_FROT_MAXI=6,
#                               COEF_REGU_FROT=1000.,
#                                SEUIL_INIT=-1.E11,
                               ),
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         INFO=1,);

E=1.0E11
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,NU=nu,RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA=('VOL','SURFINF','SURFSUP',),
                                MATER=ACIER,
                                TEMP_REF=0.0,),);

pres=100.*E/1e6

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SURFINF',DX=0.0,DY=0.0,DZ=0.0,),
                             _F(GROUP_MA='SURFSUP',DX=0.0,DY=0.0,),),     
                   PRES_REP=_F(GROUP_MA='SURFSUP',PRES=pres,),
                   INFO=1,);          


CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                                INFO=1,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,              
                   EXCIT=(_F(CHARGE=CH1,),
                          _F(CHARGE=CHXFEM,),),                  
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='VOL',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   SOLVEUR=_F(METHODE='MUMPS',
                              PCENT_PIVOT=250,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);


LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                            GROUP_NO='VOLQUAD',
                            RESULTAT=UTOT1,
                            NOM_CHAM='DEPL',
                            NUME_ORDRE=1,
                            NOM_CMP='LAGS_C',
                            OPERATION='EXTRACTION',),);

LAG=CALC_TABLE(TABLE=LAG,
               reuse=LAG,
               ACTION=_F(OPERATION='FILTRE',
                         NOM_PARA='LAGS_C',
                         CRIT_COMP='NE',
                         VALE=0.,),
               );

IMPR_TABLE(TABLE=LAG,);

# TESTS
# LES VALEURS DE PRESSIONS OBTENUES SONT EN FAIT LES PRESSIONS DIVISEES PAR COEF_ECHELLE
LAREF=-1.*pres/1.e11

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MAX',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=3.E-2,
           REFERENCE='ANALYTIQUE',);     

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MIN',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=2.E-2,
           REFERENCE='ANALYTIQUE',);      

MA_XFEM=POST_MAIL_XFEM(MODELE=MODELEK)


DEPL=POST_CHAM_XFEM(MAILLAGE=MA_XFEM,
                    MODELE=MODELEK,
                    NOM_CHAM='DEPL',
                    RESULTAT=UTOT1)


IMPR_RESU(FORMAT='IDEAS',RESU=_F(MAILLAGE=MA_XFEM,RESULTAT=DEPL))

FIN();                  

