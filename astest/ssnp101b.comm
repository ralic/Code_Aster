             _F( JUSQU_A = 30.0,       NOMBRE = 20),
             _F( JUSQU_A = 3630.0,     NOMBRE = 15),
             _F( JUSQU_A = 3660.0,     NOMBRE = 3),
             _F( JUSQU_A = 3720.0,     NOMBRE = 1))
