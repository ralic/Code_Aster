                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F( 
                ITER_GLOB_MAXI = 20,
                RESI_GLOB_RELA = 1.E-6),
                     RECH_LINEAIRE=_F( 
                ITER_LINE_MAXI = 4,
                RESI_LINE_RELA = 1.E-3)
              )

