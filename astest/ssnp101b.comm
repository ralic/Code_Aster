             _F( JUSQU_A = 3660.0,     NOMBRE = 33),
             _F( JUSQU_A = 3720.0,     NOMBRE = 11))
           )

U=STAT_NON_LINE( 
                            MODELE=MO,
                        CHAM_MATER=CM,EXCIT=(
                             _F( 
                CHARGE = CH1), _F( 
                CHARGE = CH2)),
                         COMP_INCR=_F( 
                RELATION = 'LEMAITRE',
                ITER_INTE_MAXI = 1000),
