                                   CHARGE=CH2,
                               CHAM_MATER=CHMAT,
                                CARA_ELEM=CA2,
                                   OPTION='MASS_MECA'    )

NUM2=NUME_DDL(     MATR_RIGI=RIGIEL  )

MATR2=ASSE_MATRICE(    MATR_ELEM=RIGIEL,      NUME_DDL=NUM2  )

MATM2=ASSE_MATRICE(    MATR_ELEM=MASSEL,      NUME_DDL=NUM2  )

