                       AFFE=_F(  TOUT = 'OUI',      MATER = MAT) )

#====================================================================
# ---- PLAQUE DKT APPUYEE SUR LES BORDS

MO=AFFE_MODELE(   MAILLAGE=MA, 
                   AFFE=_F(  GROUP_MA = 'COQUE',
                          PHENOMENE = 'MECANIQUE',
                          MODELISATION = 'DKT')  )

