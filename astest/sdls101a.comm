                          CREA_GROUP_NO=_F(  GROUP_MA = 'COQUE')  )

MA=CREA_MAILLAGE(   MAILLAGE=MA_INIT,
                        CREA_MAILLE=_F(  PREF_MAILLE = 'NOU',
                                      PREF_NUME = 1,
                                      MAILLE = ('M271', 'M272', 
                                                'M273', 'M274', 'M275',)),
                         CREA_GROUP_MA=_F(  NOM = 'APPU_1',
                                         PREF_MAILLE = 'NEW',
                                         PREF_NUME = 1,
                                         GROUP_MA = 'COQPAR')  )

MA=DEFI_GROUP( reuse=MA,  MAILLAGE=MA,CREA_GROUP_MA=(
                          _F(   NOM = 'APPU_2',
                                           MAILLE = ('NOU1', 'NOU2', 'NOU3',
                                                     'NOU4', 'NOU5',)),
                          _F(   NOM = 'APPUI',
                                           UNION = ('APPU_1', 'APPU_2',)))
