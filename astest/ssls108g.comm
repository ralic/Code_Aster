DEBUT(CODE=_F(NOM='SSLS108G', NIV_PUB_WEB='INTERNET'), DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MAIL=LIRE_MAILLAGE();

MAIL=MODI_MAILLAGE(reuse=MAIL,MAILLAGE=MAIL,
ORIE_SHB=_F(GROUP_MA='VOLTOT'),)
#ORIE_SHB=_F(GROUP_MA='TOUT'),)

MAIL=DEFI_GROUP(reuse =MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=(_F(GROUP_MA='ENCAS',),
                               _F(GROUP_MA='BOUT',),),);

MODMECA=AFFE_MODELE(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            PHENOMENE='MECANIQUE',
                            MODELISATION='SHB8',),);

MAT=DEFI_MATERIAU(ELAS=_F(E=29E+6,
                          NU=0.22,),);

CHM=AFFE_MATERIAU(MAILLAGE=MAIL,
                  AFFE=_F(TOUT='OUI',
                          MATER=MAT,),);


CHARGT=AFFE_CHAR_MECA(MODELE=MODMECA,
                      DDL_IMPO=(_F(GROUP_MA=('ENCAS'),
                                   DX=0.0,),
                                _F(GROUP_NO='ENCAS',
                                   DY=0.0,),
                                _F(GROUP_NO='ENCAS',
                                   DZ=0.0,),),
                      FORCE_NODALE=(_F(GROUP_NO = 'A1',
                                      FZ=0.5,),
                                    _F(GROUP_NO = 'A2',
                                      FZ=0.5,),),);

RESU1=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHM,
                    EXCIT=_F(CHARGE=CHARGT,),
                    );

RESU1=CALC_ELEM(reuse =RESU1,
                OPTION='SIEF_ELNO_ELGA',
                RESULTAT=RESU1,);

CHARGT2=AFFE_CHAR_MECA(MODELE=MODMECA,
                      DDL_IMPO=(_F(GROUP_MA=('ENCAS'),
                                   DX=0.0,),
                                _F(GROUP_NO='ENCAS',
                                   DY=0.0,),
                                _F(GROUP_NO='ENCAS',
                                   DZ=0.0,),),
                      FORCE_NODALE=(_F(GROUP_NO = 'A1',
                                      FY=0.5,),
                                    _F(GROUP_NO = 'A2',
                                      FY=0.5,),),);

RESU2=MECA_STATIQUE(MODELE=MODMECA,
                    CHAM_MATER=CHM,
                    EXCIT=_F(CHARGE=CHARGT2,),
                    );

RESU2=CALC_ELEM(reuse =RESU2,
                OPTION='SIEF_ELNO_ELGA',
                RESULTAT=RESU2,);


TEST_RESU(RESU=(
     # DEPLACEMENT FZ

            _F(  RESULTAT = RESU1, NOM_CHAM='DEPL', PRECISION = 3.E-2, REFERENCE='SOURCE_EXTERNE',NUME_ORDRE=1,
              GROUP_NO = 'A1',      NOM_CMP = 'DY',           VALE = -1.72E-3),

            _F(   RESULTAT = RESU1, NOM_CHAM='DEPL',  PRECISION = 3.E-2, REFERENCE='SOURCE_EXTERNE',NUME_ORDRE=1,
              GROUP_NO = 'A2',      NOM_CMP = 'DZ',           VALE = 5.42E-3),

     # DEPLACEMENT FY

            _F(  RESULTAT = RESU2, NOM_CHAM='DEPL', PRECISION = 3.E-2, REFERENCE='SOURCE_EXTERNE',NUME_ORDRE=1,
              GROUP_NO = 'A1',      NOM_CMP = 'DY',           VALE = 1.75E-3),

            _F(  RESULTAT = RESU2, NOM_CHAM='DEPL', PRECISION = 3.E-2, REFERENCE='SOURCE_EXTERNE',NUME_ORDRE=1,
              GROUP_NO = 'A2',      NOM_CMP = 'DZ',           VALE = -1.72E-3))
             )


FIN()

