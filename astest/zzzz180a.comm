DETRUIRE(CONCEPT=_F(NOM=(VECT1,FONCT1,FONCT2,RMS1,RMS2,)),INFO=1)

##############################
# GENERATION DES FCT ALEATOIRES, 
##############################
VECT1=GENE_FONC_ALEA(
                         INTE_SPEC=INT1,
                          )

FONCT1=RECU_FONCTION(    TABLE=VECT1,
                              FILTRE=_F(  NOM_PARA = 'NUME_ORDRE',
                                       VALE_I = 1),
                              NOM_PARA_TABL='FONCTION',    )

