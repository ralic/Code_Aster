TEST_TABLE(      TABLE=RMS1,
                 NOM_PARA='RMS',
                 REFERENCE='ANALYTIQUE',
                 VALE=sqrt(300*2)  , PRECISION=1.E-2)

RMS2=INFO_FONCTION( RMS=_F(  FONCTION = FONCT2))

TEST_TABLE(      TABLE=RMS2,
                 NOM_PARA='RMS',
