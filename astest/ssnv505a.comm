# CAS_TEST : SSNV505A
# MODELE
# MAILLAGE : ELEMENT 3D QUADRANGLE A 9 NOEUDS
# UNITES   : NEWTON , MILLIMETRE , SECONDE (N,MM,S)
#
DEBUT(CODE=_F( NOM = 'SSNV505A',NIV_PUB_WEB='INTERNET'));
#......................................................................
#  CALCUL TEST ELEMENTS FINIS DE CONTACT 3D QUA8
#                                MODELISATION COQUES_3D (QUA9)
#......................................................................
#
MAILL1=LIRE_MAILLAGE(FORMAT='MED',);

MAILL2=CREA_MAILLAGE(MAILLAGE=MAILL1,
                     MODI_MAILLE=_F(GROUP_MA='STRUCT',
                                    OPTION='QUAD8_9',),);

MAILL2=DEFI_GROUP(reuse =MAILL2,
                  MAILLAGE=MAILL2,
                  CREA_GROUP_MA=(_F(NOM='SESC',
                                    DIFFE=('POUTRE2','MA2'),),
                                 _F(NOM='SMAI',
                                    DIFFE=('POUTRE1','MA1'),),
                                 ),
                  CREA_GROUP_NO=(_F(TOUT_GROUP_MA='OUI',),
                                 _F(NOM='SOMMET',
                                    GROUP_MA='STRUCT',
                                    CRIT_NOEUD='SOMMET',),
                                 _F(NOM='NRESU',
                                    DIFFE=('POUTRE2','LCC1'),),
                                 ),
                  INFO=1
                  );

MODE=AFFE_MODELE(MAILLAGE=MAILL2,
                 VERIF='MAILLE',
                 AFFE=_F(GROUP_MA='STRUCT',
                          PHENOMENE='MECANIQUE',
                          MODELISATION='COQUE_3D',),);


MAILL2=MODI_MAILLAGE(reuse =MAILL2,
                     MAILLAGE=MAILL2,
                     ORIE_NORM_COQUE=_F(GROUP_MA='SMAI',
                                        VECT_NORM=(0.0,1.0,0.0),
                                        GROUP_NO='ORI',
                                      ),
                     );

MAILL2=MODI_MAILLAGE(reuse =MAILL2,
                     MAILLAGE=MAILL2,
                     ORIE_NORM_COQUE=_F(GROUP_MA='SESC',
                                        VECT_NORM=(0.0,-1.0,0.0),
                                        GROUP_NO='C',),
                     );

MPOUTRE=DEFI_MATERIAU(ELAS=_F(E=6.8948E3,
                              NU=0.3333,),);

CARA=AFFE_CARA_ELEM(MODELE=MODE,
                    VERIF='MAILLE',
                    COQUE=_F(GROUP_MA=('POUTRE1','POUTRE2'),
                             COQUE_NCOU =1,
                             EPAIS=63.5,
                             COEF_RIGI_DRZ=1.E-3,
                             ),);

CM=AFFE_MATERIAU(MAILLAGE=MAILL2,
                 AFFE=_F(GROUP_MA='STRUCT',
                         MATER=MPOUTRE,),);

COND_LIM=AFFE_CHAR_MECA(MODELE=MODE,
                        DDL_IMPO=(_F(GROUP_NO='SOMMET',
                                     DZ=0,DRX=0),
                                 _F(GROUP_NO=('LAA3','LBB1'),
                                     DX=0.0,DY=0.0,DRX=0,DRY=0,DRZ=0,),
                                  ),);


CH1=AFFE_CHAR_MECA(MODELE=MODE,
                   DDL_IMPO=_F(GROUP_NO=('C','C1'),
                               DY=-790.,
                             ),);

F_DIST_M=DEFI_CONSTANTE(VALE=31.75);


CH2 = DEFI_CONTACT(MODELE       = MODE,
                   FORMULATION  = 'DISCRETE',

                   ZONE=(
                        _F(
                            GROUP_MA_MAIT='SMAI',
                            GROUP_MA_ESCL='SESC',
                            SANS_GROUP_NO=('LBB1','LCC1'),
                            DIST_MAIT=F_DIST_M,
                            CARA_ELEM=CARA,
                            DIST_COQUE='OUI',
                            TOLE_PROJ_EXT=0.25,
                         ),

                       ),
                  )

RAMPE=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.0,0.0,1.0,1.0),
                    PROL_DROITE='LINEAIRE',
                    PROL_GAUCHE='LINEAIRE',);


LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.,
                                   NOMBRE=100,),);


LARCH=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=1.,
                                   NOMBRE=25,),);

U=STAT_NON_LINE(SOLVEUR=_F(SYME='OUI',),
                MODELE=MODE,
                CHAM_MATER=CM,
                CARA_ELEM=CARA,
                EXCIT=(_F(CHARGE=COND_LIM,),
                       _F(CHARGE=CH1,
                          FONC_MULT=RAMPE,),
                       ),
                CONTACT  = CH2,
                COMP_ELAS=_F(RELATION='ELAS',
                             DEFORMATION='GROT_GDEP',
                             GROUP_MA='STRUCT',),
                INCREMENT=_F(LIST_INST=LINST,
                             INST_FIN=0.8,
                             ),
                NEWTON=_F(REAC_ITER=1,
                          ),
                CONVERGENCE=_F(ITER_GLOB_MAXI=20,),
                ARCHIVAGE=_F(LIST_INST=LARCH,),
                INFO=1,);

U=CALC_CHAMP(reuse =U,
          RESULTAT=U,
          FORCE=('FORC_NODA','REAC_NODA'),
         );
TAB_DEPL=POST_RELEVE_T(ACTION=_F(INTITULE='DEPL',
                                 GROUP_NO='C',
                                 RESULTAT=U,
                                 NOM_CHAM='DEPL',
                                 NOM_CMP='DY',
                                 OPERATION='EXTRACTION',),);
IMPR_TABLE(FORMAT='TABLEAU',
           TABLE=TAB_DEPL,);

TAB_REAC=POST_RELEVE_T(ACTION=_F(INTITULE='FORCE',
                                 GROUP_NO=('LAA3','LBB1'),
                                 RESULTAT=U,
                                 NOM_CHAM='FORC_NODA',
                                 RESULTANTE='DY',
                                 OPERATION='EXTRACTION',),);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.2),
           VALE=3.1396515859657E+02,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='9.1.22',);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.4),
           VALE=1.2206114997884E+03,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='9.1.22',);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.6),
           VALE=2.4568221137607E+03,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='9.1.22',);

TEST_TABLE(TABLE=TAB_REAC,
           NOM_PARA='DY',
           FILTRE=_F(NOM_PARA='INST',
                     VALE=0.8),
           VALE=2.0677188799695E+03,
           PRECISION=0.1E-2,
           REFERENCE='NON_REGRESSION',
           VERSION='9.1.22',);


FIN();
