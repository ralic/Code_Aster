DEBUT(  CODE=_F( NOM = 'TPLP301B',NIV_PUB_WEB='INTERNET'))

#
# LECTURE DU MAILLAGE ----------------------------------------------
#

MAILL=LIRE_MAILLAGE(FORMAT='MED', )

#
# DEFINITION DES CHARGEMENTS ET DU MATERIAU ------------------------
#


MATER=DEFI_MATERIAU(    THER=_F(  RHO_CP = 1.0,
                                    LAMBDA = 1.)
                         )

#
# AFFECTATIONS: MATERIAU  MODELE  CHARGEMENT -----------------------
#

MODEL=AFFE_MODELE(    MAILLAGE=MAILL,
                           AFFE=_F(  TOUT = 'OUI',
                                  PHENOMENE = 'THERMIQUE',
                                  MODELISATION = 'PLAN')
                       )

CHMATE=AFFE_MATERIAU(    MAILLAGE=MAILL,
                              AFFE=_F(  TOUT = 'OUI',  MATER = MATER)
                          )

CHARGE=AFFE_CHAR_THER( 
                       MODELE=MODEL,TEMP_IMPO=(
                    _F(  GROUP_NO = 'NOEU_AD',  TEMP = 0.),
                    _F(  GROUP_NO = 'NOEU_DJ',  TEMP = 0.),
                    _F(  NOEUD = 'N1',          TEMP = 0.),
                    _F(  NOEUD = 'N18',         TEMP = 0.1951),
                    _F(  NOEUD = 'N35',         TEMP = 0.3827),
                    _F(  NOEUD = 'N52',         TEMP = 0.5556),
                    _F(  NOEUD = 'N69',         TEMP = 0.7071),
                    _F(  NOEUD = 'N86',         TEMP = 0.8315),
                    _F(  NOEUD = 'N103',        TEMP = 0.9239),
                    _F(  NOEUD = 'N120',        TEMP = 0.9808),
                    _F(  NOEUD = 'N137',        TEMP = 1.))
                           )

#
# RESOLUTION ------------------------------------------------------
#


TEMP=THER_LINEAIRE( 
                      MODELE=MODEL,
                  CHAM_MATER=CHMATE,
                       EXCIT=_F(  CHARGE = CHARGE)
                         )

#
# ANALYSE DES RESULTATS -------------------------------------------
#
TPR=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_TEMP_R',
NOM_CHAM='TEMP', RESULTAT=TEMP, NUME_ORDRE=0)

#

TEST_RESU(CHAM_NO=(

      _F( CHAM_GD = TPR,  NOEUD = 'N1',  NOM_CMP = 'TEMP',
                       VALE = 0.0000,   PRECISION = 1.E-2,
                                      CRITERE = 'ABSOLU'),
      _F( CHAM_GD = TPR,  NOEUD = 'N18', NOM_CMP = 'TEMP',
                       VALE = 0.1951,   PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N35', NOM_CMP = 'TEMP',
                       VALE = 0.3827,   PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N52', NOM_CMP = 'TEMP',
                       VALE = 0.5556,   PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N69', NOM_CMP = 'TEMP',
                       VALE = 0.7071,   PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N86', NOM_CMP = 'TEMP',
                       VALE = 0.8315,   PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N103', NOM_CMP = 'TEMP',
                       VALE = 0.9239,    PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N120', NOM_CMP = 'TEMP',
                       VALE = 0.9808,    PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N137', NOM_CMP = 'TEMP',
                       VALE = 1.00000,   PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N141', NOM_CMP = 'TEMP',
                       VALE = 0.45269,   PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N145', NOM_CMP = 'TEMP',
                       VALE = 0.19927,   PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N149', NOM_CMP = 'TEMP',
                       VALE = 0.075218,  PRECISION = 1.E-2),
      _F( CHAM_GD = TPR,  NOEUD = 'N153', NOM_CMP = 'TEMP',
                       VALE = 0.00000,   PRECISION = 1.E-4,
                                       CRITERE = 'ABSOLU'))

               )

FIN()
