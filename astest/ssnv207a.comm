# PRESSION DE CONSOLIDATION P0 = 50KPA


DEBUT(CODE=_F(NOM='SSNV207A',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'));

# modules mecaniques [kPa]
K=516.2E6
G=238.2E6
# =>
YOUNG = 9.*K*G /(3.*K+G)
POISSON = (3.*K-2.*G) /(6.*K+2.*G)

P0 = - 5.E4

MATE=DEFI_MATERIAU(ELAS=_F(E = YOUNG, NU = POISSON),
                   HUJEUX=_F(N=0.4,
                             BETA=24,
                             B=0.2,
                             D=2.5,
                             PHI=33,
                             ANGDIL=33,
                             PCO=-1.E6,
                             PREF=-1.E6,
                             ACYC=0.0001,
                             AMON=0.008,
                             CCYC=0.09,
                             CMON=0.18,
                             RD_ELA=0.005,
                             RI_ELA=0.001,
                             RHYS=0.05,
                             RMOB=0.9,
                             XM=1,
                             RD_CYC=0.005,
                             RI_CYC=0.001,
                             DILA  =1.,),);

TEMPS1=DEFI_LIST_REEL(DEBUT=0,
                      INTERVALLE=(_F(JUSQU_A=40, NOMBRE=80,),),);
                      

################################################################
#     ESSAI DE CISAILLEMENT CYCLIQUE AVEC MICRO-DECHARGES      #
################################################################
COEF1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0 ,0,
                          10,-1.E4,
                          15,-6.E3,
                          20,-6.3E3,
                          25,1.E4,
                          30,-5.E3,
                          35,-4.7E3,
                          40,-1.E4,),
                    PROL_DROITE='CONSTANT',);

COEF2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0 ,P0,
                          40,P0,),
                    PROL_DROITE='CONSTANT',);

DEFLIST1 =DEFI_LIST_INST(DEFI_LIST=_F(LIST_INST = TEMPS1),
                        ECHEC=_F(ACTION        = 'DECOUPE',
                                 SUBD_METHODE  = 'MANUEL',
                                 SUBD_PAS      = 10,
                                 SUBD_NIVEAU   = 10),)

U1=SIMU_POINT_MAT(COMP_INCR=_F(RELATION='HUJEUX',
                               ITER_INTE_MAXI=20,
                               RESI_INTE_RELA=1E-8,
                               ITER_INTE_PAS =-5),
                  MATER=MATE,
                  INCREMENT=_F(LIST_INST=DEFLIST1,
                               NUME_INST_INIT=0,
                               INST_FIN=40,),
                  NEWTON=_F(MATRICE='TANGENTE',
                            REAC_ITER=1,),
#                 RECH_LINEAIRE = _F(ITER_LINE_MAXI = 3),                           
                  CONVERGENCE=_F(RESI_GLOB_RELA = 1.E-6,
                                 ITER_GLOB_MAXI = 10),
                  ARCHIVAGE=_F(LIST_INST=TEMPS1,),
                  SIGM_IMPOSE=_F(SIXX=COEF2,
                                 SIYY=COEF2,
                                 SIZZ=COEF2,
                                 SIXY=COEF1,),
                  SIGM_INIT=_F(SIXX=P0,
                               SIYY=P0,
                               SIZZ=P0,),
                  EPSI_INIT=_F(EPXX=0,
                               EPYY=0,
                               EPZZ=0,
                               EPXY=0,
                               EPXZ=0,
                               EPYZ=0,),);

IMPR_TABLE(TABLE=U1,);

