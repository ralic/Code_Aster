                       MODELISATION='3D',),
               PARTITION=_F(PARALLELISME='MAIL_CONTIGU',),
               );

MAT=DEFI_MATERIAU(ELAS=_F(E=202702.7,
                          NU=0.3,
                          RHO=1.e-1,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
