                               AMOR_REDUIT=0.03,
                               MASS_INER=MAS,
                               CORR_FREQ='NON',
                               MONO_APPUI = 'OUI',
                               EXCIT=_F( AXE = ( 1., 0., 0., ),
                                         SPEC_OSCI = SPECT_XY,
                                         ECHELLE = 1.),
