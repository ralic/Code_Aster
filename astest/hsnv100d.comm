           POUTRE=_F(
       GROUP_MA = 'TOUT',
  SECTION = 'CERCLE',TUYAU_NCOU=1,
          CARA = (   'R', 'EP',  ), VALE = (  REXT, EP, ))
)

SURF=pi* (REXT*REXT-RINT*RINT)

MAT=DEFI_MATERIAU(
          ELAS=_F( E = YOUNG, NU = POISSON, ALPHA = DILAT),
           ECRO_LINE_FO=_F(  D_SIGM_EPSI = PENTE, SY = SIGY)
                        )

