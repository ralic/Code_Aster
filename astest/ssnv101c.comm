#       MODELE DE CHABOCHE A 2 CENTRES POUR LE 3D
# TEST VPCS PLAQUE CARREE EN TRACTION CISAILLEMENT CALCUL 3D
# MODELE ELASTOPLASTIQUE DE CHABOCHE AVEC TEMPERATURE IMPOSEE
# MAILLAGE : ELEMENT 3D CUBE A 8 NOEUDS
# UNITES   : NEWTON , MILLIMETRE , SECONDE (N,MM,S)
#
DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

CUBE=LIRE_MAILLAGE(    )

TROISD=AFFE_MODELE(            MAILLAGE=CUBE,
                                    AFFE=_F(   PHENOMENE = 'MECANIQUE',
                                            MODELISATION = '3D',
                                            TOUT = 'OUI',              ))

E0 = 145200.
E1 = 142500.
E2 = 151500.
T1 = 700.
T2 = 600.
Temp = T1 + ( (T2-T1)*(E0-E1)/(E2-E1))

ACIER1=INCLUDE_MATERIAU(NOM_AFNOR='Z2CND1712',
                        UNITE_LONGUEUR='MM',
                        TYPE_MODELE='REF',
                        VARIANTE='A',
                        TYPE_VALE='NOMI',
                        EXTRACTION=_F(COMPOR='ELAS',
                                      TEMP_EVAL=Temp,),  );

# LE C_I DU MODELE VMIS_CIN1_CHAB EST DETERMINE PAR C_I = C1*A1

ACIER2=DEFI_MATERIAU(         MATER=ACIER1,
                              CIN1_CHAB=_F( R_I = 151.,
                                            R_0 = 87.,
                                            B = 2.3,
                                            C_I = 63767.,
                                            G_0 = 341.,             ))

ACIER3=DEFI_MATERIAU(       MATER=ACIER1,
                            CIN2_CHAB=_F(   R_I = 151.,
                                            R_0 = 87.,
                                            B = 2.3,
                                            C1_I = 63767.,
                                            G1_0 = 341.,
                                            C2_I = 0.,
                                            G2_0 = 0.,
                                            ))

ACIER4=DEFI_MATERIAU(       MATER=ACIER1,
                            CIN2_CHAB=_F(   R_I = 151.,
                                            R_0 = 87.,
                                            B = 2.3,
                                            C1_I = 0.,
                                            G1_0 = 341.,
                                            C2_I = 63767.,
                                            G2_0 = 341.,
                                            ))

ACIER5=DEFI_MATERIAU(       MATER=ACIER1,
                            CIN2_CHAB=_F(   R_I = 151.,
                                            R_0 = 87.,
                                            B = 2.3,
                                            C1_I = 31883.5,
                                            G1_0 = 341.,
                                            C2_I = 31883.5,
                                            G2_0 = 341.,
                                             ))

MAT2=AFFE_MATERIAU(            MAILLAGE=CUBE,
                                    AFFE=_F(  MAILLE = 'MA1',
                                           MATER = ACIER2,
                                           ))

MAT3=AFFE_MATERIAU(            MAILLAGE=CUBE,
                                    AFFE=_F(  MAILLE = 'MA1',
                                           MATER = ACIER3,
                                           ))

MAT4=AFFE_MATERIAU(            MAILLAGE=CUBE,
                                    AFFE=_F(  MAILLE = 'MA1',
                                           MATER = ACIER4,
                                           ))

MAT5=AFFE_MATERIAU(            MAILLAGE=CUBE,
                                    AFFE=_F(  MAILLE = 'MA1',
                                           MATER = ACIER5,
                                           ))

LINST=DEFI_LIST_REEL(              DEBUT=0.,INTERVALLE=(
                              _F( JUSQU_A = 0.4,    NOMBRE = 1,  ),
                              _F( JUSQU_A = 1.435, NOMBRE = 12,  )))

TR_CS=AFFE_CHAR_MECA( MODELE=TROISD,
                      DDL_IMPO=(_F( NOEUD = 'NO4',  DX = 0.,DY = 0.,),
                                _F( NOEUD = 'NO8',  DX = 0.,DY = 0.,DZ = 0.,),
                                _F( NOEUD = 'NO2',  DX = 0.,),
                                _F( NOEUD = 'NO6',  DX = 0.,)),
                      FORCE_NODALE=(_F( NOEUD = ('NO3', 'NO7',),  FX = -50.,  FY = -25., ),
                                    _F( NOEUD = ('NO4', 'NO8',),  FX = -25.,  FY = 25.,  ),
                                    _F( NOEUD = ('NO2', 'NO6',),  FX = 25.,   FY = 25.,  ),
                                    _F( NOEUD = ('NO1', 'NO5',),           FY = -25., )) )

COEF=DEFI_FONCTION(      NOM_PARA='INST',
                            PROL_DROITE='LINEAIRE',
                           PROL_GAUCHE='LINEAIRE',
                          VALE=( 0.0,  0.0,
                                 1.0,  1.0,    ))


SOLNL2=STAT_NON_LINE(      TITRE='TEST TRACTION_CISAILLEMENT LOI CHABOCHE',
                                    MODELE=TROISD,
                                CHAM_MATER=MAT2,
                                   EXCIT=_F(  CHARGE = TR_CS,
                                           FONC_MULT = COEF,
                                           TYPE_CHARGE = 'FIXE_CSTE',     ),
                               COMPORTEMENT=_F(  RELATION = 'VMIS_CIN1_CHAB',
                                           RESI_INTE_RELA = 1.E-8,
                                           ITER_INTE_MAXI = 30,
                                           DEFORMATION = 'PETIT',
                                           TOUT = 'OUI',                 ),
                               INCREMENT=_F(
                                           LIST_INST = LINST,            ),
                                  NEWTON=_F(  MATRICE = 'TANGENTE',
                                           REAC_INCR = 1,
                                           REAC_ITER = 1,                ),
                             CONVERGENCE=_F(  ITER_GLOB_MAXI = 30,
                                           RESI_GLOB_RELA = 1.E-8,
                                          ),
                         )

#

SOLNL2=CALC_CHAMP(reuse=SOLNL2,RESULTAT=SOLNL2,CONTRAINTE=('SIGM_ELNO'),VARI_INTERNE=('VARI_ELNO'),DEFORMATION=('EPSI_ELNO'))


