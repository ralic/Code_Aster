DEBUT(   CODE=_F(  NOM = 'SSNP305A',NIV_PUB_WEB='INTERNET'))

MAYA=LIRE_MAILLAGE( )

MODEL=AFFE_MODELE(    MAILLAGE=MAYA,
                                VERIF='MAILLE',
                                 AFFE=_F(  TOUT = 'OUI',
                                        PHENOMENE = 'MECANIQUE',
                                        MODELISATION = 'C_PLAN')
                         )

ACIER1=DEFI_MATERIAU(         ELAS=_F(  E = 2.E7,
                                      NU = 0.)
                      )

ACIER2=DEFI_MATERIAU(         ELAS=_F(  E = 200000.,
                                      NU = 0.)
                      )

MATER=AFFE_MATERIAU(   MAILLAGE=MAYA,AFFE=(
                                _F(  GROUP_MA = 'BORD',
                                       MATER = ACIER1),
                                     _F(  GROUP_MA = 'MILIEU',
                                       MATER = ACIER2))
                         )

CAT=DEFI_LIST_REEL(    DEBUT=0.0,
                          INTERVALLE=_F(  JUSQU_A = 1.,     NOMBRE = 100)
                      )

CONDLIM=AFFE_CHAR_MECA(     MODELE=MODEL,DDL_IMPO=(
                              _F(  NOEUD = 'NO000007',
                                               DX = 0.),
                                       _F(  NOEUD = 'NO000001',
                                               DX = 0.,
                                               DY = 0.))
                          )

DY_FO=DEFI_FONCTION(       NOM_PARA='INST',
                             PROL_DROITE='EXCLU',
                            PROL_GAUCHE='EXCLU',
                                   VALE=( 0.,      0.,
                                          1.,    -2500.,     )
                        )

CHA=AFFE_CHAR_MECA_F(     MODELE=MODEL,
                                  DDL_IMPO=_F(  NOEUD = 'NO000007',
                                             DY = DY_FO)
                         )

CALCNL=STAT_NON_LINE(
                            MODELE=MODEL,
                        CHAM_MATER=MATER,
                         COMP_ELAS=_F(
                RELATION = 'ELAS',
                DEFORMATION = 'GROT_GDEP'),
                         INCREMENT=_F(
                LIST_INST = CAT),
                            NEWTON=_F(
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F(
                RESI_GLOB_RELA = 1.E-6,
                ITER_GLOB_MAXI = 50),EXCIT=(
                             _F(
                CHARGE = CHA), _F(
                CHARGE = CONDLIM)),
                     RECH_LINEAIRE=_F(
                ITER_LINE_MAXI = 3)
              )

CALCNL=CALC_ELEM( reuse=CALCNL,
                            OPTION=(
                  'SIEF_ELNO',  'VARI_ELNO_ELGA',),
                          RESULTAT=CALCNL,

                        CHAM_MATER=MATER
              )

CALCNL=CALC_ELEM( reuse=CALCNL,
                           RESULTAT=CALCNL,

                             OPTION='EPSI_ELNO_DEPL'
                      )

CALCNL=CALC_NO( reuse=CALCNL,           RESULTAT=CALCNL,
                                OPTION='FORC_NODA',
                   )

TEST_RESU(RESU=(
          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'FORC_NODA',
            NUME_ORDRE = 10,
            NOEUD = 'NO000007',
            NOM_CMP = 'DY',
            CRITERE = 'RELATIF',
            PRECISION = 5.E-2,
            VALE = -1511441.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'FORC_NODA',
            NUME_ORDRE = 20,
            NOEUD = 'NO000007',
            NOM_CMP = 'DY',
            CRITERE = 'RELATIF',
            PRECISION = 5.E-2,
            VALE = -2545584.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'FORC_NODA',
            NUME_ORDRE = 30,
            NOEUD = 'NO000007',
            NOM_CMP = 'DY',
            CRITERE = 'RELATIF',
            PRECISION = 5.E-2,
            VALE = -3155464.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'FORC_NODA',
            NUME_ORDRE = 42,
            NOEUD = 'NO000007',
            NOM_CMP = 'DY',
            CRITERE = 'RELATIF',
            PRECISION = 5.E-2,
            VALE = -3401961.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'FORC_NODA',
            NUME_ORDRE = 60,
            NOEUD = 'NO000007',
            NOM_CMP = 'DY',
            CRITERE = 'RELATIF',
            PRECISION = 5.E-2,
            VALE = -2969848.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'FORC_NODA',
            NUME_ORDRE = 80,
            NOEUD = 'NO000007',
            NOM_CMP = 'DY',
            CRITERE = 'RELATIF',
            PRECISION = 5.E-2,
            VALE = -1697056.),

          _F(  RESULTAT = CALCNL,
            NOM_CHAM = 'FORC_NODA',
            NUME_ORDRE = 100,
            NOEUD = 'NO000007',
            NOM_CMP = 'DY',
            CRITERE = 'ABSOLU',
            VALE = 0.))

          )

FIN()
