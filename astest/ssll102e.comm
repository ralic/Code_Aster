DEBUT(CODE=_F(NOM='SSLL102E',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

M=LIRE_MAILLAGE(FORMAT='MED',   )

M=DEFI_GROUP( reuse=M,       MAILLAGE=M,
                      CREA_GROUP_NO=_F( GROUP_MA = 'SURF1')
                   )

MO=AFFE_MODELE(            MAILLAGE=M,
                                   VERIF='MAILLE',
                                  AFFE=_F(  TOUT = 'OUI',
                                         PHENOMENE = 'MECANIQUE',
                                         MODELISATION = '3D'))

MA=DEFI_MATERIAU(            ELAS=_F(  E = 2.E11,
                                         NU = 0.3,
                                         ALPHA = 0.))

CM=AFFE_MATERIAU(        MAILLAGE=M,
                                  AFFE=_F(  TOUT = 'OUI',
                                         MATER = MA))

CH=AFFE_CHAR_MECA(         MODELE=MO,
                           FORCE_ARETE=_F(  GROUP_MA = 'L2',  FZ = -5.),


                        DDL_IMPO=_F(  GROUP_NO = 'SURF1',  DX = 0., DY = 0., DZ = 0.)
            )

RESU1=MECA_STATIQUE(     MODELE=MO,
                         CHAM_MATER=CM,
                         EXCIT=_F(  CHARGE = CH)
                        )

DEP1=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='NOEU_DEPL_R',
NOM_CHAM='DEPL', RESULTAT=RESU1,
                     NUME_ORDRE=1       )

#

TEST_RESU(
CHAM_NO=_F( CHAM_GD = DEP1,NOEUD = 'N62',PRECISION = 6.E-3,NOM_CMP = 'DZ',VALE = -8.E-7, REFERENCE = 'ANALYTIQUE')
                         )

#-----------------------------------------------------------------

FIN()
