F_DY_2=RECU_FONCTION( RESULTAT=U_2,
              NOM_CHAM='DEPL',NOM_CMP='DY',GROUP_NO='TOUTSEUL')
T_DY_2=CREA_TABLE(FONCTION=_F(FONCTION=F_DY_2))
