DEBUT(CODE=_F(NOM='SSNP118D',NIV_PUB_WEB='INTERNET',VISU_EFICAS='OUI'),DEBUG=_F(SDVERI='OUI'))


MA=LIRE_MAILLAGE( )

IMPR_RESU(RESU=_F(MAILLAGE=MA),FORMAT='GMSH',UNITE=19)

MO=AFFE_MODELE(

                MAILLAGE=MA,
                VERIF='MAILLE',

                AFFE= (
                _F(
                   MAILLE = ('CUBE','DROITE'),
                   PHENOMENE = 'MECANIQUE',
                   MODELISATION = 'D_PLAN'
                     ),
                 _F(
                   MAILLE = 'CUBEDEG',
                   PHENOMENE = 'MECANIQUE',
                   MODELISATION = 'PLAN_JOINT'
                   )
                 )
              )


MAT=DEFI_MATERIAU(

     ELAS=_F(
              E =  0.5,
              NU = 0.0),
     RUPT_FRAG = _F(
                     GC = 0.9,
                     SIGM_C = 1.1,
                     PENA_ADHERENCE = 0.001
                    )
            )


CM=AFFE_MATERIAU(

               MAILLAGE=MA,
               AFFE=_F(
                       TOUT = 'OUI',
                       MATER = MAT),
              )

LIAISON=AFFE_CHAR_MECA(
  MODELE=MO,
  FACE_IMPO= (
    _F(MAILLE='GAUCHE2', DX=0, DY=0),
    )
  )

# CHARGEMENT MODE I PUR
TRAC_1=AFFE_CHAR_MECA(
  MODELE=MO,
  FACE_IMPO= (
    _F(MAILLE='DROITE', DX=2.16506350946110, DY= 1.250), 
    )
  )

# CHARGEMENT MODE II PUR
TRAC_2=AFFE_CHAR_MECA(
  MODELE=MO,
  FACE_IMPO= (
     _F(MAILLE='HAUT', DX=-1.250, DY= 2.16506350946110),   
     _F(MAILLE='BAS',  DX=-1.250, DY= 2.16506350946110),  
    )
  )

L_INST=DEFI_LIST_REEL(
                     DEBUT   = -1.,
     INTERVALLE=(
      _F( JUSQU_A = 0,       NOMBRE = 1 ),
      _F( JUSQU_A = 0.0001,  NOMBRE = 1 ),
      _F( JUSQU_A = 1.7,     NOMBRE = 17 ),
      )
     )

L_INST2=DEFI_LIST_REEL(

                     DEBUT   = 0.0001,
     INTERVALLE=(
      _F( JUSQU_A = 1.7,     NOMBRE = 17 ),
      )
     )


FCT = DEFI_FONCTION(
  NOM_PARA = 'INST',
  VALE     = (-1, 0, 0, 1.E-8),
  PROL_DROITE = 'LINEAIRE'
  )

#----------------------------------------------------------------------------------  
# CALCUL AVEC CHARGEMENT EN MODE I
#-----------------------------------------------------------------------------------

U_1=STAT_NON_LINE(
  SOLVEUR=_F(METHODE='MUMPS',PARALLELISME='DISTRIBUE_MD'),
  MODELE     = MO,
  CHAM_MATER = CM,
  EXCIT      = (
    _F(CHARGE = LIAISON , ),
    _F(CHARGE = TRAC_1, FONC_MULT = FCT),
  ),
  COMP_INCR  = (
          _F(RELATION = 'CZM_LIN_REG',
             MAILLE = 'CUBEDEG', ),
          _F(RELATION = 'ELAS',
             MAILLE = 'CUBE',),
          ),
  INCREMENT  = _F(LIST_INST = L_INST, INST_FIN = 0.),
  CONVERGENCE= _F(ITER_GLOB_MAXI = 20),
  NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
  )

STAT_NON_LINE(reuse = U_1,
  SOLVEUR=_F(METHODE='MUMPS',PARALLELISME='DISTRIBUE_MD'),
  ETAT_INIT  = _F(EVOL_NOLI = U_1),
  MODELE     = MO,
  CHAM_MATER = CM,
  EXCIT      = (
    _F(CHARGE = LIAISON ),
    _F(CHARGE = TRAC_1 , TYPE_CHARGE = 'FIXE_PILO'),
  ),
  PILOTAGE   = _F(
    SELECTION = 'RESIDU',
    TYPE = 'PRED_ELAS',
    MAILLE = 'CUBEDEG',
    COEF_MULT = 1,
    ETA_PILO_R_MIN = 0.,
    ),
  COMP_INCR  = (
          _F(RELATION = 'CZM_LIN_REG',
             MAILLE = 'CUBEDEG', ),
          _F(RELATION = 'ELAS',
             GROUP_MA = 'G_CUBE',),
          ),
  INCREMENT  = _F(LIST_INST = L_INST),
  CONVERGENCE= _F(ITER_GLOB_MAXI = 5),
  NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
  ARCHIVAGE  = _F(LIST_INST = L_INST2)
  )

#----------------------------------------------------------------------------------  
# CALCUL AVEC CHARGEMENT EN MODE II
#-----------------------------------------------------------------------------------

U_2=STAT_NON_LINE(
  SOLVEUR=_F(METHODE='MUMPS',PARALLELISME='DISTRIBUE_MD'),
  MODELE     = MO,
  CHAM_MATER = CM,
  EXCIT      = (
    _F(CHARGE = LIAISON , ),
    _F(CHARGE = TRAC_2, FONC_MULT = FCT),
  ),
  COMP_INCR  = (
          _F(RELATION = 'CZM_LIN_REG',
             MAILLE = 'CUBEDEG', ),
          _F(RELATION = 'ELAS',
             MAILLE = 'CUBE',),
          ),
  INCREMENT  = _F(LIST_INST = L_INST, INST_FIN = 0.),
  CONVERGENCE= _F(ITER_GLOB_MAXI = 20),
  NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
  )

STAT_NON_LINE(reuse = U_2,
  SOLVEUR=_F(METHODE='MUMPS',PARALLELISME='DISTRIBUE_MD'),
  ETAT_INIT  = _F(EVOL_NOLI = U_2),
  MODELE     = MO,
  CHAM_MATER = CM,
  EXCIT      = (
    _F(CHARGE = LIAISON  , ),
    _F(CHARGE = TRAC_2 , TYPE_CHARGE = 'FIXE_PILO'),
  ),
  PILOTAGE   = _F(
    SELECTION = 'RESIDU',
    TYPE = 'PRED_ELAS',
    MAILLE = 'CUBEDEG',
    COEF_MULT = 1,
    ETA_PILO_R_MIN = 0.,
    ),
  COMP_INCR  = (
          _F(RELATION = 'CZM_LIN_REG',
             MAILLE = 'CUBEDEG', ),
          _F(RELATION = 'ELAS',
             GROUP_MA = 'G_CUBE',),
          ),
  INCREMENT  = _F(LIST_INST = L_INST),
  CONVERGENCE= _F(ITER_GLOB_MAXI = 5),
  NEWTON     = _F(MATRICE = 'TANGENTE', REAC_ITER=1),
  ARCHIVAGE  = _F(LIST_INST = L_INST2)
  )



