PRESSION=0.

EP=0.077

RMOY=0.3955

RMOYN=-RMOY

REXT=RMOY + (EP/2.)

RC=1.25

YOUNG=2.E11

POISSON=0.3

SIGY=200.E6

H=2.E10

MLSURMLS=0.8

RINT=RMOY - (EP/2.)

LAMBDA=EP*RC/RMOY/RMOY

SURF=pi* (REXT*REXT-RINT*RINT)

IY=pi*(REXT**4-RINT**4)/4.

IZ=IY

IX=IY + IZ

MEDR=SIGY*IY/REXT

MPDR=4.0*MEDR/pi

MPX=SIGY*IX/REXT

GAMMA=RC/RMOY

STHETA1=1.89/(LAMBDA**(0.666667))

STHETA2=0.96/(LAMBDA**(0.333333))/GAMMA

STHETA=STHETA1 - STHETA2

