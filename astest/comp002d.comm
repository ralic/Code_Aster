from Utilitai.veri_matr_tang import VERI_MATR_TANG

DEBUT(CODE=_F(NOM='COMP002D',NIV_PUB_WEB='INTERNET',VISU_EFICAS='NON'),IMPR_MACRO='OUI');

#parametres vitesse de sollicitation
vitesse = 1.e-5

t_0 = 5.e-2/(8.0*vitesse)

# liste d'archivage
temps_ar=DEFI_LIST_REEL( VALE =[t_0*i for i in range(9)],)



#parmetres elastiques
YOUNG = 200000.0
POISSON = 0.3

#parametres viscosite

S=0.0  
N=12.0 
UN_SUR_M=(1.0/9.0)  
UN_SUR_K= (1.0/2110.0)
 
#parametres endommagement

R_D=6.3  
A_D=3191.0


# def des materiaux
ACIER = [None]*3
#unite en Pa
C_Pa = 1.e+6
YOUNG_Pa = YOUNG * C_Pa
UN_SUR_K_Pa = UN_SUR_K * (1.0/C_Pa);
A_D_Pa  = A_D*C_Pa

ACIER[0]=DEFI_MATERIAU(ELAS=_F(E=YOUNG_Pa,
                               NU=POISSON,
                                              ALPHA=11.8e-6),
                    LEMAITRE=_F( N  = N,
                                 UN_SUR_M  = UN_SUR_M,
                                 UN_SUR_K  = UN_SUR_K_Pa, 
                    ),
                    VISC_ENDO=_F(SY  = S,
                                 R_D   = R_D,   
                                 A_D   = A_D_Pa,   
                                 ),);

#unite en MPa
ACIER[1]=DEFI_MATERIAU(ELAS=_F(E=YOUNG,
                               NU=POISSON,
                                              ALPHA=11.8e-6),
                    LEMAITRE=_F( N  = N,
                                 UN_SUR_M  = UN_SUR_M,
                                 UN_SUR_K  = UN_SUR_K, 
                    ),
                    VISC_ENDO=_F(SY  = S,
                                 R_D   = R_D,   
                                 A_D   = A_D,   
                                 ),);
                                 

compor='VISC_ENDO_LEMA'

INCLUDE(UNITE=92)

#Discretisation du calcul
Ncal = 7
N_pas = 4*[1] + [1, 5, 25, 125, 625, 3125]
N_cas = len(N_pas)

label_cal=['_Pa_','_Th_','_sym_','_rot_'] + 6* ['_N']
prec_ecart=4*[1.E-6] + [1.E-1] + (N_cas-5)*[3.E-2]   
prec_tgt=8.e-2

ch_param=['V7','VMIS','TRACE','SIXX','SIYY','SIZZ','SIXY','SIXZ','SIYZ']
ch_param2=['V7','VMIS','TRACE']

RESU=[None]*Ncal
R_SI=[None]*len(ch_param)
TMP_S=[None]

N_cas = len(N_pas)
#pointeur materiau
P_imat = [0] + [1] + (N_cas-2)*[1]
#pointeur deformation
P_idef = 2*[0] + [1] + [2] + (N_cas-4)*[0]
#pointeur Option thermique
P_opt_ther = ["NON", "OUI"] + (N_cas-2)*["NON"]
# conditions du calcul thermique
(inst_th, temp_th) = (3.0*t_0, 100.)

#Boucle sur l'ensemble des calculs
for i in range(Ncal):
        N = N_pas[i]
        imat = P_imat[i]
        idef = P_idef[i]
        eps_imp = V_EPS[idef]
        opt_ther = P_opt_ther[i]
        temps=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=t_0,NOMBRE=N,),
                                 _F(JUSQU_A=2.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=3.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=4.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=5.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=6.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=7.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=8.0*t_0,NOMBRE=N,),),);

#Resout le pb a deformation imposee
        RESU[i]=SIMU_POINT_MAT(
                   COMP_INCR=_F(RELATION=compor,ITER_INTE_MAXI=100,ALGO_INTE='SECANTE'),
                   NEWTON=_F(MATRICE='TANGENTE',
                             REAC_ITER=3,),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=10,),
                   MATER      = ACIER[imat],  
                   ARCHIVAGE = _F(LIST_INST = temps_ar),
                   INCREMENT=_F(LIST_INST=temps,),
                   EPSI_IMPOSE=_F(EPXX=eps_imp[0], EPYY=eps_imp[1], EPZZ=eps_imp[2],
                                  EPXY=eps_imp[3], EPXZ=eps_imp[4], EPYZ=eps_imp[5]),
                   INFO=1 );
                        
        IMPR_TABLE(TABLE=RESU[i],UNITE=8)
        
        R_SI=RENOMME(i,N_pas,label_cal,ch_param,RESU,R_SI)

        DETRUIRE ( CONCEPT = _F (NOM = temps,),)
        
# TEST_RESU sur les erreurs relatives 
prec_zero=[1.E-10, 0.01, 0.01]

TEST_ECART(ch_param2,label_cal,N_pas,Ncal,ch_param,R_SI,prec_ecart,prec_zero)
                
for i in range(Ncal):
        IMPR_TABLE(TABLE=R_SI[i])
        




# Test de la matrice tangente sur le calcul le plus fin

i=Ncal-1
N = N_pas[i]                 
imat = P_imat[i]             
idef = P_idef[i]             
eps_imp = V_EPS[idef]   
opt_ther = P_opt_ther[i]     
Linst=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=t_0,NOMBRE=N,),
                                 _F(JUSQU_A=2.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=3.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=4.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=5.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=6.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=7.0*t_0,NOMBRE=N,),
                                 _F(JUSQU_A=8.0*t_0,NOMBRE=N,),),);

RESU_TGT=SIMU_POINT_MAT(
                   COMP_INCR=_F(RELATION=compor, ITER_INTE_MAXI=100,               
                                TYPE_MATR_TANG='VERIFICATION',ALGO_INTE='SECANTE',
                                VALE_PERT_RELA=1.E-5,
                               ),
                   NEWTON=_F(MATRICE='TANGENTE',REAC_ITER=1,),
                   CONVERGENCE=_F(ITER_GLOB_MAXI=10,),
                   MATER      = ACIER[1],  
                   INCREMENT=_F(LIST_INST=Linst,),
                   EPSI_IMPOSE=_F(EPXX=eps_imp[0], EPYY=eps_imp[1], EPZZ=eps_imp[2],
                                  EPXY=eps_imp[3], EPXZ=eps_imp[4], EPYZ=eps_imp[5]),
                        );

DIFF_MAT=VERI_MATR_TANG()

IMPR_TABLE(TABLE=DIFF_MAT)

TEST_TABLE(TABLE=DIFF_MAT,
           NOM_PARA='MAT_DIFF',
           TYPE_TEST='MAX',
           VALE=0.,
           CRITERE='ABSOLU',
           PRECISION=prec_tgt,
           REFERENCE='ANALYTIQUE',);     


# TESTS DE NON REGRESSION DES QUANTITES SIGNIFICATIVES
TEST_TABLE(TABLE=R_SI[0],
           NOM_PARA='V7_Pa_1',VALE=4.69818E-02,
           FILTRE=_F(NOM_PARA='INST',VALE=8.0*t_0),
           REFERENCE='NON_REGRESSION',);     

TEST_TABLE(TABLE=R_SI[1],
           NOM_PARA='VMIS_Pa_1',VALE=5.01192E+08,
           FILTRE=_F(NOM_PARA='INST',VALE=8.0*t_0),
           REFERENCE='NON_REGRESSION',);     

TEST_TABLE(TABLE=R_SI[3],
           NOM_PARA='SIXX_Pa_1',VALE=1.09459E+08,
           FILTRE=_F(NOM_PARA='INST',VALE=8.0*t_0),
           REFERENCE='NON_REGRESSION',);     


FIN()
