#  MODELISATION 'ACOUSTIQUE' AVEC ELEMENTS PENTA15 ET FACE6
#  GUIDE D'ONDE ACOUSTIQUE EN E.F. CLASSIQUES
# VALIDATION INDEPENDANTE PAR LMS_NT (J.P.ROSSION)
#
DEBUT(CODE=_F( NOM = 'AHLV100H',NIV_PUB_WEB='INTERNET'))

PRE_IDEAS()

#
MA=LIRE_MAILLAGE()

MO=AFFE_MODELE(
         MAILLAGE=MA, 
         AFFE=_F( TOUT = 'OUI', 
              PHENOMENE = 'ACOUSTIQUE', 
              MODELISATION = '3D')
         )

#
# PROPRIETES MECANIQUES
#
AIR=DEFI_MATERIAU(
          FLUIDE=_F( RHO = 1.3,
                CELE_C = ('RI',343.0,0.,))
         )

CHM=AFFE_MATERIAU(
         MAILLAGE=MA, 
         AFFE=_F( TOUT = 'OUI',
              MATER = AIR)
         )

#
# CONDITIONS AUX LIMITES
#
VELO=AFFE_CHAR_ACOU(
         MODELE=MO,
         VITE_FACE=_F( GROUP_MA = 'VELO',
                   VNOR = ('RI',0.014,0.0,))
         )

IMPE=AFFE_CHAR_ACOU(
         MODELE=MO,
         IMPE_FACE=_F( GROUP_MA = 'IMPE',
                   IMPE = ('RI',445.9,0.0,))
         )

#
# ASSEMBLAGE
#
MACRO_MATR_ASSE(
     MODELE=MO,
     CHARGE=IMPE,
     CHAM_MATER=CHM,
     NUME_DDL=CO("NDDL"),MATR_ASSE=( 
     _F(  MATRICE = CO("MATK"), OPTION = 'RIGI_ACOU'), 
               _F(  MATRICE = CO("MATM"), OPTION = 'MASS_ACOU'), 
               _F(  MATRICE = CO("MATA"), OPTION = 'AMOR_ACOU')) 
                )

VECEL=CALC_VECT_ELEM(
         OPTION='CHAR_ACOU',
         CHAM_MATER=CHM,
         CHARGE=VELO
        )

VECAS=ASSE_VECTEUR(
         VECT_ELEM=VECEL,
         NUME_DDL=NDDL
        )

#
#  REPONSE HARMONIQUE
#
HARMO=DYNA_LINE_HARM( 
         MATR_MASS=MATM,
         MATR_RIGI=MATK,
         MATR_AMOR=MATA,
         FREQ=500.0,
          EXCIT=_F( VECT_ASSE = VECAS,
               COEF_MULT = -1.0,
               PHAS_DEG = 90.0,
               PUIS_PULS = 1)
         )

TEST_RESU(RESU=(
          _F( RESULTAT = HARMO, NOM_CHAM = 'PRES', NUME_ORDRE = 1,
           NOEUD = 'NO28', NOM_CMP = 'PRES', VALE_C = ('RI',-6.2426,0.,)),
          _F( RESULTAT = HARMO, NOM_CHAM = 'PRES', NUME_ORDRE = 1,
           NOEUD = 'NO64', NOM_CMP = 'PRES', VALE_C = ('RI',-6.2426,0.,)),
          _F( RESULTAT = HARMO, NOM_CHAM = 'PRES', NUME_ORDRE = 1,
           NOEUD = 'NO24', NOM_CMP = 'PRES', VALE_C = ('RI',6.0237,1.6387,)),
          _F( RESULTAT = HARMO, NOM_CHAM = 'PRES', NUME_ORDRE = 1,
           NOEUD = 'NO720', NOM_CMP = 'PRES', VALE_C = ('RI',6.0237,1.6387,))) 
          )

HARMO=CALC_ELEM( reuse=HARMO, MODELE=MO, 
                     CHAM_MATER=CHM,
                     TOUT_ORDRE='OUI',
                     RESULTAT=HARMO,
                     OPTION='INTE_ELNO_ACTI'
         )

TEST_RESU(RESU=(
          _F( RESULTAT = HARMO, NOM_CHAM = 'INTE_ELNO_ACTI', NUME_ORDRE = 01,
           MAILLE = 'MA1', NOEUD = 'NO28', NOM_CMP = 'INTX', VALE = 0.04370,
           PRECISION = 3.E-2),  
          _F( RESULTAT = HARMO, NOM_CHAM = 'INTE_ELNO_ACTI', NUME_ORDRE = 01,
           MAILLE = 'MA2', NOEUD = 'NO64', NOM_CMP = 'INTX', VALE = 0.04370,
           PRECISION = 3.E-2),  
          _F( RESULTAT = HARMO, NOM_CHAM = 'INTE_ELNO_ACTI', NUME_ORDRE = 01,
           MAILLE = 'MA85', NOEUD = 'NO24', NOM_CMP = 'INTX', VALE = 0.04370,
           PRECISION = 3.E-2),  
          _F( RESULTAT = HARMO, NOM_CHAM = 'INTE_ELNO_ACTI', NUME_ORDRE = 01,
           MAILLE = 'MA86', NOEUD = 'NO720', NOM_CMP = 'INTX', VALE = 0.04370,
           PRECISION = 3.E-2))  
          )

#
# CALCUL DES MODES ACOUSTIQUES
#
MATKR=COMB_MATR_ASSE( 
                       COMB_R=_F( MATR_ASSE = MATK,
                        PARTIE = 'REEL',
                        COEF_R = 1.)
                       )

MATMR=COMB_MATR_ASSE( 
                       COMB_R=_F( MATR_ASSE = MATM,
                        PARTIE = 'REEL',
                        COEF_R = 1.)
                       )

MODES=MODE_ITER_SIMULT(  MATR_A=MATKR,
                           MATR_B=MATMR,
                           CALC_FREQ=_F( OPTION = 'BANDE',
                                      FREQ = (1., 1000.,))
                         )

TEST_RESU(
          RESU=_F( RESULTAT = MODES,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 1.E-4,
           NUME_ORDRE = 1,
           VALE = 171.5)
          )

TEST_RESU(
          RESU=_F( RESULTAT = MODES,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 1.E-4,
           NUME_ORDRE = 2,
           VALE = 343.0)
          )

TEST_RESU(
          RESU=_F( RESULTAT = MODES,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 0.001,
           NUME_ORDRE = 3,
           VALE = 514.5)
          )

TEST_RESU(
          RESU=_F( RESULTAT = MODES,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 0.001,
           NUME_ORDRE = 4,
           VALE = 686.0)
          )

TEST_RESU(
          RESU=_F( RESULTAT = MODES,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 0.001,
           NUME_ORDRE = 5,
           VALE = 857.5)
          )

TEST_RESU(
          RESU=_F( RESULTAT = MODES,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 0.005,
           NUME_ORDRE = 6,
           VALE = 857.5)
          )

TEST_RESU(
          RESU=_F( RESULTAT = MODES,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 0.005,
           NUME_ORDRE = 7,
           VALE = 874.482)
          )

TEST_RESU(
          RESU=_F( RESULTAT = MODES,
           PARA = 'FREQ',
           CRITERE = 'RELATIF',
           PRECISION = 0.005,
           NUME_ORDRE = 8,
           VALE = 923.556)
          )

FIN()
#
