                           FORMULE_VIE = FVIE[i],
                           INST_INIT_CYCL = 1.0,
                           INST_CRIT = 'RELATIF',
                         )



    for j in range(6) :
        crit2 = 'RELATIF' if RESU[j + 6*i] != 0. else 'ABSOLU'
        TEST_RESU(CHAM_NO=_F(NOEUD='N1',
