for i in range(NBPR) :
    CRIT[i] = FORMULE(
           NOM_PARA=(NPAR[i]),
           VALE = NCRI[i]
           )

    FATN[i]=CALC_FATIGUE( TYPE_CALCUL   = 'FATIGUE_MULTI',
