SCALE_FX = FORMULE( NOM_PARA = ('DX'), VALE = 'DX*50./1000')
SCALE_FY = FORMULE( NOM_PARA = ('DY'), VALE = 'DY*50./1000')

T_F = CALC_TABLE( TABLE = T_F,
		  reuse = T_F,
                  ACTION = _F( OPERATION = 'OPER',
