DEBUT(CODE=_F(NOM='SSNV182D', NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

PRE_GIBI();

MAILLAG1=LIRE_MAILLAGE(INFO=1,);

MAILLAG2=CREA_MAILLAGE(MAILLAGE=MAILLAG1,
                        LINE_QUAD=_F(GROUP_MA='SURF',),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO=_F(NOM='SURFQUAD',
                                     GROUP_MA='SURF',),);

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('SURF',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN_XFEM_CONT',),
                           _F(GROUP_MA=('LIG1','LIG3',),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),),);


LN=FORMULE(NOM_PARA=('X','Y'),VALE='Y-10');
LT=FORMULE(NOM_PARA=('X','Y'),VALE='-X-1. ');

FISS=DEFI_FISS_XFEM(MODELE=MODELEIN,
                    DEFI_FISS=_F(FONC_LT=LT,FONC_LN=LN,),
                    GROUP_MA_ENRI='SURF',
                    INFO=1,);

MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=FISS,
                         CONTACT='OUI');

CTXFEM = AFFE_CHAR_MECA( CONTACT=_F( FISS_MAIT=FISS,
                                     METHODE='XFEM',
                                    ITER_CONT_MAXI=30,
                                 COEF_REGU_CONT=100.0,
                                 INTEGRATION='FPG4',
                                 FROTTEMENT='SANS',
                                 GLISSIERE='NON',
                                 CONTACT_INIT='OUI',
                                 COEF_ECHELLE=1.0,
                                 ALGO_LAGR='NON',
                                    ),
                          MODELE=MODELEK,
                          ),


                         
                         
                         

E=100.0E6
nu=0.
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(GROUP_MA=('SURF','LIG1','LIG3',),
                                MATER=ACIER,
                                ),);

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='LIG1',
                                          DX=0.0,
                                          DY=0.0,),
                                       _F(GROUP_MA='LIG3',
                                          DX=0.0,
                                          DY=-1.E-6,),),
                    INFO=1,);

CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      LIAISON_XFEM='OUI',
                      CONTACT_XFEM=CTXFEM,);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=1,),);

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                   CHAM_MATER=CHAMPMAT,
                   EXCIT=(_F(CHARGE=CHXFEM,),
                          _F(CHARGE=CTXFEM,),                       
                          _F(CHARGE=CH1,),),
                   COMP_ELAS=_F(RELATION='ELAS',
                                GROUP_MA='SURF',),
                   INCREMENT=_F(LIST_INST=L_INST,
                                INST_FIN=1.0,),
#                   CONVERGENCE=(_F(ARRET='OUI',
#                                  RESI_GLOB_RELA=1E-9)),
                   SOLVEUR=_F(METHODE='MUMPS',
                              ),
                   NEWTON=_F(REAC_ITER=10,),
                   ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA',),
                   INFO=1,);


LAG=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',

                           # maillage 5_20_20
                             NOEUD=('N33','N71','N252','N253','N254',
                                    'N255','N256','N257','N258',
                                    'N259','N260','N261','N262',
                                    'N263','N264','N265','N266','N267',
                                    'N268','N269','N270',),
#                             GROUP_NO='SURFQUAD',
                             RESULTAT=UTOT1,
                             NOM_CHAM='DEPL',
                             NUME_ORDRE=1,
                             NOM_CMP=('LAGS_C','LAGS_F1',),
                             OPERATION='EXTRACTION',),);



IMPR_TABLE(TABLE=LAG,
           FORMAT='AGRAF',
           FORMAT_R='1PE12.5',
           FORMAT_C='MODULE_PHASE',
           IMPR_FONCTION='NON',);

LAREF=-5.
LA1REF=0.
LA2REF=0.

# TESTS
TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MAX',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_C',
           TYPE_TEST='MIN',
           VALE=LAREF,
           CRITERE='RELATIF',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MAX',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);

TEST_TABLE(TABLE=LAG,
           NOM_PARA='LAGS_F1',
           TYPE_TEST='MIN',
           VALE=LA1REF,
           CRITERE='ABSOLU',
           PRECISION=1.E-12,
           REFERENCE='ANALYTIQUE',);


# -------------------------------------------------------------   
#            POST-TRAITEMENT POUR LA VISUALISATION X-FEM   
# -------------------------------------------------------------   

MA_XFEM=POST_MAIL_XFEM(MAILLAGE_SAIN = MAILLAG1,
                       MODELE        = MODELEK,
                       INFO          = 2)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                              PHENOMENE='MECANIQUE',
                              MODELISATION='C_PLAN',),) 

RES_XFEM=POST_CHAM_XFEM(MAILLAGE_SAIN = MAILLAG1,
                        MAILLAGE_FISS = MA_XFEM,
                        MODELE        = MODELEK,
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        NOM_CHAM      = ('DEPL','SIEF_ELGA'),
                        INFO          = 2)
 
IMPR_RESU(FORMAT='GMSH',UNITE=37,RESU=_F(RESULTAT=RES_XFEM,
                                            NOM_CHAM='DEPL',
                                            TYPE_CHAM='VECT_3D',
                                            NOM_CMP=('DX','DY','DZ')),);

TEST_RESU(RESU=(_F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DX',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'ABSOLU',
                     VALE = 0.0,
                     PRECISION=1.E-8,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '8.2.13'),
                _F(  RESULTAT=RES_XFEM,NUME_ORDRE=1,
                     NOM_CHAM='DEPL',NOM_CMP='DY',
                     TYPE_TEST='SOMM_ABS',
                     CRITERE = 'RELATIF',
                     VALE = 4.10E-04,
                     PRECISION=1.E-6,
                     REFERENCE = 'NON_REGRESSION',
                     VERSION = '9.0.21'),
                ),
         );


FIN();

