# interpollation polynomiale (MS Excel) de la reference Murakami : G(s)
def G_Murakami(x):
    a4 =  6.283263E+02
    a3 = -6.566832E+02
    a2 =  2.494929E+02
    a1 = -3.818541E+01
    a0 =  8.632910E+00
    return a4*x**4.+a3*x**3.+a2*x**2.+a1*x**1.+a0*x**0.
s_deb = 0.
s_fin = 0.354497665
npt_s = 100
s_Mur = [s_deb + ipt*(s_fin-s_deb)/npt_s for ipt in xrange(npt_s+1)]
G_Mur = [G_Murakami(s) for s in s_Mur]

