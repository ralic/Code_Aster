# CONTACT-FROTTANT EN GRANDS GLISSEMENTS
#  AVEC XFEM POUR DES FISSURES HORIZONTALES
#

DEBUT(CODE=_F(NOM='SSNP503L',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

MAILLAG2=LIRE_MAILLAGE(INFO=1,FORMAT='MED');
Y=2

MODELEIN=AFFE_MODELE(MAILLAGE=MAILLAG2,
                     AFFE=(_F(GROUP_MA=('VOLU'),
                              PHENOMENE='MECANIQUE',
                              MODELISATION='3D')));       

LN1=FORMULE(NOM_PARA=('X','Y','Z'),VALE='Y-2.5');

LN2=FORMULE(NOM_PARA=('X','Y','Z'),VALE='-Y+6.5');

FISS1=DEFI_FISS_XFEM(MODELE=MODELEIN,
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN1),
                     INFO=1);

FISS2=DEFI_FISS_XFEM(MODELE=MODELEIN,  
                     TYPE_DISCONTINUITE='INTERFACE',
                     DEFI_FISS=_F(FONC_LN=LN2));
                    
MODELEK=MODI_MODELE_XFEM(MODELE_IN=MODELEIN,
                         FISSURE=(FISS1,FISS2),
                         CONTACT='P1P1',
                         INFO=1);
                         
CTXFEM = DEFI_CONTACT(MODELE         = MODELEK,
                      FORMULATION    = 'XFEM',
                      REAC_GEOM      = 'CONTROLE',
                      NB_ITER_GEOM   = 5,
                      ITER_FROT_MAXI = 10,
                      ITER_CONT_MAXI = 4,
                      FROTTEMENT     = 'COULOMB',
                      ZONE = (_F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 TOLE_PROJ_EXT = 0.1,   
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'OUI',   
                                 FISS_MAIT     = FISS1,
                                 ALGO_LAGR     = 'VERSION2',
                              ),
                             _F(
                                 INTEGRATION   = 'NOEUD',
                                 COULOMB       = 0.5,
                                 TOLE_PROJ_EXT = 0.1,   
                                 CONTACT_INIT  = 'OUI',
                                 GLISSIERE     = 'OUI',   
                                 FISS_MAIT     = FISS2,
                                 ALGO_LAGR     = 'VERSION2',
                              ),
                            )
                    );
                          
E=100.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0));

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELEK,
                       AFFE=_F(TOUT='OUI',
                                MATER=ACIER));

DEPX=2.0
DEPY=1.E-3
DEPZ=0.0

CH1=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SINF',DY=DEPY,DZ=0.0,DX=0.0),
                             _F(GROUP_MA='SSUP',DY=-DEPY,DZ=0.0,DX=0.0)),
                   INFO=1);
                    
CH2=AFFE_CHAR_MECA(MODELE=MODELEK,
                   DDL_IMPO=(_F(GROUP_MA='SCOTE',
                               DX=DEPX,DZ=DEPZ)),
                   # CONDITIONS IMPOSEES A LA MAIN
                   # POUR LES MAILLES XFEM INFERIEURE
                   # ET SUPERIEURE
                    LIAISON_DDL=(_F(NOEUD=('N444','N444'),
                                    DDL=('DX','H1X'),
                                    COEF_MULT=(1,1),
                                    COEF_IMPO=DEPX),
                                 _F(NOEUD=('N1070','N1070'),
                                    DDL=('DX','H1X'),
                                    COEF_MULT=(1,1),
                                    COEF_IMPO=DEPX),
                                 _F(NOEUD=('N121','N121'),
                                    DDL=('DX','H1X'),
                                    COEF_MULT=(1,1),
                                    COEF_IMPO=DEPX),
                                 _F(NOEUD=('N750','N750'),
                                    DDL=('DX','H1X'),
                                    COEF_MULT=(1,1),
                                    COEF_IMPO=DEPX),
                                 _F(NOEUD=('N444','N444'),
                                    DDL=('DZ','H1Z'),
                                    COEF_MULT=(1,1),
                                    COEF_IMPO=DEPZ),
                                 _F(NOEUD=('N1070','N1070'),
                                    DDL=('DZ','H1Z'),
                                    COEF_MULT=(1,1),
                                    COEF_IMPO=DEPZ),
                                 _F(NOEUD=('N121','N121'),
                                    DDL=('DZ','H1Z'),
                                    COEF_MULT=(1,1),
                                    COEF_IMPO=DEPZ),
                                 _F(NOEUD=('N750','N750'),
                                    DDL=('DZ','H1Z'),
                                    COEF_MULT=(1,1),
                                    COEF_IMPO=DEPZ),
                                 ),
                   INFO=1);

VAR1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );
                    
VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                    );   
CHXFEM=AFFE_CHAR_MECA(MODELE=MODELEK,
                      CONTACT_XFEM=CTXFEM,
                      LIAISON_XFEM='OUI',
                      INFO=1);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4));

UTOT1=STAT_NON_LINE(MODELE=MODELEK,
                    CHAM_MATER=CHAMPMAT,  
                    CONTACT = CTXFEM,
                    EXCIT=(
                           _F(CHARGE=CHXFEM),                                    
                           _F(CHARGE=CH1,FONC_MULT=VAR1),
                           _F(CHARGE=CH2,FONC_MULT=VAR2)),  
                    COMP_ELAS=_F(RELATION='ELAS',
                                 GROUP_MA='VOLU'),
                    INCREMENT=_F(LIST_INST=L_INST),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=20,
                                    RESI_GLOB_MAXI=1E-06
                                    )),
                    NEWTON=_F(REAC_ITER=1),
                    SOLVEUR=_F(METHODE='MUMPS',
   
                               NPREC=-1),
                    ARCHIVAGE=_F(CHAM_EXCLU='VARI_ELGA'),
                    INFO=1);

MA_XFEM=POST_MAIL_XFEM(
                       MODELE        = MODELEK)

MOD_VISU=AFFE_MODELE(MAILLAGE=MA_XFEM,
                     AFFE=_F(TOUT='OUI',
                             PHENOMENE='MECANIQUE',
                             MODELISATION='3D'))

RES_XFEM=POST_CHAM_XFEM(
                        
                        MODELE_VISU   = MOD_VISU,
                        RESULTAT      = UTOT1,
                        )

#IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=RES_XFEM,
#                                 NOM_CHAM='DEPL',
#                                 TYPE_CHAM='VECT_3D',
#                                 NOM_CMP=('DX','DY','DZ')))),

LAG=[None]*5
inst=[0.0,0.25,0.5,0.75,1.0]

vale = [[0.0,-7.2384991E+03,-6.4285635E+03,-5.4975876E+03,-9.8231384E+02],
        [0.0,-2.6362672E+04,-1.2791423E+04,-1.1098467E+04,-1.0131368E+03],
        [0.0,0.0,-1.9277375E+04,-1.6785053E+04,-1.5709230E+03],
        [0.0,0.0,-4.4286909E+04,-2.2460135E+04,-3.1664811E+03]]


noeud= ['NM33','NM394','NM842','NM1290','NM1738']
for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                   GROUP_NO=('NFISSU'),
                                   RESULTAT=RES_XFEM,
                                   NOM_CHAM='DEPL',
                                   INST=inst[i],
                                   NOM_CMP=('LAGS_C','LAGS_F1','LAGS_F2'),
                                   OPERATION='EXTRACTION'));

   LAG[i]=CALC_TABLE(reuse = LAG[i],
                     TABLE = LAG[i],
                     ACTION = (_F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'NOEUD',
                                  CRIT_COMP = 'REGEXP',
                                  VALE_K    = 'N[^P]'),
                               _F(OPERATION = 'FILTRE',
                                  NOM_PARA  = 'COOR_Y',
                                  CRIT_COMP = 'EQ',
                                  VALE      = 2.5)));

   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              FORMAT_C='MODULE_PHASE',
              NOM_PARA=('NOEUD','INST','COOR_X','COOR_Z','LAGS_C','LAGS_F1','LAGS_F2'),
              IMPR_FONCTION='NON',
              TRI=_F(NOM_PARA=('COOR_X'),
                     ORDRE='CROISSANT'));

   for j in [0,1,2,3]: #Boucle sur les positions testees
      if (vale[i-1] [j] == 0.0):
        precision = 1.0E-12
        critere = 'ABSOLU'
      else:
        precision = 20E-2
        critere = 'RELATIF'
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE=vale[i-1] [j],
                 CRITERE=critere,
                 PRECISION=precision,
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=(_F(NOM_PARA='COOR_X',
                           CRIT_COMP='EQ',
                           VALE=j,
                           PRECISION=0.03,
                           CRITERE='ABSOLU'),
                         _F(NOM_PARA='COOR_Z',
                           CRIT_COMP='EQ',
                           VALE=0.0,
                           PRECISION=0.01,
                           CRITERE='ABSOLU'),
                         _F(NOM_PARA='NOEUD',
                           CRIT_COMP='EQ',
                           VALE_K=noeud[j])));

FIN();                 







