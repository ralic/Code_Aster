# MODELISATION POU_D_T

DEBUT(CODE=_F(NOM='SDLL135B',
NIV_PUB_WEB='INTERNET',),DEBUG=_F(SDVERI='NON'))
# SDVERI='NON' car la verification est trop couteuse en CPU

MAIL=LIRE_MAILLAGE(FORMAT='MED',);

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='POU_D_T',),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=200000E6,
                            NU=0.29,
                            RHO=7830.0,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=_F(TOUT='OUI',
                            MATER=ACIER,),);

CARAC=AFFE_CARA_ELEM(MODELE=MODELE,
                     POUTRE=_F(GROUP_MA='TOUT',
                               SECTION='CERCLE',
                               CARA=('R','EP',),
                               VALE=(0.16,0.01,),),);

LISTE=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=_F(JUSQU_A=2.E-4,
                                   PAS=1e-07,),);

LISTE2=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=3.2E-4,
                                    PAS=1e-07,),);

CL=AFFE_CHAR_MECA(MODELE=MODELE,
                  DDL_IMPO=_F(GROUP_NO='A',
                              DX=0.0,
                              DY=0.0,
                              DZ=0.0,
                              DRX=0.0,
                              DRY=0.0,
                              DRZ=0.0,),);

# CHARGEMENT SOUS LA FORME D UNE FORCE ( OU D UN MOMENT ) DE TYPE HEAVYSIDE

CHARFX=AFFE_CHAR_MECA(MODELE=MODELE,
                      FORCE_NODALE=_F(GROUP_NO='B',
                                      FX=1.0,),);

CHARMX=AFFE_CHAR_MECA(MODELE=MODELE,
                      FORCE_NODALE=_F(GROUP_NO='B',
                                      MX=1.0,),);

MACRO_MATR_ASSE(MODELE=MODELE,
                CHAM_MATER=CHMAT,
                CARA_ELEM=CARAC,
                CHARGE=CL,
                NUME_DDL=CO('NUMEDDL'),
                MATR_ASSE=(_F(MATRICE=CO('MASSE'),
                              OPTION='MASS_MECA',),
                           _F(MATRICE=CO('RIGID'),
                              OPTION='RIGI_MECA',),),);

# LISTE DISCRETISE TRES FINEMENT POUR RESPECTER
# LE RAPPORT TAILLE DE MAILLE VITESSE DE L ONDE DE COMPRESSION

DYNATRAF=DYNA_VIBRA(TYPE_CALCUL='TRAN',BASE_CALCUL='PHYS',
                        MODELE=MODELE,
                        CHAM_MATER=CHMAT,
                        CARA_ELEM=CARAC,
                        MATR_MASS=MASSE,
                        MATR_RIGI=RIGID,
                        SCHEMA_TEMPS=_F(SCHEMA='NEWMARK',),
                        EXCIT=(_F(CHARGE=CHARFX,),
                               _F(CHARGE=CL,),),
                        INCREMENT=_F(LIST_INST=LISTE,),
                        ARCHIVAGE=_F(PAS_ARCH=100,),);

DYNATRAF=CALC_ELEM(reuse =DYNATRAF,



                   RESULTAT=DYNATRAF,
                   OPTION=('SIEF_ELGA','SIEF_ELNO',),);

DYNATRAF=CALC_CHAMP(reuse =DYNATRAF,
                    RESULTAT=DYNATRAF,
                    FORCE='REAC_NODA',
                    MODELE=MODELE,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CARAC,
                    EXCIT=(_F(CHARGE=CHARFX,),
                           _F(CHARGE=CL,),),);

# TEST DU CHAMP DE DEPLACEMENT SUR LE NOEUD EXTREMITE

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                  NUME_ORDRE=10,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  GROUP_NO='B',
                  VALE=2.59473227925e-10,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                  NUME_ORDRE=15,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  GROUP_NO='B',
                  VALE=3.89209841887e-10,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                  NUME_ORDRE=20,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DX',
                  GROUP_NO='B',
                  VALE=5.18946455848e-10,
                  REFERENCE='ANALYTIQUE',),);

# TEST SUR LES REACTIONS NODALES

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
                  NUME_ORDRE=10,
                  NOM_CHAM='REAC_NODA',
                  NOM_CMP='DX',
                  GROUP_NO='A',
                  VALE=0.0,
                  CRITERE='ABSOLU',
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
