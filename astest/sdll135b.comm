                      MATR_B=MASSE,
                      CALC_FREQ=_F(
                      OPTION='PROCHE',
                      FREQ=(269.932,1077.199,2270.705,3249.207,4649.212,),),);

# REMARQUE : LA COMPARAISON DES VALEURS DES FREQUENCES PAR RAPPORT
