MODE_DRX=MODE_ITER_INV(MATR_A=RIGID,
                       MATR_B=MASSE,
                       CALC_FREQ=_F(
                       OPTION='PROCHE',
                       FREQ=(786.619,2359.856,3933.094,5506.33,),),);

TEST_RESU(RESU=_F(RESULTAT=MODE_DRX,
                  NUME_ORDRE=1,
                  PARA='FREQ',
                  VALE=786.619,
                  CRITERE='RELATIF',
                  REFERENCE='ANALYTIQUE',),);

