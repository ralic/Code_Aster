                  NOM_CHAM='REAC_NODA',
                  NOM_CMP='DX',
                  GROUP_NO='A',
                  VALE=0.0,
                  CRITERE='ABSOLU',
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=DYNATRAF,
