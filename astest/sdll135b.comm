# LA RAISON EST LA VALEUR DU COEFFICIENT DE SECTION REDUITE DIFFERENT ENRE LA
# FORMULE ANALYTIQUE CHOISIE ET CODE_ASTER

TEST_RESU(RESU=_F(RESULTAT=MODE_FZ,
                  NUME_ORDRE=1,
                  PARA='FREQ',
                  VALE=269.932,
                  CRITERE='RELATIF',
                  PRECISION=0.01,
                  REFERENCE='ANALYTIQUE',),);

TEST_RESU(RESU=_F(RESULTAT=MODE_FZ,
                  NUME_ORDRE=2,
                  PARA='FREQ',
                  VALE=1077.199,
                  CRITERE='RELATIF',
                  PRECISION=0.02,
                  REFERENCE='ANALYTIQUE',),);

