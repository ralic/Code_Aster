                        GEOM_FIBRE=GF,
                        MULTIFIBRE=(_F( GROUP_MA=('POUTRE'),
                                        GROUP_FIBRE=('SBET',)
                                        ),
                                    )
                    );

COMPPMF=DEFI_COMPOR(    GEOM_FIBRE=GF,
                        MATER_SECT=BETON,
                        MULTIFIBRE=_F(  GROUP_FIBRE='SBET',
                                        MATER=BETON,
                                    ),
                    );

CHMAT=AFFE_MATERIAU(MAILLAGE=MAPOU,
                    AFFE=_F(GROUP_MA='POUTRE', MATER=BETON,),
                    AFFE_COMPOR=_F(GROUP_MA='POUTRE',COMPOR=COMPPMF),
                          );

BLOCAGE=AFFE_CHAR_MECA( MODELE=MOPOU,
                        DDL_IMPO=(_F(GROUP_NO='ENC', DX=0.0, DY=0.0, DZ=0.0, DRX=0.0, DRY=0.0, DRZ=0.0,),
                                 ),);
CHARGE=AFFE_CHAR_MECA(  MODELE=MOPOU,
                        FORCE_NODALE=(_F(GROUP_NO='CHA',FX = 100.0,FY = 100.0,FZ = 100.)
                                 ),);

FOFO=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,3.5,3.5),
                   PROL_DROITE='EXCLU',
                   PROL_GAUCHE='EXCLU',);


