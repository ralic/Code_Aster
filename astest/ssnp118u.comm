#IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=U_HM,NOM_CHAM='VARI_ELGA',TYPE_CHAM='VECT_2D',NOM_CMP=('V12','V13'),MAILLE = ('PRISMDEG'))))

#########################################################################################################
FIN()
