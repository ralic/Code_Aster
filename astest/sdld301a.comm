DEBUT(  CODE=_F(  NOM = 'SDLD301A',NIV_PUB_WEB='INTERNET') )

#

MAILLAGE=LIRE_MAILLAGE(  )

#

MODELE=AFFE_MODELE(    MAILLAGE=MAILLAGE,AFFE=(
                             
                                  _F(  GROUP_MA = ( 'RESK', 'RES2K',),
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = 'DIS_T'),            
                                  _F(  GROUP_NO = ( 'MASSE', ),
                                    PHENOMENE = 'MECANIQUE',
                                    MODELISATION = 'DIS_T'))            
                                   )

#
CARA=AFFE_CARA_ELEM(     MODELE=MODELE,DISCRET=(
                            _F(  GROUP_MA = 'RESK',
                                      CARA = 'K_T_D_L',
                                      VALE = ( 1.E5,  1., 1., )),
                            _F(  GROUP_MA = 'RES2K',
                                      CARA = 'K_T_D_L',
                                      VALE = ( 2.E5,  1., 1., )),
                            _F(  GROUP_NO = 'MASSE',
                                      CARA = 'M_T_D_N',
                                      VALE = ( 2533., )))
                        )

#

MAT=DEFI_MATERIAU(     ELAS=_F(  E = 1.6E+11,  
                                       RHO = 1.3E+04,
                                       NU = 0.3)
                           )

#

CHAMPMAT=AFFE_MATERIAU(    MAILLAGE=MAILLAGE,
                                   AFFE=_F(  TOUT = 'OUI',
                             MATER = MAT)
                           )

#

COND_LIM=AFFE_CHAR_MECA(     MODELE=MODELE,DDL_IMPO=(
                               _F(  NOEUD = ('P1', 'P4',),
                                          DX = 0.,
                                          DY = 0.,
                                          DZ = 0.),
                                        _F(  GROUP_NO = 'MASSE', 
                                          DY = 0.,
                                          DZ = 0.))
                            )

#

RIGIELEM=CALC_MATR_ELEM(      MODELE=MODELE,
                                 OPTION='RIGI_MECA',
                                 CHARGE=COND_LIM,
                              CARA_ELEM=CARA  
                           )

#

MASSELEM=CALC_MATR_ELEM(      MODELE=MODELE,
                                 OPTION='MASS_MECA',
                                 CHARGE=COND_LIM,
                              CARA_ELEM=CARA  
                           )

#

NUMEROTA=NUME_DDL(        MATR_RIGI=RIGIELEM  )

#

MATRRIGI=ASSE_MATRICE(  MATR_ELEM=RIGIELEM,  NUME_DDL=NUMEROTA  )

MATRMASS=ASSE_MATRICE(  MATR_ELEM=MASSELEM,  NUME_DDL=NUMEROTA  )

#

MAS=POST_ELEM(         MODELE=MODELE,
                        CARA_ELEM=CARA,
                      MASS_INER=_F(  TOUT = 'OUI')
                  )

#

MODES=MODE_ITER_SIMULT( 
                                MATR_A=MATRRIGI,
                                MATR_B=MATRMASS,
                               METHODE='SORENSEN',
                             CALC_FREQ=_F(  NMAX_FREQ = 2)
                          )

#
MODES=NORM_MODE(reuse=MODES, 
                          MODE=MODES,
                         NORME='TRAN',
                     MASS_INER=MAS
                  )

#
MODES=CALC_ELEM(reuse=MODES, 
                          MODELE=MODELE,
                        RESULTAT=MODES,        
                       CARA_ELEM=CARA,
                      CHAM_MATER=CHAMPMAT,
                          OPTION=( 'SIEF_ELGA_DEPL', 
                                    'EFGE_ELNO_DEPL',)
                  )

MODES=CALC_NO(  reuse=MODES, 
                        RESULTAT=MODES,   MODELE=MODELE,     
                       CARA_ELEM=CARA,
                      CHAM_MATER=CHAMPMAT,
                          OPTION='FORC_NODA'
                  )

MODES=CALC_NO(  reuse=MODES, 
                        RESULTAT=MODES,   MODELE=MODELE,     
                       CARA_ELEM=CARA,
                      CHAM_MATER=CHAMPMAT,
                          OPTION='REAC_NODA'
                  )

#
TEST_RESU(RESU=(
     _F(  RESULTAT = MODES, PARA = 'FREQ', NUME_ORDRE = 1, VALE = 1.00,
            REFERENCE = 'ANALYTIQUE'),
          _F(  RESULTAT = MODES, PARA = 'FREQ', NUME_ORDRE = 2, VALE = 2.236,
            REFERENCE = 'ANALYTIQUE'))
          )

#
#-----------------------------------------------------------------------
#
# CALCUL DES MODES STATIQUES POUR L'ENTRAINEMENT
#
#-----------------------------------------------------------------------
#

MODE_STA=MODE_STATIQUE(   MATR_RIGI=MATRRIGI,
                              MATR_MASS=MATRMASS,
                              MODE_STAT=_F(  NOEUD = ( 'P1', 'P4', ),
                                          AVEC_CMP = ( 'DX', )) 
                           )

#
MODE_STA=CALC_ELEM(reuse=MODE_STA, 
                             MODELE=MODELE,
                           RESULTAT=MODE_STA,        
                          CARA_ELEM=CARA,
                         CHAM_MATER=CHAMPMAT,
                             OPTION=( 'SIEF_ELGA_DEPL', 
                                      'EFGE_ELNO_DEPL', )
                     )

MODE_STA=CALC_NO(  reuse=MODE_STA,  MODELE=MODELE,
                           RESULTAT=MODE_STA,
                          CARA_ELEM=CARA,
                         CHAM_MATER=CHAMPMAT,
                             OPTION='FORC_NODA'
                     )

MODE_STA=CALC_NO(  reuse=MODE_STA,  MODELE=MODELE,
                           RESULTAT=MODE_STA,
                          CARA_ELEM=CARA,
                         CHAM_MATER=CHAMPMAT,
                             OPTION='REAC_NODA'
                     )

#
TEST_RESU(RESU=(
     _F(  RESULTAT = MODE_STA, NOM_CHAM = 'DEPL', NOM_CMP = 'DX',
            NOEUD_CMP = ( 'P1', 'DX', ),     
            REFERENCE = 'ANALYTIQUE',
            NOEUD = 'P2',       VALE = 0.6),
          _F(  RESULTAT = MODE_STA, NOM_CHAM = 'DEPL', NOM_CMP = 'DX',
            NOEUD_CMP = ( 'P1', 'DX', ),     
            REFERENCE = 'ANALYTIQUE',
            NOEUD = 'P3',       VALE = 0.4),
          _F(  RESULTAT = MODE_STA, NOM_CHAM = 'DEPL', NOM_CMP = 'DX',
            NOEUD_CMP = ( 'P4', 'DX', ),     
            REFERENCE = 'ANALYTIQUE',
            NOEUD = 'P2',       VALE = 0.4),
          _F(  RESULTAT = MODE_STA, NOM_CHAM = 'DEPL', NOM_CMP = 'DX',
            NOEUD_CMP = ( 'P4', 'DX', ),     
            REFERENCE = 'ANALYTIQUE',
            NOEUD = 'P3',       VALE = 0.6))
          )

#
#
#-----------------------------------------------------------------------
#
# CALCUL DES MODES STATIQUES POUR LA CORRECTION STATIQUE
#
#-----------------------------------------------------------------------
#

MODE_CO2=MODE_STATIQUE(   MATR_RIGI=MATRRIGI,
                          MATR_MASS=MATRMASS,
                          PSEUDO_MODE=_F(  NOEUD = ( 'P1', 'P4', ),
                                           AVEC_CMP = ( 'DX', ))
                          )

#
MODE_CO2=CALC_ELEM(reuse=MODE_CO2,
                             MODELE=MODELE,
                           RESULTAT=MODE_CO2,
                          CARA_ELEM=CARA,
                         CHAM_MATER=CHAMPMAT,
                             OPTION=( 'SIEF_ELGA_DEPL',
                                      'EFGE_ELNO_DEPL', )
                     )

MODE_CO2=CALC_NO(  reuse=MODE_CO2,  MODELE=MODELE,
                           RESULTAT=MODE_CO2,
                          CARA_ELEM=CARA,
                         CHAM_MATER=CHAMPMAT,
                             OPTION='FORC_NODA'
                     )

MODE_CO2=CALC_NO(  reuse=MODE_CO2,  MODELE=MODELE,
                           RESULTAT=MODE_CO2,
                          CARA_ELEM=CARA,
                         CHAM_MATER=CHAMPMAT,
                             OPTION='REAC_NODA'
                     )

#
TEST_RESU(RESU=(
     _F(  RESULTAT = MODE_CO2, NOM_CHAM = 'DEPL', NOM_CMP = 'DX',
            NOEUD_CMP = ( 'P1', 'DX', ),
            REFERENCE = 'ANALYTIQUE',
            NOEUD = 'P2',       VALE = 1.317E-2),
          _F(  RESULTAT = MODE_CO2, NOM_CHAM = 'DEPL', NOM_CMP = 'DX',
            NOEUD_CMP = ( 'P1', 'DX', ),
            REFERENCE = 'ANALYTIQUE',
            NOEUD = 'P3',       VALE = 1.216E-2),
          _F(  RESULTAT = MODE_CO2, NOM_CHAM = 'DEPL', NOM_CMP = 'DX',
            NOEUD_CMP = ( 'P4', 'DX', ),
            REFERENCE = 'ANALYTIQUE',
            NOEUD = 'P2',       VALE = 1.216E-2),
          _F(  RESULTAT = MODE_CO2, NOM_CHAM = 'DEPL', NOM_CMP = 'DX',
            NOEUD_CMP = ( 'P4', 'DX', ),
            REFERENCE = 'ANALYTIQUE',
            NOEUD = 'P3',       VALE = 1.317E-2))
          )

#
# -------------------------------
#
#   DEFINITION DU SEISME
#
#--------------------------------
#
F0=DEFI_VALEUR(    R8=1.0    )

#
F1=DEFI_VALEUR(    R8=1.5    )

F2=DEFI_VALEUR(    R8=2.0    )

OMEGA1=DEFI_VALEUR(    R8=EVAL("""2*PI*F1 """) )

OMEGA2=DEFI_VALEUR(    R8=EVAL("""2*PI*F2 """) )

A1=DEFI_VALEUR(    R8=0.5    )

A2=DEFI_VALEUR(    R8=0.5    )

#
L_FREQ=DEFI_LIST_REEL(        DEBUT=0.01,
                             INTERVALLE=_F( JUSQU_A = 20., PAS = 0.002))

#
FSRO1 = FORMULE(REEL="""(REEL:FREQ) =
A1*(2*PI*FREQ)**2/ABS(OMEGA1**2-(2*PI*FREQ)**2)""")
#
FSRO2 = FORMULE(REEL="""(REEL:FREQ) =
A2*(2*PI*FREQ)**2/ABS(OMEGA2**2-(2*PI*FREQ)**2)""")
#
SRO1_1=DEFI_VALEUR(    R8=EVAL("""FSRO1(F0        )"""))

SRO1_2=DEFI_VALEUR(    R8=EVAL("""FSRO1(2.236*F0  )"""))

SRO2_1=DEFI_VALEUR(    R8=EVAL("""FSRO2(F0        )"""))

SRO2_2=DEFI_VALEUR(    R8=EVAL("""FSRO2(2.236*F0  )"""))

#

SRO1=CALC_FONC_INTERP(           FONCTION=FSRO1,
                                 NOM_PARA='FREQ',
                                  LIST_PARA=L_FREQ,
                                  PROL_DROITE='LINEAIRE',
                                PROL_GAUCHE='LINEAIRE',
                                   INTERPOL='LOG'
                        )

SRO2=CALC_FONC_INTERP(           FONCTION=FSRO2,
                                 NOM_PARA='FREQ',
                                  LIST_PARA=L_FREQ,
                                  PROL_DROITE='LINEAIRE',
                                PROL_GAUCHE='LINEAIRE',
                                   INTERPOL='LOG'
                        )

#
SPEC1=DEFI_NAPPE(                NOM_PARA='AMOR',
                                   INTERPOL=( 'LIN', 'LOG', ),
                                       PARA=( 0.00, 0.05,   ),
                                   FONCTION=( SRO1,  SRO1,  )  
                       )

SPEC2=DEFI_NAPPE(                NOM_PARA='AMOR',
                                   INTERPOL=( 'LIN', 'LOG', ),
                                       PARA=( 0.00, 0.05,   ),
                                   FONCTION=( SRO2,  SRO2,  )  
                       )

#
L_TEMPS=DEFI_LIST_REEL(        DEBUT=0.0,
                             INTERVALLE=_F( JUSQU_A = 30., PAS = 0.01))

