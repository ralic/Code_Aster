DEBUT( CODE=_F(NOM='SSNV156C',NIV_PUB_WEB='INTERNET') )

MA  =  LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),)

MA  =  DEFI_GROUP(reuse  = MA, 
  MAILLAGE = MA, 
  CREA_GROUP_MA = (
    _F(NOM = 'HAUT',      GROUP_MA = 'GMA31'), 
    _F(NOM = 'COTE',      GROUP_MA = 'GMA32'),
    _F(NOM = 'MATIERE',   GROUP_MA = 'GMA33'),
    ),
  CREA_GROUP_NO = (
    _F(NOM = 'MESURE', GROUP_MA = 'GMA30'),
    _F(TOUT_GROUP_MA = 'OUI'),
    )
  )

COURBE = DEFI_FONCTION(
  NOM_PARA = 'EPSI',
  VALE     = (0.001, 100, 0.002, 110),
  PROL_DROITE = 'LINEAIRE'
  )

METAL = DEFI_MATERIAU(
  ELAS       = _F(E = 1.E5,  NU = 0.3), 
  ECRO_LINE  = _F(SY= 100, D_SIGM_EPSI = 1.E4),
  TRACTION   = _F(SIGM = COURBE),
  NON_LOCAL  = _F(LONG_CARA = 0.982707629824)
  )

CHMAT = AFFE_MATERIAU(
  MAILLAGE = MA, 
  AFFE = (
    _F(GROUP_MA = 'MATIERE', MATER = METAL),
    )
  )
  
MO = AFFE_MODELE(
  MAILLAGE = MA, 
  AFFE     = _F(
    GROUP_MA     = ('MATIERE','HAUT'),
    PHENOMENE    = 'MECANIQUE', 
    MODELISATION = 'AXIS_GRAD_VARI'
    )
  )
  
LIAISON = AFFE_CHAR_MECA(
  MODELE    = MO, 
  FACE_IMPO = (
    _F(GROUP_MA = 'HAUT', DY = 0),
    _F(GROUP_MA = 'COTE', DX = 0)
    ),
  )

TRACTION = AFFE_CHAR_MECA(
  MODELE = MO, 
  FORCE_INTERNE = (
    _F(GROUP_MA = 'MATIERE', FY=-1), 
    )
  )

RAMPE = DEFI_FONCTION(
  NOM_PARA = 'INST', 
  VALE = (
    0, 0,
    1, 1
    ), 
  PROL_DROITE = 'LINEAIRE'
  )


INSTANTS = DEFI_LIST_REEL(
  DEBUT = 0,   INTERVALLE = (
    _F(JUSQU_A =  87.5         , NOMBRE = 1),
    _F(JUSQU_A = 104.811963481 , NOMBRE = 1),
    _F(JUSQU_A = 146.159407709 , NOMBRE = 1),
    _F(JUSQU_A = 250.078993329 , NOMBRE = 1),
    _F(JUSQU_A = 875.079453484 , NOMBRE = 1),
    )
  )

EVOL = STAT_NON_LINE(
  MODELE = MO, 
  CHAM_MATER = CHMAT, 
  EXCIT = (
    _F(CHARGE = LIAISON), 
    _F(CHARGE = TRACTION, FONC_MULT = RAMPE)
    ), 
  COMP_INCR = _F(
    RELATION = 'VMIS_ISOT_TRAC', 
    DEFORMATION = 'PETIT'
    ), 
  INCREMENT = _F(
    LIST_INST = INSTANTS,
    NUME_INST_FIN = 3,
    ), 
  NEWTON = _F(
    MATRICE = 'ELASTIQUE',
    REAC_INCR = 0
    ), 
  CONVERGENCE = _F(
    RESI_GLOB_RELA = 1.E-6, 
    ITER_GLOB_MAXI = 100, 
    ),
  LAGR_NON_LOCAL = _F(
    ITER_DUAL_MAXI = 15,
    RESI_DUAL_ABSO = 1.E-7,
    RESI_PRIM_ABSO = 1.E-8,
    R = 1000.0,
    ITER_PRIM_MAXI = 20
    ),
  )

EVOL = STAT_NON_LINE(
  reuse = EVOL,
  ETAT_INIT = _F(EVOL_NOLI = EVOL),
  MODELE = MO, 
  CHAM_MATER = CHMAT, 
  EXCIT = (
    _F(CHARGE = LIAISON), 
    _F(CHARGE = TRACTION, FONC_MULT = RAMPE)
    ), 
  COMP_INCR = _F(
    RELATION = 'VMIS_ISOT_LINE', 
    DEFORMATION = 'PETIT'
    ), 
  INCREMENT = _F(
    LIST_INST = INSTANTS,
    ), 
  NEWTON = _F(
    MATRICE = 'ELASTIQUE',
    REAC_INCR = 0
    ), 
  CONVERGENCE = _F(
    RESI_GLOB_RELA = 1.E-6, 
    ITER_GLOB_MAXI = 100, 
    ),
  LAGR_NON_LOCAL = _F(
    ITER_DUAL_MAXI = 15,
    RESI_DUAL_ABSO = 1.E-7,
    RESI_PRIM_ABSO = 1.E-8,
    R = 1000.0,
    ITER_PRIM_MAXI = 20
    ),
  )


EVOL = CALC_ELEM (reuse = EVOL,
  RESULTAT   = EVOL,
  MODELE     = MO,
  CHAM_MATER = CHMAT,
  OPTION     = ('SIEF_ELNO_ELGA', 'EPSI_ELNO_DEPL', 'EQUI_ELNO_SIGM',
                'VARI_ELNO_ELGA')
  )

EVOL = CALC_NO (reuse = EVOL,
  RESULTAT = EVOL,
  OPTION   = ('VARI_NOEU_ELGA','SIEF_NOEU_ELGA','EQUI_NOEU_SIGM',
              'EPSI_NOEU_DEPL')
   ) 


