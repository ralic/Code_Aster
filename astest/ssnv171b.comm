DEBUT(CODE=_F(NOM='SSNV171B',  NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))

CUBE=LIRE_MAILLAGE();

TROISD=AFFE_MODELE(MAILLAGE=CUBE,
                   AFFE=_F(TOUT='OUI',
                           PHENOMENE='MECANIQUE',
                           MODELISATION='3D',),);
TFIN = 1.5;


LINST=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=0.87,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=TFIN,
                                    NOMBRE=100,),),);
LINST2=DEFI_LIST_REEL(DEBUT=0.0,
                     INTERVALLE=(_F(JUSQU_A=0.87,
                                    NOMBRE=1,),
                                 _F(JUSQU_A=TFIN,
                                    NOMBRE=1000,),),);

ACIER=DEFI_MATERIAU(ELAS=_F(E=145200.0,
                            NU=0.3,),
                    ECOU_VISC2=_F(N=10.0,
                                  K=40.0,
                                  C=1.0,
                                  D=36.68,
                                  A=10.0,),
                    ECRO_ISOT2=_F(R_0=75.5,
                                  Q1=9.77,
                                  B1=19.34,
                                  H=0.5,
                                  Q2=-33.27,
                                  B2=5.345,),
                    ECOU_VISC1=_F(N=10.0,
                                  K=40.0,
                                  C=1.0,),
                    ECRO_ISOT1=_F(R_0=75.5,
                                  Q=9.77,
                                  B=19.34,
                                  H=0.0,),
                    ECRO_CINE1=_F(D=36.68,),);

COEF=DEFI_FONCTION(NOM_PARA='INST',
                   VALE=(0.0,0.0,1.0,1.0,),
                   PROL_DROITE='LINEAIRE',
                   PROL_GAUCHE='LINEAIRE',);

MAT=AFFE_MATERIAU(MAILLAGE=CUBE,
                  AFFE=_F(TOUT='OUI',
                          MATER=ACIER,),);

TRAC=AFFE_CHAR_MECA(MODELE=TROISD,
                    DDL_IMPO=(_F(NOEUD='NO4',
                                 DX=0.0,
                                 DY=0.0,),
                              _F(NOEUD='NO8',
                                 DX=0.0,
                                 DY=0.0,
                                 DZ=0.0,),
                              _F(NOEUD='NO2',
                                 DX=0.0,),
                              _F(NOEUD='NO6',
                                 DX=0.0,),),
                    FORCE_NODALE=(_F(NOEUD=('NO3','NO7',),
                                     FX=35.0,),
                                  _F(NOEUD=('NO1','NO5',),
                                     FX=35.0,),),);

COMPOR1=DEFI_COMPOR(MONOCRISTAL=(_F(MATER=ACIER,
                                    ECOULEMENT='ECOU_VISC1',
                                    ECRO_ISOT='ECRO_ISOT1',
                                    ECRO_CINE='ECRO_CINE1',
                                    ELAS='ELAS',
                                    FAMI_SYST_GLIS='OCTAEDRIQUE',),),);

COMPOR2=DEFI_COMPOR(MONOCRISTAL=(_F(MATER=ACIER,
                                    ECOULEMENT='ECOU_VISC2',
                                    ECRO_ISOT='ECRO_ISOT2',
                                    ECRO_CINE='ECRO_CINE1',
                                    ELAS='ELAS',
                                    FAMI_SYST_GLIS='OCTAEDRIQUE',),),);


ORIEN=AFFE_CARA_ELEM(MODELE=TROISD,
                     MASSIF=_F(GROUP_MA='TOUT',
                               ANGL_REP=(30.0,0.0,0.0,),),);

MONOIMPL=STAT_NON_LINE(MODELE=TROISD,
                    CHAM_MATER=MAT,
                    CARA_ELEM=ORIEN,
                    EXCIT=(_F(CHARGE=TRAC,
                              FONC_MULT=COEF,
                              TYPE_CHARGE='FIXE_CSTE',),),
                    COMP_INCR=(_F(RELATION='MONOCRISTAL',
                                  COMPOR=COMPOR1,
                                  DEFORMATION='PETIT',
                                  ITER_INTE_MAXI=100,
                                  ITER_INTE_PAS=10,
                                  RESO_INTE='IMPLICITE',
                                  TOUT='OUI',),),
                    INCREMENT=(_F(LIST_INST=LINST,
                     SUBD_METHODE='EXTRAPOLE',
                     SUBD_PAS=10,
                     SUBD_NIVEAU=10,
                     ),),
                    ARCHIVAGE=_F(LIST_INST=LINST),
                    NEWTON=(_F(REAC_ITER=1,),),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=100,),),);

MONOIMPL=CALC_ELEM(reuse =MONOIMPL,
                RESULTAT=MONOIMPL,
                OPTION=('EPSI_ELGA_DEPL','EPSP_ELGA',),);

TEST_RESU(RESU=(_F(RESULTAT=MONOIMPL,
                   INST=TFIN,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=-210.0,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=MONOIMPL,
                   INST=TFIN,
                   NOM_CHAM='EPSI_ELGA_DEPL',
                   NOM_CMP='EPXX',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=-0.0018908,
                   PRECISION=(0.001,1e-06,),
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=MONOIMPL,
                   INST=TFIN,
                   NOM_CHAM='EPSI_ELGA_DEPL',
                   NOM_CMP='EPYY',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=0.00050265,
                   PRECISION=(0.001,1e-06,),
                   REFERENCE='NON_REGRESSION',),
                _F(RESULTAT=MONOIMPL,
                   INST=TFIN,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPYY',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=6.876676e-05,
                   PRECISION=(0.001,1e-06,),
                   REFERENCE='NON_REGRESSION',),),);


MONOEXPL=STAT_NON_LINE(MODELE=TROISD,
                     CHAM_MATER=MAT,
                     CARA_ELEM=ORIEN,
                     EXCIT=(_F(CHARGE=TRAC,
                               FONC_MULT=COEF,
                               TYPE_CHARGE='FIXE_CSTE',),),
                     COMP_INCR=(_F(RELATION='MONOCRISTAL',
                                   COMPOR=COMPOR1,
                                   DEFORMATION='PETIT',
                                   RESO_INTE='RUNGE_KUTTA_2',
                                   TOUT='OUI',),),
                     ARCHIVAGE=_F(LIST_INST=LINST),
                     INCREMENT=(_F(LIST_INST=LINST2,
                     SUBD_METHODE='EXTRAPOLE',
                     SUBD_PAS=10,
                     SUBD_NIVEAU=10,
                     ),),
                     NEWTON=(_F(PREDICTION='EXTRAPOL',
                                MATRICE='ELASTIQUE',
                                REAC_ITER=0,),),
                     CONVERGENCE=(_F(ITER_GLOB_MAXI=10,
                                     ),),);

MONOEXPL=CALC_ELEM(reuse =MONOEXPL,
                 RESULTAT=MONOEXPL,
                 OPTION=('EPSI_ELGA_DEPL','EPSP_ELGA',),);

TEST_RESU(RESU=(_F(RESULTAT=MONOEXPL,
                   INST=TFIN,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=-210.0,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=MONOEXPL,
                   INST=TFIN,
                   NOM_CHAM='EPSI_ELGA_DEPL',
                   NOM_CMP='EPXX',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=-0.0018908,
                   PRECISION=(0.007,1e-06,),
                   REFERENCE='AUTRE_ASTER',),
                _F(RESULTAT=MONOEXPL,
                   INST=TFIN,
                   NOM_CHAM='EPSI_ELGA_DEPL',
                   NOM_CMP='EPYY',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=0.00050265,
                   PRECISION=(0.004,1e-06,),
                   REFERENCE='AUTRE_ASTER',),
                _F(RESULTAT=MONOEXPL,
                   INST=TFIN,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPYY',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=6.876676e-05,
                   PRECISION=(0.03,1e-06,),
                   REFERENCE='AUTRE_ASTER',),),);

COMPORP=DEFI_COMPOR(POLYCRISTAL=(
                               _F(MONOCRISTAL=COMPOR1,
                                  FRAC_VOL=1.0,
                                  ANGL_REP=(30.,0.,0.)),),
                    LOCALISATION='BETA',DA=0.,DL=0.,
                    );


POLYEXPL=STAT_NON_LINE(MODELE=TROISD,
                    CHAM_MATER=MAT,
                    EXCIT=(_F(CHARGE=TRAC,
                              FONC_MULT=COEF,
                              TYPE_CHARGE='FIXE_CSTE',),),
                    COMP_INCR=(_F(RELATION='POLYCRISTAL',
                                  COMPOR=COMPORP,
                                  DEFORMATION='PETIT',
                               #   RESI_INTE_RELA=1.E-4,
                                  RESO_INTE='RUNGE_KUTTA_2',
                                  TOUT='OUI',),),
                     ARCHIVAGE=_F(LIST_INST=LINST),
                     INCREMENT=(_F(LIST_INST=LINST2,
                     SUBD_METHODE='EXTRAPOLE',
                     SUBD_PAS=10,
                     SUBD_NIVEAU=10,
                    ),),
                     NEWTON=(_F(PREDICTION='EXTRAPOL',
                                MATRICE='ELASTIQUE',
                                REAC_ITER=0,),),
                    CONVERGENCE=(_F(ITER_GLOB_MAXI=10),),);

POLYEXPL=CALC_ELEM(reuse =POLYEXPL,
                 RESULTAT=POLYEXPL,
                 OPTION=('EPSI_ELGA_DEPL','EPSP_ELGA',),);

TEST_RESU(RESU=(_F(RESULTAT=POLYEXPL,
                   INST=TFIN,
                   NOM_CHAM='SIEF_ELGA',
                   NOM_CMP='SIXX',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=-210.0,
                   REFERENCE='ANALYTIQUE',),
                _F(RESULTAT=POLYEXPL,
                   INST=TFIN,
                   NOM_CHAM='EPSI_ELGA_DEPL',
                   NOM_CMP='EPXX',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=-0.0018908,
                   PRECISION=(0.007,1e-06,),
                   REFERENCE='AUTRE_ASTER',),
                _F(RESULTAT=POLYEXPL,
                   INST=TFIN,
                   NOM_CHAM='EPSI_ELGA_DEPL',
                   NOM_CMP='EPYY',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=0.00050265,
                   PRECISION=(0.004,1e-06,),
                   REFERENCE='AUTRE_ASTER',),
                _F(RESULTAT=POLYEXPL,
                   INST=TFIN,
                   NOM_CHAM='EPSP_ELGA',
                   NOM_CMP='EPYY',
                   MAILLE='MA1',
                   POINT=1,
                   VALE=6.876676e-05,
                   PRECISION=(0.03,1e-06,),
                   REFERENCE='AUTRE_ASTER',),),);


FIN()

