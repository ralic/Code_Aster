IMPR_TABLE( TABLE=TCARS )

#
#     Affectation du reel contenu dans la table TCARS
#     (ligne 'DX', colonne 1) dans la variable python pop
#

rext=TCARS['Y_MAX',2]

#rext=2.5E-2

MA=LIRE_MAILLAGE(VERI_MAIL=_F(VERIF='NON'),UNITE=21);

MO=AFFE_MODELE(MAILLAGE=MA,
               AFFE=_F(TOUT='OUI',
                       PHENOMENE='MECANIQUE',
                       MODELISATION='POU_D_E',),);

E=2.E11;

MATR=DEFI_MATERIAU(ELAS=_F(E=E,
                           NU=0.29999999999999999,),);

CHMAT=AFFE_MATERIAU(MAILLAGE=MA,
                    AFFE=_F(TOUT='OUI',
                            MATER=MATR,),);

CAREL=AFFE_CARA_ELEM(MODELE=MO,
                     INFO=2,
                     POUTRE=_F(GROUP_MA='G4',
                               SECTION='CERCLE',
                               CARA=('R'),
                            #   VALE=(0.025000000000000001)
                               VALE=rext)
                               ,)

FTOT=1000. ;

CHARG5=AFFE_CHAR_MECA(MODELE=MO,
                      DDL_IMPO=_F(GROUP_NO='A',
                                  DX=0.0,
                                  DY=0.0,
                                  DZ=0.0,
                                  DRX=0.0,
                                  DRY=0.0,
                                  DRZ=0.0,),
                      FORCE_NODALE=_F(GROUP_NO='B',
                                      FX=FTOT,),);

MEST5=MECA_STATIQUE(MODELE=MO,
                    CHAM_MATER=CHMAT,
                    CARA_ELEM=CAREL,
                    EXCIT=_F(CHARGE=CHARG5,),
                    );

MEST5=CALC_NO(reuse =MEST5,
              RESULTAT=MEST5,
              OPTION='FORC_NODA',);
              

# VALEUR DE REFERENCE
              
R = 0.025;

UX = FTOT/E/pi/R/R



TEST_RESU(RESU=(
                _F(RESULTAT=MEST5,
                   NUME_ORDRE=1,
                   NOM_CHAM='DEPL',
                   NOM_CMP='DX',
                   GROUP_NO='B',REFERENCE='ANALYTIQUE',
                   VALE=UX,),
                _F(RESULTAT=MEST5,
                   NUME_ORDRE=1,
                   NOM_CHAM='FORC_NODA',
                   NOM_CMP='DX',REFERENCE='ANALYTIQUE',
                   GROUP_NO='B',
                   VALE=FTOT,),
                   ),
                   )

