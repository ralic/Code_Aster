# UN HUITIEME DE SPHERE


MAT=DEFI_MATERIAU(ELAS=_F( E = 2.E11, NU = 0.3, RHO = 7800.))

MAIL=LIRE_MAILLAGE(FORMAT='MED',)

MAIL=DEFI_GROUP( reuse=MAIL,   MAILLAGE=MAIL,
  CREA_GROUP_MA=_F(  NOM = 'TOUT', TOUT = 'OUI'))

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL, AFFE=_F(  TOUT = 'OUI', MATER = MAT) )

MO=AFFE_MODELE(  MAILLAGE=MAIL, VERIF='MAILLE',
         AFFE=_F( TOUT = 'OUI', MODELISATION = 'DKT', PHENOMENE = 'MECANIQUE'))

EP=AFFE_CARA_ELEM(   MODELE=MO, COQUE=_F(
       GROUP_MA = 'TOUT',
 EPAIS = 0.10))

CH=AFFE_CHAR_MECA(MODELE=MO,DDL_IMPO=(
         _F( GROUP_NO = 'X_NUL', DX = 0., DRY = 0., DRZ = 0.),
         _F( GROUP_NO = 'Y_NUL', DY = 0., DRX = 0., DRZ = 0.),
         _F( GROUP_NO = 'Z_NUL', DZ = 0., DRX = 0., DRY = 0.),
         _F( NOEUD = 'NO2', DX = 0., DY = 0., DRX = 0., DRY = 0., DRZ = 0.),
         _F( NOEUD = 'NO1', DZ = 0., DY = 0., DRX = 0., DRY = 0., DRZ = 0.),
         _F( NOEUD = 'NO3', DX = 0., DZ = 0., DRX = 0., DRY = 0., DRZ = 0.)))

KE=CALC_MATR_ELEM(MODELE=MO, CHARGE=CH, CHAM_MATER=CHMAT,
                        CARA_ELEM=EP, OPTION='RIGI_MECA')

ME=CALC_MATR_ELEM(MODELE=MO, CHARGE=CH, CHAM_MATER=CHMAT,
                        CARA_ELEM=EP, OPTION='MASS_MECA')

NUM=NUME_DDL( MATR_RIGI=KE)

KA=ASSE_MATRICE(MATR_ELEM=KE,NUME_DDL=NUM)

MA=ASSE_MATRICE(MATR_ELEM=ME,NUME_DDL=NUM)

MD=MODE_ITER_SIMULT( MATR_RIGI=KA,
                     MATR_MASS=MA,
                     CALC_FREQ=_F(  OPTION = 'BANDE', FREQ = (230., 530.,))
                    )


