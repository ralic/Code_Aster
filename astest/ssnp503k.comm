# CONTACT-FROTTANT EN GRANDS GLISSEMENTS
# POUR DES FISSURES HORIZONTALES
#

DEBUT(CODE=_F(NOM='SSNP503K',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'));

MAILLAG2=LIRE_MAILLAGE(INFO=1,FORMAT='MED',);

MAILLAG2=MODI_MAILLAGE(reuse =MAILLAG2,
                       MAILLAGE=MAILLAG2,
                       ORIE_PEAU_3D=_F(GROUP_MA=('S13','S21','S23','S31',),),);

MAILLAG2=DEFI_GROUP(reuse =MAILLAG2,
                    MAILLAGE=MAILLAG2,
                    CREA_GROUP_NO =(_F(NOM = 'S13N',
                                       GROUP_MA = 'S13',),),
                    );

MODELE=AFFE_MODELE(MAILLAGE=MAILLAG2,
                   AFFE=(_F(TOUT='OUI', 
                            PHENOMENE='MECANIQUE',
                            MODELISATION='3D',),),);       

CONT   = DEFI_CONTACT(MODELE         = MODELE,
                      FORMULATION    = 'CONTINUE',
                      ITER_FROT_MAXI = 10,
                      ITER_CONT_MAXI = 4,
                      ITER_GEOM_MAXI = 10,
                      FROTTEMENT     = 'COULOMB',
                      ZONE = (_F(
                                 GROUP_MA_MAIT = 'S21',
                                 GROUP_MA_ESCL = 'S13',
                                 INTEGRATION   = 'AUTO',
                                 SEUIL_INIT    = 0.0,
                                 COULOMB       = 0.5,
                                 TOLE_PROJ_EXT = 0.1,   
                                 CONTACT_INIT  = 'INTERPENETRE',
                              ),
                             _F(
                                 GROUP_MA_MAIT = 'S23',
                                 GROUP_MA_ESCL = 'S31',
                                 INTEGRATION   = 'AUTO',
                                 SEUIL_INIT    = 0.0,
                                 COULOMB       = 0.5,
                                 TOLE_PROJ_EXT = 0.1,   
                                 CONTACT_INIT  = 'INTERPENETRE',
                              ),
                            )
                    );


E=100.0E6
nu=0.3
ACIER=DEFI_MATERIAU(ELAS=_F(E=E,
                            NU=nu,
                            RHO=7800.0,),);

CHAMPMAT=AFFE_MATERIAU(MAILLAGE=MAILLAG2,
                       MODELE=MODELE,
                       AFFE=_F(TOUT='OUI',
                       MATER=ACIER,
                      ),);

DEPX=2.0
DEPY=1.E-3
DEPZ=0.0

CH1=AFFE_CHAR_MECA(MODELE=MODELE,
                   DDL_IMPO=(_F(GROUP_MA='S11',DY=DEPY,DZ=0.0,DX=0.0,),
                             _F(GROUP_MA='S33',DY=-DEPY,DZ=0.0,DX=0.0,),),
                   INFO=1,);

CH2=AFFE_CHAR_MECA(MODELE=MODELE,
                   DDL_IMPO=_F(GROUP_MA='S24',
                               DX=DEPX,DZ=DEPZ),
                   INFO=1,);

VAR1=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),
                   );

VAR2=DEFI_FONCTION(NOM_PARA='INST',
                    VALE=(0.,0.,1.,1.),);

L_INST=DEFI_LIST_REEL(DEBUT=0.0,
                      INTERVALLE=_F(JUSQU_A=1.0,
                                    NOMBRE=4,),);

UTOT1=STAT_NON_LINE(MODELE=MODELE,
                   CHAM_MATER=CHAMPMAT,
                   CONTACT = CONT,
                   EXCIT=(
                          _F(CHARGE=CH1,FONC_MULT=VAR1),
                          _F(CHARGE=CH2,FONC_MULT=VAR2),),
                   COMP_ELAS=_F(RELATION='ELAS',),
                   INCREMENT=_F(LIST_INST=L_INST,),
                   CONVERGENCE=(_F(ITER_GLOB_MAXI=8,
                                  RESI_GLOB_MAXI=1E-06)),
                   NEWTON=_F(REAC_ITER=1,),
                   INFO=1,);

#IMPR_RESU(FORMAT='GMSH',RESU=(_F(RESULTAT=UTOT1,
#                                 NOM_CHAM='DEPL',
#                                 TYPE_CHAM='VECT_3D',
#                                 NOM_CMP=('DX','DY','DZ',)))),

LAG=[None]*5
inst=[0.0,0.25,0.5,0.75,1.0]


vale = [[0.0,-7.2384991E+03,-6.4285635E+03,-5.4975876E+03,-9.8231384E+02],
        [0.0,-2.6362672E+04,-1.2791423E+04,-1.1098467E+04,-1.0131368E+03],
        [0.0,0.0,-1.9277375E+04,-1.6785053E+04,-1.5709230E+03],
        [0.0,0.0,-4.4286909E+04,-2.2460135E+04,-3.1664811E+03]]


for i in [1,2,3,4]: #Boucle sur les increments
   LAG[i]=POST_RELEVE_T(ACTION=_F(INTITULE='DEPLE',
                                         GROUP_NO=('S13N'),
                                         RESULTAT=UTOT1,
                                         NOM_CHAM='DEPL',
                                         INST=inst[i],
                                         NOM_CMP=('LAGS_C','LAGS_F1','LAGS_F2'),
                                         OPERATION='EXTRACTION',),);
   IMPR_TABLE(TABLE=LAG[i],
              FORMAT='AGRAF',
              FORMAT_R='1PE12.7',
              FORMAT_C='MODULE_PHASE',
              NOM_PARA=('NOEUD','INST','COOR_X','COOR_Z','LAGS_C','LAGS_F1','LAGS_F2'),
              IMPR_FONCTION='NON',
              TRI=_F(NOM_PARA=('COOR_X','COOR_Z'),
                     ORDRE='CROISSANT',),);
   
   for j in [0,1,2,3,4]: #Boucle sur les positions testees
      if (vale[i-1] [j] == 0.0):
        precision = 1.0E-12
        critere = 'ABSOLU'
      else:
        precision = 5.E-2
        critere = 'RELATIF'
      TEST_TABLE(TABLE=LAG[i],
                 NOM_PARA='LAGS_C',
                 VALE=vale[i-1] [j],
                 CRITERE=critere,
                 PRECISION=precision,
                 REFERENCE='AUTRE_ASTER',
                 FILTRE=(_F(NOM_PARA='COOR_X',
                                    CRIT_COMP='EQ',
                                    VALE=j,
                                    PRECISION=0.03,
                                    CRITERE='ABSOLU',),
                               _F(NOM_PARA='COOR_Z',
                                    CRIT_COMP='EQ',
                                    VALE=0.0,
                                    PRECISION=0.01,
                                    CRITERE='ABSOLU',),),);

FIN();                  

