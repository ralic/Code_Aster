DEBUT(CODE=_F( NOM = 'SSNP103A',NIV_PUB_WEB='INTERNET')
        )

#......................................................................
# EPROUVETTE DE BETON EN COMPRESSION UNIAXIALE
# ELASTOPLASTICITE  NADAI_B  (CONTRAINTES PLANES)
#......................................................................


M=LIRE_MAILLAGE( )

MO=AFFE_MODELE( 
            MAILLAGE=M,    VERIF='MAILLE',
           AFFE=_F( TOUT = 'OUI',PHENOMENE = 'MECANIQUE',MODELISATION = 'C_PLAN')
           )

EPRO=DEFI_MATERIAU( 
                ELAS=_F( E = 34000., NU = 0.18),
              
                NADAI_B=_F( 
                F_C = 32.8,
                F_T = 3.28,
                CRIT_E_C = 0.,
                EPSP_R_C = 5.E-3,
                EPSP_P_C = 9.64705E-4,
                EPSI_R_T = 5.E-4,
                FAC_T_C = 0.4)
           )

CM=AFFE_MATERIAU( 
            MAILLAGE=M,   
           AFFE=_F( TOUT = 'OUI',MATER = EPRO)
           )

LIAISON=AFFE_CHAR_MECA( 
            MODELE=MO,DDL_IMPO=(
           _F( NOEUD = 'NO1',  DX = 0.,  DY = 0.),
           _F( NOEUD = 'NO2',  DY = 0.))

           )

CH_FO=AFFE_CHAR_MECA( 
            MODELE=MO,FORCE_NODALE=(
           _F( NOEUD = 'NO3',       FY = -20.),
                        _F( NOEUD = 'NO4',       FY = -20.))

           )

RAMPE=DEFI_FONCTION(   NOM_PARA='INST',
           PROL_GAUCHE='LINEAIRE',  PROL_DROITE='LINEAIRE',
           VALE=(0.0,0.0,1.0,1.0,))

L_INST=DEFI_LIST_REEL(   DEBUT=0.0,INTERVALLE=(
             _F( JUSQU_A = 0.1,     NOMBRE = 1),
                        _F( JUSQU_A = 0.2,     NOMBRE = 1),
                        _F( JUSQU_A = 0.25,    NOMBRE = 1))
           )

# STAT_NON_LINE 'NADAI_B'
#-------------------------------



U=STAT_NON_LINE( 
                            MODELE=MO,
                        CHAM_MATER=CM,EXCIT=(
                             _F( 
                CHARGE = CH_FO,
                FONC_MULT = RAMPE), _F( 
                CHARGE = LIAISON)),
                         COMP_INCR=_F( 
                RELATION = 'NADAI_B'),
                         INCREMENT=_F( 
                LIST_INST = L_INST),
                            NEWTON=_F( 
                MATRICE = 'TANGENTE',
                REAC_ITER = 1),
                       CONVERGENCE=_F( 
                RESI_GLOB_MAXI = 1.E-6,
                ITER_INTE_MAXI = 30,
                ITER_GLOB_MAXI = 30,
                RESI_INTE_RELA = 1.E-6),
                     RECH_LINEAIRE=_F( 
                ITER_LINE_MAXI = 3)
              )

U=CALC_ELEM( reuse=U,
                            OPTION=(
                  'SIEF_ELNO_ELGA',  'VARI_ELNO_ELGA',),
                          RESULTAT=U,
              )

U=CALC_ELEM(
              reuse=U,
                  RESULTAT=U,
                   OPTION='EPSI_ELNO_DEPL'
              )

VARI=CREA_CHAMP(  OPERATION='EXTR', TYPE_CHAM='ELNO_VARI_R',
              NOM_CHAM='VARI_ELNO_ELGA',
              RESULTAT=U,
              INST=0.25
              )

TEST_RESU(RESU=(
              
                _F( 
                RESULTAT = U,
                NOM_CHAM = 'SIEF_ELNO_ELGA',
                MAILLE = 'MA1',
                NOEUD = 'NO1',
                INST = 0.25,
                NOM_CMP = 'SIXX',
                VALE = 0.,
                CRITERE = 'ABSOLU'),
                _F( 
                RESULTAT = U,
                NOM_CHAM = 'SIEF_ELNO_ELGA',
                MAILLE = 'MA1',
                NOEUD = 'NO1',
                INST = 0.25,
                NOM_CMP = 'SIYY',
                VALE = -10.),
                _F( 
                RESULTAT = U,
                NOM_CHAM = 'SIEF_ELNO_ELGA',
                MAILLE = 'MA1',
                NOEUD = 'NO1',
                INST = 0.25,
                NOM_CMP = 'SIXY',
                VALE = 0.,
                CRITERE = 'ABSOLU'),
                _F( 
                RESULTAT = U,
                NOM_CHAM = 'EPSI_ELNO_DEPL',
                MAILLE = 'MA1',
                NOEUD = 'NO1',
                INST = 0.25,
                NOM_CMP = 'EPXX',
                VALE = 8.54077E-5),
                _F( 
                RESULTAT = U,
                NOM_CHAM = 'EPSI_ELNO_DEPL',
                MAILLE = 'MA1',
                NOEUD = 'NO1',
                INST = 0.25,
                NOM_CMP = 'EPYY',
                VALE = -3.40046E-4)),

           
               CHAM_ELEM=_F(  CHAM_GD = VARI,
                MAILLE = 'MA1',
                NOEUD = 'NO1',
                NOM_CMP = 'V1',
                VALE = 4.59283E-5)
              )

FIN()
#
