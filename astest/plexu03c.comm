                      MODELE=MODELE,DDL_IMPO=(
                    _F(  GROUP_NO='ENCAST',
                               DX = 0.,
                               DY = 0.,
                               DZ = 0.,
                               DRX = 0.,
                               DRY = 0.,
                               DRZ = 0.),
                               ),
                RELA_CINE_BP=_F(  CABLE_BP = CABLEPR,
                               SIGM_BPEL = 'NON',
                               RELA_CINE = 'OUI'),
                        INFO=1 )


PRESS=AFFE_CHAR_MECA(
                      MODELE=MODELE,
                FORCE_COQUE=_F(PRES = -1.E1,GROUP_MA = 'GROUPE_____VOILE',),
                        INFO=1 )


FONCCB=DEFI_FONCTION(NOM_PARA='INST',
                     VALE=(0.0,    0,
                           1.0,    1,
                           ),
                     PROL_DROITE='CONSTANT',
                     PROL_GAUCHE='CONSTANT',
                     );


