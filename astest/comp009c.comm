def verif(VAL_CAL,VAL_REF):
   if abs(VAL_REF) > 1.E-30 :
      diff=abs (VAL_CAL - VAL_REF)/VAL_REF
   else :
      diff=abs (VAL_CAL - VAL_REF)
   return diff

