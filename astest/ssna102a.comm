DEBUT(CODE=_F(NIV_PUB_WEB='INTERNET'),
DEBUG=_F(SDVERI='OUI'),
IGNORE_ALARM=('CONTACT_93'))

# Test de la methode RESOLUTION='NON'. Interpenetration donc alarme CONTACT_93 ignoree

# --------------------------------------------------------------------
#       LECTURE DU MAILLAGE ET CREATION DE GROUPES DE NOEUDS
# --------------------------------------------------------------------

MAIL=LIRE_MAILLAGE(FORMAT='MED', )

#

MAIL=DEFI_GROUP( reuse=MAIL,  MAILLAGE=MAIL,CREA_GROUP_NO=(
# GROUPES DE NOEUDS POUR LES DEGRES DE LIBERTE IMPOSES
                        _F(  GROUP_MA = 'LCD1'),
                        _F(  GROUP_MA = 'LBC4'),
# GROUPES DE NOEUDS POUR LES CONDITIONS DE CONTACT
                        _F(  GROUP_MA = 'LAB1'),
                        _F(  GROUP_MA = 'CEP2'),
                        _F(  GROUP_MA = 'CME2'),
                        _F(  GROUP_MA = 'CEP3'),
                        _F(  GROUP_MA = 'CME3'),
                        _F(  GROUP_MA = 'LCD4'),
                        _F(  GROUP_MA = 'LCD4I'),
# LCD4P EST LCD4 SAUF NOEUD PC4 ENCASTRE (POUR LE POST-TRAITEMENT)
                        _F(  NOM = 'LCD4P',
                                        DIFFE = ('LCD4I', 'PC4',)))
                     )

#
# --------------------------------------------------------------------
#                   MODELISATION 2D AXISYMETRIQUE
# --------------------------------------------------------------------
#

MOD=AFFE_MODELE(
                       MAILLAGE=MAIL,
                       AFFE=_F(  TOUT = 'OUI',
                              PHENOMENE = 'MECANIQUE',
                              MODELISATION = 'AXIS')
                    )

#
# --------------------------------------------------------------------
#         DEFINITION ET AFFECTATION D'UN MATERIAU ELASTIQUE
# --------------------------------------------------------------------
#

ELAS=DEFI_MATERIAU(
                          ELAS=_F(  E = 200000.,
                                 NU = 0.3)
                       )

#

CHMAT=AFFE_MATERIAU(
                           MAILLAGE=MAIL,
                           AFFE=_F(  TOUT = 'OUI',
                                  MATER = ELAS)
                        )

#
# --------------------------------------------------------------------
#                   ENCASTREMENT DE LA LIGNE LBC4
# --------------------------------------------------------------------
#

BLOQ=AFFE_CHAR_MECA(
                           MODELE=MOD,
                              DDL_IMPO=_F(  GROUP_NO = 'LBC4',
                                         DX = 0.0,  DY = 0.0)
                        )

#
# --------------------------------------------------------------------
#               DEPLACEMENT IMPOSE SUR LA LIGNE LCD1
# --------------------------------------------------------------------
#

DEPL=AFFE_CHAR_MECA(
                           MODELE=MOD,
                              DDL_IMPO=_F(  GROUP_NO = 'LCD1',
                                                  DY = -2.0)
                       )

#
# --------------------------------------------------------------------
#     SUPPRESSION DES MOUVEMENTS DE CORPS RIGIDE DES DEUX SPHERES
# --------------------------------------------------------------------
#

LIAI=AFFE_CHAR_MECA(
                           MODELE=MOD,LIAISON_DDL=(
                           _F(  GROUP_NO = ('PA1',  'PP2', ),
                                         DDL = ('DY', 'DY',),
                                         COEF_MULT = (1., -1.,),
                                         COEF_IMPO = 0.),
                           _F(  GROUP_NO = ('PM3',  'PD4', ),
                                         DDL = ('DY', 'DY',),
                                         COEF_MULT = (1., -1.,),
                                         COEF_IMPO = 0.))
                       )


CONT = DEFI_CONTACT(MODELE      = MOD,
                    FORMULATION = 'DISCRETE',
                    ZONE=(
                               _F(  GROUP_MA_MAIT = 'LAB1',
                                    GROUP_MA_ESCL = 'CEP2',
                                    SANS_GROUP_NO = ('PA1', 'PP2',),),
                               _F(  GROUP_MA_MAIT = 'CME2',
                                    GROUP_MA_ESCL = 'CEP3',),
                               _F(  GROUP_MA_MAIT = 'CME3',
                                    GROUP_MA_ESCL = 'LCD4',
                                    SANS_GROUP_NO = ('PM3', 'PD4',)),
                          ),
                     );

CONT2 = DEFI_CONTACT(MODELE      = MOD,
                    FORMULATION = 'DISCRETE',
                    ZONE=(
                               _F(  GROUP_MA_MAIT = 'LAB1',
                                    GROUP_MA_ESCL = 'CEP2',
                                    RESOLUTION    = 'NON',
                                    SANS_GROUP_NO = ('PA1', 'PP2',),),
                               _F(  GROUP_MA_MAIT = 'CME2',
                                    GROUP_MA_ESCL = 'CEP3',),
                               _F(  GROUP_MA_MAIT = 'CME3',
                                    GROUP_MA_ESCL = 'LCD4',
                                    SANS_GROUP_NO = ('PM3', 'PD4',)),
                          ),
                     );

#
# --------------------------------------------------------------------
#         DECOUPAGE EN INCREMENTS DE CHARGE (1 EN ELASTICITE)
#         ET RAMPE MULTIPLICATRICE POUR LE DEPLACEMENT IMPOSE
# --------------------------------------------------------------------
#

FONC=DEFI_FONCTION(
                           NOM_PARA='INST',
                           VALE=( 0.,   0.0,
                                  1.,   1.0, )
                        )

#

LINST=DEFI_LIST_REEL(
                            DEBUT=0.0,
                            INTERVALLE=_F(  JUSQU_A = 1.,  NOMBRE = 1)
                         )

#
# --------------------------------------------------------------------
#                            CALCUL NON LINEAIRE
# --------------------------------------------------------------------
#

RESU=STAT_NON_LINE(
SOLVEUR=_F(SYME='OUI',),
                          MODELE=MOD,
                          CHAM_MATER=CHMAT,EXCIT=(
                          _F(  CHARGE = BLOQ),
                                _F(  CHARGE = DEPL,   FONC_MULT = FONC),
                                _F(  CHARGE = LIAI),),
                          CONTACT  = CONT,
                          COMPORTEMENT=_F(  RELATION = 'ELAS'),
                          NEWTON=_F(  MATRICE = 'ELASTIQUE'),
                          INCREMENT=_F(  LIST_INST = LINST),
                          CONVERGENCE=_F(
                                        RESI_GLOB_MAXI = 1.E-6,
                                        ITER_GLOB_MAXI = 50,
                                        ARRET = 'OUI'),
                          INFO=1
                       )

#
# --------------------------------------------------------------------
#                    CALCUL DES CONTRAINTES AUX NOEUDS
# --------------------------------------------------------------------
#

RESU=CALC_CHAMP( reuse=RESU,  RESULTAT=RESU,
                        CONTRAINTE=('SIGM_ELNO'),
                  )

RESU=CALC_CHAMP( reuse=RESU,  RESULTAT=RESU,
                     CONTRAINTE='SIGM_NOEU'
                  )

#
# --------------------------------------------------------------------
#          CALCUL DES FORCES NODALES ET DES REACTIONS NODALES
# --------------------------------------------------------------------
#


RESU=CALC_CHAMP( reuse=RESU,  RESULTAT=RESU,
                     FORCE='REAC_NODA'
                  )

#
# --------------------------------------------------------------------
#                COMPARAISON SOURCE EXTRENE
# --------------------------------------------------------------------

TEST_RESU(RESU=_F(NUME_ORDRE=1,
                  GROUP_NO='PA4',
                  REFERENCE='SOURCE_EXTERNE',
                  RESULTAT=RESU,
                  NOM_CHAM='DEPL',
                  NOM_CMP='DY',
                  VALE_CALC=-0.841259653998,
                  VALE_REFE=-0.81,
                  CRITERE='RELATIF',
                  PRECISION=0.04,),
          )

#
# --------------------------------------------------------------------
#                CALCUL DE LA REACTION A L'ENCASTREMENT
# --------------------------------------------------------------------
#                COMPARAISON SOURCE EXTRENE
# --------------------------------------------------------------------


REACENC=POST_RELEVE_T(
                 ACTION=_F(  RESULTAT = RESU,
                          INST = 1.,
                          INTITULE = 'ENCASTREMENT',
                          NOM_CHAM = 'REAC_NODA',
                          REPERE = 'GLOBAL',
                          OPERATION = 'EXTRACTION',
                          GROUP_NO = 'LBC4',
                          RESULTANTE = ('DX', 'DY',))
              )

#

TEST_TABLE(CRITERE='RELATIF',
           REFERENCE='SOURCE_EXTERNE',
           PRECISION=0.016,
           VALE_CALC=17496.6104063,
           VALE_REFE=1.7229580000000002E4,
           NOM_PARA='DY',
           TABLE=REACENC,)

#
# --------------------------------------------------------------------
#                            CALCUL NON LINEAIRE - MODE MIXTE
# --------------------------------------------------------------------
#

RESU2=STAT_NON_LINE(
                          SOLVEUR=_F(SYME='OUI',),
                          MODELE=MOD,
                          CHAM_MATER=CHMAT,EXCIT=(
                          _F(  CHARGE = BLOQ),
                                _F(  CHARGE = DEPL,   FONC_MULT = FONC),
                                _F(  CHARGE = LIAI),),
                          CONTACT  = CONT2,
                          COMPORTEMENT=_F(  RELATION = 'ELAS'),
                          NEWTON=_F(  MATRICE = 'ELASTIQUE'),
                          INCREMENT=_F(  LIST_INST = LINST),
                          CONVERGENCE=_F(
                                        RESI_GLOB_MAXI = 1.E-6,
                                        ITER_GLOB_MAXI = 50,
                                        ARRET = 'OUI'),
                          INFO=1
                       )

TAB_VC3=POST_RELEVE_T(ACTION=_F(OPERATION='EXTRACTION',
                               INTITULE='JEU METHODE VERIF ',
                               RESULTAT=RESU2,
                               NOM_CHAM='CONT_NOEU',
                               GROUP_NO='CEP2',
                               NOM_CMP=('JEU',),),);

IMPR_TABLE(TABLE=TAB_VC3);

TEST_TABLE(
           VALE_CALC=-1.74853265065,
           NOM_PARA='JEU',
           TYPE_TEST='MIN',
           TABLE=TAB_VC3,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

TEST_TABLE(
           VALE_CALC=3.27994475079,
           NOM_PARA='JEU',
           TYPE_TEST='MAX',
           TABLE=TAB_VC3,
           FILTRE=_F(NOM_PARA='NUME_ORDRE',
                     VALE_I=1,),
           )

FIN()
#
