#########################################################################
# VALIDATION DU SOLVEUR MUMPS EN MODULANT:
#     - POUR LA PRISE EN COMPTE DES DIRICHLETS (AFFE_CHAR_CINE/MECA)
#     - POUR LES OPTIONS DE TYPE DE PB DE MUMPS (SYMDEF/SYMGEN/NONSYM)
#     - POUR LE PARALLELISME (CENTRALISE, MAIL_DISPERSE, SOUS_DOMAINE).
#     - LES RENUMEROTEURS UTILISES (METIS, SCOTCH, PORD, AMD, AMF, QAMD).
#
# WARNING: EN MODE PARALLELE, PAS PLUS DE 4 PROCS CAR LES 3 DERNIERS
#          CALCULS SONT BASES SUR UNE REPARTITION EN 4 SOUS-DOMAINES.
#          POUR LANCER SUR PLUS DE PROC, AUGMENTER NB_PART EN CONSEQUENCE.
# PARALLELE: VALEUR CONSEILLEE 2 PROCS
#########################################################################

DEBUT(  CODE=_F(NOM='MUMPS01A',NIV_PUB_WEB='INTERNET'),DEBUG=_F(SDVERI='OUI'))
MA=LIRE_MAILLAGE(FORMAT='MED')
DEFI_GROUP(reuse=MA, MAILLAGE=MA, CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI'))

MO=AFFE_MODELE(   MAILLAGE=MA,
           AFFE=_F( TOUT = 'OUI',  PHENOMENE = 'MECANIQUE', MODELISATION = '3D')
                  )
#
MAT=DEFI_MATERIAU(   ELAS=_F( E = 1.E+05,  NU = 0.3, RHO=9800.)  )

#
CHAM_MAT=AFFE_MATERIAU(   MAILLAGE=MA,  AFFE=_F( TOUT = 'OUI', MATER = MAT) )

#
CHAR=AFFE_CHAR_MECA( MODELE=MO,
       DDL_IMPO=  _F( GROUP_MA = 'BASE1',  DZ = 0., DY = 0.),
       PESANTEUR=_F(GRAVITE=9.81,
                    DIRECTION=(0., 0., -1.)))
CHARP=AFFE_CHAR_MECA( MODELE=MO, PESANTEUR=_F(GRAVITE=9.81,
                                              DIRECTION=(0., 0., -1.)))
CHARCI=AFFE_CHAR_CINE( MODELE=MO, MECA_IMPO=  _F( GROUP_NO = 'BASE1',  DX = 10.))
CHARCI3=AFFE_CHAR_CINE( MODELE=MO, MECA_IMPO=  _F( GROUP_NO = 'BASE1',  DX = 10., DY=0., DZ=0.))

L1=DEFI_LIST_REEL(VALE=(1., 2.))

# 1 : calcul de reference (METIS)
#----------------------------------------
RESU0=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), 
                                   _F(CHARGE = CHARCI)
                                    ),
                           SOLVEUR=_F(METHODE='MULT_FRONT'), 
                   )
TEST_RESU( RESU=_F(  RESULTAT = RESU0,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL', REFERENCE='NON_REGRESSION',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

# 2.1.1 : MUMPS (centralise) charge_dualisee + charge_cine + SYMGEN + AMD:
# -----------------------------------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='CENTRALISE'))
RESUM1=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS',TYPE_RESOL='SYMGEN',ELIM_LAGR2='NON',PCENT_PIVOT=20,
                        RENUM='AMD'))
TEST_RESU( RESU=_F(  RESULTAT = RESUM1,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

# 2.1.2 : MUMPS (centralise) charge_dualisee + charge_cine + SYMGEN + AMF:
# -----------------------------------------------------------------------
RESUM2=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS',TYPE_RESOL='SYMGEN',ELIM_LAGR2='NON',
                          RENUM='AMF'))
TEST_RESU( RESU=_F(  RESULTAT = RESUM2,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

# 2.1.3 : MUMPS (centralise) charge_dualisee + charge_cine + SYMGEN + SCOTCH:
# ---------------------------------------------------------------------------
RESUM3=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS',TYPE_RESOL='SYMGEN',ELIM_LAGR2='NON',
                          RENUM='SCOTCH'))
TEST_RESU( RESU=_F(  RESULTAT = RESUM3,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
     GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

# 2.1.4 : MUMPS (centralise) charge_dualisee + charge_cine + SYMGEN + PORD:
# ---------------------------------------------------------------------------
RESUM4=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS',TYPE_RESOL='SYMGEN',ELIM_LAGR2='NON',
                          RENUM='PORD'))
TEST_RESU( RESU=_F(  RESULTAT = RESUM4,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

# 2.1.5 : MUMPS (centralise) charge_dualisee + charge_cine + SYMGEN + METIS:
# ---------------------------------------------------------------------------
RESUM5=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS',TYPE_RESOL='SYMGEN',ELIM_LAGR2='NON',
                          RENUM='METIS'))
TEST_RESU( RESU=_F(  RESULTAT = RESUM5,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

# 2.1.6 : MUMPS (centralise) charge_dualisee + charge_cine + SYMGEN + QAMD:
# ---------------------------------------------------------------------------
RESUM6=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS',TYPE_RESOL='SYMGEN',ELIM_LAGR2='NON',
                          RENUM='QAMD'))
TEST_RESU( RESU=_F(  RESULTAT = RESUM6,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

# 2.2 : MUMPS (centralise) charge_cine + SYMDEF:
# ----------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='CENTRALISE'))
RESU3=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHARCI3),_F(CHARGE = CHARP)  ),
                   SOLVEUR=_F(METHODE='MUMPS', TYPE_RESOL='SYMDEF',NPREC=-8))
TEST_RESU( RESU=_F(  RESULTAT = RESU3,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

#  2.3 : MUMPS (centralise)charge_dualisee + charge_cine + NONSYM:
# -----------------------------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='CENTRALISE'))
RESU4=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS',TYPE_RESOL='NONSYM',RESI_RELA=-1.,ELIM_LAGR2='NON'))
TEST_RESU( RESU=_F(  RESULTAT = RESU4,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))


# 3.1 : MUMPS (distribue par mailles) charge_dualisee + charge_cine + SYMGEN:
# ----------------------------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='MAIL_CONTIGU'))
RESU5=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS', TYPE_RESOL='SYMGEN',ELIM_LAGR2='NON'))
TEST_RESU( RESU=_F(  RESULTAT = RESU5,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

# 3.2 : MUMPS (distribue par mailles) charge_cine + SYMDEF:
# ----------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='MAIL_CONTIGU'))
RESU6=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHARCI3),_F(CHARGE = CHARP)  ),
                   SOLVEUR=_F(METHODE='MUMPS', TYPE_RESOL='SYMDEF'))
TEST_RESU( RESU=_F(  RESULTAT = RESU6,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

#  3.3 : MUMPS (distribue par mailles) charge_dualisee + charge_cine + NONSYM:
# ----------------------------------------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='MAIL_CONTIGU'))
RESU7=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS', TYPE_RESOL='NONSYM',RESI_RELA=-1.,ELIM_LAGR2='NON'))
TEST_RESU( RESU=_F(  RESULTAT = RESU7,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))



SDFETI = DEFI_PART_FETI(
                MODELE=MO,
                NB_PART=4,
                EXCIT=(_F(CHARGE=CHAR)),
                METHODE='KMETIS',
                NOM_GROUP_MA='SD');
# 4.1 : MUMPS (distribue par sous-domaines) charge_dualisee + charge_cine + SYMGEN:
# ----------------------------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='SOUS_DOMAINE',PARTITION=SDFETI))
RESU8=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS', TYPE_RESOL='SYMGEN',ELIM_LAGR2='NON'))
TEST_RESU( RESU=_F(  RESULTAT = RESU8,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

# 4.2 : MUMPS (distribue par sous-domaines) charge_cine + SYMDEF:
# ----------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='SOUS_DOMAINE',PARTITION=SDFETI))
RESU9=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHARCI3),_F(CHARGE = CHARP)  ),
                   SOLVEUR=_F(METHODE='MUMPS', TYPE_RESOL='SYMDEF'))
TEST_RESU( RESU=_F(  RESULTAT = RESU9,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

#  4.3 : MUMPS (distribue par sous-domaines) charge_dualisee + charge_cine + NONSYM:
# -----------------------------------------------------------------
MODI_MODELE(reuse=MO, MODELE=MO, PARTITION=_F(PARALLELISME='SOUS_DOMAINE',PARTITION=SDFETI))
RESU10=MECA_STATIQUE(       MODELE=MO, CHAM_MATER=CHAM_MAT, LIST_INST=L1,INFO=1,
                           EXCIT=( _F(CHARGE = CHAR), _F(CHARGE = CHARCI)  ),
                   SOLVEUR=_F(METHODE='MUMPS', TYPE_RESOL='NONSYM',RESI_RELA=-1.,ELIM_LAGR2='NON'))
TEST_RESU( RESU=_F(  RESULTAT = RESU10,  NUME_ORDRE = 1, NOM_CHAM = 'DEPL',REFERENCE='AUTRE_ASTER',
                  GROUP_NO='C',NOM_CMP = 'DZ', VALE =-4.1607571899560E+01 ,PRECISION=1.e-8 ))

