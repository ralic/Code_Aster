                                          SY = 2.E8)
                          )

#

CHMAT=AFFE_MATERIAU(
                            MAILLAGE=MAIL,
                            AFFE=_F(  TOUT = 'OUI',
                                   MATER = MATER,
                                   )

                         )

#

CARBA=AFFE_CARA_ELEM(     MODELE=MOD,
                          BARRE=_F(  GROUP_MA = 'TOUT',
                                     SECTION = 'GENERALE',
                                     CARA = ( 'A', ),
                                     VALE = ( 5.E-4,))
