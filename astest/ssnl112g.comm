UELAS=MECA_STATIQUE(

                     MODELE=MOD,
                 CHAM_MATER=CHMTM,
                 CARA_ELEM=CARBA,EXCIT=(
                _F(  CHARGE = LIAISON),),
                  LIST_INST=LINST,


