# Etude du choc de deux poutres maillees 3D
# en traction compression.
# Les deux poutres sont mobiles
# CONTACT avec la methode continue, formulation en vitesse

DEBUT( CODE=_F( NOM ='SDNL111F',NIV_PUB_WEB='INTERNET'),)

PRE_GIBI();
MAIL=LIRE_MAILLAGE()

MAIL=DEFI_GROUP( reuse=MAIL,
                MAILLAGE=MAIL,
                CREA_GROUP_NO=_F(TOUT_GROUP_MA='OUI'),
               )

MODELE=AFFE_MODELE(MAILLAGE=MAIL,
       AFFE=(
             _F(GROUP_MA=('POU1','POU2'),
                PHENOMENE='MECANIQUE',
                MODELISATION='3D',),
             ),)


MAIL=MODI_MAILLAGE(reuse=MAIL,
                   MAILLAGE=MAIL,
                   ORIE_PEAU_3D=(
                                 _F(GROUP_MA='CNT_POU1',),
                                 _F(GROUP_MA='CNT_POU2',),
                                  ),)            
             

MATER=DEFI_MATERIAU(ELAS=_F(E=2.E11,NU=0.3,RHO=7800.,),)

CHMAT=AFFE_MATERIAU(MAILLAGE=MAIL,
                    AFFE=(
                          _F(GROUP_MA=('POU1'), 
                             MATER=MATER,),
                          _F(GROUP_MA=('POU2'), 
                             MATER=MATER,),
                          ),)


# *******************************************

CONDLIM=AFFE_CHAR_MECA(MODELE=MODELE,
                       DDL_IMPO=(
                                 _F(TOUT='OUI',
                                    DX=0.,DY=0.),
                                  ),
                                 )

VITINI=CREA_CHAMP(   OPERATION='AFFE', 
                     TYPE_CHAM='NOEU_DEPL_R',
                     MAILLAGE=MAIL,
                     AFFE=(_F( GROUP_NO = 'POU1',
                               NOM_CMP = 'DZ',
                               VALE = -1.0),
                           _F( GROUP_NO = 'POU2',
                                NOM_CMP = 'DZ',
                                VALE = 1.0),
                                ),)

                                   
# *******************************************


CONTACT=AFFE_CHAR_MECA(MODELE=MODELE,
                       CONTACT=_F(METHODE='CONTINUE',
                                    FORMULATION='VITE',
                                    NORMALE='MAIT_ESCL',
                                    COEF_REGU_CONT=1.0E-6,
                                    GROUP_MA_MAIT='CNT_POU1',
                                    GROUP_MA_ESCL='CNT_POU2',),)

 
# ************************************************************
# 
L_INST=DEFI_LIST_REEL(DEBUT=0.,
                      INTERVALLE=_F(JUSQU_A=5.0E-4,
                                    PAS=5.0E-6,
                                    ),)


U=DYNA_NON_LINE(MODELE=MODELE,
                CHAM_MATER=CHMAT,
                EXCIT=(
                       _F(CHARGE=CONTACT,),
                       _F(CHARGE=CONDLIM,),
                       ),
                ETAT_INIT=_F(VITE= VITINI),       
                COMP_INCR=_F(RELATION='ELAS',
                             DEFORMATION='PETIT',),                
                INCREMENT=_F(LIST_INST=L_INST,
                             SUBD_METHODE='UNIFORME', SUBD_PAS=3,
                             SUBD_PAS_MINI=1.E-8,),
                SCHEMA_TEMPS=_F(SCHEMA='THETA_METHODE',
                                FORMULATION='DEPLACEMENT',
                                THETA=0.7,),
#                NEWTON=_F(MATRICE='ELASTIQUE',),
                CONVERGENCE =_F(RESI_GLOB_MAXI = 1.E-3,),
                SOLVEUR=_F(METHODE='MULT_FRONT',),)

##############################################
# TEST DE LA CONFORMITE AVEC LA SOLUTION DE REFERENCE ANALYTIQUE
##############################################

DEPA=RECU_FONCTION(RESULTAT=U,
                TITRE='DEPA',
                NOM_CHAM='DEPL',
                NOM_CMP='DZ',
                GROUP_NO='A',
                INTERPOL='LIN',)
IMPR_FONCTION(FORMAT='TABLEAU',
           COURBE=_F(FONCTION=DEPA,),)
DEPB=RECU_FONCTION(RESULTAT=U,
                TITRE='DEPB',
                NOM_CHAM='DEPL',
                NOM_CMP='DZ',
                GROUP_NO='B',
                INTERPOL='LIN',)
IMPR_FONCTION(FORMAT='TABLEAU',
           COURBE=_F(FONCTION=DEPB,),)

VITA=RECU_FONCTION(RESULTAT=U,
                TITRE='VITA',
                NOM_CHAM='VITE',
                NOM_CMP='DZ',
                GROUP_NO='A',
                INTERPOL='LIN',)
IMPR_FONCTION(FORMAT='TABLEAU',
           COURBE=_F(FONCTION=VITA,),)

TEST_RESU(
      RESU=(
            _F(
                  RESULTAT = U,
                  INST = 0.5E-5,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'A',
                  NOM_CMP = 'DZ',
                  VALE = -0.5E-5,
                  PRECISION = (0.001, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = U,
                  INST = 1.5E-5,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'A',
                  NOM_CMP = 'DZ',
                  VALE = -1.0E-5,
                  PRECISION = (0.001, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = U,
                  INST = 25.E-5,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'A',
                  NOM_CMP = 'DZ',
                  VALE = -1.0E-5,
                  PRECISION = (0.001, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = U,
                  INST = 34.E-5,
                  NOM_CHAM = 'DEPL',
                  GROUP_NO = 'A',
                  NOM_CMP = 'DZ',
                  VALE = -1.0E-5,
                  PRECISION = (0.001, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
            _F(
                  RESULTAT = U,
                  INST = 50.E-5,
                  NOM_CHAM = 'VITE',
                  GROUP_NO = 'A',
                  NOM_CMP = 'DZ',
                  VALE = 1.0,
                  PRECISION = (0.001, 1.E-7),
                  REFERENCE='ANALYTIQUE',
                  CRITERE = 'RELATIF'),
                  ),)




FIN();
#
